    			else
    				PlaceAtMe "A1V1_NecroRg_f" 1, 0, 0
    			endif
    		elseif ( summon <= 20 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroRg2" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroRg2_f" 1, 0, 0
    			endif
    		else
