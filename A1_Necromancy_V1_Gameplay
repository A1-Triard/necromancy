SCPT
SCHD nc_de_skelly_pauldron 4 0 1 637 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_de_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_dunmer_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

SCPT
SCHD nc_de_skelly_pauldron_f 4 0 1 643 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_de_skelly_pauldron_f
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk_f" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_dunmer_pauldron_sk_f"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_de_skelly_NPC_f" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    
    

SCPT
SCHD nc_death_add_altmer 3 6 1 1916 56
SCVR temp;hi;low;id;tempLong;count;hash;npcfile;skills;tempf;;
SCDT 
SCTX
    Begin nc_death_add_altmer
    
    long id
    long tempLong
    long count
    short temp
    short hi
    short low
    long hash
    long npcfile
    long skills
    float tempf
    
    setx id to xGetBaseID
    setx tempLong to xStringBuild "tempA1Necro"
    xFileRewind tempLong
    xFileWriteString tempLong, id
    xFileWriteShort tempLong, 0
    xFileRewind tempLong
    set hash to 0
    set temp to 1
    whilex ( temp )
    	setx count,temp to xFileReadShort tempLong, 1
    	if ( temp != 0 )
    		set hi to temp / 256
    		set low to temp - 256 * hi
    		set hash to hash * 1664525 + low + 1013904223
    		if ( hi != 0 )
    			set hash to hash * 1664525 + hi + 1013904223
    		endif
    	endif
    endwhile
    
    set temp to 0
    if ( hash == -2147483648)
    	set npcfile to 0
    elseif ( hash < 0 )
    	set temp to 1
    	set npcfile to -hash
    else
    	set npcfile to hash
    endif
    
    ifx ( temp )
    	setx npcfile to xStringBuild "A1NPC_0%d", npcfile
    else
    	setx npcfile to xStringBuild "A1NPC_%d", hash
    endif
    
    xFileRewind npcfile
    setx count,temp to xFileReadShort npcfile, 1
    if ( temp == 20558 )
    	set count to 1
    else
    	set count to 0
    endif
    
    ifx ( count )
    	xFileReadString npcfile
    	setx id to xFileReadString npcfile
    	xAddItem id 1
    endif
    
    Additem "nc_altmer_pauldron" 1
    AddItem "nc_altmer_skull" 1
    AddItem A1_NecroMarker, 1
    
    set tempf to GetBlock
    set skills to tempf
    set tempf to GetMediumArmor
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills1 to skills
    set tempf to GetHeavyArmor
    set skills to tempf
    set tempf to GetBluntWeapon
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills2 to skills
    set tempf to GetLongBlade
    set skills to tempf
    set tempf to GetAxe
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills3 to skills
    set tempf to GetSpear
    set skills to tempf
    set tempf to GetDestruction
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills4 to skills
    set tempf to GetAlteration
    set skills to tempf
    set tempf to GetIllusion
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills5 to skills
    set tempf to GetConjuration
    set skills to tempf
    set tempf to GetMysticism
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills6 to skills
    set tempf to GetRestoration
    set skills to tempf
    set tempf to GetUnarmored
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills7 to skills
    set tempf to GetLightArmor
    set skills to tempf
    set tempf to GetShortBlade
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills8 to skills
    set tempf to GetMarksman
    set skills to tempf
    set tempf to GetHandToHand
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills9 to skills
    
    if ( player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    else
    	player->Removeitem "nc_soulgem_blackened" 1
    	set tempLong to hash / 65536
    	set A1V1_SoulGemHashHi to tempLong
    	set hash to hash - tempLong * 65536
    	set A1V1_SoulGemHashLow to hash
    	player->Additem "nc_SoulGem_altmer" 1
    	MessageBox "Вы захватили душу."
    endif
    
    StopScript nc_death_add_altmer
    
    End

SCPT
SCHD nc_death_add_altmer_f 3 6 1 1922 56
SCVR temp;hi;low;id;tempLong;count;hash;npcfile;skills;tempf;;
SCDT 
SCTX
    Begin nc_death_add_altmer_f
    
    long id
    long tempLong
    long count
    short temp
    short hi
    short low
    long hash
    long npcfile
    long skills
    float tempf
    
    setx id to xGetBaseID
    setx tempLong to xStringBuild "tempA1Necro"
    xFileRewind tempLong
    xFileWriteString tempLong, id
    xFileWriteShort tempLong, 0
    xFileRewind tempLong
    set hash to 0
    set temp to 1
    whilex ( temp )
    	setx count,temp to xFileReadShort tempLong, 1
    	if ( temp != 0 )
    		set hi to temp / 256
    		set low to temp - 256 * hi
    		set hash to hash * 1664525 + low + 1013904223
    		if ( hi != 0 )
    			set hash to hash * 1664525 + hi + 1013904223
    		endif
    	endif
    endwhile
    
    set temp to 0
    if ( hash == -2147483648)
    	set npcfile to 0
    elseif ( hash < 0 )
    	set temp to 1
    	set npcfile to -hash
    else
    	set npcfile to hash
    endif
    
    ifx ( temp )
    	setx npcfile to xStringBuild "A1NPC_0%d", npcfile
    else
    	setx npcfile to xStringBuild "A1NPC_%d", hash
    endif
    
    xFileRewind npcfile
    setx count,temp to xFileReadShort npcfile, 1
    if ( temp == 20558 )
    	set count to 1
    else
    	set count to 0
    endif
    
    ifx ( count )
    	xFileReadString npcfile
    	setx id to xFileReadString npcfile
    	xAddItem id 1
    endif
    
    Additem "nc_altmer_pauldron_f" 1
    AddItem "nc_altmer_skull_f" 1
    AddItem A1_NecroMarker, 1
    
    set tempf to GetBlock
    set skills to tempf
    set tempf to GetMediumArmor
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills1 to skills
    set tempf to GetHeavyArmor
    set skills to tempf
    set tempf to GetBluntWeapon
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills2 to skills
    set tempf to GetLongBlade
    set skills to tempf
    set tempf to GetAxe
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills3 to skills
    set tempf to GetSpear
    set skills to tempf
    set tempf to GetDestruction
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills4 to skills
    set tempf to GetAlteration
    set skills to tempf
    set tempf to GetIllusion
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills5 to skills
    set tempf to GetConjuration
    set skills to tempf
    set tempf to GetMysticism
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills6 to skills
    set tempf to GetRestoration
    set skills to tempf
    set tempf to GetUnarmored
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills7 to skills
    set tempf to GetLightArmor
    set skills to tempf
    set tempf to GetShortBlade
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills8 to skills
    set tempf to GetMarksman
    set skills to tempf
    set tempf to GetHandToHand
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills9 to skills
    
    if ( player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    else
    	player->Removeitem "nc_soulgem_blackened" 1
    	set tempLong to hash / 65536
    	set A1V1_SoulGemHashHi to tempLong
    	set hash to hash - tempLong * 65536
    	set A1V1_SoulGemHashLow to hash
    	player->Additem "nc_SoulGem_altmer" 1
    	MessageBox "Вы захватили душу."
    endif
    
    StopScript nc_death_add_altmer_f
    
    End

