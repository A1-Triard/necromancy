    set tempf to GetMarksman
    set skills to tempf
    set tempf to GetHandToHand
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills9 to skills
    
    if ( player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    else
    	player->Removeitem "nc_soulgem_blackened" 1
    	set tempLong to hash / 65536
    	set A1V1_SoulGemHashHi to tempLong
    	set hash to hash - tempLong * 65536
    	set A1V1_SoulGemHashLow to hash
    	player->Additem "nc_SoulGem_arg" 1
    	MessageBox "Вы захватили душу."
    endif
    
    StopScript nc_death_add_argonian
    
    End

