SCHD nc_fire_corpse_obj 1 0 0 179 8
SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEG5jX2ZpcmVfcGF1bGRyb24BAAcBAQAQLgDN5fHy6CDh7uv8+OUsIPfl7CDu5O3uIPLl6+4g5+Ag8ODnIC0g7eXz5O7h7e4uAAAJAQkBAQE=
SCTX
    begin nc_fire_corpse_obj
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_fire_pauldron" 1
    	else
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    	endif
    endif
    
    end
    

