    			set gender to 0
    			MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
    		endif
    	endif
    	if ( gender == 1 )
    		if ( player->GetItemCount "misc_clothbolt_01" < 2 )
    			set gender to 0
    			MessageBox "Вам нужны два рулона синей ткани для этого ритуала."
