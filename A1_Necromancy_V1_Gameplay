    begin nc_comp3Script_we_f
    
    short setUp
    long hash
    long skills1
    long skills2
    long skills3
    long skills4
    long skills5
    long skills6
    long skills7
    long skills8
    long skills9
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
    float timer
    float sheatheTimer
    float moveTimer
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short doOnce4
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
    short counter
    short p_Check
    short emb_hook_nominion
    short call
    short travel
    long temp
    long count
    long npcfile
    long name
    
    if ( MenuMode == 1 )
    	if ( setUp == 2 )
    		set setUp to 3
    	endif
    	return
    endif
    
    if ( setUp == 2 )
    	return
    endif
    
    set temp to 0
    if ( setUp == 0 )
    	set setUp to 1
    	set hash to A1V1_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V1_SoulGemHashLow
    	set hash to hash + temp
    	set A1V1_SoulGemHashHi to 0
    	set A1V1_SoulGemHashLow to 0
    	set skills1 to A1V1_NecroSkills1
    	set skills2 to A1V1_NecroSkills2
    	set skills3 to A1V1_NecroSkills3
    	set skills4 to A1V1_NecroSkills4
    	set skills5 to A1V1_NecroSkills5
    	set skills6 to A1V1_NecroSkills6
    	set skills7 to A1V1_NecroSkills7
    	set skills8 to A1V1_NecroSkills8
    	set skills9 to A1V1_NecroSkills9
    	set companion to 1
    	set emb_hook_nominion to 1
    	AddTopic "- Бой"
    	AddTopic "- Сопровождение"
    	AddTopic "- Инструкции"
    	AddTopic "- Перемещение"
    	return
    elseif ( setUp == 3 )
    	set temp to 1
    endif
    
    ifx ( temp )
    	set setUp to 1
    	ifx ( name )
    		xSetName name
    		set name to 0
    	endif
    endif
    
    if ( weapUse == 1 )
    	if ( Player->GetWeaponType <= 8 )
    		setmarksman 0
    	else
    		setmarksman 200
    	endif
    endif
    
    if ( combatCheck == 1 )
    	set sheathetimer to sheatheTimer + GetSecondsPassed
    	if ( GetSoundPlaying, "Weapon Swish" == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, crossbowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, bowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( sheatheTimer > 4 )
    		if ( GetSoundPlaying "Weapon Swish" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "crossbowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "bowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		endif
    	endif
    endif
    
    if ( soundPlayed == 1 )
    	equip "silver dagger"
    	removeitem "silver dagger" 1
    	; messagebox "Working"
    	set soundPlayed to 0
    	set combatCheck to 0
    	set combatDone to 1
    endif
    
    if ( combatDone == 1 )
    	set timer to timer + GetSecondsPassed
    	if ( timer > 6 )
    		set timer to 0
    		set combatDone to 0
    		set warp to 0
    	endif
    endif
    
    set temp to 0
    if ( OnActivate == 1 )
    	set temp to 1
    endif
    ifx ( temp )
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx name to xGetName
    		setx temp to xFileReadString npcfile
    		xSetName temp
    	else
    		set name to 0
    	endif
    	set setUp to 2
    	Activate
    	return
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetPCSneaking == 1 )
    		ForceSneak
    	else
    		ClearForceSneak
    	endif
    
    	if ( player->GetEffect sEffectLevitate == 1 )
    		if ( flyCheck == 0 )
    			Cast nc_comp2_lev_bog player
    			AddSpell nc_comp2_lev
    			set flyCheck to 1
    		endif
    	else
    		if ( flyCheck == 1 )
    			RemoveSpell nc_comp2_lev
    			set flyCheck to 0
    		endif
    	endif
    
    	if ( player->GetEffect sEffectWaterWalking == 1 )
    		if ( wwCheck == 0 )
    			Cast nc_comp2_ww_bog player
    			AddSpell nc_comp2_ww
    			set wwCheck to 1
    		endif
    	else
    		if ( wwCheck == 1 )
    			RemoveSpell nc_comp2_ww
    			set wwCheck to 0
    		endif
    	endif
    
    	if ( flyCheck == 1 )
    		if ( GetWeaponDrawn == 0 )
    			set playz to ( Player->GetPos z )
    			SetPos z playz
    		endif
    	endif
    
    	if ( GetWeaponDrawn == 1 )
    		if ( flyCheck == 0 )
    			set combatCheck to 1
    		else
    			set combatDone to 1
    		endif
    		set timer to 0
    		set warp to 1
    		return
    	endif
    
    	set ax to ( Player->GetPos x )
    	set ay to ( Player->GetPos y )
    
    	if ( doOnce == 0 )
    		set bx to ( Player->GetPos x )
    		set by to ( Player->GetPos y )
    		set bz to ( Player->GetPos z )
    		set doOnce to 1
    	endif
    
    	set t1 to ( ax - bx )
    	set t1 to ( t1 * t1 )
    	set t2 to ( ay - by )
    	set t2 to ( t2 * t2 )
    	set t1 to ( t1 + t2 )
    	set coDist to ( GetSquareRoot, t1 )
    
    	if ( coDist > 360 )
    		set doOnce to 0
    	endif
    
    	if ( coDist > 180 )
    		if ( doOnce2 == 0 )
    			set cx to ( Player->GetPos x )
    			set cy to ( Player->GetPos y )
    			set cz to ( Player->GetPos z )
    			set doOnce2 to 1
    		endif
    	endif
    
    	set t3 to ( ax - cx )
    	set t3 to ( t3 * t3 )
    	set t4 to ( ay - cy )
    	set t4 to ( t4 * t4 )
    	set t3 to ( t3 + t4 )
    	set coDist2 to ( GetSquareRoot, t3 )
    
    	if ( coDist2 > 360 )
    		set doOnce2 to 0
    	endif
    
    	if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    	if ( see_p_warlord == 0 )
    
    	if ( warp == 0 )
    		if ( coDist > 350 )
    			if ( GetDistance Player > 680 )
    				SetPos x bx
    				SetPos y by
    				SetPos z bz
    				AiFollow Player 0 0 0 0
    			endif
    		endif
    	endif
    
    	endif
    	endif
    	endif
    endif
    
    if ( see_f_warlord == 1 )
    	if ( nc_f_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_fr_warlord == 1 )
    	if ( nc_fr_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_p_warlord == 1 )
    	if ( nc_p_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
    
    if ( compMove == 1 )
    	if ( c_move == 0 )
    		if ( GetCurrentAIPackage == 3 )
    			if ( GetDistance Player < 70 )
    				AIWander 300 0 0 0 0
    				set c_move to 1
    			endif
    		endif
    	endif
    endif
    
    if ( c_move == 1 )
    	if ( GetDistance Player > 100 )
    		AIFollow Player 0 0 0 0
    		set c_move to 0
    	endif
    endif
    
    if ( oneTimeMove == 1 )
    	set moveTimer to moveTimer + GetSecondsPassed
    	if ( moveTimer > 4 )
    		set moveTimer to 0
    		AiFollow Player 0 0 0 0
    		set oneTimeMove to 0
    	endif
    endif
    
    if ( call == 0 )
    	if ( nc_callrev == 1 )
    		AIWander 0 0 0
    		set call to 1
    	elseif ( nc_callrev == 2 )
    		AIFollow player 0 0 0 0
    		set call to 2
    	elseif ( nc_callrev == 3 )
    		AIWander 500 0 0
    		set call to 3
    	endif
    	if ( nc_callmessage == 0 )
    		set nc_callmessage to 1
    	endif
    endif
    
    if ( nc_callrev == 0 )
    	set call to 0
    endif
    
    if ( travel > 0 )
    	if ( CellChanged == 0 )
    		if ( travel == 1 )
    			AIWander 500 0 0
    			PositionCell -23265.2344, -15409.3721, 556.0000, 0, "Balmora"
    			set travel to 0
    		endif 
    		if ( travel == 2 )
    			AIWander 500 0 0
    			PositionCell -14688.3975, 52477.6445, 2119.9502, 0, "Ald-ruhn"
    			set travel to 0
    		endif 
    		if ( travel == 3 )
    			AIWander 500 0 0
    			PositionCell 48308.0547, -81831.6328, 576.7466, 0, "Vivec"
    			set travel to 0
    		endif 
    		if ( travel == 4 )
    			AIWander 500 0 0
    			PositionCell 105102.6641, -61811.3477, 635.0835, 0, "Molag Mar"
    			set travel to 0
    		endif 
    		if ( travel == 5 )
    			AIWander 500 0 0
    			PositionCell 149019.1250, 29684.0938, 700.0000, 0, "Wolverine Hall"
    			set travel to 0
    		endif 
    		if ( travel == 6 )
    			AIWander 500 0 0
    			PositionCell 98660.6641, 111530.5625, 308.0195, 0, "Vos"
    			set travel to 0
    		endif 
    		if ( travel == 7 )
    			AIWander 500 0 0
    			PositionCell 59803.8828, 183183.3594, 314.2573, 0, "Dagon Fel"
    			set travel to 0
    		endif 
    		if ( travel == 8 )
    			AIWander 500 0 0
    			PositionCell -67142.3594, 139129.8594, 205.1514, 0, "Khuul"
    			set travel to 0
    		endif 
    		if ( travel == 9 )
    			AIWander 500 0 0
    			PositionCell -84440.8203, 92241.1953, 1054.9253, 0, "Gnisis"
    			set travel to 0
    		endif 
    		if ( travel == 10 )
    			AIWander 500 0 0
    			PositionCell 20736.9922, 38396.8008, 1161.2622, 0, "Ghostgate"
    			set travel to 0
    		endif
    		if ( travel == 11 )
    			AIWander 500 0 0
    			PositionCell -5.3433, 2476.1021, 173.3262, 0, "Mournhold, Plaza Brindisi Dorom"
    			set travel to 0
    		endif 
    	endif
    endif
    
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
    
    if ( counter < 20 )
    	set counter to counter + 1
    	return
    endif
    set counter to 0
    
    if ( GetAlchemy < 100 )
    	set temp to 1
    else
    	set temp to 0
    endif
    ifx ( temp)
    	SetAlchemy 100
    	set temp to skills1 / 256
    	SetBlock temp
    	set temp to skills1 - 256 * temp
    	SetMediumArmor temp
    	set temp to skills2 / 256
    	SetHeavyArmor temp
    	set temp to skills2 - 256 * temp
    	SetBluntWeapon temp
    	set temp to skills3 / 256
    	SetLongBlade temp
    	set temp to skills3 - 256 * temp
    	SetAxe temp
    	set temp to skills4 / 256
    	SetSpear temp
    	set temp to skills4 - 256 * temp
    	SetDestruction temp
    	set temp to skills5 / 256
    	SetAlteration temp
    	set temp to skills5 - 256 * temp
    	SetIllusion temp
    	set temp to skills6 / 256
    	SetConjuration temp
    	set temp to skills6 - 256 * temp
    	SetMysticism temp
    	set temp to skills7 / 256
    	SetRestoration temp
    	set temp to skills7 - 256 * temp
    	SetUnarmored temp
    	set temp to skills8 / 256
    	SetLightArmor temp
    	set temp to skills8 - 256 * temp
    	SetShortBlade temp
    	set temp to skills9 / 256
    	SetMarksman temp
    	set temp to skills9 - 256 * temp
    	SetHandToHand temp
    
    ;	set count to 0
    ;	if ( hash == -2147483648)
    ;		set temp to 0
    ;	elseif ( hash < 0 )
    ;		set count to 1
    ;		set temp to -hash
    ;	else
    ;		set temp to hash
    ;	endif
    ;
    ;	ifx ( count )
    ;		setx npcfile to xStringBuild "A1NPC_0%d", temp
    ;	else
    ;		setx npcfile to xStringBuild "A1NPC_%d", temp
    ;	endif
    ;
    ;	xFileRewind npcfile
    ;	setx count,temp to xFileReadShort npcfile, 1
    ;	if ( temp == 20558 )
    ;		set count to 1
    ;	else
    ;		set count to 0
    ;	endif
    ;	ifx ( count )
    ;		xFileReadString npcfile
    ;		setx temp to xFileReadString npcfile
    ;		whilex ( count )
    ;			setx count,temp to xFileReadShort npcfile, 1
    ;			if ( temp == 20563 )
    ;				set count to 1
    ;			else
    ;				set count to 0
    ;			endif
    ;			ifx ( count )
    ;				setx temp to xFileReadString npcfile
    ;				xAddSpell temp
    ;			endif
    ;		endwhile
    ;	endif
    endif
    
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
    
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
    endif
    
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
    endif
    
    if ( doOnce4 == 0 )
    	if ( GetItemCount,  "nc_bosmer_skull_f" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_bonemeal_01" < 15 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    endif
    
    if ( doOnce3 == 0 )
    	if ( GetHealth <= 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_bosmer_pauldron_sk_f" 1
    		set doOnce3 to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_control_ring 3 2 0 829 35
SCVR OnPCEquip;button;status;lkey;ltemp;;
SCDT 
SCTX
    begin nc_control_ring
    
    short OnPCEquip
    short button
    short status
    long lkey
    long ltemp
    
    if ( menumode )
    	return
    endif
    
    if ( OnPCEquip == 1 )
    	MessageBox "Кому вы хотите отдать приказ?", "Мертвецам", "Слугам", "И тем, и другим", "Никому", 
    	set OnPCEquip to 0
    	set status to 1
    endif
    
    if ( status == 1 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;revenants
    		set status to 2
    	elseif ( button == 1 ) ;warlords
    		set status to 3
    	elseif ( button == 2 ) ;both
    			set status to 4
    	elseif ( button == 3 ) ;none
    		set status to 5
    	endif
    elseif ( status == 2 ) ;revenants
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest_rev
    	set status to 0
    elseif ( status == 3 ) ;warlords
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	startscript nc_keytest_warlord
    	set status to 0
    elseif ( status == 4 ) ;both
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest
    	set status to 0
    elseif ( status == 5 ) ;none
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	set status to 0
    endif
    
    
    end
    

SCPT
SCHD nc_corpse_check 1 0 0 2770 6
SCVR delay;;
SCDT 
SCTX
    begin nc_corpse_check
    
    short delay
    
    set delay to ( delay + 1 )
    
    if ( delay < 30 )
    	return
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	startscript nc_mult_arg
    elseif ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	startscript nc_mult_alt
    elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	startscript nc_mult_bret
    elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	startscript nc_mult_bos
    elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	startscript nc_mult_dun
    elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	startscript nc_mult_imp
    elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	startscript nc_mult_khajiit
    elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	startscript nc_mult_nord
    elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	startscript nc_mult_orc
    elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	startscript nc_mult_rg
    elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	startscript nc_mult_arg_sk
    elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	startscript nc_mult_alt_sk
    elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	startscript nc_mult_bret_sk
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	startscript nc_mult_bos_sk
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	startscript nc_mult_dun_sk
    elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	startscript nc_mult_imp_sk
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	startscript nc_mult_khajiit_sk
    elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	startscript nc_mult_nord_sk
    elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	startscript nc_mult_orc_sk
    elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	startscript nc_mult_rg_sk
    elseif ( Player->GetItemCount "nc_arg_pauldron_f" >= 1 )
    	startscript nc_mult_arg_f
    elseif ( Player->GetItemCount "nc_altmer_pauldron_f" >= 1 )
    	startscript nc_mult_alt_f
    elseif ( Player->GetItemCount "nc_breton_pauldron_f" >= 1 )
    	startscript nc_mult_bret_f
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_f" >= 1 )
    	startscript nc_mult_bos_f
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_f" >= 1 )
    	startscript nc_mult_dun_f
    elseif ( Player->GetItemCount "nc_imperial_pauldron_f" >= 1 )
    	startscript nc_mult_imp_f
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_f" >= 1 )
    	startscript nc_mult_khajiit_f
    elseif ( Player->GetItemCount "nc_nord_pauldron_f" >= 1 )
    	startscript nc_mult_nord_f
    elseif ( Player->GetItemCount "nc_orc_pauldron_f" >= 1 )
    	startscript nc_mult_orc_f
    elseif ( Player->GetItemCount "nc_rg_pauldron_f" >= 1 )
    	startscript nc_mult_rg_f
    elseif ( Player->GetItemCount "nc_arg_pauldron_sk_f" >= 1 )
    	startscript nc_mult_arg_sk_f
    elseif ( Player->GetItemCount "nc_altmer_pauldron_sk_f" >= 1 )
    	startscript nc_mult_alt_sk_f
    elseif ( Player->GetItemCount "nc_breton_pauldron_sk_f" >= 1 )
    	startscript nc_mult_bret_sk_f
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk_f" >= 1 )
    	startscript nc_mult_bos_sk_f
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk_f" >= 1 )
    	startscript nc_mult_dun_sk_f
    elseif ( Player->GetItemCount "nc_imperial_pauldron_sk_f" >= 1 )
    	startscript nc_mult_imp_sk_f
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk_f" >= 1 )
    	startscript nc_mult_khajiit_sk_f
    elseif ( Player->GetItemCount "nc_nord_pauldron_sk_f" >= 1 )
    	startscript nc_mult_nord_sk_f
    elseif ( Player->GetItemCount "nc_orc_pauldron_sk_f" >= 1 )
    	startscript nc_mult_orc_sk_f
    elseif ( Player->GetItemCount "nc_rg_pauldron_sk_f" >= 1 )
    	startscript nc_mult_rg_sk_f
    elseif ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	startscript nc_mult_fire
    elseif ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	startscript nc_mult_frost
    elseif ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	startscript nc_mult_poison
    else
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    endif
    	
    
    set delay to 0
    
    if ( nc_has_corpse == 0 )
    	stopscript nc_corpse_check
    endif
    
    end

SCPT
SCHD nc_de_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_de_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_dunmer_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_dunmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
