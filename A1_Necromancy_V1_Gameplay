    	AddTopic "- Сопровождение"
    	AddTopic "- Инструкции"
    	AddTopic "- Перемещение"
    	return
    elseif ( setUp == 3 )
    	set temp to 1
    endif
    
    ifx ( temp )
    	set setUp to 1
    	ifx ( name )
    		xSetName name
    		set name to 0
    	endif
    endif
    
    if ( weapUse == 1 )
    	if ( Player->GetWeaponType <= 8 )
    		setmarksman 0
    	else
    		setmarksman 200
    	endif
    endif
    
    if ( combatCheck == 1 )
    	set sheathetimer to sheatheTimer + GetSecondsPassed
    	if ( GetSoundPlaying, "Weapon Swish" == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, crossbowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, bowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( sheatheTimer > 4 )
    		if ( GetSoundPlaying "Weapon Swish" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "crossbowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "bowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		endif
    	endif
    endif
    
    if ( soundPlayed == 1 )
    	equip "silver dagger"
    	removeitem "silver dagger" 1
    	; messagebox "Working"
    	set soundPlayed to 0
    	set combatCheck to 0
    	set combatDone to 1
    endif
    
    if ( combatDone == 1 )
    	set timer to timer + GetSecondsPassed
    	if ( timer > 6 )
    		set timer to 0
    		set combatDone to 0
    		set warp to 0
    	endif
    endif
    
    set temp to 0
    if ( OnActivate == 1 )
    	set temp to 1
    endif
    ifx ( temp )
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx name to xGetName
    		setx temp to xFileReadString npcfile
    		xSetName temp
    	else
    		set name to 0
    	endif
    	set setUp to 2
    	Activate
    	return
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetPCSneaking == 1 )
    		ForceSneak
    	else
    		ClearForceSneak
    	endif
    
    	if ( player->GetEffect sEffectLevitate == 1 )
    		if ( flyCheck == 0 )
    			Cast nc_comp2_lev_bog player
    			AddSpell nc_comp2_lev
    			set flyCheck to 1
    		endif
    	else
    		if ( flyCheck == 1 )
    			RemoveSpell nc_comp2_lev
    			set flyCheck to 0
    		endif
    	endif
    
    	if ( player->GetEffect sEffectWaterWalking == 1 )
    		if ( wwCheck == 0 )
    			Cast nc_comp2_ww_bog player
    			AddSpell nc_comp2_ww
    			set wwCheck to 1
    		endif
    	else
    		if ( wwCheck == 1 )
    			RemoveSpell nc_comp2_ww
    			set wwCheck to 0
    		endif
    	endif
    
    	if ( flyCheck == 1 )
    		if ( GetWeaponDrawn == 0 )
    			set playz to ( Player->GetPos z )
    			SetPos z playz
    		endif
    	endif
    
    	if ( GetWeaponDrawn == 1 )
    		if ( flyCheck == 0 )
    			set combatCheck to 1
    		else
    			set combatDone to 1
    		endif
    		set timer to 0
    		set warp to 1
    		return
    	endif
    
    	set ax to ( Player->GetPos x )
    	set ay to ( Player->GetPos y )
    
    	if ( doOnce == 0 )
    		set bx to ( Player->GetPos x )
    		set by to ( Player->GetPos y )
    		set bz to ( Player->GetPos z )
    		set doOnce to 1
    	endif
    
    	set t1 to ( ax - bx )
    	set t1 to ( t1 * t1 )
    	set t2 to ( ay - by )
    	set t2 to ( t2 * t2 )
    	set t1 to ( t1 + t2 )
    	set coDist to ( GetSquareRoot, t1 )
    
    	if ( coDist > 360 )
    		set doOnce to 0
    	endif
    
    	if ( coDist > 180 )
    		if ( doOnce2 == 0 )
    			set cx to ( Player->GetPos x )
    			set cy to ( Player->GetPos y )
    			set cz to ( Player->GetPos z )
    			set doOnce2 to 1
    		endif
    	endif
    
    	set t3 to ( ax - cx )
    	set t3 to ( t3 * t3 )
    	set t4 to ( ay - cy )
    	set t4 to ( t4 * t4 )
    	set t3 to ( t3 + t4 )
    	set coDist2 to ( GetSquareRoot, t3 )
    
    	if ( coDist2 > 360 )
    		set doOnce2 to 0
    	endif
    
    	if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    	if ( see_p_warlord == 0 )
    
    	if ( warp == 0 )
    		if ( coDist > 350 )
    			if ( GetDistance Player > 680 )
    				SetPos x bx
    				SetPos y by
    				SetPos z bz
    				AiFollow Player 0 0 0 0
    			endif
    		endif
    	endif
    
    	endif
    	endif
    	endif
    endif
    
    if ( see_f_warlord == 1 )
    	if ( nc_f_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_fr_warlord == 1 )
    	if ( nc_fr_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_p_warlord == 1 )
