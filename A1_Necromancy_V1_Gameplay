      setspeed 40
    endif
    
    if ( doOnce4 == 0 )
    	if ( GetItemCount,  "nc_bosmer_skull" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_bonemeal_01" < 15 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
