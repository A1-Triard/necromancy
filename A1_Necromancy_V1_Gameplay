    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroBreton2" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroBreton2_f" 1, 0, 0
    			endif
    		else
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroBreton3" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroBreton3_f" 1, 0, 0
    			endif
    		endif
    		PlaySound, "ThunderClap"
