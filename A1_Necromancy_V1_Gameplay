SCPT
SCHD A1_NecroOnWar 0 0 0 18 0
SCDT HBANQTFfTmVjcm9PbldhcgEB
SCTX
    begin A1_NecroOnWar
    	StopScript A1_NecroOnWar
    end

SCPT
SCHD A1_NecroWearHair_sc 0 4 0 243 30
SCVR invitem;invcount;invref;match;;
SCDT 
SCTX
    Begin A1_NecroWearHair_sc
    
    long invitem
    long invcount
    long invref
    long match
    
    if ( MenuMode == 1 )
    	return
    endif
    
    set match to 0
    setx invitem,invcount,invref to xInventory
    whilex ( invcount )
    	setx match to xStringMatch invitem, "^A1V1H"
    	ifx ( match )
    		set invcount to 0
    	else
    		setx invitem,invcount,invref to xNextStack invref
    	endif
    endwhile
    
    ifx ( match )
    	xEquip invitem
    endif
    
    StopScript A1_NecroWearHair_sc
    
    End

SCPT
SCHD A1V1_NecroMirror_sc 0 0 0 208 0
SCDT DAEGcGxheWVyBgECHyBY/RAgbxRuY19Tb3VsR2VtX2JsYWNrZW5lZCA+IDAMAQZwbGF5ZXLVEBRuY19Tb3VsR2VtX2JsYWNrZW5lZAEADAEGcGxheWVy1BARbmNfU291bEdlbV9wbGF5ZXIBAAcBAQAQQADC4Owg7fPm5e0g9+Xw7fvpIOrg7OXt/CDk8/gsIPfy7uH7IPHu5+Tg8vwg7vLw4Obl7ejlIPHi7uXpIOTz+OguAAAJARwQE0ExVjFfTmVjcm9NaXJyb3Jfc2MBAQ==
SCTX
    begin A1V1_NecroMirror_sc
    
    if ( player->GetItemCount nc_SoulGem_blackened > 0 )
    	player->RemoveItem nc_SoulGem_blackened, 1
    	player->AddItem nc_SoulGem_player, 1
    	MessageBox "Вам поместили отражение своей души в черный камень душ."
    else
    	MessageBox "Вам нужен черный камень душ, чтобы создать отражение своей души."
    endif
    
    StopScript A1V1_NecroMirror_sc
    
    end

SCPT
SCHD A1V1_NecroTrack 6 0 1 846 49
SCVR doonce2;doonce3;doonce4;doonce5;wait;wait2;timer;;
SCDT 
SCTX
    Begin A1V1_NecroTrack
    
    float timer
    short doonce2
    short doonce3
    short doonce4
    short doonce5
    short wait
    short wait2
    
    if ( MenuMode == 1 )
    	return
    endif
    
    ; Dont waste time checking spells more than once per second
    set timer to ( timer + GetSecondsPassed )
    if ( timer < 1 )		
    	return
    endif
    set timer to 0
    	
    if ( nc_has_corpse > 0 )
    	if ( ScriptRunning "nc_corpse_check" == 0 )
    		startscript nc_corpse_check
    	endif
    endif
    
    if ( nc_necro_skill == 10 )
    	if ( doonce4 == 0 )
    		MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать более сильных мертвецов."
    		set doonce4 to 1
    	endif
    endif	
    
    if ( nc_necro_skill == 20 )
    	if ( doonce3 == 0 )
    		MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать больших мертвецов."
    		set doonce3 to 1
    	endif
    endif	
    
    if ( nc_necro_skill == 30 )
    	if ( doonce2 == 0 )
    		MessageBox "Вы увеличили свое умение вызова мертвецов. Идите и поговорите с Трагриларом."
    		set doonce2 to 1
    	endif
    endif
    
    if ( player->GetSpellEffects A1_NecroTrap == 1 )
    	if ( wait == 0 )
    		startscript nc_death_trap
    		set wait to 1
    	endif
    else
    	if ( wait == 1 )
    		set wait to 0
    		stopscript nc_death_trap
    	endif
    endif
    
    if ( player->GetSpellEffects A1_NecroMirror == 1 )
    	if ( wait2 == 0 )
    		startscript A1V1_NecroMirror_sc
    		set wait2 to 1
    	endif
    else
    	set wait2 to 0
    endif
    
    if ( player->getspelleffects nc_painful_touch == 1 )
    	startscript nc_warlord_attack
    endif
    
    End

SCPT
SCHD A1V1_SoulGemAltmer_sc 4 13 0 2712 126
SCVR state;OnPCEquip;gender;summon;hash;skills1;skills2;skills3;skills4;skills5;skills6;skills7;skills8;skills9;temp;count;npcfile;;
SCDT 
SCTX
    Begin A1V1_SoulGemAltmer_sc
    
    long hash
    long skills1
    long skills2
    long skills3
    long skills4
    long skills5
    long skills6
    long skills7
    long skills8
    long skills9
    short state
    short OnPCEquip
    long temp
    long count
    long npcfile
    short gender
    short summon
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( state == 0 )
    	set state to 1
    	set hash to A1V1_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V1_SoulGemHashLow
    	set hash to hash + temp
    	set A1V1_SoulGemHashHi to 0
    	set A1V1_SoulGemHashLow to 0
    	set skills1 to A1V1_NecroSkills1
    	set skills2 to A1V1_NecroSkills2
    	set skills3 to A1V1_NecroSkills3
    	set skills4 to A1V1_NecroSkills4
    	set skills5 to A1V1_NecroSkills5
    	set skills6 to A1V1_NecroSkills6
    	set skills7 to A1V1_NecroSkills7
    	set skills8 to A1V1_NecroSkills8
    	set skills9 to A1V1_NecroSkills9
    endif
    
    set temp to 0
    if ( OnActivate == 1 )
    	if ( player->GetSpellEffects A1_NecroSummon == 1 )
    		set temp to 1
    	else
    		Activate
    	endif
    endif
    
    ifx ( temp )
    	setx count to xGetRef "nc_he_skelly_NPC"
    	ifx ( count )
    		set gender to 1
    	else
    		setx count to xGetRef "nc_he_skelly_NPC_f"
    		ifx ( count )
    			set gender to 2
    		else
    			MessageBox "Заклинание не может найти скелет или труп."
    			set gender to 0
    		endif
    	endif
    	if ( gender == 1 )
    		if ( player->GetItemCount nc_altmer_skull == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп высокого эльфа для этого ритуала."
    		endif
    	elseif ( gender == 2 )
    		if ( player->GetItemCount nc_altmer_skull_f == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп высокой эльфийки для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_scrap_metal_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_netch_leather_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_bonemeal_01 < 30 )
    			set gender to 0
    			MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
    		endif
    	endif
    	ifx ( gender )
    		xSetRef count
    		Disable
    		if ( gender == 1 )
    			player->RemoveItem nc_altmer_skull, 1
    		else
    			player->RemoveItem nc_altmer_skull_f, 1
    		endif
    		player->RemoveItem ingred_scrap_metal_01, 2
    		player->RemoveItem ingred_netch_leather_01, 2
    		player->RemoveItem ingred_bonemeal_01, 30
    		set temp to hash / 65536
    		set A1V1_SoulGemHashHi to temp
    		set hash to hash - temp * 65536
    		set A1V1_SoulGemHashLow to hash
    		set A1V1_NecroSkills1 to skills1
    		set A1V1_NecroSkills2 to skills2
    		set A1V1_NecroSkills3 to skills3
    		set A1V1_NecroSkills4 to skills4
    		set A1V1_NecroSkills5 to skills5
    		set A1V1_NecroSkills6 to skills6
    		set A1V1_NecroSkills7 to skills7
    		set A1V1_NecroSkills8 to skills8
    		set A1V1_NecroSkills9 to skills9
    		set summon to nc_necro_skill
    		if ( summon <= 10 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroHE" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroHE_f" 1, 0, 0
    			endif
    		elseif ( summon <= 20 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroHE2" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroHE2_f" 1, 0, 0
    			endif
    		else
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroHE3" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroHE3_f" 1, 0, 0
    			endif
    		endif
    		PlaySound, "ThunderClap"
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		Disable
    	endif
    	return
    endif
    
    ifx ( OnPCEquip )
    	set OnPCEquip to 0
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx count to xFileReadString npcfile
    		xMessageFix count "OK"
    		MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" "OK"
    	endif
    endif
    
    End

SCPT
SCHD A1V1_SoulGemArg_sc 4 13 0 2723 126
SCVR state;OnPCEquip;gender;summon;hash;skills1;skills2;skills3;skills4;skills5;skills6;skills7;skills8;skills9;temp;count;npcfile;;
SCDT 
SCTX
    Begin A1V1_SoulGemArg_sc
    
    long hash
    long skills1
    long skills2
    long skills3
    long skills4
    long skills5
    long skills6
    long skills7
    long skills8
    long skills9
    short state
    short OnPCEquip
    long temp
    long count
    long npcfile
    short gender
    short summon
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( state == 0 )
    	set state to 1
    	set hash to A1V1_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V1_SoulGemHashLow
    	set hash to hash + temp
    	set A1V1_SoulGemHashHi to 0
    	set A1V1_SoulGemHashLow to 0
    	set skills1 to A1V1_NecroSkills1
    	set skills2 to A1V1_NecroSkills2
    	set skills3 to A1V1_NecroSkills3
    	set skills4 to A1V1_NecroSkills4
    	set skills5 to A1V1_NecroSkills5
    	set skills6 to A1V1_NecroSkills6
    	set skills7 to A1V1_NecroSkills7
    	set skills8 to A1V1_NecroSkills8
    	set skills9 to A1V1_NecroSkills9
    endif
    
    set temp to 0
    if ( OnActivate == 1 )
    	if ( player->GetSpellEffects A1_NecroSummon == 1 )
    		set temp to 1
    	else
    		Activate
    	endif
    endif
    
    ifx ( temp )
    	setx count to xGetRef "nc_arg_skelly_NPC"
    	ifx ( count )
    		set gender to 1
    	else
    		setx count to xGetRef "nc_arg_skelly_NPC_f"
    		ifx ( count )
    			set gender to 2
    		else
    			MessageBox "Заклинание не может найти скелет или труп."
    			set gender to 0
    		endif
    	endif
    	if ( gender == 1 )
    		if ( player->GetItemCount nc_argonian_skull == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп аргонианина для этого ритуала."
    		endif
    	elseif ( gender == 2 )
    		if ( player->GetItemCount nc_argonian_skull_f == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп аргонианки для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_scrap_metal_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_netch_leather_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_bonemeal_01 < 30 )
    			set gender to 0
    			MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
    		endif
    	endif
    	ifx ( gender )
    		xSetRef count
    		Disable
    		if ( gender == 1 )
    			player->RemoveItem nc_argonian_skull, 1
    		else
    			player->RemoveItem nc_argonian_skull_f, 1
    		endif
    		player->RemoveItem ingred_scrap_metal_01, 2
    		player->RemoveItem ingred_netch_leather_01, 2
    		player->RemoveItem ingred_bonemeal_01, 30
    		set temp to hash / 65536
    		set A1V1_SoulGemHashHi to temp
    		set hash to hash - temp * 65536
    		set A1V1_SoulGemHashLow to hash
    		set A1V1_NecroSkills1 to skills1
    		set A1V1_NecroSkills2 to skills2
    		set A1V1_NecroSkills3 to skills3
    		set A1V1_NecroSkills4 to skills4
    		set A1V1_NecroSkills5 to skills5
    		set A1V1_NecroSkills6 to skills6
    		set A1V1_NecroSkills7 to skills7
    		set A1V1_NecroSkills8 to skills8
    		set A1V1_NecroSkills9 to skills9
    		set summon to nc_necro_skill
    		if ( summon <= 10 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroArgo" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroArgo_f" 1, 0, 0
    			endif
    		elseif ( summon <= 20 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroArgo2" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroArgo2_f" 1, 0, 0
    			endif
    		else
    			if ( gender == 1 )
    				PlaceAtMe "A1V1_NecroArgo3" 1, 0, 0
    			else
    				PlaceAtMe "A1V1_NecroArgo3_f" 1, 0, 0
    			endif
    		endif
    		PlaySound, "ThunderClap"
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		Disable
    	endif
    	return
    endif
    
    ifx ( OnPCEquip )
    	set OnPCEquip to 0
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx count to xFileReadString npcfile
    		xMessageFix count "OK"
    		MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" "OK"
    	endif
    endif
    
    End

