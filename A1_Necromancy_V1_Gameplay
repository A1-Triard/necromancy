    set nc_illusion to ( GetIllusion )
    set nc_alteration to ( GetAlteration )
    set nc_destruction to ( GetDestruction )
    set nc_restoration to ( GetRestoration )
    
    messagebox "Короткие клинки: %g - Длинные клинки: %g - Секиры: %g - Меткость: %g - Копья: %g - Тупое оружие: %g", nc_shortblade, nc_longblade, nc_axe, nc_marksman, nc_spear, nc_bluntweapon
    messagebox "Рукопашаная: %g - Бездоспешный бой: %g - Блок: %g - Легкая броня: %g - Средняя броня: %g - Тяжелая броня: %g", nc_handtohand, nc_unarmored, nc_block, nc_lightarmor, nc_mediumarmor, nc_heavyarmor
    messagebox "Мистицизм: %g - Колдовство: %g - Иллюзия: %g - Изменение: %g - Разрушение: %g - Восстановление: %g", nc_mysticism, nc_conjuration, nc_illusion, nc_alteration, nc_destruction, nc_restoration
    
