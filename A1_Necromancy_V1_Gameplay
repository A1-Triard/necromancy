SCVR doonce;;
SCDT 
SCTX
    begin nc_wander_rev_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_wander_rev_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 3
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут бродить вокруг."
    	endif
    endif
    
    set doonce to 2
    
    end

