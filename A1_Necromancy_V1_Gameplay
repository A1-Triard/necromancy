    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    endif
    
    if ( doOnce3 == 0 )
    	if ( GetHealth <= 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_breton_pauldron_sk" 1
    		set doOnce3 to 1
    	endif
    endif
    
    end

