      else
        set p_Check to 0
      endif
    endif
    
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
    endif
    
    if ( doOnce4 == 0 )
    	if ( GetItemCount,  "nc_altmer_skull_f" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_bonemeal_01" < 15 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    endif
    
    if ( doOnce3 == 0 )
    	if ( GetHealth <= 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_altmer_pauldron_sk_f" 1
    		set doOnce3 to 1
    	endif
    endif
    
    end

