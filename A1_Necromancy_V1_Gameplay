    	set timer to timer + GetSecondsPassed
    	if ( timer > 6 )
    		set timer to 0
    		set combatDone to 0
    		set warp to 0
    	endif
    endif
    
    set temp to 0
    if ( OnActivate == 1 )
    	set temp to 1
    endif
    ifx ( temp )
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx name to xGetName
    		setx temp to xFileReadString npcfile
    		xSetName temp
    	else
    		set name to 0
    	endif
    	set setUp to 2
    	Activate
    	return
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetPCSneaking == 1 )
    		ForceSneak
    	else
    		ClearForceSneak
    	endif
    
