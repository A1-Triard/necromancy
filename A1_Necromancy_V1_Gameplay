    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callwar to 0
    	StopScript "nc_follow_war_script"
    	return
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши слуги будут следовать за вами."
    	endif
    endif
    
    set doonce to 2
    
    end

