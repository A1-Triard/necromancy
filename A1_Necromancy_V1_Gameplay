    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_altmer_pauldron_f" >= 1 )
    	Player->Drop "nc_altmer_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron_f" >= 1 )
    	Player->Drop "nc_arg_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_breton_pauldron_f" >= 1 )
    	Player->Drop "nc_breton_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_bosmer_pauldron_f" >= 1 )
    	Player->Drop "nc_bosmer_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_dunmer_pauldron_f" >= 1 )
    	Player->Drop "nc_dunmer_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_imperial_pauldron_f" >= 1 )
    	Player->Drop "nc_imperial_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_khajiit_pauldron_f" >= 1 )
    	Player->Drop "nc_khajiit_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_nord_pauldron_f" >= 1 )
    	Player->Drop "nc_nord_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_orc_pauldron_f" >= 1 )
    	Player->Drop "nc_orc_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_rg_pauldron_f" >= 1 )
    	Player->Drop "nc_rg_pauldron_f" 1
    endif
    
    if ( Player->GetItemCount "nc_altmer_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk_f" 1
    	PlaceAtPC "nc_he_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk_f" 1
    	PlaceAtPC "nc_arg_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_breton_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk_f" 1
    	PlaceAtPC "nc_bret_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk_f" 1
    	PlaceAtPC "nc_we_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk_f" 1
    	PlaceAtPC "nc_de_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_imperial_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_imperial_pauldron_sk_f" 1
    	PlaceAtPC "nc_imp_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk_f" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_nord_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk_f" 1
    	PlaceAtPC "nc_nord_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_orc_pauldron_sk_f" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk_f" 1
    	PlaceAtPC "nc_orc_skelly_NPC_f" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_rg_pauldron_sk_f" >= 2 )
    	Player->RemoveItem "nc_rg_pauldron_sk_f" 1
    	PlaceAtPC "nc_rg_skelly_NPC_f" 1 50 1
    endif
    
    stopscript nc_mult_rg_sk_f
    
    end
    

SCPT
SCHD nc_nord_corpse_cure 5 0 0 557 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_nord_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_nord_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_nord_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
