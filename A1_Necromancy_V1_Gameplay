    if ( OnActivate == 1 )
    	if ( player->GetSpellEffects A1_NecroSummon == 1 )
    		set temp to 1
    	else
    		Activate
    	endif
    endif
    
    ifx ( temp )
    	setx count to xGetRef "nc_rg_skelly_NPC"
    	ifx ( count )
    		set gender to 1
    	else
    		setx count to xGetRef "nc_rg_skelly_NPC_f"
    		ifx ( count )
    			set gender to 2
    		else
    			MessageBox "Заклинание не может найти скелет или труп."
    			set gender to 0
    		endif
    	endif
    	if ( gender == 1 )
    		if ( player->GetItemCount nc_redguard_skull == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп мужчины-редгарда для этого ритуала."
    		endif
    	elseif ( gender == 2 )
    		if ( player->GetItemCount nc_redguard_skull_f == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп женщины-редгарда для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_scrap_metal_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_netch_leather_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_bonemeal_01 < 30 )
    			set gender to 0
    			MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
    		endif
    	endif
