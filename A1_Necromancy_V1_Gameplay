    	setx npcfile to xStringBuild "A1NPC_%d", hash
    endif
    
    xFileRewind npcfile
    setx count,temp to xFileReadShort npcfile, 1
    if ( temp == 20558 )
    	set count to 1
    else
    	set count to 0
    endif
    
    ifx ( count )
    	xFileReadString npcfile
    	setx id to xFileReadString npcfile
    	xAddItem id 1
    endif
    
    Additem "nc_rg_pauldron_f" 1
    AddItem "nc_redguard_skull_f" 1
    AddItem A1_NecroMarker, 1
    
    set tempf to GetBlock
    set skills to tempf
    set tempf to GetMediumArmor
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills1 to skills
    set tempf to GetHeavyArmor
    set skills to tempf
    set tempf to GetBluntWeapon
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills2 to skills
    set tempf to GetLongBlade
    set skills to tempf
    set tempf to GetAxe
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills3 to skills
    set tempf to GetSpear
    set skills to tempf
    set tempf to GetDestruction
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills4 to skills
    set tempf to GetAlteration
    set skills to tempf
    set tempf to GetIllusion
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills5 to skills
    set tempf to GetConjuration
    set skills to tempf
    set tempf to GetMysticism
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills6 to skills
    set tempf to GetRestoration
    set skills to tempf
    set tempf to GetUnarmored
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills7 to skills
    set tempf to GetLightArmor
    set skills to tempf
    set tempf to GetShortBlade
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills8 to skills
    set tempf to GetMarksman
    set skills to tempf
    set tempf to GetHandToHand
    set tempLong to tempf
    set skills to skills * 256 + tempLong
    set A1V1_NecroSkills9 to skills
    
    if ( player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    else
    	player->Removeitem "nc_soulgem_blackened" 1
    	set tempLong to hash / 65536
    	set A1V1_SoulGemHashHi to tempLong
    	set hash to hash - tempLong * 65536
    	set A1V1_SoulGemHashLow to hash
    	player->Additem "nc_SoulGem_redguard" 1
    	MessageBox "Вы захватили душу."
    endif
    
    StopScript nc_death_add_redguard_f
    
    End

SCPT
SCHD nc_death_trap 12 2 0 2314 88
SCVR trap;race1;race2;race3;race4;race5;race6;race7;race8;race9;race10;is_female;target;temp;;
SCDT 
SCTX
    begin nc_death_trap
    
    long target
    long temp
    short trap
    short race1
    short race2
    short race3
    short race4
    short race5
    short race6
    short race7
    short race8
    short race9
    short race10
    short is_female
    
    set race1 to 0
    set race2 to 0
    set race3 to 0
    set race4 to 0
    set race5 to 0
    set race6 to 0
    set race7 to 0
    set race8 to 0
    set race9 to 0
    set race10 to 0
    
    setx target to xGetPCTarget
    
    ifx ( target )
    	set trap to 1
    
    	setx temp to target->xRefType
    	if ( temp != 1598246990 )   ; it's not an NPC
    		set trap to 0
    	endif
    
    	ifx ( trap )
    		xSetRef target
    		if ( GetHealth > 0 )
    			set trap to 0
    			stopscript nc_death_trap
    		endif
    	endif
    
    	ifx ( trap )
    		xSetRef target
    		if ( GetItemCount A1_NecroMarker > 0 )
    			set trap to 0
    			stopscript nc_death_trap
    		endif
    	endif
    
    	ifx ( trap )
    		set trap to 0
    		setx temp to target->xIsFemale
    		set is_female to temp
    
    		xSetRef target
    		if ( GetRace "Argonian" == 1 )
    			set race1 to 1
    			set trap to 1
    		endif
    		
    		xSetRef target
    		if ( GetRace "Breton" == 1 )
    			set race2 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Dark Elf" == 1 )
    			set race3 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "High Elf" == 1 )
    			set race4 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Imperial" == 1 )
    			set race5 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Khajiit" == 1 )
    			set race6 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Nord" == 1 )
    			set race7 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Orc" == 1 )
    			set race8 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Redguard" == 1 )
    			set race9 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Wood Elf" == 1 )
    			set race10 to 1
    			set trap to 1
    		endif
    	endif
    
    	if ( trap == 0 )
    		MessageBox "Заклинание не может найти цель."
    	endif
    endif
    
    ifx ( race1 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_argonian_f
    	else
    		xSetRef target
    		startscript nc_death_add_argonian
    	endif
    	set race1 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race2 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_breton_f
    	else
    		xSetRef target
    		startscript nc_death_add_breton
    	endif
    	set race2 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race3 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_dunmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_dunmer
    	endif
    	set race3 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race4 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_altmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_altmer
    	endif
    	set race4 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race5 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_imperial_f
    	else
    		xSetRef target
    		startscript nc_death_add_imperial
    	endif
    	set race5 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race6 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_khajiit_f
    	else
    		xSetRef target
    		startscript nc_death_add_khajiit
    	endif
    	set race6 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race7 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_nord_f
    	else
    		xSetRef target
    		startscript nc_death_add_nord
    	endif
    	set race7 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race8 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_orc_f
    	else
    		xSetRef target
    		startscript nc_death_add_orc
    	endif
    	set race8 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race9 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_redguard_f
    	else
    		xSetRef target
    		startscript nc_death_add_redguard
    	endif
    	set race9 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race10 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_bosmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_bosmer
    	endif
    	set race10 to 0
    	stopscript nc_death_trap
    endif
    
    end

