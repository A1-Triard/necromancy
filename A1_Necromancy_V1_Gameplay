    		ifx ( tempLong )
    			set gender to 1
    		else
    			setx tempLong to xGetRef "nc_fire_cured"
    			ifx ( tempLong )
    				set gender to 2
    			else
    				setx tempLong to xGetRef "nc_poison_cured"
    				ifx ( tempLong )
    					set gender to 3
    				else
    					MessageBox "Заклинание не может найти мумию."
    				endif
    			endif
    		endif
    	endif
    	if ( gender == 1 )
    		if ( nc_fr_warlord_glob >= 1 )
