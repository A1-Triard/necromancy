SCHD nc_wander_war_script 1 0 0 222 7
SCVR doonce;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEGAQQJIHMBACA9PSAyBQFzAQACIDAFAUcKbmNfY2FsbHdhcgIgMBwQFG5jX3dhbmRlcl93YXJfc2NyaXB0JAEJAQYBARIgRwpuY19jYWxsd2FyID09IDAFAUcKbmNfY2FsbHdhcgIgMwkBBgEEFiBHDm5jX2NhbGxtZXNzYWdlID09IDEGAQIJIHMBACA9PSAwBQFzAQACIDEAECAAwuD46CDx6/Pj6CDh8+Tz8iDh8O7k6PL8IOLu6vDz4y4AAAkBCQEFAXMBAAIgMgEB
