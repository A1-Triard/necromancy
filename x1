SCHD nc_skelly_rg 1 0 0 126 8
SCVR counter;;
SCDT BgEBBSBYIBAAJAEJAQYBBgkgWNwQID09IDEFAXMBAAggcwEAIDEgKwYBAQkgcwEAIDwgMTAkAQkBqxEBACQBCQEGAQIFIFgYEADbEAwBBnBsYXllctQQEW5jX3JnX3BhdWxkcm9uX3NrAQAJARwQDG5jX3NrZWxseV9yZwEB
SCTX
    begin nc_skelly_rg
    
    short counter
    
    if ( MenuMode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate  )
    	disable
    	player-> additem "nc_rg_pauldron_sk" 1
    endif
    
    stopscript nc_skelly_rg
    end

