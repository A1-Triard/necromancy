    			Player->removeitem "nc_soulgem_redguard" 1
    			startscript nc_summon_redguard
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	else
    		set state to 1
    	endif
    
    
    if ( state == 1 )
    	stopscript nc_gem_test
    endif
    
    end

SCPT
SCHD nc_get_war\0p 1 3 0 364 34
SCVR state;prospectref;prospectid;temp;;
SCDT 
SCTX
    begin nc_get_war
    
    short state
    long prospectref   ; reference to an NPC (or creature)
    long prospectid    ; string holding the NPC's id
    
    long temp          ; used to control ifx statements
    
    
        ; loop through each of the NPCs and creatures in the cell
        setx prospectref to xFirstNPC
    	 set nc_phere to 0
    	 set nc_fhere to 0
    	 set nc_frhere to 0
        whilex ( prospectref )
    
    
            ; get this NPCs id and see if it's poison warlord
            setx prospectid to prospectref->xGetBaseID 
            setx temp to xStringCompare prospectid "nc_p_warlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_phere to 1
    				;MessageBox "Got a poison.  nc_phere is %g", nc_phere
            endif
    
            setx temp to xStringCompare prospectid "nc_f_warlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_fhere to 1
    				;MessageBox "Got a fire.  nc_fhere is %g", nc_fhere
            endif
    
            setx temp to xStringCompare prospectid "nc_fr_warlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_frhere to 1
    				;MessageBox "Got a frost.  nc_frhere is %g", nc_frhere
            endif
    
    		 setx prospectref to xNextRef prospectref
        endwhile
    
    ;MessageBox "nc_phere is %g, nc_fhere is %g, nc_frhere is %g", nc_phere, nc_fhere, nc_frhere
    
    stopscript nc_get_war
    end

SCPT
SCHD nc_ghost_scare 3 0 0 182 25
SCVR doonce;currentday;modday;;
SCDT BgEBCSBYoxEgPT0gMCQBBwEGBgEECSBzAQAgPT0gMOYQEW5jX2FuY2VzdG9yX2dob3N0AQAAAJZDAAACEAxzY3JpYiBzY3JlYW0FAXMCAAggRwREYXkAAAUBcwEAAiAxCQEJAQYBAg8gcwIAICE9IEcERGF5AAAFAXMCAAggRwREYXkAAAUBcwMACCBzAwAgMSArCQEGAQIJIHMDACA+PSAzBQFzAwACIDAFAXMBAAIgMAkBAQE=
SCTX
    begin nc_ghost_scare
    
    short doonce
    short currentday
    short modday
    
    
    if ( GetCollidingPC == 0 )
    	return
    else
    	if ( doonce == 0 ) ;PC is touching it
    		PlaceAtPC "nc_ancestor_ghost" 1 300 0 ;startle them good
    		PlaySound, "scrib scream"
    		set currentday to Day
    		set doonce to 1
    	endif
    endif
    
    if ( currentday != Day ) ;won't happen again for 2 days
    	set currentday to Day
    	set modday to ( modday + 1 )
    endif
    
    if ( modday >= 3 )
    	set modday to 0
    	set doonce to 0
    endif
    
    
    
    end

SCPT
SCHD nc_ghost_script 1 0 1 85 13
SCVR doonce;timer;;
SCDT BgECCSBzAQAgPT0gMI4RcT2KPwUBcwEAAiAxCQEFAWYBAAogZgEAIFgSECArBgEBCyBmAQAgPj0gMC432xAIAQEJIGYBACA+PSA0qxEBAAkBJAEBAQ==
SCTX
    begin nc_ghost_script
    
    float timer
    short doonce
    
    if ( doonce == 0 )
    	setscale 1.08
    	set doonce to 1
    endif
    
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 0.7 )
    	disable
    elseif ( timer >= 4 )
    	SetDelete, 1
    endif
    
    return
    
    end

SCPT
SCHD nc_guard_attack 9 3 0 1200 82
SCVR call;call2;call3;call4;call5;call6;call7;call8;doonce;prospectref;prospectid;temp;;
SCDT 
SCTX
    begin nc_guard_attack
    
        short call 
        short call2
        short call3        ; status variable - should this NPC be called?
        short call4
        short call5
        short call6
        short call7
        short call8
    	 short doonce
        long prospectref   ; reference to an NPC (or creature)
        long prospectid    ; string holding the NPC's id
        long temp          ; used to control ifx statements
    
    
    
        
    
    
    
        ; loop through each of the NPCs and creatures in the cell
        setx prospectref to xFirstNPC
    	 
        whilex ( prospectref )
    
            ; start by assuming we want to call this one
            set call to 1
            set call2 to 1
            set call3 to 1
            set call4 to 1
            set call5 to 1
            set call6 to 1
            set call7 to 1
            set call8 to 1
    
            ; check the type because we don't want creatures (Non-NPCs)
            setx temp to prospectref->xRefType
            if ( temp != 1598246990 )   ; it's not an NPC
                set call to 0
            endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "Imperial Guard"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "hlaalu guard"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "ordinator stationary"
        if ( temp != 0 ) ; it's not a guard
            set call4 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "ordinator wander"
        if ( temp != 0 ) ; it's not a guard
            set call5 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "redoran guard male"
        if ( temp != 0 ) ; it's not a guard
            set call6 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "redoran guard female"
        if ( temp != 0 ) ; it's not a guard
            set call7 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "telvanni guard"
        if ( temp != 0 ) ; it's not a guard
            set call8 to 0
        endif
    
            ; if call is still 1 we want to call this NPC to the player
            ifx ( call2 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call3 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call4 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call5 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call6 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call7 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call8 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
        	setx prospectref to xNextRef prospectref
        endwhile
    set doonce to 0
    stopscript nc_guard_attack
    
    end

SCPT
SCHD nc_he_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_he_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_altmer_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_altmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_he_corpse_drop 2 0 0 108 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBJuY19hbHRtZXJfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_he_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_altmer_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

SCPT
