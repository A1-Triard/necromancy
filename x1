SCHD nc_gem_test 1 0 0 1971 6
SCVR state;;
SCDT 
SCTX
    begin nc_gem_test
    
    short state
    ;global nc_gem_race
    
    
    	if ( nc_gem_race == 1 )
    		if ( Player->GetItemCount "nc_soulgem_arg" >= 1 )
    			Player->removeitem "nc_soulgem_arg" 1
    			startscript nc_summon_argonian
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 2 )	
    		if ( Player->GetItemCount "nc_soulgem_altmer" >= 1 )
    			Player->removeitem "nc_soulgem_altmer" 1
    			startscript nc_summon_altmer
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 3 )	
    		if ( Player->GetItemCount "nc_soulgem_bosmer" >= 1 )
    			Player->removeitem "nc_soulgem_bosmer" 1
    			startscript nc_summon_bosmer
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 4 )	
    		if ( Player->GetItemCount "nc_soulgem_breton" >= 1 )
    			Player->removeitem "nc_soulgem_breton" 1
    			startscript nc_summon_breton
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 5 )	
    		if ( Player->GetItemCount "nc_soulgem_dunmer" >= 1 )
    			Player->removeitem "nc_soulgem_dunmer" 1
    			startscript nc_summon_dunmer
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 6 )	
    		if ( Player->GetItemCount "nc_soulgem_imperial" >= 1 )
    			Player->removeitem "nc_soulgem_imperial" 1
    			startscript nc_summon_imperial
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 7 )	
    		if ( Player->GetItemCount "nc_soulgem_khajiit" >= 1 )
    			Player->removeitem "nc_soulgem_khajiit" 1
    			startscript nc_summon_khajiit
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 8 )	
    		if ( Player->GetItemCount "nc_soulgem_nord" >= 1 )
    			Player->removeitem "nc_soulgem_nord" 1
    			startscript nc_summon_nord
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 9 )	
    		if ( Player->GetItemCount "nc_soulgem_orc" >= 1 )
    			Player->removeitem "nc_soulgem_orc" 1
    			startscript nc_summon_orc
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
