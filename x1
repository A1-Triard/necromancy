SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEA9uY19vcmNfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_orc_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_orc_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

