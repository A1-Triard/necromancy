    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_bosmer_pauldron_sk" 1
    	else
