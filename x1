SCPT
SCHD nc_control_ring 3 2 0 829 35
SCVR OnPCEquip;button;status;lkey;ltemp;;
SCDT 
SCTX
    begin nc_control_ring
    
    short OnPCEquip
    short button
    short status
    long lkey
    long ltemp
    
    if ( menumode )
    	return
    endif
    
    if ( OnPCEquip == 1 )
    	MessageBox "Кому вы хотите отдать приказ?", "Мертвецам", "Слугам", "И тем, и другим", "Никому", 
    	set OnPCEquip to 0
    	set status to 1
    endif
    
    if ( status == 1 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;revenants
    		set status to 2
    	elseif ( button == 1 ) ;warlords
    		set status to 3
    	elseif ( button == 2 ) ;both
    			set status to 4
    	elseif ( button == 3 ) ;none
    		set status to 5
    	endif
    elseif ( status == 2 ) ;revenants
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest_rev
    	set status to 0
    elseif ( status == 3 ) ;warlords
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	startscript nc_keytest_warlord
    	set status to 0
    elseif ( status == 4 ) ;both
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest
    	set status to 0
    elseif ( status == 5 ) ;none
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	set status to 0
    endif
    
    
    end
    

SCPT
SCHD nc_corpse_check 1 0 0 1498 6
SCVR delay;;
SCDT 
SCTX
    begin nc_corpse_check
    
    short delay
    
    set delay to ( delay + 1 )
    
    if ( delay < 30 )
    	return
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	startscript nc_mult_arg
    elseif ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	startscript nc_mult_alt
    elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	startscript nc_mult_bret
    elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	startscript nc_mult_bos
    elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	startscript nc_mult_dun
    elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	startscript nc_mult_imp
    elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	startscript nc_mult_khajiit
    elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	startscript nc_mult_nord
    elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	startscript nc_mult_orc
    elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	startscript nc_mult_rg
    elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	startscript nc_mult_arg_sk
    elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	startscript nc_mult_alt_sk
    elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	startscript nc_mult_bret_sk
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	startscript nc_mult_bos_sk
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	startscript nc_mult_dun_sk
    elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	startscript nc_mult_imp_sk
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	startscript nc_mult_khajiit_sk
    elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	startscript nc_mult_nord_sk
    elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	startscript nc_mult_orc_sk
    elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	startscript nc_mult_rg_sk
    elseif ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	startscript nc_mult_fire
    elseif ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	startscript nc_mult_frost
    elseif ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	startscript nc_mult_poison
    else
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    endif
    	
    
    set delay to 0
    
    if ( nc_has_corpse == 0 )
    	stopscript nc_corpse_check
    endif
    
    end

SCPT
SCHD nc_crypt_visible 0 0 0 47 0
SCDT BgEBECBHCG5jX3N0YXJ0ID49IDLaEAgBAQ8gRwhuY19zdGFydCA8IDLbEAkBAQE=
SCTX
    begin nc_crypt_visible
    
    if ( nc_start >= 2 )
    	enable
    elseif ( nc_start < 2 )
    	disable
    endif
    
    end

SCPT
SCHD nc_cursed_mummy 1 0 0 75 7
SCVR doonce;;
SCDT BgEFCSBYGBAgPT0gMRcQBgECCSBzAQAgPT0gMOYQFmFuY2VzdG9yX2dob3N0X2dyZWF0ZXIBAAAA+kMBAAUBcwEAAiAxCQEJAQEB
SCTX
    begin nc_cursed_mummy
    
    short doonce
    
    if ( OnActivate == 1 )
    	activate
    	if ( doonce == 0 )
    		PlaceAtPC "ancestor_ghost_greater" 1 500 1
    		set doonce to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_de_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_de_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_dunmer_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_dunmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_de_corpse_drop 2 0 0 108 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBJuY19kdW5tZXJfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_de_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_dunmer_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

SCPT
SCHD nc_de_skelly_drop 1 0 0 186 8
SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX2R1bm1lcl9wYXVsZHJvbl9zawEABwEBABAwAM3l8fLoIOHu6/z45Swg9+XsIO7k6O0g8erl6+XyIOfgIPDg5yAtIO3l8+Tu4e3uLgAACQEJAQEB
SCTX
    begin nc_de_skelly_drop
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_dunmer_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
    end

SCPT
SCHD nc_de_skelly_pauldron 4 0 1 637 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_de_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_dunmer_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

SCPT
SCHD nc_death_add 1 0 0 110 7
SCVR doonce;;
SCDT BgEFGiBY/RAgbw9uY19taXNjX3NrdWxsMTAgPCAxBgEDCSBY8BAgPT0gMdQQD25jX21pc2Nfc2t1bGwxMAEABQFzAQACIDEcEAxuY19kZWF0aF9hZGQJAQcBARwQDG5jX2RlYXRoX2FkZAkBAQE=
SCTX
    begin nc_death_add
    short doonce
    
    if ( GetItemCount,  "nc_misc_skull10" < 1 )
    	if ( OnDeath == 1 )
    		additem "nc_misc_skull10" 1
    		set doonce to 1
    		stopscript nc_death_add
    	endif
    else
    	stopscript nc_death_add
    endif
    
    
    end
    

SCPT
SCHD nc_death_add_altmer 0 5 1 547 43
SCVR invitem;invcount;invref;pcref;npcref;timer;;
SCDT 
SCTX
    begin nc_death_add_altmer
    
    
    
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    float timer
    
    set timer to ( timer + GetSecondsPassed )
    	
    if ( timer >= 60 )
    	startscript nc_activate
    	stopscript nc_death_add_altmer
    	set timer to 0
    endif
    
    
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
    
    
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
    
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
    
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_altmer" 1
    pcref->Additem "nc_altmer_pauldron" 1
    pcref->AddItem "nc_altmer_skull" 1
    
    stopscript nc_death_add_altmer
    
    end

SCPT
