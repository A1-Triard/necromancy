SCPT
SCHD nc_death_trap\0ltmer 13 7 0 1585 128
SCVR trap;race1;race2;race3;race4;race5;race6;race7;race8;race9;race10;race;soulgem;invitem;invcount;invref;pcref;npcref;target;temp;;
SCDT 
SCTX
    begin nc_death_trap
    
    
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    long target
    long temp
    short trap
    short race1
    short race2
    short race3
    short race4
    short race5
    short race6
    short race7
    short race8
    short race9
    short race10
    short race
    short soulgem
    
    if ( "Player"->GetItemCount "nc_SoulGem_blackened" <= 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    	stopscript nc_death_trap
    endif
    
    setx target to xGetPCTarget
    
    
    ifx ( target )
    
    	set trap to 1
    
    	setx temp to target->xRefType
    	if ( temp != 1598246990 )   ; it's not an NPC
    		set trap to 0
    	endif
    
    	xSetRef target
    	if ( GetHealth < 1 )
    		stopscript nc_death_trap
    	endif
    
    	ifx ( trap )
    		xSetRef target
    		if ( GetRace "Argonian" == 1 )
    			set race1 to 1
    			set race to 1
    		endif
    		
    		xSetRef target
    		if ( GetRace "Breton" == 1 )
    			set race2 to 1
    			set race to 2
    		endif
    
    		xSetRef target
    		if ( GetRace "Dark Elf" == 1 )
    			set race3 to 1
    			set race to 3
    		endif
    
    		xSetRef target
    		if ( GetRace "High Elf" == 1 )
    			set race4 to 1
    			set race to 4
    		endif
    
    		xSetRef target
    		if ( GetRace "Imperial" == 1 )
    			set race5 to 1
    			set race to 5
    		endif
    
    		xSetRef target
    		if ( GetRace "Khajiit" == 1 )
    			set race6 to 1
    			set race to 6
    		endif
    
    		xSetRef target
    		if ( GetRace "Nord" == 1 )
    			set race7 to 1
    			set race to 7
    		endif
    
    		xSetRef target
    		if ( GetRace "Orc" == 1 )
    			set race8 to 1
    			set race to 8
    		endif
    
    		xSetRef target
    		if ( GetRace "Redguard" == 1 )
    			set race9 to 1
    			set race to 9
    		endif
    
    		xSetRef target
    		if ( GetRace "Wood Elf" == 1 )
    			set race10 to 1
    			set race to 10
    		endif
    	endif
    
    ;MessageBox "Race is %g" race
    
    endif
    
    ifx ( race1 )
    	xSetRef target
    	startscript nc_death_add_argonian
    	set race1 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race2 )
    	xSetRef target
    	startscript nc_death_add_breton
    	set race2 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race3 )
    	xSetRef target
    	startscript nc_death_add_dunmer
    	set race3 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race4 )
    	xSetRef target
    	startscript nc_death_add_altmer
    	set race4 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race5 )
    	xSetRef target
    	startscript nc_death_add_imperial
    	set race5 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race6 )
    	xSetRef target
    	startscript nc_death_add_khajiit
    	set race6 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race7 )
    	xSetRef target
    	startscript nc_death_add_nord
    	set race7 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race8 )
    	xSetRef target
    	startscript nc_death_add_orc
    	set race8 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race9 )
    	xSetRef target
    	startscript nc_death_add_redguard
    	set race9 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race10 )
    	xSetRef target
    	startscript nc_death_add_bosmer
    	set race10 to 0
    	stopscript nc_death_trap
    endif
    ;stopscript nc_death_trap
    
    end

