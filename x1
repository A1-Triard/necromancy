SCHD nc_summon_poison 16 0 0 2978 211
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_cloth;nc_alt_skull1;nc_arg_skull1;nc_bos_skull1;nc_bret_skull1;nc_imp_skull1;nc_dun_skull1;nc_kha_skull1;nc_nord_skull1;nc_orc_skull1;nc_rg_skull1;state;;
SCDT 
SCTX
    begin nc_summon_poison
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_cloth
    short nc_alt_skull1
    short nc_arg_skull1
    short nc_bos_skull1
    short nc_bret_skull1
    short nc_imp_skull1
    short nc_dun_skull1
    short nc_kha_skull1
    short nc_nord_skull1
    short nc_orc_skull1
    short nc_rg_skull1
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_cloth to getItemCount "misc_clothbolt_03"
    set nc_alt_skull1 to getItemCount "nc_altmer_skull"
    set nc_arg_skull1 to getItemCount "nc_argonian_skull"
    set nc_bos_skull1 to getItemCount "nc_bosmer_skull"
    set nc_bret_skull1 to getItemCount "nc_breton_skull"
    set nc_imp_skull1 to getItemCount "nc_imperial_skull"
    set nc_dun_skull1 to getItemCount "nc_dunmer_skull"
    set nc_kha_skull1 to getItemCount "nc_khajiit_skull"
    set nc_nord_skull1 to getItemCount "nc_nord_skull"
    set nc_orc_skull1 to getItemCount "nc_orc_skull"
    set nc_rg_skull1 to getItemCount "nc_redguard_skull"
    
    if ( nc_pclevel >= 29 )
    	set state to 1
    elseif ( nc_pclevel < 29 )
    	MessageBox "Вы недостаточно опытны, чтобы вызвать мумифицированного слугу."
    	set state to 10
    endif
    
    if ( nc_p_warlord_glob >= 1 )
    	MessageBox "Вы можете контролировать только одного ядовитого слугу."
    	set state to 10
    endif
    
    if ( state == 1 )
    	if ( nc_cloth >= 1 )
    		if ( nc_scrap_metal >= 1 )
    			if ( nc_netch_leather >= 1 )
    				if ( nc_bonemeal >= 10 )
    					if ( nc_alt_skull1 >= 1 )
    	            			removeitem "nc_altmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_arg_skull1 >= 1 )
    	            			removeitem "nc_argonian_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_bos_skull1 >= 1 )
    	            			removeitem "nc_bosmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_bret_skull1 >= 1 )
    	            			removeitem "nc_breton_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_imp_skull1 >= 1 )
    	            			removeitem "nc_imperial_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_dun_skull1 >= 1 )
    	            			removeitem "nc_dunmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_kha_skull1 >= 1 )
    	            			removeitem "nc_khajiit_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_nord_skull1 >= 1 )
    	            			removeitem "nc_nord_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_orc_skull1 >= 1 )
    	            			removeitem "nc_orc_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_rg_skull1 >= 1 )
    	            			removeitem "nc_redguard_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					else
    	            			MessageBox "Вам нужен череп для этого ритуала."
    						set state to 10
    					endif
    				else
    					MessageBox "Вам нужно некоторое количество костяной муки для этого ритуала."
    					set state to 10
    				endif
    			else
    				MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
    				set state to 10
    			endif
    		else
    			MessageBox "Вам нужны обрезки металла для этого ритуала."
    			set state to 10
    		endif
    	else
    		MessageBox "Вам нужен рулон зеленой ткани для этого ритуала."
    		set state to 10
    	endif
    endif
    
    if ( state == 2 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_p_warlord" 1, 0, 0
    		set nc_p_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
    
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_poison
    endif
    
    end

SCPT
SCHD nc_summon_redguard 6 0 0 1211 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_redguard
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_redguard_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    		  removeitem "nc_rg_pauldron_sk" 1
                removeitem "nc_redguard_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    	MessageBox "Вам нужен череп Редгарда для этого ритуала."
    	set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_redguard" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )	
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_redguard
    endif
    
    end

SCPT
SCHD nc_time_bounty 2 0 3 1650 28
SCVR rand;kill;timer;hour;timer2;;
SCDT 
SCTX
    begin nc_time_bounty
    
    short rand
    short kill
    float timer
    float hour
    float timer2
    
    if ( MenuMode == 1 )
    	return
    elseif ( GetPCCell "Храм Танатоса" == 1 )
    	return
    elseif ( VH_MaskDummyFlag > 0 )
    	return
    endif
    
    set timer2 to ( timer2 + GetSecondsPassed )
    if ( timer2 >= 14 )
    
    		if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		else
    			set kill to 1
    		endif
    endif
    
    set hour to GameHour
    set rand to Random, 11
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 15 )
    
    if ( hour < 8 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			MessageBox "Кто-то увидел, как вы несете тело."
    			ModPCCrimeLevel 500
    		endif
    	endif
    elseif ( hour < 20 )
    	if ( hour >= 8 )
    		;MessageBox "Daytime. Time is %f. Random is %g.", hour, rand
    		if ( rand <= 7 )
    			if ( GetDetected Player == 1 )
    				MessageBox "Кто-то увидел, как вы несете тело."
    				ModPCCrimeLevel 500
    			endif
    		endif
    	endif
    elseif ( hour >= 20 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			MessageBox "Кто-то увидел, как вы несете тело."
    			ModPCCrimeLevel 500
    		endif
    	endif
    endif
    
    	set timer to 0
    endif
    
    if ( kill == 1 )
    	stopscript nc_time_bounty
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	set timer2 to 0
    	set kill to 0
    endif
    
    end

SCPT
SCHD nc_tragrilar_death 2 0 0 146 20
SCVR currentday;deathday;;
SCDT BgECDyBzAQAgIT0gRwREYXkAAAUBcwEACCBHBERheQAABQFzAgAIIHMCACAxICsJAQYBBQkgcwIAID49IDIGAQMZIFgSESBjDdXw4Owg0uDt4PLu8eAgPT0gMRsQE25jX2RlYXRoX2FkZF9hbHRtZXKNEAAAAAAcEBJuY190cmFncmlsYXJfZGVhdGgJAQkBAQE=
SCTX
    begin nc_tragrilar_death
    
    short currentday
    short deathday
    
    if ( currentday != Day )
    	set currentday to Day
    	set deathday to ( deathday + 1 )
    endif
    
    if ( deathday >= 2 )
    	if ( GetPCCell "Храм Танатоса" == 1 )
    		startscript nc_death_add_altmer
    		sethealth 0
    		stopscript nc_tragrilar_death
    	endif
    endif
    
    
    end

