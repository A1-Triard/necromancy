SCHD nc_follow_rev_script 1 0 0 228 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_follow_rev_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_follow_rev_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
    	endif
    endif
    
    set doonce to 2
    
    end

SCPT
SCHD nc_follow_script 1 0 0 282 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_follow_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_follow_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
    	endif
    endif
    
    set doonce to 2
    
    end

