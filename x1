SCPT
SCHD nc_spell_timer 1 0 1 245 12
SCVR state;timer;;
SCDT 
SCTX
    begin nc_spell_timer
    
    float timer
    short state
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( CellChanged == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectRecall == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectDivineIntervention == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectAlmsiviIntervention == 1 )
    	set state to 1
    endif
    
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 120 )
    	set state to 1
    	set timer to 0
    endif
    
    if ( state == 1 )
    	player-> RemoveSpell "nc_summon"
    	stopscript nc_spell_timer
    	set state to 0
    endif
    
    end

SCPT
SCHD nc_stand_rev_script 1 0 0 222 7
SCVR doonce;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEGAQQJIHMBACA9PSAyBQFzAQACIDAFAUcKbmNfY2FsbHJldgIgMBwQE25jX3N0YW5kX3Jldl9zY3JpcHQkAQkBBgEBEiBHCm5jX2NhbGxyZXYgPT0gMAUBRwpuY19jYWxscmV2AiAxCQEGAQQWIEcObmNfY2FsbG1lc3NhZ2UgPT0gMQYBAgkgcwEAID09IDAFAXMBAAIgMQAQIQDC4PjoIOzl8PLi5fb7IOHz5PPyIPHy7v/y/CDn5OXx/C4AAAkBCQEFAXMBAAIgMgEB
SCTX
    begin nc_stand_rev_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_stand_rev_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
    endif
    ;set timer to timer + GetSecondsPassed 
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
    	endif
    endif
    
    set doonce to 2
    
    end

