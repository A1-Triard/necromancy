SCHD nc_stand_script 1 0 0 276 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_stand_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_stand_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 1
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
    	endif
    endif
    
    set doonce to 2
    
    end

