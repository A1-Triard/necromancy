    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_imperial_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

