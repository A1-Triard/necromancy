    	set nc_callwar to 0
    	StopScript "nc_wander_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 3
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 3
