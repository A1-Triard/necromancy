SCHD nc_poison_corpse_obj 1 0 0 181 8
SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEm5jX3BvaXNvbl9wYXVsZHJvbgEABwEBABAuAM3l8fLoIOHu6/z45Swg9+XsIO7k7e4g8uXr7iDn4CDw4OcgLSDt5fPk7uHt7i4AAAkBCQEBAQ==
SCTX
    begin nc_poison_corpse_obj
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_poison_pauldron" 1
    	else
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    	endif
    endif
    
    end

SCPT
SCHD nc_poison_vat 16 0 0 3098 123
SCVR currentday;localdayspassed;pstate;salt;coprinus;russula;nord;orc;altmer;arg;bosmer;breton;khajiit;dunmer;redguard;imperial;;
SCDT 
SCTX
    begin nc_poison_vat
    
    short currentday
    short localdayspassed
    short pstate
    short salt
    short coprinus
    short russula
    short nord
    short orc
    short altmer
    short arg
    short bosmer
    short breton
    short khajiit
    short dunmer
    short redguard
    short imperial
    
    if ( menumode == 1 )
    	return
    endif
    
        set altmer to getItemCount "nc_altmer_pauldron" 
        set arg to getItemCount "nc_arg_pauldron" 
        set bosmer to getItemCount "nc_bosmer_pauldron" 
        set breton to getItemCount "nc_breton_pauldron" 
        set dunmer to getItemCount "nc_dunmer_pauldron"
        set imperial to getItemCount "nc_imperial_pauldron" 
        set khajiit to getItemCount "nc_khajiit_pauldron" 
        set nord to getItemCount "nc_nord_pauldron" 
        set orc to getItemCount "nc_orc_pauldron" 
        set redguard to getItemCount "nc_rg_pauldron"
       	set salt to getItemCount "nc_salt"
    	set coprinus to getItemCount "ingred_coprinus_01"
    	set russula to getItemCount "ingred_russula_01"
    
    
    set pstate to 2
    if ( salt >= 500 )
       if ( coprinus >= 5 )
           if ( russula >= 5 )
               set pstate to 1
           endif
       endif
    endif
    
    
    if ( altmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( arg >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_arg_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( bosmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_bosmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( breton >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_breton_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( dunmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_dunmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( imperial >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_imperial_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( khajiit >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_khajiit_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( orc >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_orc_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    if ( redguard >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_rg_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
    endif
    
    end

SCPT
SCHD nc_postman 1 0 0 152 5
SCVR rand;;
SCDT DAEGcGxheWVyBgEKCSBYVxEgPj0gNQUBcwEACCBYIRBkADAwBgEHCiBzAQAgPD0gMTAGAQUJIFjgECA9PSAx5hAPbmNfc3RhcnRfc2tlbGx5AQAAAABDAQAAEBgAwvsg8ev7+Ojy5SDx8vDg7e376SD48+wuAADhEBwQCm5jX3Bvc3RtYW4FAXMBAAQgMTAwCQEJAQkBAQE=
SCTX
    begin nc_postman
    
    short rand
    
    	if ( player->GetLevel >= 5 )
    		set rand to Random, 100
    		if ( rand <= 10 )
    			if ( GetPCSleep == 1 )
    				PlaceAtPC "nc_start_skelly" 1 128 1
    				MessageBox "Вы слышите странный шум."
    				WakeUpPC
    				stopscript nc_postman
    				set rand to 100
    			endif
    		endif
    	endif
    
    
    end

SCPT
SCHD nc_random_kill 3 3 0 192 50
SCVR OnPCEquip;doonce;call;prospectref;prospectid;temp;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEaPBM4AAATOGwAAjwTOAAAEzhsAAA8DzgEAAw4rQAFAXMDAAIgMRM4AAATOGwAADwYPAg8EzgCABM4bAACPAYBARIgbAMAICE9IDE1OTgyNDY5OTAFAXMDAAIgMAkBEzgCABM4cwAAPA84BAAMOJMAEzgAABM4bAAAPB08jRAAAAAAEzgAABM4bAAAPBs8EzgAABM4bAACPAo4HQAcEA5uY19yYW5kb21fa2lsbAEB
SCTX
    begin nc_random_kill
    
    short OnPCEquip
    short doonce
    short call ; status variable - should this NPC be called?
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
    
    
    if ( menumode == 1 )
        return
    endif
    
    ; Not equipped, abort
    ;if ( OnPCEquip == 0 )
    ;    return
    ;endif
    
    ;set OnPCEquip to 0
    
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
    
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
    
        ; start by assuming we want to call this one
        set call to 1
    	
    
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
        endif
    
    
    
        ; if call is still 1 we want to call this NPC to the player
        ifx ( call ) ; we use extended functions inside so need ifx		  
          xGetRef prospectref
    		sethealth 0
    
    
        endif
        ; The MWSE docs example script has a typo here.
        ; prospectref, not prospect
        setx prospectref to xNextRef prospectref
    
    
    ;temp->xstartcombat "player"
    
    endwhile
    stopscript nc_random_kill
    
    ; this only needs to run for one frame
    
    
    
    end
    

SCPT
SCHD nc_real_startscript 0 0 1 182 6
SCVR timer;;
SCDT BQFmAQAKIGYBACBYEhAgKwYBCgkgZgEAID49IDIGAQMWIFgdECBtCm5jX3Bvc3RtYW4gPT0gMAYBARcgWM0QIGQMbmNfbmVjcm9fMDAxIDwgNRsQCm5jX3Bvc3RtYW4JAQkBBgEBGiBYHRAgbQ5uY19zdGFydHNjcmlwdCA9PSAwGxAObmNfc3RhcnRzY3JpcHQJAQUBZgEAAiAwHBATbmNfcmVhbF9zdGFydHNjcmlwdAkBAQE=
SCTX
    begin nc_real_startscript
    
    float timer
    
    set timer to ( timer + GetSecondsPassed )
    if ( timer >= 2 )
    	
    	if ( ScriptRunning "nc_postman" == 0 )
    		if ( GetJournalIndex "nc_necro_001" < 5 )
    			startscript nc_postman
    		endif
    	endif
    
    	if ( ScriptRunning "nc_startscript" == 0 )
    		startscript nc_startscript
    	endif
    	set timer to 0
    	stopscript nc_real_startscript
    endif
    
    
    
    end

SCPT
SCHD nc_rg_corpse_cure 5 0 0 559 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_rg_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_rg_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_redguard_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
