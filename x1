SCHD nc_fire_vat 16 0 0 3085 122
SCVR currentday;localdayspassed;fstate;salt;firesalts;petal;nord;orc;altmer;arg;bosmer;breton;khajiit;dunmer;redguard;imperial;;
SCDT 
SCTX
    begin nc_fire_vat
    
    short currentday
    short localdayspassed
    short fstate
    short salt
    short firesalts
    short petal
    short nord
    short orc
    short altmer
    short arg
    short bosmer
    short breton
    short khajiit
    short dunmer
    short redguard
    short imperial
    
    if ( menumode == 1 )
    	return
    endif
    
        set altmer to getItemCount "nc_altmer_pauldron" 
        set arg to getItemCount "nc_arg_pauldron" 
        set bosmer to getItemCount "nc_bosmer_pauldron" 
        set breton to getItemCount "nc_breton_pauldron" 
        set dunmer to getItemCount "nc_dunmer_pauldron"
        set imperial to getItemCount "nc_imperial_pauldron" 
        set khajiit to getItemCount "nc_khajiit_pauldron" 
        set nord to getItemCount "nc_nord_pauldron" 
        set orc to getItemCount "nc_orc_pauldron" 
        set redguard to getItemCount "nc_rg_pauldron"
        	set salt to getItemCount "nc_salt"
    	set firesalts to getItemCount "ingred_fire_salts_01"
    	set petal to getItemCount "ingred_fire_petal_01"
    
    
    set fstate to 2
    if ( salt >= 500 )
       if ( firesalts >= 5 )
           if ( petal >= 5 )
               set fstate to 1
           endif
       endif
    endif
    
    
    if ( altmer >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "nc_fire_pauldron" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( arg >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_arg_pauldron" 1
    			additem "nc_fire_pauldron" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( bosmer >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_bosmer_pauldron" 1
    			additem "nc_fire_pauldron" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( breton >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
