    set temp to 0
    if ( MenuMode == 1 )
    	set temp to 1
    endif
    ifx ( temp )
    	ifx ( name )
    		set name to 0
    		set count to 0
    		if ( hash == -2147483648)
    			set temp to 0
    		elseif ( hash < 0 )
    			set count to 1
    			set temp to -hash
    		else
    			set temp to hash
    		endif
    
    		ifx ( count )
    			setx npcfile to xStringBuild "A1NPC_0%d", temp
    		else
    			setx npcfile to xStringBuild "A1NPC_%d", temp
    		endif
    
    		xFileRewind npcfile
    		setx count,temp to xFileReadShort npcfile, 1
    		if ( temp == 20558 )
    			set count to 1
    		else
    			set count to 0
    		endif
    		ifx ( count )
    			setx temp to xFileReadString npcfile
    			xMessageFix temp
    			MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
    		endif
    	endif
    	return
    endif
    
    set temp to 0
    if ( setUp == 0 )
    	set setUp to 1
    	set hash to A1V3_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V3_SoulGemHashLow
    	set hash to hash + temp
    	set A1V3_SoulGemHashHi to 0
    	set A1V3_SoulGemHashLow to 0
    	set skills1 to A1V3_NecroSkills1
    	set skills2 to A1V3_NecroSkills2
    	set skills3 to A1V3_NecroSkills3
    	set skills4 to A1V3_NecroSkills4
    	set skills5 to A1V3_NecroSkills5
    	set skills6 to A1V3_NecroSkills6
    	set skills7 to A1V3_NecroSkills7
    	set skills8 to A1V3_NecroSkills8
    	set skills9 to A1V3_NecroSkills9
    	set companion to 1
    	set emb_hook_nominion to 1
    	AddTopic "- Бой"
    	AddTopic "- Сопровождение"
    	AddTopic "- Инструкции"
    	AddTopic "- Перемещение"
    	return
    endif
    
    if ( weapUse == 1 )
    	if ( Player->GetWeaponType <= 8 )
    		setmarksman 0
    	else
    		setmarksman 200
    	endif
    endif
    
    if ( combatCheck == 1 )
    	set sheathetimer to sheatheTimer + GetSecondsPassed
    	if ( GetSoundPlaying, "Weapon Swish" == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, crossbowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, bowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( sheatheTimer > 4 )
    		if ( GetSoundPlaying "Weapon Swish" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "crossbowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "bowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		endif
    	endif
    endif
    
    if ( soundPlayed == 1 )
    	equip "silver dagger"
    	removeitem "silver dagger" 1
    	; messagebox "Working"
    	set soundPlayed to 0
    	set combatCheck to 0
    	set combatDone to 1
    endif
    
