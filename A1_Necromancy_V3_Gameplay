    endif
    
    ifx ( temp )
    	setx npcfile to xStringBuild "A1NPC_0%d", npcfile
    else
    	setx npcfile to xStringBuild "A1NPC_%d", hash
    endif
    
    xFileRewind npcfile
    setx count,temp to xFileReadShort npcfile, 1
    if ( temp == 20558 )
    	set count to 1
    else
    	set count to 0
    endif
    
    ifx ( count )
    	xFileReadString npcfile
    	setx id to xFileReadString npcfile
    	xAddItem id 1
    endif
    
    Additem "nc_redguard_corpse" 1
    AddItem "nc_redguard_skull" 1
    AddItem A1_NecroMarker, 1
    
    set tempf to GetBlock
    set skills to tempf
    set tempf to GetMediumArmor
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills1 to skills
    set tempf to GetHeavyArmor
    set skills to tempf
    set tempf to GetBluntWeapon
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills2 to skills
    set tempf to GetLongBlade
    set skills to tempf
    set tempf to GetAxe
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills3 to skills
    set tempf to GetSpear
    set skills to tempf
    set tempf to GetDestruction
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills4 to skills
    set tempf to GetAlteration
    set skills to tempf
    set tempf to GetIllusion
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills5 to skills
    set tempf to GetConjuration
    set skills to tempf
    set tempf to GetMysticism
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills6 to skills
    set tempf to GetRestoration
    set skills to tempf
    set tempf to GetUnarmored
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills7 to skills
    set tempf to GetLightArmor
    set skills to tempf
    set tempf to GetShortBlade
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills8 to skills
    set tempf to GetMarksman
    set skills to tempf
    set tempf to GetHandToHand
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills9 to skills
    
    if ( player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    else
    	player->Removeitem "nc_soulgem_blackened" 1
    	set tempLong to ( hash / 65536 )
    	set A1V3_SoulGemHashHi to tempLong
    	set hash to ( hash - tempLong * 65536 )
    	set A1V3_SoulGemHashLow to hash
    	player->Additem "nc_SoulGem_redguard" 1
    	MessageBox "Вы захватили душу."
    endif
    
    StopScript nc_death_add_redguard
    
    End

SCPT
SCHD nc_death_add_redguard_f 3 6 1 1924 56
SCVR temp;hi;low;id;tempLong;count;hash;npcfile;skills;tempf;;
SCDT 
SCTX
    Begin nc_death_add_redguard_f
    
    long id
    long tempLong
    long count
    short temp
    short hi
    short low
    long hash
    long npcfile
    long skills
    float tempf
    
    setx id to xGetBaseID
    setx tempLong to xStringBuild "tempA1Necro"
    xFileRewind tempLong
    xFileWriteString tempLong, id
    xFileWriteShort tempLong, 0
    xFileRewind tempLong
    set hash to 0
    set temp to 1
    whilex ( temp )
    	setx count,temp to xFileReadShort tempLong, 1
    	if ( temp != 0 )
    		set hi to ( temp / 256 )
    		set low to ( temp - 256 * hi )
    		set hash to ( hash * 1664525 + low + 1013904223 )
    		if ( hi != 0 )
    			set hash to ( hash * 1664525 + hi + 1013904223 )
    		endif
    	endif
    endwhile
    
    set temp to 0
    if ( hash == -2147483648)
    	set npcfile to 0
    elseif ( hash < 0 )
    	set temp to 1
    	set npcfile to -hash
    else
    	set npcfile to hash
    endif
    
    ifx ( temp )
    	setx npcfile to xStringBuild "A1NPC_0%d", npcfile
    else
    	setx npcfile to xStringBuild "A1NPC_%d", hash
    endif
    
    xFileRewind npcfile
    setx count,temp to xFileReadShort npcfile, 1
    if ( temp == 20558 )
    	set count to 1
    else
    	set count to 0
    endif
    
    ifx ( count )
    	xFileReadString npcfile
    	setx id to xFileReadString npcfile
    	xAddItem id 1
    endif
    
    Additem "nc_redguard_corpse_f" 1
    AddItem "nc_redguard_skull_f" 1
    AddItem A1_NecroMarker, 1
    
    set tempf to GetBlock
    set skills to tempf
    set tempf to GetMediumArmor
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills1 to skills
    set tempf to GetHeavyArmor
    set skills to tempf
    set tempf to GetBluntWeapon
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills2 to skills
    set tempf to GetLongBlade
    set skills to tempf
    set tempf to GetAxe
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills3 to skills
    set tempf to GetSpear
    set skills to tempf
    set tempf to GetDestruction
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills4 to skills
    set tempf to GetAlteration
    set skills to tempf
    set tempf to GetIllusion
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills5 to skills
    set tempf to GetConjuration
    set skills to tempf
    set tempf to GetMysticism
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills6 to skills
    set tempf to GetRestoration
    set skills to tempf
    set tempf to GetUnarmored
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills7 to skills
    set tempf to GetLightArmor
    set skills to tempf
    set tempf to GetShortBlade
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills8 to skills
    set tempf to GetMarksman
    set skills to tempf
    set tempf to GetHandToHand
    set tempLong to tempf
    set skills to ( skills * 256 + tempLong )
    set A1V3_NecroSkills9 to skills
    
    if ( player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    else
    	player->Removeitem "nc_soulgem_blackened" 1
    	set tempLong to ( hash / 65536 )
    	set A1V3_SoulGemHashHi to tempLong
    	set hash to ( hash - tempLong * 65536 )
    	set A1V3_SoulGemHashLow to hash
    	player->Additem "nc_SoulGem_redguard" 1
    	MessageBox "Вы захватили душу."
    endif
    
    StopScript nc_death_add_redguard_f
    
    End

SCPT
SCHD nc_death_trap 12 2 0 2314 88
SCVR trap;race1;race2;race3;race4;race5;race6;race7;race8;race9;race10;is_female;target;temp;;
SCDT 
SCTX
    begin nc_death_trap
    
    long target
    long temp
    short trap
    short race1
    short race2
    short race3
    short race4
    short race5
    short race6
    short race7
    short race8
    short race9
    short race10
    short is_female
    
    set race1 to 0
    set race2 to 0
    set race3 to 0
    set race4 to 0
    set race5 to 0
    set race6 to 0
    set race7 to 0
    set race8 to 0
    set race9 to 0
    set race10 to 0
    
    setx target to xGetPCTarget
    
    ifx ( target )
    	set trap to 1
    
    	setx temp to target->xRefType
    	if ( temp != 1598246990 )   ; it's not an NPC
    		set trap to 0
    	endif
    
    	ifx ( trap )
    		xSetRef target
    		if ( GetHealth > 0 )
    			set trap to 0
    			stopscript nc_death_trap
    		endif
    	endif
    
    	ifx ( trap )
    		xSetRef target
    		if ( GetItemCount A1_NecroMarker > 0 )
    			set trap to 0
    			stopscript nc_death_trap
    		endif
    	endif
    
    	ifx ( trap )
    		set trap to 0
    		setx temp to target->xIsFemale
    		set is_female to temp
    
    		xSetRef target
    		if ( GetRace "Argonian" == 1 )
    			set race1 to 1
    			set trap to 1
    		endif
    		
    		xSetRef target
    		if ( GetRace "Breton" == 1 )
    			set race2 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Dark Elf" == 1 )
    			set race3 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "High Elf" == 1 )
    			set race4 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Imperial" == 1 )
    			set race5 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Khajiit" == 1 )
    			set race6 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Nord" == 1 )
    			set race7 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Orc" == 1 )
    			set race8 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Redguard" == 1 )
    			set race9 to 1
    			set trap to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Wood Elf" == 1 )
    			set race10 to 1
    			set trap to 1
    		endif
    	endif
    
    	if ( trap == 0 )
    		MessageBox "Заклинание не может найти цель."
    	endif
    endif
    
    ifx ( race1 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_argonian_f
    	else
    		xSetRef target
    		startscript nc_death_add_argonian
    	endif
    	set race1 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race2 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_breton_f
    	else
    		xSetRef target
    		startscript nc_death_add_breton
    	endif
    	set race2 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race3 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_dunmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_dunmer
    	endif
    	set race3 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race4 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_altmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_altmer
    	endif
    	set race4 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race5 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_imperial_f
    	else
    		xSetRef target
    		startscript nc_death_add_imperial
    	endif
    	set race5 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race6 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_khajiit_f
    	else
    		xSetRef target
    		startscript nc_death_add_khajiit
    	endif
    	set race6 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race7 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_nord_f
    	else
    		xSetRef target
    		startscript nc_death_add_nord
    	endif
    	set race7 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race8 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_orc_f
    	else
    		xSetRef target
    		startscript nc_death_add_orc
    	endif
    	set race8 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race9 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_redguard_f
    	else
    		xSetRef target
    		startscript nc_death_add_redguard
    	endif
    	set race9 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race10 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_bosmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_bosmer
    	endif
    	set race10 to 0
    	stopscript nc_death_trap
    endif
    
    end

SCPT
SCHD nc_fire_vat 16 0 0 3085 122
SCVR currentday;localdayspassed;fstate;salt;firesalts;petal;nord;orc;altmer;arg;bosmer;breton;khajiit;dunmer;redguard;imperial;;
SCDT 
SCTX
    begin nc_fire_vat
    
    short currentday
    short localdayspassed
    short fstate
    short salt
    short firesalts
    short petal
    short nord
    short orc
    short altmer
    short arg
    short bosmer
    short breton
    short khajiit
    short dunmer
    short redguard
    short imperial
    
    if ( menumode == 1 )
    	return
    endif
    
        set altmer to getItemCount "nc_altmer_pauldron" 
        set arg to getItemCount "nc_arg_pauldron" 
        set bosmer to getItemCount "nc_bosmer_pauldron" 
        set breton to getItemCount "nc_breton_pauldron" 
        set dunmer to getItemCount "nc_dunmer_pauldron"
        set imperial to getItemCount "nc_imperial_pauldron" 
        set khajiit to getItemCount "nc_khajiit_pauldron" 
        set nord to getItemCount "nc_nord_pauldron" 
        set orc to getItemCount "nc_orc_pauldron" 
        set redguard to getItemCount "nc_rg_pauldron"
        	set salt to getItemCount "nc_salt"
    	set firesalts to getItemCount "ingred_fire_salts_01"
    	set petal to getItemCount "ingred_fire_petal_01"
    
    
    set fstate to 2
    if ( salt >= 500 )
       if ( firesalts >= 5 )
           if ( petal >= 5 )
               set fstate to 1
           endif
       endif
    endif
    
    
    if ( altmer >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( arg >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_arg_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( bosmer >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_bosmer_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( breton >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_breton_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( dunmer >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_dunmer_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( imperial >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_imperial_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( khajiit >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_khajiit_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( orc >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_orc_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    if ( redguard >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_rg_pauldron" 1
    			additem "A1V3_FireCured" 1
    			MessageBox "Огненная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
    	endif
    endif
    
    
    end
    

SCPT
SCHD nc_follow_rev_script 1 0 0 228 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_follow_rev_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_follow_rev_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
    	endif
    endif
    
    set doonce to 2
    
    end

SCPT
SCHD nc_follow_script 1 0 0 282 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_follow_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_follow_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
    	endif
    endif
    
    set doonce to 2
    
    end

SCPT
SCHD nc_follow_war_script 1 0 0 225 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_follow_war_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callwar to 0
    	StopScript "nc_follow_war_script"
    	return
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши слуги будут следовать за вами."
    	endif
    endif
    
    set doonce to 2
    
    end

SCPT
SCHD nc_frost_vat 16 0 0 3081 122
SCVR currentday;localdayspassed;state;salt;frostsalts;holly;nord;orc;altmer;arg;bosmer;breton;khajiit;dunmer;redguard;imperial;;
SCDT 
SCTX
    begin nc_frost_vat
    
    short currentday
    short localdayspassed
    short state
    short salt
    short frostsalts
    short holly
    short nord
    short orc
    short altmer
    short arg
    short bosmer
    short breton
    short khajiit
    short dunmer
    short redguard
    short imperial
    
    if ( menumode == 1 )
    	return
    endif
    
        set altmer to getItemCount "nc_altmer_pauldron" 
        set arg to getItemCount "nc_arg_pauldron" 
        set bosmer to getItemCount "nc_bosmer_pauldron" 
        set breton to getItemCount "nc_breton_pauldron" 
        set dunmer to getItemCount "nc_dunmer_pauldron"
        set imperial to getItemCount "nc_imperial_pauldron" 
        set khajiit to getItemCount "nc_khajiit_pauldron" 
        set nord to getItemCount "nc_nord_pauldron" 
        set orc to getItemCount "nc_orc_pauldron" 
        set redguard to getItemCount "nc_rg_pauldron"
        	set salt to getItemCount "nc_salt"
    	set frostsalts to getItemCount "ingred_frost_salts_01"
    	set holly to getItemCount "ingred_holly_01"
    
    
    set state to 2
    if ( salt >= 500 )
       if ( frostsalts >= 5 )
          if ( holly >= 5 )
              set state to 1
         endif
       endif
    endif
    
    
    if ( altmer >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "A1V3_FrostCured" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( arg >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_arg_pauldron" 1
    			additem "A1V3_FrostCured" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( bosmer >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
