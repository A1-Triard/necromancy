    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    	Disable
    endif
    
    end

SCPT
SCHD nc_imp_skelly_pauldron_f 6 0 0 283 52
SCVR state;check;checkCounter;OnPCDrop;OnPCEquip;counter;;
SCDT 
SCTX
    begin nc_imp_skelly_pauldron_f
    
    short state
    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_imp_skelly_NPC_f" 1 50 1
    	Disable
    endif
    
    end

SCPT
SCHD nc_keytest 2 2 0 473 25
SCVR counter;stemp;lkey;ltemp;;
SCDT 
SCTX
    Begin nc_keytest
    
    short counter
    short stemp
    long lkey
    long ltemp
    
    if ( MenuMode == 1 )
    	return
    endif
    
    ; Only run every 15th frame. 
    set counter to ( counter + 1 )
    if ( counter < 15 )
    	return
    endif
    set counter to 0
    
    ; Get status for customizeable key (default 'b')
    set lkey to nc_keycode  ; Cannot use globals with setx
    setx ltemp to xKeyPressed lkey
    
    ; If the player wants to configure his keycode setting, he can
    ; set nc_keycode to 0 (zero) and this script will just output
    ; the code for the pressed key.
    if ( lkey == 0 )
    	if ( ltemp )  ; Skip if no key was pressed
    		messagebox "Код нажатой клавиши: %g" ltemp
    		return
    	endif
    endif
    
    ; Our key was pressed?
    ifx ( ltemp )
    else
    	return
    endif
    
    ; Give a longer delay to avoid multiple messages
    set counter to -20
    
    ; Sneak: Skip wander
    if ( nc_follow == 2 )
    	if ( GetPCSneaking == 1 )
    		set nc_follow to 0
    	endif
    endif
    
    stopscript nc_stand_script
    stopscript nc_follow_script
    stopscript nc_wander_script
    
    if ( nc_follow == 0 )
    ;	messagebox "Your revenants will stay here."
    	startscript nc_stand_script
    	set nc_follow to 1
    elseif ( nc_follow == 1 )
    ;	MessageBox "Your revenants will follow you."
    	startscript nc_follow_script
    	set nc_follow to 2
    elseif ( nc_follow == 2 )
    ;	MessageBox "I will do nothing."
    	startscript nc_wander_script
    	set nc_follow to 0
    endif
    
    End
    

SCPT
SCHD nc_keytest_rev 2 2 0 497 25
