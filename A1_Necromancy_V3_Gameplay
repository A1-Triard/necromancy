    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "A1V3_PoisonCured" 1 50 1
    	Disable
    endif
    
    end

SCPT
