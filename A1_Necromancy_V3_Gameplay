    		AIFollow Player 0 0 0 0
    		set c_move to 0
    	endif
    endif
    
    if ( oneTimeMove == 1 )
    	set moveTimer to moveTimer + GetSecondsPassed
    	if ( moveTimer > 4 )
    		set moveTimer to 0
    		AiFollow Player 0 0 0 0
    		set oneTimeMove to 0
    	endif
    endif
    
    if ( call == 0 )
    	if ( nc_callrev == 1 )
    		AIWander 0 0 0
    		set call to 1
    	elseif ( nc_callrev == 2 )
    		AIFollow player 0 0 0 0
    		set call to 2
    	elseif ( nc_callrev == 3 )
    		AIWander 500 0 0
    		set call to 3
    	endif
    	if ( nc_callmessage == 0 )
    		set nc_callmessage to 1
    	endif
    endif
    
    if ( nc_callrev == 0 )
    	set call to 0
    endif
    
    if ( travel > 0 )
    	if ( CellChanged == 0 )
    		if ( travel == 1 )
    			AIWander 500 0 0
    			PositionCell -23265.2344, -15409.3721, 556.0000, 0, "Balmora"
    			set travel to 0
    		endif 
    		if ( travel == 2 )
    			AIWander 500 0 0
    			PositionCell -14688.3975, 52477.6445, 2119.9502, 0, "Ald-ruhn"
    			set travel to 0
    		endif 
    		if ( travel == 3 )
    			AIWander 500 0 0
    			PositionCell 48308.0547, -81831.6328, 576.7466, 0, "Vivec"
    			set travel to 0
    		endif 
    		if ( travel == 4 )
    			AIWander 500 0 0
    			PositionCell 105102.6641, -61811.3477, 635.0835, 0, "Molag Mar"
    			set travel to 0
    		endif 
    		if ( travel == 5 )
    			AIWander 500 0 0
    			PositionCell 149019.1250, 29684.0938, 700.0000, 0, "Wolverine Hall"
    			set travel to 0
    		endif 
    		if ( travel == 6 )
    			AIWander 500 0 0
    			PositionCell 98660.6641, 111530.5625, 308.0195, 0, "Vos"
    			set travel to 0
    		endif 
    		if ( travel == 7 )
    			AIWander 500 0 0
    			PositionCell 59803.8828, 183183.3594, 314.2573, 0, "Dagon Fel"
    			set travel to 0
    		endif 
    		if ( travel == 8 )
    			AIWander 500 0 0
    			PositionCell -67142.3594, 139129.8594, 205.1514, 0, "Khuul"
    			set travel to 0
    		endif 
    		if ( travel == 9 )
    			AIWander 500 0 0
    			PositionCell -84440.8203, 92241.1953, 1054.9253, 0, "Gnisis"
    			set travel to 0
    		endif 
    		if ( travel == 10 )
    			AIWander 500 0 0
    			PositionCell 20736.9922, 38396.8008, 1161.2622, 0, "Ghostgate"
    			set travel to 0
    		endif
    		if ( travel == 11 )
    			AIWander 500 0 0
    			PositionCell -5.3433, 2476.1021, 173.3262, 0, "Mournhold, Plaza Brindisi Dorom"
    			set travel to 0
    		endif 
    	endif
    endif
    
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
    
    if ( counter < 20 )
    	set counter to ( counter + 1 )
    	return
    endif
    set counter to 0
    
    if ( GetAlchemy < 100 )
    	set temp to 1
    else
    	set temp to 0
    endif
    ifx ( temp)
    	SetAlchemy 100
    	set temp to skills1 / 256
    	SetBlock temp
    	set temp to skills1 - 256 * temp
    	SetMediumArmor temp
    	set temp to skills2 / 256
    	SetHeavyArmor temp
    	set temp to skills2 - 256 * temp
    	SetBluntWeapon temp
    	set temp to skills3 / 256
    	SetLongBlade temp
    	set temp to skills3 - 256 * temp
    	SetAxe temp
    	set temp to skills4 / 256
    	SetSpear temp
    	set temp to skills4 - 256 * temp
    	SetDestruction temp
    	set temp to skills5 / 256
    	SetAlteration temp
    	set temp to skills5 - 256 * temp
    	SetIllusion temp
    	set temp to skills6 / 256
    	SetConjuration temp
    	set temp to skills6 - 256 * temp
    	SetMysticism temp
    	set temp to skills7 / 256
    	SetRestoration temp
    	set temp to skills7 - 256 * temp
    	SetUnarmored temp
    	set temp to skills8 / 256
    	SetLightArmor temp
    	set temp to skills8 - 256 * temp
    	SetShortBlade temp
    	set temp to skills9 / 256
    	SetMarksman temp
    	set temp to skills9 - 256 * temp
    	SetHandToHand temp
    
    ;	set count to 0
    ;	if ( hash == -2147483648)
    ;		set temp to 0
    ;	elseif ( hash < 0 )
    ;		set count to 1
    ;		set temp to -hash
    ;	else
    ;		set temp to hash
    ;	endif
    ;
    ;	ifx ( count )
    ;		setx npcfile to xStringBuild "A1NPC_0%d", temp
    ;	else
    ;		setx npcfile to xStringBuild "A1NPC_%d", temp
    ;	endif
    ;
    ;	xFileRewind npcfile
    ;	setx count,temp to xFileReadShort npcfile, 1
    ;	if ( temp == 20558 )
    ;		set count to 1
    ;	else
    ;		set count to 0
    ;	endif
    ;	ifx ( count )
    ;		xFileReadString npcfile
    ;		setx temp to xFileReadString npcfile
    ;		whilex ( count )
    ;			setx count,temp to xFileReadShort npcfile, 1
    ;			if ( temp == 20563 )
    ;				set count to 1
    ;			else
    ;				set count to 0
    ;			endif
    ;			ifx ( count )
    ;				setx temp to xFileReadString npcfile
    ;				xAddSpell temp
    ;			endif
    ;		endwhile
    ;	endif
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetDistance Player < 300 )
    		SetSpeed 15
    		SetAthletics 15
    	else
    		set c_speed to ( ( Player->GetSpeed ) * 2.25 )
    		set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
    		SetSpeed c_speed
    		SetAthletics c_athletics
    	endif
    
    	if ( GetItemCount p_restore_health_b >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_c >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_e >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_q >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_s >= 1 )
    		set p_Check to 1
    	else
    		set p_Check to 0
    	endif
    else
    	SetSpeed 40
    endif
    
    if ( doOnce3 == 0 )
    	if ( GetHealth <= 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_rg_pauldron_sk_f", 1
    		AddItem "nc_redguard_skull_f", 1
    		AddItem "ingred_bonemeal_01", 15
    		AddItem "ingred_scrap_metal_01", 1
    		AddItem "ingred_netch_leather_01", 1
    		if ( GetItemCount nc_SoulGem_blackened > 0 )
    			set coDist to ( player->GetLuck )
    			set c_speed to coDist
    			if ( c_speed < 40 )
    				set conj to ( c_speed - 40 )
    			elseif ( c_speed < 60 )
    				set c_speed to ( c_speed - 40 )
    				set conj to ( c_speed / 4 )
    			else
    				set c_speed to ( c_speed - 60 )
    				set c_speed to ( c_speed / 8 )
    				set conj to ( 5 + c_speed )
    			endif
    			set coDist to ( player->GetConjuration )
    			set c_speed to coDist
    			set conj to ( conj + c_speed )
    			set c_speed to ( Random 100 )
    			if ( c_speed < conj )
    				RemoveItem "nc_SoulGem_blackened", 1
    				set A1V3_NecroSkills1 to skills1
    				set A1V3_NecroSkills2 to skills2
    				set A1V3_NecroSkills3 to skills3
    				set A1V3_NecroSkills4 to skills4
    				set A1V3_NecroSkills5 to skills5
    				set A1V3_NecroSkills6 to skills6
    				set A1V3_NecroSkills7 to skills7
    				set A1V3_NecroSkills8 to skills8
    				set A1V3_NecroSkills9 to skills9
    				set count to hash
    				set temp to ( count / 65536 )
    				set A1V3_SoulGemHashHi to temp
    				set count to ( count - temp * 65536 )
    				set A1V3_SoulGemHashLow to count
    				AddItem "nc_SoulGem_redguard", 1
    			endif
    		endif
    		set doOnce3 to 1
    	endif
    endif
    
    end

SCPT
SCHD A1V3_NecroTrack 6 0 1 769 49
SCVR doonce2;doonce3;doonce4;doonce5;wait;wait2;timer;;
SCDT 
SCTX
    Begin A1V3_NecroTrack
    
    float timer
    float timer2
    float seconds
    short doonce2
    short doonce3
    short doonce4
    short doonce5
    short wait
    short wait2
    short temp
    short burden
    
    if ( MenuMode == 1 )
    	return
    endif
    
    ; Dont waste time checking spells more than once per second
    set seconds to GetSecondsPassed
    set timer to ( timer + seconds )
    if ( timer < 1 )
    	return
    endif
    set timer to 0
    
    if ( nc_necro_skill == 10 )
    	if ( doonce4 == 0 )
    		MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать более сильных мертвецов."
    		set doonce4 to 1
    	endif
    endif	
    
    if ( nc_necro_skill == 20 )
    	if ( doonce3 == 0 )
    		MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать больших мертвецов."
    		set doonce3 to 1
    	endif
    endif	
    
    if ( nc_necro_skill == 30 )
    	if ( doonce2 == 0 )
    		MessageBox "Вы увеличили свое умение вызова мертвецов. Идите и поговорите с Трагриларом."
    		set doonce2 to 1
    	endif
    endif
    
    if ( player->GetSpellEffects A1_NecroTrap == 1 )
    	if ( wait == 0 )
    		startscript nc_death_trap
    		set wait to 1
    	endif
    else
    	if ( wait == 1 )
    		set wait to 0
    		stopscript nc_death_trap
    	endif
    endif
    
    if ( player->GetSpellEffects A1_NecroMirror == 1 )
    	if ( wait2 == 0 )
    		startscript A1V3_NecroMirror_sc
    		set wait2 to 1
    	endif
    else
    	set wait2 to 0
    endif
    
    if ( player->getspelleffects nc_painful_touch == 1 )
    	startscript nc_warlord_attack
    endif
    
    if ( A1V3_NecroCorpse <= 0 )
    	set A1V3_NecroCorpse to 0
    	if ( corpse == 1 )
    		set corpse to 0
    		ModPCCrimeLevel -10000
    	endif
    else
    	if ( corpse == 0 )
    		set corpse to 1
    		ModPCCrimeLevel 10000
    	endif
    endif
    
    if ( A1V3_NecroBurden != 0 )
    	if ( GetPCRunning == 1 )
    		if ( A1V3_NecroBurden == 1 )
    			player->ModCurrentFatigue -2
    		else
    			player->ModCurrentFatigue -4
    		endif
    	endif
    endif
    set timer2 to ( timer2 + seconds )
    if ( timer2 < 14 )
    	return
    endif
    set timer2 to 0
    
    set temp to 0
    if ( player->GetItemCount "nc_altmer_corpse" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "nc_altmer_corpse"
    		set temp to 1
    	else
    		player->RemoveItem "nc_altmer_corpse", 1
    		player->AddItem "nc_altmer_pauldron", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "nc_altmer_corpse_f" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "nc_altmer_corpse_f"
    		set temp to 1
    	else
    		player->RemoveItem "nc_altmer_corpse_f", 1
    		player->AddItem "nc_altmer_pauldron_f", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "nc_arg_corpse" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "nc_arg_corpse"
    		set temp to 1
    	else
    		player->RemoveItem "nc_arg_corpse", 1
    		player->AddItem "nc_arg_pauldron", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "nc_arg_corpse_f" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "nc_arg_corpse_f"
    		set temp to 1
    	else
    		player->RemoveItem "nc_arg_corpse_f", 1
    		player->AddItem "nc_arg_pauldron_f", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "nc_bosmer_corpse" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "nc_bosmer_corpse"
    		set temp to 1
    	else
    		player->RemoveItem "nc_bosmer_corpse", 1
    		player->AddItem "nc_bosmer_pauldron", 1
    		set temp to 2
    	endif
