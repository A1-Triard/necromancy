SCDT 
SCTX
    begin nc_pauldron_altmer_script
    
    short state
    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_altmer_corpse" 1 50 1
    	Disable
    endif
    
    end

