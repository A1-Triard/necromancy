    		player->AddItem "nc_rg_pauldron", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "nc_redguard_corpse_f" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "nc_redguard_corpse_f"
    		set temp to 1
    	else
    		player->RemoveItem "nc_redguard_corpse_f", 1
    		player->AddItem "nc_rg_pauldron_f", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "A1V3_FireCured" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "A1V3_FireCured"
    		set temp to 1
    	else
    		player->RemoveItem "A1V3_FireCured", 1
    		player->AddItem "nc_fire_pauldron", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "A1V3_FrostCured" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "A1V3_FrostCured"
    		set temp to 1
    	else
    		player->RemoveItem "A1V3_FrostCured", 1
    		player->AddItem "nc_frost_pauldron", 1
    		set temp to 2
    	endif
    elseif ( player->GetItemCount "A1V3_PoisonCured" == 1 )
    	if ( A1V3_NecroBurden != 0 )
    		player->Drop "A1V3_PoisonCured"
    		set temp to 1
    	else
