    				PlaceAtMe "A1V3_NecroOrc3" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroOrc3_f" 1, 0, 0
    			endif
    		endif
    		PlaySound, "ThunderClap"
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		Disable
    	endif
    	return
    endif
    
    ifx ( OnPCEquip )
    	set OnPCEquip to 0
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx count to xFileReadString npcfile
    		xMessageFix count "OK"
    		MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" "OK"
    	endif
    endif
    
    End

