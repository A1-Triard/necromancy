    		if ( flyCheck == 0 )
    			Cast nc_comp2_lev_bog player
    			AddSpell nc_comp2_lev
    			set flyCheck to 1
    		endif
    	else
    		if ( flyCheck == 1 )
    			RemoveSpell nc_comp2_lev
    			set flyCheck to 0
    		endif
    	endif
    
    	if ( player->GetEffect sEffectWaterWalking == 1 )
    		if ( wwCheck == 0 )
    			Cast nc_comp2_ww_bog player
    			AddSpell nc_comp2_ww
    			set wwCheck to 1
    		endif
    	else
    		if ( wwCheck == 1 )
    			RemoveSpell nc_comp2_ww
    			set wwCheck to 0
    		endif
    	endif
    
    	if ( flyCheck == 1 )
    		if ( GetWeaponDrawn == 0 )
    			set playz to ( Player->GetPos z )
    			SetPos z playz
    		endif
    	endif
    
    	if ( GetWeaponDrawn == 1 )
    		if ( flyCheck == 0 )
    			set combatCheck to 1
    		else
    			set combatDone to 1
    		endif
