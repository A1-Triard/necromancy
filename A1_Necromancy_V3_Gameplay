    	if ( c_move == 0 )
    		if ( GetCurrentAIPackage == 3 )
    			if ( GetDistance Player < 70 )
    				AIWander 300 0 0 0 0
    				set c_move to 1
    			endif
    		endif
    	endif
    endif
    
    if ( c_move == 1 )
    	if ( GetDistance Player > 100 )
    		AIFollow Player 0 0 0 0
    		set c_move to 0
    	endif
    endif
    
    if ( oneTimeMove == 1 )
    	set moveTimer to moveTimer + GetSecondsPassed
    	if ( moveTimer > 4 )
    		set moveTimer to 0
    		AiFollow Player 0 0 0 0
    		set oneTimeMove to 0
    	endif
    endif
    
    if ( call == 0 )
    	if ( nc_callrev == 1 )
    		AIWander 0 0 0
    		set call to 1
    	elseif ( nc_callrev == 2 )
    		AIFollow player 0 0 0 0
    		set call to 2
    	elseif ( nc_callrev == 3 )
    		AIWander 500 0 0
    		set call to 3
    	endif
    	if ( nc_callmessage == 0 )
    		set nc_callmessage to 1
    	endif
    endif
    
    if ( nc_callrev == 0 )
    	set call to 0
    endif
    
