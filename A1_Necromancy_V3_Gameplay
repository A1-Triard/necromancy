    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	if ( player->HasItemEquipped "nc_dunmer_pauldron_sk" == 0 )
    		player->Equip "nc_dunmer_pauldron_sk"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_dunmer_pauldron_sk_f" >= 1 )
    	if ( player->HasItemEquipped "nc_dunmer_pauldron_sk_f" == 0 )
    		player->Equip "nc_dunmer_pauldron_sk_f"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	if ( player->HasItemEquipped "nc_imperial_pauldron_sk" == 0 )
    		player->Equip "nc_imperial_pauldron_sk"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_imperial_pauldron_sk_f" >= 1 )
    	if ( player->HasItemEquipped "nc_imperial_pauldron_sk_f" == 0 )
    		player->Equip "nc_imperial_pauldron_sk_f"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	if ( player->HasItemEquipped "nc_khajiit_pauldron_sk" == 0 )
    		player->Equip "nc_khajiit_pauldron_sk"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_khajiit_pauldron_sk_f" >= 1 )
    	if ( player->HasItemEquipped "nc_khajiit_pauldron_sk_f" == 0 )
    		player->Equip "nc_khajiit_pauldron_sk_f"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	if ( player->HasItemEquipped "nc_nord_pauldron_sk" == 0 )
    		player->Equip "nc_nord_pauldron_sk"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_nord_pauldron_sk_f" >= 1 )
    	if ( player->HasItemEquipped "nc_nord_pauldron_sk_f" == 0 )
    		player->Equip "nc_nord_pauldron_sk_f"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	if ( player->HasItemEquipped "nc_orc_pauldron_sk" == 0 )
    		player->Equip "nc_orc_pauldron_sk"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_orc_pauldron_sk_f" >= 1 )
    	if ( player->HasItemEquipped "nc_orc_pauldron_sk_f" == 0 )
    		player->Equip "nc_orc_pauldron_sk_f"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	if ( player->HasItemEquipped "nc_rg_pauldron_sk" == 0 )
    		player->Equip "nc_rg_pauldron_sk"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_rg_pauldron_sk_f" >= 1 )
    	if ( player->HasItemEquipped "nc_rg_pauldron_sk_f" == 0 )
    		player->Equip "nc_rg_pauldron_sk_f"
    		MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    endif
    
    StopScript A1V3_EquipCorpse
    
    end

SCPT
SCHD A1V3_NecroArgo_sc 30 13 18 5660 459
SCVR setUp;see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;conj;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;p_Check;emb_hook_nominion;call;travel;name;hash;skills1;skills2;skills3;skills4;skills5;skills6;skills7;skills8;skills9;temp;count;npcfile;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;sheatheTimer;moveTimer;;
SCDT 
SCTX
    begin A1V3_NecroArgo_sc
    
    short setUp
    long hash
    long skills1
    long skills2
    long skills3
    long skills4
    long skills5
    long skills6
    long skills7
    long skills8
    long skills9
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
    float timer
    float sheatheTimer
    float moveTimer
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short conj
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
    short counter
    short p_Check
    short emb_hook_nominion
    short call
    short travel
    short name
    long temp
    long count
    long npcfile
    
    set temp to 0
    if ( MenuMode == 1 )
    	set temp to 1
    endif
    ifx ( temp )
    	ifx ( name )
    		set name to 0
    		set count to 0
    		if ( hash == -2147483648)
    			set temp to 0
    		elseif ( hash < 0 )
    			set count to 1
    			set temp to -hash
    		else
    			set temp to hash
    		endif
    
    		ifx ( count )
    			setx npcfile to xStringBuild "A1NPC_0%d", temp
    		else
    			setx npcfile to xStringBuild "A1NPC_%d", temp
    		endif
    
    		xFileRewind npcfile
    		setx count,temp to xFileReadShort npcfile, 1
    		if ( temp == 20558 )
    			set count to 1
    		else
    			set count to 0
    		endif
    		ifx ( count )
    			setx temp to xFileReadString npcfile
    			xMessageFix temp
    			MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
    		endif
    	endif
    	return
    endif
    
    set temp to 0
    if ( setUp == 0 )
    	set setUp to 1
    	set hash to A1V3_SoulGemHashHi
    	set hash to ( hash * 65536 )
    	set temp to A1V3_SoulGemHashLow
    	set hash to ( hash + temp )
    	set A1V3_SoulGemHashHi to 0
    	set A1V3_SoulGemHashLow to 0
    	set skills1 to A1V3_NecroSkills1
    	set skills2 to A1V3_NecroSkills2
    	set skills3 to A1V3_NecroSkills3
    	set skills4 to A1V3_NecroSkills4
    	set skills5 to A1V3_NecroSkills5
    	set skills6 to A1V3_NecroSkills6
    	set skills7 to A1V3_NecroSkills7
    	set skills8 to A1V3_NecroSkills8
    	set skills9 to A1V3_NecroSkills9
    	set companion to 1
    	set emb_hook_nominion to 1
    	AddTopic "- Бой"
    	AddTopic "- Сопровождение"
    	AddTopic "- Инструкции"
    	AddTopic "- Перемещение"
    	return
    endif
    
    if ( weapUse == 1 )
    	if ( Player->GetWeaponType <= 8 )
    		setmarksman 0
    	else
    		setmarksman 200
    	endif
    endif
    
    if ( combatCheck == 1 )
    	set sheathetimer to sheatheTimer + GetSecondsPassed
    	if ( GetSoundPlaying, "Weapon Swish" == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, crossbowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, bowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( sheatheTimer > 4 )
    		if ( GetSoundPlaying "Weapon Swish" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "crossbowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "bowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		endif
    	endif
    endif
    
    if ( soundPlayed == 1 )
    	equip "silver dagger"
    	removeitem "silver dagger" 1
    	; messagebox "Working"
    	set soundPlayed to 0
    	set combatCheck to 0
    	set combatDone to 1
    endif
    
    if ( combatDone == 1 )
    	set timer to timer + GetSecondsPassed
    	if ( timer > 6 )
    		set timer to 0
    		set combatDone to 0
    		set warp to 0
    	endif
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetPCSneaking == 1 )
    		ForceSneak
    	else
    		ClearForceSneak
    	endif
    
    	if ( player->GetEffect sEffectLevitate == 1 )
    		if ( flyCheck == 0 )
    			Cast nc_comp2_lev_bog player
    			AddSpell nc_comp2_lev
    			set flyCheck to 1
    		endif
    	else
    		if ( flyCheck == 1 )
    			RemoveSpell nc_comp2_lev
    			set flyCheck to 0
    		endif
    	endif
    
    	if ( player->GetEffect sEffectWaterWalking == 1 )
    		if ( wwCheck == 0 )
    			Cast nc_comp2_ww_bog player
    			AddSpell nc_comp2_ww
    			set wwCheck to 1
    		endif
    	else
    		if ( wwCheck == 1 )
    			RemoveSpell nc_comp2_ww
    			set wwCheck to 0
    		endif
    	endif
    
    	if ( flyCheck == 1 )
    		if ( GetWeaponDrawn == 0 )
    			set playz to ( Player->GetPos z )
    			SetPos z playz
    		endif
    	endif
    
    	if ( GetWeaponDrawn == 1 )
    		if ( flyCheck == 0 )
    			set combatCheck to 1
    		else
    			set combatDone to 1
    		endif
    		set timer to 0
    		set warp to 1
    		return
    	endif
    
    	set ax to ( Player->GetPos x )
    	set ay to ( Player->GetPos y )
    
    	if ( doOnce == 0 )
    		set bx to ( Player->GetPos x )
    		set by to ( Player->GetPos y )
    		set bz to ( Player->GetPos z )
    		set doOnce to 1
    	endif
    
    	set t1 to ( ax - bx )
    	set t1 to ( t1 * t1 )
    	set t2 to ( ay - by )
    	set t2 to ( t2 * t2 )
    	set t1 to ( t1 + t2 )
    	set coDist to ( GetSquareRoot, t1 )
    
    	if ( coDist > 360 )
    		set doOnce to 0
    	endif
    
    	if ( coDist > 180 )
    		if ( doOnce2 == 0 )
    			set cx to ( Player->GetPos x )
    			set cy to ( Player->GetPos y )
    			set cz to ( Player->GetPos z )
    			set doOnce2 to 1
    		endif
    	endif
    
    	set t3 to ( ax - cx )
    	set t3 to ( t3 * t3 )
    	set t4 to ( ay - cy )
    	set t4 to ( t4 * t4 )
    	set t3 to ( t3 + t4 )
    	set coDist2 to ( GetSquareRoot, t3 )
    
    	if ( coDist2 > 360 )
    		set doOnce2 to 0
    	endif
    
    	if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    	if ( see_p_warlord == 0 )
    
    	if ( warp == 0 )
    		if ( coDist > 350 )
    			if ( GetDistance Player > 680 )
    				SetPos x bx
    				SetPos y by
    				SetPos z bz
    				AiFollow Player 0 0 0 0
    			endif
    		endif
    	endif
    
    	endif
    	endif
    	endif
    endif
    
    if ( see_f_warlord == 1 )
    	if ( nc_f_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_fr_warlord == 1 )
    	if ( nc_fr_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_p_warlord == 1 )
    	if ( nc_p_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
    
    if ( compMove == 1 )
    	if ( c_move == 0 )
    		if ( GetCurrentAIPackage == 3 )
    			if ( GetDistance Player < 70 )
    				AIWander 300 0 0 0 0
    				set c_move to 1
    			endif
    		endif
    	endif
    endif
    
    if ( c_move == 1 )
    	if ( GetDistance Player > 100 )
    		AIFollow Player 0 0 0 0
    		set c_move to 0
    	endif
    endif
    
    if ( oneTimeMove == 1 )
    	set moveTimer to moveTimer + GetSecondsPassed
    	if ( moveTimer > 4 )
    		set moveTimer to 0
    		AiFollow Player 0 0 0 0
    		set oneTimeMove to 0
    	endif
    endif
    
    if ( call == 0 )
    	if ( nc_callrev == 1 )
    		AIWander 0 0 0
    		set call to 1
    	elseif ( nc_callrev == 2 )
    		AIFollow player 0 0 0 0
    		set call to 2
    	elseif ( nc_callrev == 3 )
    		AIWander 500 0 0
    		set call to 3
    	endif
    	if ( nc_callmessage == 0 )
    		set nc_callmessage to 1
    	endif
    endif
    
    if ( nc_callrev == 0 )
    	set call to 0
    endif
    
    if ( travel > 0 )
    	if ( CellChanged == 0 )
    		if ( travel == 1 )
    			AIWander 500 0 0
    			PositionCell -23265.2344, -15409.3721, 556.0000, 0, "Balmora"
    			set travel to 0
    		endif 
    		if ( travel == 2 )
    			AIWander 500 0 0
    			PositionCell -14688.3975, 52477.6445, 2119.9502, 0, "Ald-ruhn"
    			set travel to 0
    		endif 
    		if ( travel == 3 )
    			AIWander 500 0 0
    			PositionCell 48308.0547, -81831.6328, 576.7466, 0, "Vivec"
    			set travel to 0
    		endif 
    		if ( travel == 4 )
    			AIWander 500 0 0
    			PositionCell 105102.6641, -61811.3477, 635.0835, 0, "Molag Mar"
    			set travel to 0
    		endif 
    		if ( travel == 5 )
    			AIWander 500 0 0
    			PositionCell 149019.1250, 29684.0938, 700.0000, 0, "Wolverine Hall"
    			set travel to 0
    		endif 
    		if ( travel == 6 )
    			AIWander 500 0 0
    			PositionCell 98660.6641, 111530.5625, 308.0195, 0, "Vos"
    			set travel to 0
    		endif 
    		if ( travel == 7 )
    			AIWander 500 0 0
    			PositionCell 59803.8828, 183183.3594, 314.2573, 0, "Dagon Fel"
    			set travel to 0
    		endif 
    		if ( travel == 8 )
    			AIWander 500 0 0
    			PositionCell -67142.3594, 139129.8594, 205.1514, 0, "Khuul"
    			set travel to 0
    		endif 
    		if ( travel == 9 )
    			AIWander 500 0 0
    			PositionCell -84440.8203, 92241.1953, 1054.9253, 0, "Gnisis"
    			set travel to 0
    		endif 
    		if ( travel == 10 )
    			AIWander 500 0 0
    			PositionCell 20736.9922, 38396.8008, 1161.2622, 0, "Ghostgate"
    			set travel to 0
    		endif
    		if ( travel == 11 )
    			AIWander 500 0 0
    			PositionCell -5.3433, 2476.1021, 173.3262, 0, "Mournhold, Plaza Brindisi Dorom"
    			set travel to 0
    		endif 
    	endif
    endif
    
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
    
    if ( counter < 20 )
    	set counter to ( counter + 1 )
    	return
    endif
    set counter to 0
    
    if ( GetAlchemy < 100 )
    	set temp to 1
    else
    	set temp to 0
    endif
    ifx ( temp)
    	SetAlchemy 100
    	set temp to ( skills1 / 256 )
    	SetBlock temp
    	set temp to ( skills1 - 256 * temp )
    	SetMediumArmor temp
    	set temp to ( skills2 / 256 )
    	SetHeavyArmor temp
    	set temp to ( skills2 - 256 * temp )
    	SetBluntWeapon temp
    	set temp to ( skills3 / 256 )
    	SetLongBlade temp
    	set temp to ( skills3 - 256 * temp )
    	SetAxe temp
    	set temp to ( skills4 / 256 )
    	SetSpear temp
    	set temp to ( skills4 - 256 * temp )
    	SetDestruction temp
    	set temp to ( skills5 / 256 )
    	SetAlteration temp
    	set temp to ( skills5 - 256 * temp )
    	SetIllusion temp
    	set temp to ( skills6 / 256 )
    	SetConjuration temp
    	set temp to ( skills6 - 256 * temp )
    	SetMysticism temp
    	set temp to ( skills7 / 256 )
    	SetRestoration temp
    	set temp to ( skills7 - 256 * temp )
    	SetUnarmored temp
    	set temp to ( skills8 / 256 )
    	SetLightArmor temp
    	set temp to ( skills8 - 256 * temp )
    	SetShortBlade temp
    	set temp to ( skills9 / 256 )
    	SetMarksman temp
    	set temp to ( skills9 - 256 * temp )
    	SetHandToHand temp
    
    ;	set count to 0
    ;	if ( hash == -2147483648)
    ;		set temp to 0
    ;	elseif ( hash < 0 )
    ;		set count to 1
    ;		set temp to -hash
    ;	else
    ;		set temp to hash
    ;	endif
    ;
    ;	ifx ( count )
    ;		setx npcfile to xStringBuild "A1NPC_0%d", temp
    ;	else
    ;		setx npcfile to xStringBuild "A1NPC_%d", temp
    ;	endif
    ;
    ;	xFileRewind npcfile
    ;	setx count,temp to xFileReadShort npcfile, 1
    ;	if ( temp == 20558 )
    ;		set count to 1
    ;	else
    ;		set count to 0
    ;	endif
    ;	ifx ( count )
    ;		xFileReadString npcfile
    ;		setx temp to xFileReadString npcfile
    ;		whilex ( count )
    ;			setx count,temp to xFileReadShort npcfile, 1
    ;			if ( temp == 20563 )
    ;				set count to 1
    ;			else
    ;				set count to 0
    ;			endif
    ;			ifx ( count )
    ;				setx temp to xFileReadString npcfile
    ;				xAddSpell temp
    ;			endif
    ;		endwhile
    ;	endif
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetDistance Player < 300 )
    		SetSpeed 15
    		SetAthletics 15
    	else
    		set c_speed to ( ( Player->GetSpeed ) * 2.25 )
    		set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
    		SetSpeed c_speed
    		SetAthletics c_athletics
    	endif
    
    	if ( GetItemCount p_restore_health_b >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_c >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_e >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_q >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_s >= 1 )
    		set p_Check to 1
    	else
    		set p_Check to 0
    	endif
    else
    	SetSpeed 40
    endif
    
    if ( doOnce3 == 0 )
    	if ( GetHealth <= 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_arg_pauldron_sk", 1
    		AddItem "nc_argonian_skull", 1
    		AddItem "ingred_bonemeal_01", 15
    		AddItem "ingred_scrap_metal_01", 1
    		AddItem "ingred_netch_leather_01", 1
    		if ( GetItemCount nc_SoulGem_blackened > 0 )
    			set coDist to ( player->GetLuck )
    			set c_speed to coDist
    			if ( c_speed < 40 )
    				set conj to ( c_speed - 40 )
    			elseif ( c_speed < 60 )
    				set c_speed to ( c_speed - 40 )
    				set conj to ( c_speed / 4 )
    			else
    				set c_speed to ( c_speed - 60 )
    				set c_speed to ( c_speed / 8 )
    				set conj to ( 5 + c_speed )
    			endif
    			set coDist to ( player->GetConjuration )
    			set c_speed to coDist
    			set conj to ( conj + c_speed )
    			set c_speed to ( Random 100 )
    			if ( c_speed < conj )
    				RemoveItem "nc_SoulGem_blackened", 1
    				set A1V3_NecroSkills1 to skills1
    				set A1V3_NecroSkills2 to skills2
    				set A1V3_NecroSkills3 to skills3
    				set A1V3_NecroSkills4 to skills4
    				set A1V3_NecroSkills5 to skills5
    				set A1V3_NecroSkills6 to skills6
    				set A1V3_NecroSkills7 to skills7
    				set A1V3_NecroSkills8 to skills8
    				set A1V3_NecroSkills9 to skills9
    				set count to hash
    				set temp to ( count / 65536 )
    				set A1V3_SoulGemHashHi to temp
    				set count to ( count - temp * 65536 )
    				set A1V3_SoulGemHashLow to count
    				AddItem "nc_SoulGem_arg", 1
    			endif
    		endif
    		set doOnce3 to 1
    	endif
    endif
    
    end

SCPT
SCHD A1V3_NecroArgoF_sc 30 13 18 5664 459
SCVR setUp;see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;conj;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;p_Check;emb_hook_nominion;call;travel;name;hash;skills1;skills2;skills3;skills4;skills5;skills6;skills7;skills8;skills9;temp;count;npcfile;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;sheatheTimer;moveTimer;;
SCDT 
SCTX
    begin A1V3_NecroArgoF_sc
    
    short setUp
    long hash
    long skills1
    long skills2
    long skills3
    long skills4
    long skills5
    long skills6
    long skills7
    long skills8
    long skills9
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
    float timer
    float sheatheTimer
    float moveTimer
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short conj
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
    short counter
    short p_Check
    short emb_hook_nominion
    short call
    short travel
    short name
    long temp
    long count
    long npcfile
    
    set temp to 0
    if ( MenuMode == 1 )
    	set temp to 1
    endif
    ifx ( temp )
    	ifx ( name )
    		set name to 0
    		set count to 0
    		if ( hash == -2147483648)
    			set temp to 0
    		elseif ( hash < 0 )
    			set count to 1
    			set temp to -hash
    		else
    			set temp to hash
    		endif
    
    		ifx ( count )
    			setx npcfile to xStringBuild "A1NPC_0%d", temp
    		else
    			setx npcfile to xStringBuild "A1NPC_%d", temp
    		endif
    
    		xFileRewind npcfile
    		setx count,temp to xFileReadShort npcfile, 1
    		if ( temp == 20558 )
    			set count to 1
    		else
    			set count to 0
    		endif
    		ifx ( count )
    			setx temp to xFileReadString npcfile
    			xMessageFix temp
    			MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
    		endif
    	endif
    	return
    endif
    
    set temp to 0
    if ( setUp == 0 )
    	set setUp to 1
    	set hash to A1V3_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V3_SoulGemHashLow
    	set hash to hash + temp
    	set A1V3_SoulGemHashHi to 0
    	set A1V3_SoulGemHashLow to 0
    	set skills1 to A1V3_NecroSkills1
    	set skills2 to A1V3_NecroSkills2
    	set skills3 to A1V3_NecroSkills3
    	set skills4 to A1V3_NecroSkills4
    	set skills5 to A1V3_NecroSkills5
    	set skills6 to A1V3_NecroSkills6
    	set skills7 to A1V3_NecroSkills7
    	set skills8 to A1V3_NecroSkills8
    	set skills9 to A1V3_NecroSkills9
    	set companion to 1
    	set emb_hook_nominion to 1
    	AddTopic "- Бой"
    	AddTopic "- Сопровождение"
    	AddTopic "- Инструкции"
    	AddTopic "- Перемещение"
    	return
    endif
    
    if ( weapUse == 1 )
    	if ( Player->GetWeaponType <= 8 )
    		setmarksman 0
    	else
    		setmarksman 200
    	endif
    endif
    
    if ( combatCheck == 1 )
    	set sheathetimer to sheatheTimer + GetSecondsPassed
    	if ( GetSoundPlaying, "Weapon Swish" == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, crossbowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( GetSoundPlaying, bowShoot == 1 )
    		set sheatheTimer to 0
    		return
    	elseif ( sheatheTimer > 4 )
    		if ( GetSoundPlaying "Weapon Swish" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "crossbowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		elseif ( GetSoundPlaying "bowShoot" == 0 )
    			set sheatheTimer to 0
    			set soundPlayed to 1
    		endif
    	endif
    endif
    
    if ( soundPlayed == 1 )
    	equip "silver dagger"
    	removeitem "silver dagger" 1
    	; messagebox "Working"
    	set soundPlayed to 0
    	set combatCheck to 0
    	set combatDone to 1
    endif
    
    if ( combatDone == 1 )
    	set timer to timer + GetSecondsPassed
    	if ( timer > 6 )
    		set timer to 0
    		set combatDone to 0
    		set warp to 0
    	endif
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetPCSneaking == 1 )
    		ForceSneak
    	else
    		ClearForceSneak
    	endif
    
    	if ( player->GetEffect sEffectLevitate == 1 )
    		if ( flyCheck == 0 )
    			Cast nc_comp2_lev_bog player
    			AddSpell nc_comp2_lev
    			set flyCheck to 1
    		endif
    	else
    		if ( flyCheck == 1 )
    			RemoveSpell nc_comp2_lev
    			set flyCheck to 0
    		endif
    	endif
    
    	if ( player->GetEffect sEffectWaterWalking == 1 )
    		if ( wwCheck == 0 )
    			Cast nc_comp2_ww_bog player
    			AddSpell nc_comp2_ww
    			set wwCheck to 1
    		endif
    	else
    		if ( wwCheck == 1 )
    			RemoveSpell nc_comp2_ww
    			set wwCheck to 0
    		endif
    	endif
    
    	if ( flyCheck == 1 )
    		if ( GetWeaponDrawn == 0 )
    			set playz to ( Player->GetPos z )
    			SetPos z playz
    		endif
    	endif
    
    	if ( GetWeaponDrawn == 1 )
    		if ( flyCheck == 0 )
    			set combatCheck to 1
    		else
    			set combatDone to 1
    		endif
    		set timer to 0
    		set warp to 1
    		return
    	endif
    
    	set ax to ( Player->GetPos x )
    	set ay to ( Player->GetPos y )
    
    	if ( doOnce == 0 )
    		set bx to ( Player->GetPos x )
    		set by to ( Player->GetPos y )
    		set bz to ( Player->GetPos z )
    		set doOnce to 1
    	endif
    
    	set t1 to ( ax - bx )
    	set t1 to ( t1 * t1 )
    	set t2 to ( ay - by )
    	set t2 to ( t2 * t2 )
    	set t1 to ( t1 + t2 )
    	set coDist to ( GetSquareRoot, t1 )
    
    	if ( coDist > 360 )
    		set doOnce to 0
    	endif
    
    	if ( coDist > 180 )
    		if ( doOnce2 == 0 )
    			set cx to ( Player->GetPos x )
    			set cy to ( Player->GetPos y )
    			set cz to ( Player->GetPos z )
    			set doOnce2 to 1
    		endif
    	endif
    
    	set t3 to ( ax - cx )
    	set t3 to ( t3 * t3 )
    	set t4 to ( ay - cy )
    	set t4 to ( t4 * t4 )
    	set t3 to ( t3 + t4 )
    	set coDist2 to ( GetSquareRoot, t3 )
    
    	if ( coDist2 > 360 )
    		set doOnce2 to 0
    	endif
    
    	if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    	if ( see_p_warlord == 0 )
    
    	if ( warp == 0 )
    		if ( coDist > 350 )
    			if ( GetDistance Player > 680 )
    				SetPos x bx
    				SetPos y by
    				SetPos z bz
    				AiFollow Player 0 0 0 0
    			endif
    		endif
    	endif
    
    	endif
    	endif
    	endif
    endif
    
    if ( see_f_warlord == 1 )
    	if ( nc_f_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_fr_warlord == 1 )
    	if ( nc_fr_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_p_warlord == 1 )
    	if ( nc_p_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
    
    if ( compMove == 1 )
    	if ( c_move == 0 )
    		if ( GetCurrentAIPackage == 3 )
    			if ( GetDistance Player < 70 )
    				AIWander 300 0 0 0 0
    				set c_move to 1
    			endif
    		endif
    	endif
    endif
    
    if ( c_move == 1 )
    	if ( GetDistance Player > 100 )
    		AIFollow Player 0 0 0 0
    		set c_move to 0
    	endif
    endif
    
    if ( oneTimeMove == 1 )
    	set moveTimer to moveTimer + GetSecondsPassed
    	if ( moveTimer > 4 )
    		set moveTimer to 0
    		AiFollow Player 0 0 0 0
    		set oneTimeMove to 0
    	endif
    endif
    
    if ( call == 0 )
    	if ( nc_callrev == 1 )
    		AIWander 0 0 0
    		set call to 1
    	elseif ( nc_callrev == 2 )
    		AIFollow player 0 0 0 0
    		set call to 2
    	elseif ( nc_callrev == 3 )
    		AIWander 500 0 0
    		set call to 3
    	endif
    	if ( nc_callmessage == 0 )
    		set nc_callmessage to 1
    	endif
    endif
    
    if ( nc_callrev == 0 )
    	set call to 0
    endif
    
    if ( travel > 0 )
    	if ( CellChanged == 0 )
    		if ( travel == 1 )
    			AIWander 500 0 0
    			PositionCell -23265.2344, -15409.3721, 556.0000, 0, "Balmora"
    			set travel to 0
    		endif 
    		if ( travel == 2 )
    			AIWander 500 0 0
    			PositionCell -14688.3975, 52477.6445, 2119.9502, 0, "Ald-ruhn"
    			set travel to 0
    		endif 
    		if ( travel == 3 )
    			AIWander 500 0 0
    			PositionCell 48308.0547, -81831.6328, 576.7466, 0, "Vivec"
    			set travel to 0
    		endif 
    		if ( travel == 4 )
    			AIWander 500 0 0
    			PositionCell 105102.6641, -61811.3477, 635.0835, 0, "Molag Mar"
    			set travel to 0
    		endif 
    		if ( travel == 5 )
    			AIWander 500 0 0
    			PositionCell 149019.1250, 29684.0938, 700.0000, 0, "Wolverine Hall"
    			set travel to 0
    		endif 
    		if ( travel == 6 )
    			AIWander 500 0 0
    			PositionCell 98660.6641, 111530.5625, 308.0195, 0, "Vos"
    			set travel to 0
    		endif 
    		if ( travel == 7 )
    			AIWander 500 0 0
    			PositionCell 59803.8828, 183183.3594, 314.2573, 0, "Dagon Fel"
    			set travel to 0
    		endif 
    		if ( travel == 8 )
    			AIWander 500 0 0
    			PositionCell -67142.3594, 139129.8594, 205.1514, 0, "Khuul"
    			set travel to 0
    		endif 
    		if ( travel == 9 )
    			AIWander 500 0 0
    			PositionCell -84440.8203, 92241.1953, 1054.9253, 0, "Gnisis"
    			set travel to 0
    		endif 
    		if ( travel == 10 )
    			AIWander 500 0 0
    			PositionCell 20736.9922, 38396.8008, 1161.2622, 0, "Ghostgate"
    			set travel to 0
    		endif
    		if ( travel == 11 )
    			AIWander 500 0 0
    			PositionCell -5.3433, 2476.1021, 173.3262, 0, "Mournhold, Plaza Brindisi Dorom"
    			set travel to 0
    		endif 
    	endif
    endif
    
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
    
    if ( counter < 20 )
    	set counter to ( counter + 1 )
    	return
    endif
    set counter to 0
    
    if ( GetAlchemy < 100 )
    	set temp to 1
    else
    	set temp to 0
    endif
    ifx ( temp)
    	SetAlchemy 100
    	set temp to skills1 / 256
    	SetBlock temp
    	set temp to skills1 - 256 * temp
    	SetMediumArmor temp
    	set temp to skills2 / 256
    	SetHeavyArmor temp
    	set temp to skills2 - 256 * temp
    	SetBluntWeapon temp
    	set temp to skills3 / 256
    	SetLongBlade temp
    	set temp to skills3 - 256 * temp
    	SetAxe temp
    	set temp to skills4 / 256
    	SetSpear temp
    	set temp to skills4 - 256 * temp
    	SetDestruction temp
    	set temp to skills5 / 256
    	SetAlteration temp
    	set temp to skills5 - 256 * temp
    	SetIllusion temp
    	set temp to skills6 / 256
    	SetConjuration temp
    	set temp to skills6 - 256 * temp
    	SetMysticism temp
    	set temp to skills7 / 256
    	SetRestoration temp
    	set temp to skills7 - 256 * temp
    	SetUnarmored temp
    	set temp to skills8 / 256
    	SetLightArmor temp
    	set temp to skills8 - 256 * temp
    	SetShortBlade temp
    	set temp to skills9 / 256
    	SetMarksman temp
    	set temp to skills9 - 256 * temp
    	SetHandToHand temp
    
    ;	set count to 0
    ;	if ( hash == -2147483648)
    ;		set temp to 0
    ;	elseif ( hash < 0 )
    ;		set count to 1
    ;		set temp to -hash
    ;	else
    ;		set temp to hash
    ;	endif
    ;
    ;	ifx ( count )
    ;		setx npcfile to xStringBuild "A1NPC_0%d", temp
    ;	else
    ;		setx npcfile to xStringBuild "A1NPC_%d", temp
    ;	endif
    ;
    ;	xFileRewind npcfile
    ;	setx count,temp to xFileReadShort npcfile, 1
    ;	if ( temp == 20558 )
    ;		set count to 1
    ;	else
    ;		set count to 0
    ;	endif
    ;	ifx ( count )
    ;		xFileReadString npcfile
    ;		setx temp to xFileReadString npcfile
    ;		whilex ( count )
    ;			setx count,temp to xFileReadShort npcfile, 1
    ;			if ( temp == 20563 )
    ;				set count to 1
    ;			else
    ;				set count to 0
    ;			endif
