    	set t1 to ( t1 * t1 )
    	set t2 to ( ay - by )
    	set t2 to ( t2 * t2 )
    	set t1 to ( t1 + t2 )
    	set coDist to ( GetSquareRoot, t1 )
    
    	if ( coDist > 360 )
    		set doOnce to 0
    	endif
    
    	if ( coDist > 180 )
    		if ( doOnce2 == 0 )
    			set cx to ( Player->GetPos x )
    			set cy to ( Player->GetPos y )
    			set cz to ( Player->GetPos z )
    			set doOnce2 to 1
    		endif
    	endif
    
    	set t3 to ( ax - cx )
    	set t3 to ( t3 * t3 )
    	set t4 to ( ay - cy )
    	set t4 to ( t4 * t4 )
    	set t3 to ( t3 + t4 )
    	set coDist2 to ( GetSquareRoot, t3 )
    
