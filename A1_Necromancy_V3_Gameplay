    		if ( wwCheck == 0 )
    			Cast nc_comp2_ww_bog player
    			AddSpell nc_comp2_ww
    			set wwCheck to 1
    		endif
    	else
    		if ( wwCheck == 1 )
    			RemoveSpell nc_comp2_ww
    			set wwCheck to 0
    		endif
    	endif
    
