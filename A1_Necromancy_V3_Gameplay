    		endif 
    		if ( travel == 8 )
    			AIWander 500 0 0
    			PositionCell -67142.3594, 139129.8594, 205.1514, 0, "Khuul"
    			set travel to 0
    		endif 
    		if ( travel == 9 )
    			AIWander 500 0 0
    			PositionCell -84440.8203, 92241.1953, 1054.9253, 0, "Gnisis"
    			set travel to 0
    		endif 
    		if ( travel == 10 )
    			AIWander 500 0 0
    			PositionCell 20736.9922, 38396.8008, 1161.2622, 0, "Ghostgate"
    			set travel to 0
    		endif
    		if ( travel == 11 )
    			AIWander 500 0 0
    			PositionCell -5.3433, 2476.1021, 173.3262, 0, "Mournhold, Plaza Brindisi Dorom"
    			set travel to 0
    		endif 
    	endif
    endif
    
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
    
    if ( counter < 20 )
    	set counter to ( counter + 1 )
    	return
    endif
    set counter to 0
    
    if ( GetAlchemy < 100 )
    	set temp to 1
    else
    	set temp to 0
    endif
    ifx ( temp)
    	SetAlchemy 100
    	set temp to skills1 / 256
    	SetBlock temp
    	set temp to skills1 - 256 * temp
    	SetMediumArmor temp
    	set temp to skills2 / 256
    	SetHeavyArmor temp
    	set temp to skills2 - 256 * temp
    	SetBluntWeapon temp
    	set temp to skills3 / 256
    	SetLongBlade temp
    	set temp to skills3 - 256 * temp
    	SetAxe temp
    	set temp to skills4 / 256
    	SetSpear temp
    	set temp to skills4 - 256 * temp
    	SetDestruction temp
    	set temp to skills5 / 256
    	SetAlteration temp
    	set temp to skills5 - 256 * temp
    	SetIllusion temp
    	set temp to skills6 / 256
    	SetConjuration temp
    	set temp to skills6 - 256 * temp
    	SetMysticism temp
    	set temp to skills7 / 256
    	SetRestoration temp
    	set temp to skills7 - 256 * temp
    	SetUnarmored temp
    	set temp to skills8 / 256
    	SetLightArmor temp
    	set temp to skills8 - 256 * temp
    	SetShortBlade temp
    	set temp to skills9 / 256
    	SetMarksman temp
    	set temp to skills9 - 256 * temp
    	SetHandToHand temp
    
    ;	set count to 0
    ;	if ( hash == -2147483648)
    ;		set temp to 0
    ;	elseif ( hash < 0 )
    ;		set count to 1
    ;		set temp to -hash
    ;	else
    ;		set temp to hash
    ;	endif
    ;
    ;	ifx ( count )
    ;		setx npcfile to xStringBuild "A1NPC_0%d", temp
    ;	else
    ;		setx npcfile to xStringBuild "A1NPC_%d", temp
    ;	endif
    ;
    ;	xFileRewind npcfile
    ;	setx count,temp to xFileReadShort npcfile, 1
    ;	if ( temp == 20558 )
    ;		set count to 1
    ;	else
    ;		set count to 0
    ;	endif
    ;	ifx ( count )
    ;		xFileReadString npcfile
    ;		setx temp to xFileReadString npcfile
    ;		whilex ( count )
    ;			setx count,temp to xFileReadShort npcfile, 1
    ;			if ( temp == 20563 )
    ;				set count to 1
    ;			else
    ;				set count to 0
    ;			endif
    ;			ifx ( count )
    ;				setx temp to xFileReadString npcfile
    ;				xAddSpell temp
    ;			endif
    ;		endwhile
    ;	endif
    endif
    
    if ( GetCurrentAIPackage == 3 )
    	if ( GetDistance Player < 300 )
    		SetSpeed 15
    		SetAthletics 15
    	else
    		set c_speed to ( ( Player->GetSpeed ) * 2.25 )
    		set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
    		SetSpeed c_speed
    		SetAthletics c_athletics
    	endif
    
    	if ( GetItemCount p_restore_health_b >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_c >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_e >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_q >= 1 )
    		set p_Check to 1
    	elseif ( GetItemCount p_restore_health_s >= 1 )
    		set p_Check to 1
    	else
    		set p_Check to 0
    	endif
    else
    	SetSpeed 40
    endif
    
    if ( doOnce3 == 0 )
    	if ( GetHealth <= 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_imperial_pauldron_sk", 1
    		AddItem "nc_imperial_skull", 1
    		AddItem "ingred_bonemeal_01", 15
    		AddItem "ingred_scrap_metal_01", 1
    		AddItem "ingred_netch_leather_01", 1
    		if ( GetItemCount nc_SoulGem_blackened > 0 )
    			set coDist to ( player->GetLuck )
    			set c_speed to coDist
    			if ( c_speed < 40 )
    				set conj to ( c_speed - 40 )
    			elseif ( c_speed < 60 )
    				set c_speed to ( c_speed - 40 )
    				set conj to ( c_speed / 4 )
    			else
    				set c_speed to ( c_speed - 60 )
    				set c_speed to ( c_speed / 8 )
    				set conj to ( 5 + c_speed )
    			endif
    			set coDist to ( player->GetConjuration )
    			set c_speed to coDist
    			set conj to ( conj + c_speed )
    			set c_speed to ( Random 100 )
    			if ( c_speed < conj )
    				RemoveItem "nc_SoulGem_blackened", 1
    				set A1V3_NecroSkills1 to skills1
    				set A1V3_NecroSkills2 to skills2
    				set A1V3_NecroSkills3 to skills3
    				set A1V3_NecroSkills4 to skills4
    				set A1V3_NecroSkills5 to skills5
    				set A1V3_NecroSkills6 to skills6
    				set A1V3_NecroSkills7 to skills7
    				set A1V3_NecroSkills8 to skills8
    				set A1V3_NecroSkills9 to skills9
    				set count to hash
    				set temp to ( count / 65536 )
    				set A1V3_SoulGemHashHi to temp
    				set count to ( count - temp * 65536 )
    				set A1V3_SoulGemHashLow to count
    				AddItem "nc_SoulGem_imperial", 1
    			endif
    		endif
    		set doOnce3 to 1
    	endif
    endif
    
    end

SCPT
SCHD A1V3_NecroImpF_sc 30 13 18 5674 459
SCVR setUp;see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;conj;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;p_Check;emb_hook_nominion;call;travel;name;hash;skills1;skills2;skills3;skills4;skills5;skills6;skills7;skills8;skills9;temp;count;npcfile;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;sheatheTimer;moveTimer;;
