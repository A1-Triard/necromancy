    		if ( gender == 1 )
    			player->RemoveItem nc_frost_pauldron, 1
    		elseif ( gender == 2 )
    			player->RemoveItem nc_fire_pauldron, 1
    		else
    			player->RemoveItem nc_poison_pauldron, 1
    		endif
    		if ( summon == 1 )
    			player->RemoveItem "nc_altmer_skull", 1
    		elseif ( summon == 2 )
    			player->RemoveItem "nc_argonian_skull", 1
    		elseif ( summon == 3 )
    			player->RemoveItem "nc_bosmer_skull", 1
    		elseif ( summon == 4 )
    			player->RemoveItem "nc_breton_skull", 1
    		elseif ( summon == 5 )
    			player->RemoveItem "nc_imperial_skull", 1
    		elseif ( summon == 6 )
    			player->RemoveItem "nc_dunmer_skull", 1
    		elseif ( summon == 7 )
    			player->RemoveItem "nc_khajiit_skull", 1
    		elseif ( summon == 8 )
    			player->RemoveItem "nc_nord_skull", 1
    		elseif ( summon == 9 )
    			player->RemoveItem "nc_orc_skull", 1
    		elseif ( summon == 10 )
    			player->RemoveItem "nc_redguard_skull", 1
    		elseif ( summon == 11 )
    			player->RemoveItem "nc_altmer_skull_f", 1
    		elseif ( summon == 12 )
    			player->RemoveItem "nc_argonian_skull_f", 1
    		elseif ( summon == 13 )
    			player->RemoveItem "nc_bosmer_skull_f", 1
    		elseif ( summon == 14 )
    			player->RemoveItem "nc_breton_skull_f", 1
    		elseif ( summon == 15 )
    			player->RemoveItem "nc_imperial_skull_f", 1
    		elseif ( summon == 16 )
    			player->RemoveItem "nc_dunmer_skull_f", 1
    		elseif ( summon == 17 )
    			player->RemoveItem "nc_khajiit_skull_f", 1
    		elseif ( summon == 18 )
    			player->RemoveItem "nc_nord_skull_f", 1
    		elseif ( summon == 19 )
    			player->RemoveItem "nc_orc_skull_f", 1
    		else
    			player->RemoveItem "nc_redguard_skull_f", 1
    		endif
    		player->RemoveItem ingred_scrap_metal_01, 2
    		player->RemoveItem ingred_netch_leather_01, 2
    		player->RemoveItem ingred_bonemeal_01, 10
    		set A1V3_NecroSkills1 to skills1
    		set A1V3_NecroSkills2 to skills2
    		set A1V3_NecroSkills3 to skills3
    		set A1V3_NecroSkills4 to skills4
    		set A1V3_NecroSkills5 to skills5
    		set A1V3_NecroSkills6 to skills6
    		set A1V3_NecroSkills7 to skills7
    		set A1V3_NecroSkills8 to skills8
    		set A1V3_NecroSkills9 to skills9
