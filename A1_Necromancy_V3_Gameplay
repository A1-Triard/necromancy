    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_he_skelly_NPC_f" 1 50 1
