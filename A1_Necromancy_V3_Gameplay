    short OnPCEquip
    long temp
    long count
    long npcfile
    short gender
    short summon
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( state == 0 )
    	set state to 1
    	set hash to A1V3_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V3_SoulGemHashLow
    	set hash to hash + temp
