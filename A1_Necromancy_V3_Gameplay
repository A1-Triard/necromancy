    	if ( player->GetItemCount nc_arg_pauldron_sk == 1 )
    		set gender to 1
    	elseif ( player->GetItemCount nc_arg_pauldron_sk_f == 1 )
    		set gender to 2
    	else
    		MessageBox "Вам нужен скелет или труп для этого ритуала."
    		set gender to 0
    	endif
    	if ( gender == 1 )
    		if ( player->GetItemCount nc_argonian_skull == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп аргонианина для этого ритуала."
    		endif
    	elseif ( gender == 2 )
    		if ( player->GetItemCount nc_argonian_skull_f == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп аргонианки для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_scrap_metal_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_netch_leather_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_bonemeal_01 < 30 )
    			set gender to 0
    			MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    		if ( gender == 1 )
    			player->RemoveItem nc_arg_pauldron_sk, 1
    			player->RemoveItem nc_argonian_skull, 1
    		else
    			player->RemoveItem nc_arg_pauldron_sk_f, 1
    			player->RemoveItem nc_argonian_skull_f, 1
    		endif
    		player->RemoveItem ingred_scrap_metal_01, 2
    		player->RemoveItem ingred_netch_leather_01, 2
    		player->RemoveItem ingred_bonemeal_01, 30
    		set temp to hash / 65536
    		set A1V3_SoulGemHashHi to temp
    		set hash to hash - temp * 65536
    		set A1V3_SoulGemHashLow to hash
    		set A1V3_NecroSkills1 to skills1
    		set A1V3_NecroSkills2 to skills2
    		set A1V3_NecroSkills3 to skills3
    		set A1V3_NecroSkills4 to skills4
    		set A1V3_NecroSkills5 to skills5
    		set A1V3_NecroSkills6 to skills6
    		set A1V3_NecroSkills7 to skills7
    		set A1V3_NecroSkills8 to skills8
    		set A1V3_NecroSkills9 to skills9
    		set summon to nc_necro_skill
    		if ( summon <= 10 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V3_NecroArgo" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroArgo_f" 1, 0, 0
    			endif
    		elseif ( summon <= 20 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V3_NecroArgo2" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroArgo2_f" 1, 0, 0
    			endif
    		else
    			if ( gender == 1 )
    				PlaceAtMe "A1V3_NecroArgo3" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroArgo3_f" 1, 0, 0
    			endif
    		endif
    		PlaySound, "ThunderClap"
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		Disable
    	endif
    	return
    endif
    
    ifx ( OnPCEquip )
    	set OnPCEquip to 0
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx count to xFileReadString npcfile
    		xMessageFix count "OK"
    		MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" "OK"
    	endif
    endif
    
    End

SCPT
SCHD A1V3_SoulGemBosmer_sc 4 13 0 2710 126
SCVR state;OnPCEquip;gender;summon;hash;skills1;skills2;skills3;skills4;skills5;skills6;skills7;skills8;skills9;temp;count;npcfile;;
SCDT 
SCTX
    Begin A1V3_SoulGemBosmer_sc
    
    long hash
    long skills1
    long skills2
    long skills3
    long skills4
    long skills5
    long skills6
    long skills7
    long skills8
    long skills9
    short state
    short OnPCEquip
    long temp
    long count
    long npcfile
    short gender
    short summon
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( state == 0 )
    	set state to 1
    	set hash to A1V3_SoulGemHashHi
    	set hash to hash * 65536
    	set temp to A1V3_SoulGemHashLow
    	set hash to hash + temp
    	set A1V3_SoulGemHashHi to 0
    	set A1V3_SoulGemHashLow to 0
    	set skills1 to A1V3_NecroSkills1
    	set skills2 to A1V3_NecroSkills2
    	set skills3 to A1V3_NecroSkills3
    	set skills4 to A1V3_NecroSkills4
    	set skills5 to A1V3_NecroSkills5
    	set skills6 to A1V3_NecroSkills6
    	set skills7 to A1V3_NecroSkills7
    	set skills8 to A1V3_NecroSkills8
    	set skills9 to A1V3_NecroSkills9
    endif
    
    set temp to 0
    if ( OnActivate == 1 )
    	if ( player->GetSpellEffects A1_NecroSummon == 1 )
    		set temp to 1
    	else
    		Activate
    	endif
    endif
    
    if ( temp == 1 )
    	if ( player->GetItemCount nc_bosmer_pauldron_sk == 1 )
    		set gender to 1
    	elseif ( player->GetItemCount nc_bosmer_pauldron_sk_f == 1 )
    		set gender to 2
    	else
    		MessageBox "Вам нужен скелет или труп для этого ритуала."
    		set gender to 0
    	endif
    	if ( gender == 1 )
    		if ( player->GetItemCount nc_bosmer_skull == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп лесного эльфа для этого ритуала."
    		endif
    	elseif ( gender == 2 )
    		if ( player->GetItemCount nc_bosmer_skull_f == 0 )
    			set gender to 0
    			MessageBox "Вам нужен череп лесной эльфийки для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_scrap_metal_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_netch_leather_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_bonemeal_01 < 30 )
    			set gender to 0
    			MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
    		endif
    	endif
    	if ( gender != 0 )
    		set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    		if ( gender == 1 )
    			player->RemoveItem nc_bosmer_pauldron_sk, 1
    			player->RemoveItem nc_bosmer_skull, 1
    		else
    			player->RemoveItem nc_bosmer_pauldron_sk_f, 1
    			player->RemoveItem nc_bosmer_skull_f, 1
    		endif
    		player->RemoveItem ingred_scrap_metal_01, 2
    		player->RemoveItem ingred_netch_leather_01, 2
    		player->RemoveItem ingred_bonemeal_01, 30
    		set temp to hash / 65536
    		set A1V3_SoulGemHashHi to temp
    		set hash to hash - temp * 65536
    		set A1V3_SoulGemHashLow to hash
    		set A1V3_NecroSkills1 to skills1
    		set A1V3_NecroSkills2 to skills2
    		set A1V3_NecroSkills3 to skills3
    		set A1V3_NecroSkills4 to skills4
    		set A1V3_NecroSkills5 to skills5
    		set A1V3_NecroSkills6 to skills6
    		set A1V3_NecroSkills7 to skills7
    		set A1V3_NecroSkills8 to skills8
    		set A1V3_NecroSkills9 to skills9
    		set summon to nc_necro_skill
    		if ( summon <= 10 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V3_NecroWE" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroWE_f" 1, 0, 0
    			endif
    		elseif ( summon <= 20 )
    			if ( gender == 1 )
    				PlaceAtMe "A1V3_NecroWE2" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroWE2_f" 1, 0, 0
    			endif
    		else
    			if ( gender == 1 )
    				PlaceAtMe "A1V3_NecroWE3" 1, 0, 0
    			else
    				PlaceAtMe "A1V3_NecroWE3_f" 1, 0, 0
    			endif
    		endif
    		PlaySound, "ThunderClap"
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		Disable
    	endif
    	return
    endif
    
    ifx ( OnPCEquip )
    	set OnPCEquip to 0
    	set count to 0
    	if ( hash == -2147483648)
    		set temp to 0
    	elseif ( hash < 0 )
    		set count to 1
    		set temp to -hash
    	else
    		set temp to hash
    	endif
    
    	ifx ( count )
    		setx npcfile to xStringBuild "A1NPC_0%d", temp
    	else
    		setx npcfile to xStringBuild "A1NPC_%d", temp
    	endif
    
    	xFileRewind npcfile
    	setx count,temp to xFileReadShort npcfile, 1
    	if ( temp == 20558 )
    		set count to 1
    	else
    		set count to 0
    	endif
    	ifx ( count )
    		setx count to xFileReadString npcfile
    		xMessageFix count "OK"
    		MessageBox "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" "OK"
    	endif
    endif
    
    End

