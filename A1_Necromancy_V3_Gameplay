    short temp
    short gender
    short summon
    
    if ( GetDisabled == 1 )
    	SetDelete 1
    	return
    endif
    
    if ( state == 0 )
    	set state to 1
    	set tempf to ( player->GetBlock )
    	set skills to tempf
    	set tempf to ( player->GetMediumArmor )
    	set tempLong to tempf
    	set skills1 to skills * 256 + tempLong
    	set tempf to ( player->GetHeavyArmor )
    	set skills to tempf
