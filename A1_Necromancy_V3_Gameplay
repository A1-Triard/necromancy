SCPT
SCHD A1_NecroOnWar 0 0 0 18 0
SCDT HBANQTFfTmVjcm9PbldhcgEB
SCTX
    begin A1_NecroOnWar
    	StopScript A1_NecroOnWar
    end

SCPT
SCHD A1_NecroWearHair_sc 0 4 0 243 30
SCVR invitem;invcount;invref;match;;
SCDT 
SCTX
    Begin A1_NecroWearHair_sc
    
    long invitem
    long invcount
    long invref
    long match
    
    if ( MenuMode == 1 )
    	return
    endif
    
    set match to 0
    setx invitem,invcount,invref to xInventory
    whilex ( invcount )
    	setx match to xStringMatch invitem, "^A1V1H"
    	ifx ( match )
    		set invcount to 0
    	else
    		setx invitem,invcount,invref to xNextStack invref
    	endif
    endwhile
    
    ifx ( match )
    	xEquip invitem
    endif
    
    StopScript A1_NecroWearHair_sc
    
    End

SCPT
SCHD A1V3_EquipCorpse 1 0 0 9117 8
SCVR counter;;
SCDT 
SCTX
    begin A1V3_EquipCorpse
    
    short counter
    
    if ( MenuMode == 1 )
    	return
    endif
    
    set counter to ( counter + 1 )
    if ( counter < 10 )
    	return
    endif
    set counter to 0
    
    if ( player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	if ( player->HasItemEquipped "nc_altmer_pauldron" == 0 )
    		player->Equip "nc_altmer_pauldron"
    		MessageBox "Нести тело в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    	endif
    elseif ( player->GetItemCount "nc_altmer_pauldron_f" >= 1 )
    	if ( player->HasItemEquipped "nc_altmer_pauldron_f" == 0 )
