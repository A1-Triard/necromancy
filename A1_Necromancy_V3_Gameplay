SCHD nc_arg_skelly_pauldron 6 0 0 281 52
SCVR state;check;checkCounter;OnPCDrop;OnPCEquip;counter;;
SCDT 
SCTX
    begin nc_arg_skelly_pauldron
    
    short state
    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    	Disable
    endif
    
    end

