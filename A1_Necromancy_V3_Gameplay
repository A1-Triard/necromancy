SCPT
SCHD nc_control_ring 3 2 0 829 35
SCVR OnPCEquip;button;status;lkey;ltemp;;
SCDT 
SCTX
    begin nc_control_ring
    
    short button
    short status
    long lkey
    long ltemp
    
    if ( menumode )
    	return
    endif
    
    if ( OnPCEquip == 1 )
    	MessageBox "Кому вы хотите отдать приказ?", "Мертвецам", "Слугам", "И тем, и другим", "Никому", 
    	set OnPCEquip to 0
    	set status to 1
    endif
    
    if ( status == 1 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;revenants
    		set status to 2
    	elseif ( button == 1 ) ;warlords
    		set status to 3
    	elseif ( button == 2 ) ;both
    			set status to 4
    	elseif ( button == 3 ) ;none
    		set status to 5
    	endif
    elseif ( status == 2 ) ;revenants
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest_rev
    	set status to 0
    elseif ( status == 3 ) ;warlords
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	startscript nc_keytest_warlord
    	set status to 0
    elseif ( status == 4 ) ;both
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest
    	set status to 0
    elseif ( status == 5 ) ;none
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	set status to 0
    endif
    
    
    end
    

SCPT
