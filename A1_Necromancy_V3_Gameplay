    		set temp to 1
    	else
    		Activate
    	endif
    endif
    
    if ( temp == 1 )
    	set gender to 0
    	set summon to nc_necro_skill
    	if ( summon < 30 )
    		set temp to 0
    	endif
    	if ( temp == 0 )
    		MessageBox "Вы недостаточно опытны, чтобы вызвать мумифицированного слугу."
    	elseif ( player->GetItemCount nc_frost_pauldron == 1 )
    		set gender to 1
    	elseif ( player->GetItemCount nc_fire_pauldron == 1 )
    		set gender to 2
    	elseif ( player->GetItemCount nc_poison_pauldron == 1 )
    		set gender to 3
    	else
    		MessageBox "Вам нужна мумия для этого ритуала."
    	endif
    	if ( gender == 1 )
    		if ( A1V3_FrostWarlordG >= 1 )
    			set gender to 0
    			MessageBox "Вы можете контролировать только одного ледяного слугу."
    		endif
    	elseif ( gender == 2 )
    		if ( A1V3_FireWarlordG >= 1 )
    			set summon to 0
    			MessageBox "Вы можете контролировать только одного огненного слугу."
    		endif
    	elseif ( gender == 3 )
    		if ( A1V3_PoisonWarlordG >= 1 )
    			set summon to 0
    			MessageBox "Вы можете контролировать только одного ядовитого слугу."
    		endif
    	endif
    	set summon to 0
    	if ( gender == 0 )
    	elseif ( player->GetItemCount "nc_altmer_skull" > 0 )
    		set summon to 1
    	elseif ( player->GetItemCount "nc_argonian_skull" > 0 )
    		set summon to 2
    	elseif ( player->GetItemCount "nc_bosmer_skull" > 0 )
    		set summon to 3
    	elseif ( player->GetItemCount "nc_breton_skull" > 0 )
    		set summon to 4
    	elseif ( player->GetItemCount "nc_imperial_skull" > 0 )
    		set summon to 5
    	elseif ( player->GetItemCount "nc_dunmer_skull" > 0 )
    		set summon to 6
    	elseif ( player->GetItemCount "nc_khajiit_skull" > 0 )
    		set summon to 7
    	elseif ( player->GetItemCount "nc_nord_skull" > 0 )
    		set summon to 8
    	elseif ( player->GetItemCount "nc_orc_skull" > 0 )
    		set summon to 9
    	elseif ( player->GetItemCount "nc_redguard_skull" > 0 )
    		set summon to 10
    	elseif ( player->GetItemCount "nc_altmer_skull_f" > 0 )
    		set summon to 11
    	elseif ( player->GetItemCount "nc_argonian_skull_f" > 0 )
    		set summon to 12
    	elseif ( player->GetItemCount "nc_bosmer_skull_f" > 0 )
    		set summon to 13
    	elseif ( player->GetItemCount "nc_breton_skull_f" > 0 )
    		set summon to 14
    	elseif ( player->GetItemCount "nc_imperial_skull_f" > 0 )
    		set summon to 15
    	elseif ( player->GetItemCount "nc_dunmer_skull_f" > 0 )
    		set summon to 16
    	elseif ( player->GetItemCount "nc_khajiit_skull_f" > 0 )
    		set summon to 17
    	elseif ( player->GetItemCount "nc_nord_skull_f" > 0 )
    		set summon to 18
    	elseif ( player->GetItemCount "nc_orc_skull_f" > 0 )
    		set summon to 19
    	elseif ( player->GetItemCount "nc_redguard_skull_f" > 0 )
    		set summon to 20
    	else
    		set gender to 0
    		MessageBox "Вам нужен череп для этого ритуала."
    	endif
    	if ( gender != 0 )
    		if ( player->GetItemCount ingred_scrap_metal_01 < 2 )
    			set gender to 0
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    		endif
    	endif
