SCPT
SCHD nc_get_war 1 3 0 364 34
SCVR state;prospectref;prospectid;temp;;
SCDT 
SCTX
    begin nc_get_war
    
    short state
    long prospectref   ; reference to an NPC (or creature)
    long prospectid    ; string holding the NPC's id
    
    long temp          ; used to control ifx statements
    
    
        ; loop through each of the NPCs and creatures in the cell
        setx prospectref to xFirstNPC
    	 set nc_phere to 0
    	 set nc_fhere to 0
    	 set nc_frhere to 0
        whilex ( prospectref )
    
    
            ; get this NPCs id and see if it's poison warlord
            setx prospectid to prospectref->xGetBaseID 
            setx temp to xStringCompare prospectid "A1V3_PWarlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_phere to 1
    				;MessageBox "Got a poison.  nc_phere is %g", nc_phere
            endif
    
            setx temp to xStringCompare prospectid "A1V3_FWarlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_fhere to 1
    				;MessageBox "Got a fire.  nc_fhere is %g", nc_fhere
            endif
    
            setx temp to xStringCompare prospectid "A1V3_TWarlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_frhere to 1
    				;MessageBox "Got a frost.  nc_frhere is %g", nc_frhere
            endif
    
    		 setx prospectref to xNextRef prospectref
        endwhile
    
    ;MessageBox "nc_phere is %g, nc_fhere is %g, nc_frhere is %g", nc_phere, nc_fhere, nc_frhere
    
    stopscript nc_get_war
    end

SCPT
SCHD nc_guard_attack 9 3 0 1200 82
SCVR call;call2;call3;call4;call5;call6;call7;call8;doonce;prospectref;prospectid;temp;;
SCDT 
SCTX
    begin nc_guard_attack
    
        short call 
        short call2
        short call3        ; status variable - should this NPC be called?
        short call4
        short call5
        short call6
        short call7
        short call8
    	 short doonce
        long prospectref   ; reference to an NPC (or creature)
        long prospectid    ; string holding the NPC's id
        long temp          ; used to control ifx statements
    
    
    
        
    
    
    
        ; loop through each of the NPCs and creatures in the cell
        setx prospectref to xFirstNPC
    	 
        whilex ( prospectref )
    
            ; start by assuming we want to call this one
            set call to 1
            set call2 to 1
            set call3 to 1
            set call4 to 1
            set call5 to 1
            set call6 to 1
            set call7 to 1
            set call8 to 1
    
            ; check the type because we don't want creatures (Non-NPCs)
            setx temp to prospectref->xRefType
            if ( temp != 1598246990 )   ; it's not an NPC
                set call to 0
            endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "Imperial Guard"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "hlaalu guard"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "ordinator stationary"
        if ( temp != 0 ) ; it's not a guard
            set call4 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "ordinator wander"
        if ( temp != 0 ) ; it's not a guard
            set call5 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "redoran guard male"
        if ( temp != 0 ) ; it's not a guard
            set call6 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "redoran guard female"
        if ( temp != 0 ) ; it's not a guard
            set call7 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "telvanni guard"
        if ( temp != 0 ) ; it's not a guard
            set call8 to 0
        endif
    
            ; if call is still 1 we want to call this NPC to the player
            ifx ( call2 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call3 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call4 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call5 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call6 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call7 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
            ifx ( call8 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
    
        	setx prospectref to xNextRef prospectref
        endwhile
    set doonce to 0
    stopscript nc_guard_attack
    
    end

SCPT
SCHD nc_he_corpse_cure 5 0 0 634 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_he_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    	set getcell to 1
    	if ( state == 1 )
    		Activate
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		MessageBox "Здесь тело будет очень долго оставаться нетронутым."
            set state to 1
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_he_skelly_NPC" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		Disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_he_corpse_cure_f 5 0 0 638 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_he_corpse_cure_f
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    	set getcell to 1
    	if ( state == 1 )
    		Activate
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		MessageBox "Здесь тело будет очень долго оставаться нетронутым."
            set state to 1
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_he_skelly_NPC_f" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		Disable
    		set state to 1
    	endif
    endif
    
    end
    

SCPT
SCHD nc_he_skelly_pauldron 6 0 0 280 52
SCVR state;check;checkCounter;OnPCDrop;OnPCEquip;counter;;
SCDT 
SCTX
    begin nc_he_skelly_pauldron
    
    short state
    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    	Disable
    endif
    
    end

SCPT
