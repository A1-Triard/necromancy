    		if ( flyCheck == 0 )
    			set combatCheck to 1
    		else
    			set combatDone to 1
    		endif
    		set timer to 0
    		set warp to 1
    		return
    	endif
    
    	set ax to ( Player->GetPos x )
    	set ay to ( Player->GetPos y )
    
