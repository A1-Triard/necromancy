SCPT
SCHD nc_warlord_attack 4 5 1 818 80
SCVR doonce;call;call2;call3;prospectref;prospectid;temp;target;target2;targethealth;;
SCDT 
SCTX
    begin nc_warlord_attack
    
    
    short doonce
    short call ; status variable - should this NPC be called?
    short call2
    short call3
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
    long target
    long target2
    float targethealth
    
    
    if ( menumode == 1 )
        return
    endif
    
    
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
    setx target to xGetPCTarget
    
    if ( target == 0 )
    	return
    endif
    
    ifx ( target )
    
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
    
        ; start by assuming we want to call this one
        set call to 1
    	 set call2 to 1
    	 set call3 to 1
    
    
    
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
            set call2 to 0
            set call3 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "A1V3_FWarlord"
        if ( temp != 0 ) ; it's not a guard
            set call to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "A1V3_TWarlord"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "A1V3_PWarlord"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call3 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call2 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
        ifx ( call ) ; we use extended functions inside so need ifx
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		  
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    
    
    
    
        endif
    
    
        setx prospectref to xNextRef prospectref
    
    
    endwhile
    set doonce to 0
    set target to 0 ;target
    endif
    startscript nc_guard_attack
    stopscript nc_warlord_attack
    
    
    end

SCPT
SCHD nc_we_corpse_cure 5 0 0 634 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_we_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    	set getcell to 1
    	if ( state == 1 )
    		Activate
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		MessageBox "Здесь тело будет очень долго оставаться нетронутым."
            set state to 1
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_we_skelly_NPC" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		Disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_we_corpse_cure_f 5 0 0 638 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_we_corpse_cure_f
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    	set getcell to 1
    	if ( state == 1 )
    		Activate
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		MessageBox "Здесь тело будет очень долго оставаться нетронутым."
            set state to 1
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_we_skelly_NPC_f" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		Disable
    		set state to 1
    	endif
    endif
    
    end
    

SCPT
SCHD nc_we_skelly_pauldron 6 0 0 280 52
SCVR state;check;checkCounter;OnPCDrop;OnPCEquip;counter;;
SCDT 
SCTX
    begin nc_we_skelly_pauldron
    
    short state
    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
    	Disable
    endif
    
    end

SCPT
SCHD nc_we_skelly_pauldron_f 6 0 0 282 52
SCVR state;check;checkCounter;OnPCDrop;OnPCEquip;counter;;
SCDT 
SCTX
    begin nc_we_skelly_pauldron_f
    
    short state
    short check
    short checkCounter
    short OnPCDrop
    short OnPCEquip
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	SetDelete 1
    	return
    endif
    
    if ( check == 1 )
    	set checkCounter to ( checkCounter + 1 )
    	if ( checkCounter > 30 )
    		set check to 0
    		StartScript A1V3_EquipCorpse
    	endif
    	return
    endif
    
    if ( OnPCEquip == 0 )
    	if ( state == 0 )
    		set state to 1
    		set checkCounter to 0
    		set check to 1
    	endif
    else
    	set state to 0
    endif
    
    if ( OnPCDrop == 1 )
    	set A1V3_NecroCorpse to ( A1V3_NecroCorpse - 1 )
    	PlaceAtPC "nc_we_skelly_NPC_f" 1 50 1
    	Disable
    endif
    
    end

