    	if ( see_p_warlord == 0 )
    
    	if ( warp == 0 )
    		if ( coDist > 350 )
    			if ( GetDistance Player > 680 )
    				SetPos x bx
    				SetPos y by
    				SetPos z bz
    				AiFollow Player 0 0 0 0
    			endif
    		endif
    	endif
    
    	endif
    	endif
    	endif
    endif
    
    if ( see_f_warlord == 1 )
    	if ( nc_f_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
    endif
    
    if ( see_fr_warlord == 1 )
    	if ( nc_fr_follow == 0 )
    		set warp to 1
    	else
    		set warp to 0
    	endif
