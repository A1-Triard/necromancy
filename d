    if ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	Player->Drop "nc_dunmer_pauldron" 1
    endif
#else /* ABCD */
    end

SCPT
SCHD nc_random_kill 3 3 0 192 50
SCVR OnPCEquip;doonce;call;prospectref;prospectid;temp;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEaPBM4AAATOGwAAjwTOAAAEzhsAAA8DzgEAAw4rQAFAXMDAAIgMRM4AAATOGwAADwYPAg8EzgCABM4bAACPAYBARIgbAMAICE9IDE1OTgyNDY5OTAFAXMDAAIgMAkBEzgCABM4cwAAPA84BAAMOJMAEzgAABM4bAAAPB08jRAAAAAAEzgAABM4bAAAPBs8EzgAABM4bAACPAo4HQAcEA5uY19yYW5kb21fa2lsbAEB
SCTX
    begin nc_random_kill
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	Player->Drop "nc_imperial_pauldron" 1
    endif
#else /* ABCD */
    short OnPCEquip
    short doonce
    short call ; status variable - should this NPC be called?
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 2 )
    	Player->Drop "nc_khajiit_pauldron" 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
#else /* ABCD */
    if ( menumode == 1 )
        return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
    endif
#else /* ABCD */
    ; Not equipped, abort
    ;if ( OnPCEquip == 0 )
    ;    return
    ;endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
    endif
#else /* ABCD */
    ;set OnPCEquip to 0
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
        ; start by assuming we want to call this one
        set call to 1
    	
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk" 1
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_imperial_pauldron_sk" 1
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
        ; if call is still 1 we want to call this NPC to the player
        ifx ( call ) ; we use extended functions inside so need ifx		  
          xGetRef prospectref
    		sethealth 0
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
        endif
        ; The MWSE docs example script has a typo here.
        ; prospectref, not prospect
        setx prospectref to xNextRef prospectref
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
#else /* ABCD */
    ;temp->xstartcombat "player"
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
    endif
#else /* ABCD */
    endwhile
    stopscript nc_random_kill
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
    endif
#else /* ABCD */
    ; this only needs to run for one frame
#endif /* ABCD */
    
#ifndef ABCD
    stopscript nc_mult_khajiit
#endif /* ! ABCD */
    
    
    end
#ifdef ABCD
    
#endif /* ABCD */

