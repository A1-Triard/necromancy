SCHD nc_gem_test 1 0 0 1971 6
SCVR state;;
SCDT 
SCTX
    begin nc_gem_test
#endif /* ABCD */
    
#ifndef ABCD
    ;AND ABOVE HERE.
#else /* ABCD */
    short state
    ;global nc_gem_race
#endif /* ABCD */
    
#ifndef ABCD
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( counter < 20 )
      set counter to counter + 1
      return
#else /* ABCD */
    	if ( nc_gem_race == 1 )
    		if ( Player->GetItemCount "nc_soulgem_arg" >= 1 )
    			Player->removeitem "nc_soulgem_arg" 1
    			startscript nc_summon_argonian
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 2 )	
    		if ( Player->GetItemCount "nc_soulgem_altmer" >= 1 )
    			Player->removeitem "nc_soulgem_altmer" 1
    			startscript nc_summon_altmer
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 3 )	
    		if ( Player->GetItemCount "nc_soulgem_bosmer" >= 1 )
    			Player->removeitem "nc_soulgem_bosmer" 1
    			startscript nc_summon_bosmer
    			set state to 1
    		else
    			MessageBox "Чтобы провести ритуал, вам нужен черный камень душ."
    			set state to 1
    		endif
    	elseif ( nc_gem_race == 4 )	
