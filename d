SCPT
SCHD nc_startscript 6 0 2 995 57
SCVR doonce;doonce2;doonce3;doonce4;doonce5;wait;timer;timer2;;
SCDT 
SCTX
    begin nc_startscript
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    	float timer
    	float timer2
    	short doonce
    	short doonce2
    	short doonce3
    	short doonce4
    	short doonce5
    	short wait
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    	if ( menumode == 1 )
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
#else /* ABCD */
    	; Dont waste time checking spells more than once per second
    	set timer to ( timer + getsecondspassed )
    	if ( timer < 1 )		
    		return
    	endif
    	set timer to 0
    		
    	if ( nc_has_corpse > 0 )
    		if ( ScriptRunning "nc_corpse_check" == 0 )
    			startscript nc_corpse_check
    		endif
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
    endif
#else /* ABCD */
    	if ( nc_necro_skill == 10 )
    		if ( doonce4 == 0 )
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать более сильных мертвецов."
    			set doonce4 to 1
    		endif
    	endif	
    
    	if ( nc_necro_skill == 20 )
    		if ( doonce3 == 0 )
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать больших мертвецов."
    			set doonce3 to 1
    		endif
    	endif	
    
    	if ( nc_necro_skill == 30 )
    		if ( doonce2 == 0 )
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Идите и поговорите с Трагриларом."
    			set doonce2 to 1
    		endif
    	endif
    
    
    	if ( Player->getspelleffects nc_skull_spell == 1 )
    		set nc_spell_cast to 1
    		if ( wait == 0 )
    			startscript nc_death_trap
    			set wait to 1
    		endif
    		set timer2 to ( timer2 + GetSecondsPassed )
    		if ( timer2 >= 15 )
    			set wait to 0
    			set timer2 to 0
    		endif
    	else
    		set nc_spell_cast to 0
    		set timer2 to 0
    		set wait to 0
    		stopscript nc_death_trap
    	endif
    
    	if ( player->getspelleffects nc_summon == 1 )
    		if ( doonce5 == 0 )
    			set nc_summon_spell to 1
    			set doonce5 to 1
    		endif
    	else
    		set nc_summon_spell to 0
    		set doonce5 to 0
    	endif
    
    	if ( player->getspelleffects nc_painful_touch == 1 )
    		set nc_warlord_spell to 1
    		startscript nc_warlord_attack
    	else
    		set nc_warlord_spell to 0
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    stopscript nc_mult_rg
#endif /* ! ABCD */
    
    end
#ifdef ABCD
    	
    
#endif /* ABCD */

