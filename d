SCPT
SCHD nc_activate 2 0 0 122 23
SCVR currenthour;targethour;;
SCDT BgEBBSBYGBAAFxAJAQYBBwkgWB8QID09IDAGAQITIHMBACAhPSBHCEdhbWVIb3VyAAUBcwEADCBHCEdhbWVIb3VyAAUBcwIACCBzAgAgMSArCQEGAQEKIHMCACA8PSA0OCQBCQEHAQEcEAtuY19hY3RpdmF0ZQkBAQE=
SCTX
    begin nc_activate
    
    short currenthour
    short targethour
    
    if ( OnActivate )
    	activate
    endif
    
    if ( CellChanged == 0 )
    	if ( currenthour != GameHour )
    		set currenthour to GameHour
    		set targethour to ( targethour + 1 )
    	endif
    
    	if ( targethour <= 48 )
    		return
    	endif
    else
    	stopscript nc_activate
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_arg_corpse_cure 7 1 0 699 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_arg_corpse_cure 5 0 0 555 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_arg_corpse_cure
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
#endif /* ! ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	set getcell to 1
#else /* ABCD */
    	set getcell to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ifx ( getcell )
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
    set DoOnce1 to 1
    
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_arg_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( temp != 0 )
    	if ( state == 0 )
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
#endif /* ABCD */
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
#ifndef ABCD
    			set getcell to 0
    			set temp to 0
#else /* ABCD */
    			set getcell to 1
#endif /* ABCD */
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
#ifndef ABCD
    			disable
    			player-> additem "nc_arg_pauldron" 1
#endif /* ! ABCD */
    			set state to 1
    		endif
#ifdef ABCD
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
#endif /* ABCD */
    	endif
#ifndef ABCD
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
    endif
    
    set getcell to 0
    
    endif
#endif /* ! ABCD */
    
#ifdef ABCD
    	set getcell to 1
#endif /* ABCD */
    
#ifndef ABCD
    if ( state == 1 )
    	return
#endif /* ! ABCD */
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_arg_skelly" 1 1 1
