    pcref->Additem "nc_SoulGem_breton" 1
    pcref->Additem "nc_breton_pauldron" 1
    pcref->AddItem "nc_breton_skull" 1
    
    stopscript nc_death_add_breton
    
#endif /* ABCD */
    end
