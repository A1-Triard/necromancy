SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBJuY19kdW5tZXJfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_de_corpse_drop
#endif /* ABCD */
    
#ifndef ABCD
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
#else /* ABCD */
    short OnPCDrop
    short counter
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( compMove == 1 )
      if ( c_move == 0 )
      if ( GetDistance Player < 70 )
        AIWander 300 0 0 0 0
        set c_move to 1
      endif
      endif
      endif
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( c_move == 1 )
      if ( GetDistance Player > 100 )
        AIFollow Player 0 0 0 0
      set c_move to 0
      endif
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_dunmer_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
#ifdef ABCD
    	
    end

