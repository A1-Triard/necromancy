SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX2Jvc21lcl9wYXVsZHJvbl9zawEABwEBABAwAM3l8fLoIOHu6/z45Swg9+XsIO7k6O0g8erl6+XyIOfgIPDg5yAtIO3l8+Tu4e3uLgAACQEJAQEB
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_pauldron_imp_script
#else /* ABCD */
    begin nc_we_skelly_drop
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
#endif /* ! ABCD */
    short counter
    
#ifndef ABCD
    if ( menumode )
    	return
    endif
    
#endif /* ! ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
#else /* ABCD */
    if ( OnActivate )
#endif /* ABCD */
    	if ( nc_has_corpse == 0 )
#ifndef ABCD
    		activate
#else /* ABCD */
    		disable
    		player-> additem "nc_bosmer_pauldron_sk" 1
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_imperial_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_imperial_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
    		endif
#else /* ABCD */
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
#endif /* ABCD */
    	endif
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_imperial_corpse" 1 1 1
    	Disable
    	set OnPCDrop to 0
    endif
    
#endif /* ! ABCD */
    end
#ifndef ABCD
    
#endif /* ! ABCD */

SCPT
#ifndef ABCD
SCHD nc_pauldron_kh_script 4 0 1 653 40
#else /* ABCD */
SCHD nc_we_skelly_pauldron 4 0 1 637 40
#endif /* ABCD */
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_pauldron_kh_script
#else /* ABCD */
    begin nc_we_skelly_pauldron
#endif /* ABCD */
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
#ifndef ABCD
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
#else /* ABCD */
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
#endif /* ABCD */
    		return
    	endif
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron" >=1 )
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >=1 )
#endif /* ABCD */
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
#ifndef ABCD
    			Player-> equip "nc_khajiit_pauldron"
#else /* ABCD */
    			Player-> equip "nc_bosmer_pauldron_sk"
#endif /* ABCD */
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
#ifndef ABCD
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
#else /* ABCD */
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
#endif /* ABCD */
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
#ifndef ABCD
    				ModCurrentFatigue -4
#else /* ABCD */
    				ModCurrentFatigue -2
#endif /* ABCD */
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
#ifndef ABCD
    	PlaceAtPC "nc_khajiit_corpse" 1 1 1
#else /* ABCD */
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
#endif /* ABCD */
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
#ifndef ABCD

