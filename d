        set timer to 0
        set warp to 1
        return
      elseif ( GetWeaponDrawn == 1 )
      if ( flyCheck == 1 )
        set combatDone to 1
        set timer to 0
        set warp to 1
        return
      endif
      endif
      endif
#else /* ABCD */
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      set ax to ( Player->GetPos x )
      set ay to ( Player->GetPos y )
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( doOnce == 0 )
        set bx to ( Player->GetPos x )
        set by to ( Player->GetPos y )
        set bz to ( Player->GetPos z )
        set doOnce to 1
      endif
#else /* ABCD */
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
#endif /* ABCD */
    
#ifndef ABCD
      set t1 to ( ax - bx )
      set t1 to ( t1 * t1 )
      set t2 to ( ay - by )
      set t2 to ( t2 * t2 )
      set t1 to ( t1 + t2 )
      set coDist to ( GetSquareRoot, t1 )
#else /* ABCD */
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 360 )
        set doOnce to 0
      endif
#else /* ABCD */
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_arg" 1
    pcref->Additem "nc_arg_pauldron" 1
    pcref->AddItem "nc_argonian_skull" 1
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 180 )
      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
#else /* ABCD */
    stopscript nc_death_add_argonian
#endif /* ABCD */
    
#ifndef ABCD
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
#else /* ABCD */
    end
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist2 > 360 )
        set doOnce2 to 0
      endif
#else /* ABCD */

