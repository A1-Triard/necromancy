    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "nc_fire_pauldron" 1
    			MessageBox "Your fire cured corpse should be ready by now."
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set fstate to 0
    			set doonce to 1
    		endif
    	endif
