    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    set fstate to 2
    if ( salt >= 500 )
       if ( firesalts >= 5 )
           if ( petal >= 5 )
               set fstate to 1
           endif
       endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
#endif /* ABCD */
    endif
    
