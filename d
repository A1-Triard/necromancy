    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Бретона для этого ритуала."
    		set state to 2
#endif /* ABCD */
    endif
    
