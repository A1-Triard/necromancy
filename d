    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
    	endif
    endif
#endif /* ABCD */
    
#ifdef ABCD
    set doonce to 2
#endif /* ABCD */
    
#ifdef ABCD
    end

