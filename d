SCHD nc_guard_attack 9 3 0 1200 82
SCVR call;call2;call3;call4;call5;call6;call7;call8;doonce;prospectref;prospectid;temp;;
SCDT 
SCTX
    begin nc_guard_attack
#endif /* ABCD */
    
#ifndef ABCD
    if ( soundPlayed == 1 )
      equip "silver dagger"
      removeitem "silver dagger" 1
    ;  messagebox "Working"
      set soundPlayed to 0
      set combatCheck to 0
      set combatDone to 1
    endif
#else /* ABCD */
        short call 
        short call2
        short call3        ; status variable - should this NPC be called?
        short call4
        short call5
        short call6
        short call7
        short call8
    	 short doonce
        long prospectref   ; reference to an NPC (or creature)
        long prospectid    ; string holding the NPC's id
        long temp          ; used to control ifx statements
#endif /* ABCD */
    
#ifndef ABCD
    if ( combatDone == 1 )
        set timer to timer + GetSecondsPassed
      if ( timer > 6 )
        set timer to 0
        set combatDone to 0
        set warp to 0
      endif
    endif  
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      ;if ( GetPCSleep == 1 )
       ; StartScript 1gr_compLevel
      ;endif
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( GetPCSneaking == 1 )
        ForceSneak
      elseif ( GetPCSneaking == 0 )
        ClearForceSneak
      endif
#else /* ABCD */
        
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 1 )
      if ( flyCheck == 0 )
        cast nc_comp2_lev_bog player
        addspell nc_comp2_lev
        set flyCheck to 1
      endif
      endif
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 0 )
      if ( flyCheck == 1 )
        removespell nc_comp2_lev
        set flyCheck to 0
      endif
      endif
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 1 )
      if ( wwCheck == 0 )
        cast nc_comp2_ww_bog player
        addspell nc_comp2_ww
        set wwCheck to 1
      endif
      endif
#else /* ABCD */
        ; loop through each of the NPCs and creatures in the cell
        setx prospectref to xFirstNPC
    	 
        whilex ( prospectref )
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 0 )
      if ( wwCheck == 1 )
        removespell nc_comp2_ww
        set wwCheck to 0
      endif
      endif
#else /* ABCD */
            ; start by assuming we want to call this one
            set call to 1
            set call2 to 1
            set call3 to 1
            set call4 to 1
            set call5 to 1
            set call6 to 1
            set call7 to 1
            set call8 to 1
#endif /* ABCD */
    
#ifndef ABCD
      if ( flyCheck == 1 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( Player->GetPos z )
        SetPos z playz
      elseif ( flyCheck == 1 )
      if ( GetCurrentAIPackage != 3 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( GetPos z )
        SetPos z playz
      endif
      endif
      endif
      endif
#else /* ABCD */
            ; check the type because we don't want creatures (Non-NPCs)
            setx temp to prospectref->xRefType
            if ( temp != 1598246990 )   ; it's not an NPC
                set call to 0
            endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetWeaponDrawn == 1 )
      if ( flyCheck == 0 )
        set combatCheck to 1
        set timer to 0
        set warp to 1
        return
      elseif ( GetWeaponDrawn == 1 )
      if ( flyCheck == 1 )
        set combatDone to 1
        set timer to 0
        set warp to 1
        return
      endif
      endif
      endif
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "Imperial Guard"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
      set ax to ( Player->GetPos x )
      set ay to ( Player->GetPos y )
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "hlaalu guard"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( doOnce == 0 )
        set bx to ( Player->GetPos x )
        set by to ( Player->GetPos y )
        set bz to ( Player->GetPos z )
        set doOnce to 1
      endif
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "ordinator stationary"
        if ( temp != 0 ) ; it's not a guard
            set call4 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
      set t1 to ( ax - bx )
      set t1 to ( t1 * t1 )
      set t2 to ( ay - by )
      set t2 to ( t2 * t2 )
      set t1 to ( t1 + t2 )
      set coDist to ( GetSquareRoot, t1 )
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "ordinator wander"
        if ( temp != 0 ) ; it's not a guard
            set call5 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 360 )
        set doOnce to 0
      endif
    
      if ( coDist > 180 )
      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
    
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "redoran guard male"
        if ( temp != 0 ) ; it's not a guard
            set call6 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist2 > 360 )
        set doOnce2 to 0
      endif
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "redoran guard female"
        if ( temp != 0 ) ; it's not a guard
            set call7 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    		if ( see_p_warlord == 0 )
      if ( warp == 0 )
      if ( coDist > 350 )
      if ( GetDistance Player > 680 )
        SetPos x bx
        SetPos y by
        SetPos z bz
        AiFollow Player 0 0 0 0
      elseif ( warp == 0 )
      if ( coDist2 > 350 )
      if ( GetDistance Player > 680 )
        SetPos x cx
        SetPos y cy
        SetPos z cz
        AiFollow Player 0 0 0 0
      endif
      endif
      endif
      endif
      endif
    endif
    endif
    endif
    endif
#else /* ABCD */
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "telvanni guard"
        if ( temp != 0 ) ; it's not a guard
            set call8 to 0
        endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_f_warlord == 1 )
    if ( nc_f_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
#else /* ABCD */
            ; if call is still 1 we want to call this NPC to the player
            ifx ( call2 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_fr_warlord == 1 )
    if ( nc_fr_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
#else /* ABCD */
            ifx ( call3 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_p_warlord == 1 )
    if ( nc_p_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
#else /* ABCD */
            ifx ( call4 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
#else /* ABCD */
            ifx ( call5 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    set zomtimer to ( zomtimer + GetSecondsPassed )
    if ( zomtimer >= 30 )
    	modcurrenthealth -1
    	set zomtimer to 0
    endif
#else /* ABCD */
            ifx ( call6 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( compMove == 1 )
      if ( c_move == 0 )
      if ( GetDistance Player < 70 )
        AIWander 300 0 0 0 0
        set c_move to 1
      endif
      endif
      endif
    endif
#else /* ABCD */
            ifx ( call7 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( c_move == 1 )
      if ( GetDistance Player > 100 )
        AIFollow Player 0 0 0 0
      set c_move to 0
      endif
    endif
#else /* ABCD */
            ifx ( call8 )  ; we use extended functions inside so need ifx
    				if ( doonce == 0 )
    					ModPCCrimeLevel 1500
    					;MessageBox "This works"
    					set doonce to 1
    				endif
    		  endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( oneTimeMove == 1 )
      set moveTimer to moveTimer + GetSecondsPassed
      if ( moveTimer > 4 )
        set moveTimer to 0
        AiFollow Player 0 0 0 0
        set oneTimeMove to 0
      endif
    endif
#else /* ABCD */
        	setx prospectref to xNextRef prospectref
        endwhile
    set doonce to 0
    stopscript nc_guard_attack
#endif /* ABCD */
    
#ifndef ABCD
    ;AND ABOVE HERE.
#else /* ABCD */
    end

