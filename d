SCPT
#ifndef ABCD
SCHD nc_pauldron_dunmer_script 4 0 1 650 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#else /* ABCD */
SCHD nc_warlord_attack 4 5 1 818 80
SCVR doonce;call;call2;call3;prospectref;prospectid;temp;target;target2;targethealth;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_pauldron_dunmer_script
#else /* ABCD */
    begin nc_warlord_attack
    
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
#endif /* ! ABCD */
    short doonce
#ifndef ABCD
    float timer
    short OnPCDrop
    short counter
#else /* ABCD */
    short call ; status variable - should this NPC be called?
    short call2
    short call3
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
    long target
    long target2
    float targethealth
#endif /* ABCD */
    
#ifndef ABCD
    if ( menumode )
    	return
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#else /* ABCD */
    if ( menumode == 1 )
        return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
#else /* ABCD */
    
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
    setx target to xGetPCTarget
    
    if ( target == 0 )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_dunmer_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
#else /* ABCD */
    ifx ( target )
    
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
    
        ; start by assuming we want to call this one
        set call to 1
    	 set call2 to 1
    	 set call3 to 1
    
    
    
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
            set call2 to 0
            set call3 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_f_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_fr_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_p_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call3 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
#endif /* ABCD */
    		endif
#ifndef ABCD
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
    			startscript nc_time_bounty
    			set doonce to 1
#else /* ABCD */
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call2 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
#endif /* ABCD */
    		endif
#ifndef ABCD
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
#else /* ABCD */
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
        ifx ( call ) ; we use extended functions inside so need ifx
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		  
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
#endif /* ABCD */
    		endif
#ifndef ABCD
    	endif
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_dunmer_corpse" 1 1 1
    	Disable
    	set OnPCDrop to 0
#else /* ABCD */
    
    
    
        endif
    
    
        setx prospectref to xNextRef prospectref
    
    
    endwhile
    set doonce to 0
    set target to 0 ;target
#endif /* ABCD */
    endif
#ifdef ABCD
    startscript nc_guard_attack
    stopscript nc_warlord_attack
#endif /* ABCD */
    
#ifndef ABCD
    end
#endif /* ! ABCD */
    
#ifdef ABCD
    end
#endif /* ABCD */

SCPT
#ifndef ABCD
SCHD nc_pauldron_fire_script 4 0 1 643 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#else /* ABCD */
SCHD nc_we_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_pauldron_fire_script
#else /* ABCD */
    begin nc_we_corpse_cure
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
#else /* ABCD */
    short state
    short currentday
    short movingday
    short getcell
#endif /* ABCD */
    short counter
    
#ifndef ABCD
    if ( menumode )
    	return
    endif
    
#endif /* ! ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
#else /* ABCD */
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_bosmer_pauldron" 1
#endif /* ABCD */
    		return
    	endif
#ifndef ABCD
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_fire_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_fire_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
    			startscript nc_time_bounty
    			set doonce to 1
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
#endif /* ABCD */
    		endif
#ifndef ABCD
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
#else /* ABCD */
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
#endif /* ABCD */
    		endif
    	endif
#ifdef ABCD
    
    	set getcell to 1
    
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_fire_cured" 1 1 1
    	Disable
    	set OnPCDrop to 0
#else /* ABCD */
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_bosmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
#endif /* ABCD */
    endif
    
    end
#ifndef ABCD
    
#endif /* ! ABCD */

