SCPT
SCHD nc_follow_rev_script 3 3 0 741 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
SCTX
    begin nc_follow_rev_script
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_follow_rev_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIFollow player 0 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will follow you."
    			set doonce to 1
    		endif
    		
    	endif
    	
    end

SCPT
SCHD nc_follow_script 3 3 0 554 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
SCTX
    begin nc_follow_script
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_follow_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIFollow player 0 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will follow you."
    			set doonce to 1
    		endif
    		
    	endif
    	
    end

SCPT
SCHD nc_follow_war_script 6 3 0 829 52
SCVR doonce;call;state;fwar;frwar;pwar;npcref;npcid;temp;;
SCDT 
SCTX
    begin nc_follow_war_script
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	short fwar
    	short frwar
    	short pwar
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_follow_war_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    			set fwar to 0
    			set frwar to 0
    			set pwar to 0
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk" 1
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_imperial_pauldron_sk" 1
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			set fwar to 1
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			set frwar to 1
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			set pwar to 1
    		endif
    		
    		if ( fwar == 0 );not a fire
    			if ( frwar == 0 );not a frost
    				if ( pwar == 0 );not a warlord
    					return
    				endif
    			endif
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		npcref->AIFollow player 0 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your warlords will follow you."
    			set doonce to 1
    		endif
    		
    	endif
    	
    end

