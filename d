SCPT
#ifndef ABCD
SCHD nc_comp3Script_nordz 28 0 21 2951 384
SCVR see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;doOnce4;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;counter2;addOnce;p_Check;emb_hook_nominion;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;timer2;sheatheTimer;moveTimer;h_check;zomtimer;;
SCDT 
#else /* ABCD */
SCHD nc_death_add_altmer 0 5 1 547 43
SCVR invitem;invcount;invref;pcref;npcref;timer;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_comp3Script_nordz
#else /* ABCD */
    begin nc_death_add_altmer
#endif /* ABCD */
    
#ifndef ABCD
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
#else /* ABCD */
    
    
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
#endif /* ABCD */
    float timer
#ifndef ABCD
    float timer2
    float sheatheTimer
    float moveTimer
    float h_check
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short doOnce4
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
    short counter
    short counter2
    short addOnce
    short p_Check
    short emb_hook_nominion
    float zomtimer
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( MenuMode == 1 )
      ;if ( GetPCSleep == 0 )
        return
      ;endif
#else /* ABCD */
    set timer to ( timer + GetSecondsPassed )
    	
    if ( timer >= 60 )
    	startscript nc_activate
    	stopscript nc_death_add_altmer
    	set timer to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set emb_hook_nominion to 1
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter2 to ( counter2 + 1 )
    	if ( counter2 < 10 )
#else /* ABCD */
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
#endif /* ABCD */
    		return
    	endif
#ifndef ABCD
    	setdelete 1
    	return
    endif
    
    if ( weapUse == 1 )
      if ( Player->GetWeaponType <= 8 )
        setmarksman 0
      elseif ( weapUse == 1 )
      if ( Player->GetWeaponType >= 9 )
        setmarksman 200
      endif
      endif
#endif /* ! ABCD */
    endif
    
#ifndef ABCD
    if ( combatCheck == 1 )
      set sheathetimer to sheatheTimer + GetSecondsPassed
      if ( GetSoundPlaying, "Weapon Swish" == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, crossbowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, bowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "Weapon Swish" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "crossbowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "bowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      endif
      endif
      endif
      endif
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( soundPlayed == 1 )
      equip "silver dagger"
      removeitem "silver dagger" 1
    ;  messagebox "Working"
      set soundPlayed to 0
      set combatCheck to 0
      set combatDone to 1
    endif
#else /* ABCD */
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
#endif /* ABCD */
    
#ifndef ABCD
    if ( combatDone == 1 )
        set timer to timer + GetSecondsPassed
      if ( timer > 6 )
        set timer to 0
        set combatDone to 0
        set warp to 0
      endif
    endif  
#else /* ABCD */
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      ;if ( GetPCSleep == 1 )
       ; StartScript 1gr_compLevel
      ;endif
#else /* ABCD */
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_altmer" 1
    pcref->Additem "nc_altmer_pauldron" 1
    pcref->AddItem "nc_altmer_skull" 1
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetPCSneaking == 1 )
        ForceSneak
      elseif ( GetPCSneaking == 0 )
        ClearForceSneak
      endif
#else /* ABCD */
    stopscript nc_death_add_altmer
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 1 )
      if ( flyCheck == 0 )
        cast nc_comp2_lev_bog player
        addspell nc_comp2_lev
        set flyCheck to 1
      endif
      endif
#else /* ABCD */
    end

