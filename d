#ifndef ABCD
      if ( flyCheck == 1 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( Player->GetPos z )
        SetPos z playz
      elseif ( flyCheck == 1 )
      if ( GetCurrentAIPackage != 3 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( GetPos z )
        SetPos z playz
      endif
      endif
      endif
      endif
