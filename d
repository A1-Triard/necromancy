SCHD nc_ghost_script 1 0 1 85 13
SCVR doonce;timer;;
SCDT BgECCSBzAQAgPT0gMI4RcT2KPwUBcwEAAiAxCQEFAWYBAAogZgEAIFgSECArBgEBCyBmAQAgPj0gMC432xAIAQEJIGYBACA+PSA0qxEBAAkBJAEBAQ==
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_comp3Script_rgz
#else /* ABCD */
    begin nc_ghost_script
#endif /* ABCD */
    
#ifndef ABCD
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
#endif /* ! ABCD */
    float timer
#ifndef ABCD
    float timer2
    float sheatheTimer
    float moveTimer
    float h_check
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short doOnce4
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
    short counter
    short counter2
    short addOnce
    short p_Check
    short emb_hook_nominion
    float zomtimer
#else /* ABCD */
    short doonce
#endif /* ABCD */
    
#ifndef ABCD
    if ( MenuMode == 1 )
      ;if ( GetPCSleep == 0 )
        return
      ;endif
#else /* ABCD */
    if ( doonce == 0 )
    	setscale 1.08
    	set doonce to 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set emb_hook_nominion to 1
#else /* ABCD */
    set timer to ( timer + GetSecondsPassed )
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter2 to ( counter2 + 1 )
    	if ( counter2 < 10 )
    		return
    	endif
    	setdelete 1
    	return
#else /* ABCD */
    if ( timer >= 0.7 )
    	disable
    elseif ( timer >= 4 )
    	SetDelete, 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( weapUse == 1 )
      if ( Player->GetWeaponType <= 8 )
        setmarksman 0
      elseif ( weapUse == 1 )
      if ( Player->GetWeaponType >= 9 )
        setmarksman 200
      endif
      endif
    endif
#else /* ABCD */
    return
#endif /* ABCD */
    
#ifndef ABCD
    if ( combatCheck == 1 )
      set sheathetimer to sheatheTimer + GetSecondsPassed
      if ( GetSoundPlaying, "Weapon Swish" == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, crossbowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, bowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "Weapon Swish" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "crossbowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "bowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      endif
      endif
      endif
      endif
    endif
#else /* ABCD */
    end

