    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_follow_rev_script"
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( warp == 0 )
      if ( coDist > 350 )
      if ( GetDistance Player > 680 )
        SetPos x bx
        SetPos y by
        SetPos z bz
        AiFollow Player 0 0 0 0
      elseif ( warp == 0 )
      if ( coDist2 > 350 )
      if ( GetDistance Player > 680 )
        SetPos x cx
        SetPos y cy
        SetPos z cz
        AiFollow Player 0 0 0 0
      endif
      endif
      endif
      endif
      endif
#else /* ABCD */
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
#endif /* ABCD */
    endif
    
