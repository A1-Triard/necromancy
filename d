    			PlaySound, "ThunderClap"
    			PlaceAtPC "nc_nord" 1, 762, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_pclevel <= 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_nord_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_nord_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
    		endif
    		set state to 2
    	endif
    
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_nord
    	endif
    
    end

