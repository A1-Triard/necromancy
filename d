SCPT
SCHD nc_time_bounty 2 0 3 1667 28
SCVR rand;kill;timer;hour;timer2;;
SCDT 
SCTX
    begin nc_time_bounty
    
    short rand
    short kill
    float timer
    float hour
    float timer2
    
    if ( MenuMode == 1 )
    	return
    elseif ( GetPCCell "Thanatos Temple" == 1 )
    	return
    elseif ( VH_MaskDummyFlag > 0 )
    	return
    endif
    
    set timer2 to ( timer2 + GetSecondsPassed )
    if ( timer2 >= 14 )
    
    		if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		else
    			set kill to 1
    		endif
    endif
    
    set hour to GameHour
    set rand to Random, 11
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 15 )
    
    if ( hour < 8 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			MessageBox "Someone has seen you carrying a corpse."
    			ModPCCrimeLevel 100
    		endif
    	endif
    elseif ( hour < 20 )
    	if ( hour >= 8 )
    		;MessageBox "Daytime. Time is %f. Random is %g.", hour, rand
    		if ( rand <= 7 )
    			if ( GetDetected Player == 1 )
    				MessageBox "Someone has seen you carrying a corpse."
    				ModPCCrimeLevel 100
    			endif
    		endif
    	endif
    elseif ( hour >= 20 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			MessageBox "Someone has seen you carrying a corpse."
    			ModPCCrimeLevel 100
    		endif
    	endif
    endif
    
    	set timer to 0
    endif
    
    if ( kill == 1 )
    	stopscript nc_time_bounty
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	set timer2 to 0
    	set kill to 0
    endif
    
    end

SCPT
SCHD nc_tragrilar_death 4 0 0 165 33
SCVR currentday;deathday;state;nolore;;
SCDT BgEBCSBzAwAgPT0gMCQBCQEGAQIPIHMBACAhPSBHBERheQAABQFzAQAIIEcERGF5AAAFAXMCAAggcwIAIDEgKwkBBgEECSBzAgAgPj0gMgYBAhsgWBIRIGMPVGhhbmF0b3MgVGVtcGxlID09IDAbEBNuY19kZWF0aF9hZGRfYWx0bWVyjRAAAAAACQEJARwQEm5jX3RyYWdyaWxhcl9kZWF0aAEB
SCTX
    begin nc_tragrilar_death
    
    short currentday
    short deathday
    short state
    short nolore
    
    if ( state == 0 )
    	return
    endif
    
    if ( currentday != Day )
    	set currentday to Day
    	set deathday to ( deathday + 1 )
    endif
    
    if ( deathday >= 2 )
    	if ( GetPCCell "Thanatos Temple" == 0 )
    		startscript nc_death_add_altmer
    		sethealth 0
    	endif
    endif
    
    stopscript nc_tragrilar_death
    
    end

SCPT
SCHD nc_wander_rev_script 3 3 0 740 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
SCTX
    begin nc_wander_rev_script
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_wander_rev_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
    
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			return
    		endif
    
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			return
    		endif
    
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIWander 500 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will stay around here."
    			set doonce to 1
    		endif
    		
    	endif
    	
    end

SCPT
SCHD nc_wander_script 3 3 0 553 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
SCTX
    begin nc_wander_script
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_wander_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
    
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIWander 500 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will stay around here."
    			set doonce to 1
    		endif
    		
    	endif
    	
    
    end

SCPT
SCHD nc_wander_war_script 6 3 0 828 52
SCVR doonce;call;state;fwar;frwar;pwar;npcref;npcid;temp;;
SCDT 
SCTX
    begin nc_wander_war_script
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	short fwar
    	short frwar
    	short pwar
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_wander_war_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    			set fwar to 0
    			set frwar to 0
    			set pwar to 0
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
    
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			set fwar to 1
    		endif
    
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			set frwar to 1
    		endif
    
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			set pwar to 1
    		endif
    		
    		if ( fwar == 0 );not a fire
    			if ( frwar == 0 );not a frost
    				if ( pwar == 0 );not a warlord
    					return
    				endif
    			endif
    		endif
    
    		npcref->AIWander 500 0 0
    		if ( doonce == 0 )
    			MessageBox "Your warlords will stay around here."
    			set doonce to 1
    		endif
    		
    	endif
    	
    end

SCPT
SCHD nc_warlord_attack 4 5 1 818 80
SCVR doonce;call;call2;call3;prospectref;prospectid;temp;target;target2;targethealth;;
SCDT 
SCTX
    begin nc_warlord_attack
    
    
    short doonce
    short call ; status variable - should this NPC be called?
    short call2
    short call3
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
    long target
    long target2
    float targethealth
    
    
    if ( menumode == 1 )
        return
    endif
    
    
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
    setx target to xGetPCTarget
    
    if ( target == 0 )
    	return
    endif
    
    ifx ( target )
    
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
    
        ; start by assuming we want to call this one
        set call to 1
    	 set call2 to 1
    	 set call3 to 1
    
    
    
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
            set call2 to 0
            set call3 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_f_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_fr_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_p_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call3 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call2 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
        ifx ( call ) ; we use extended functions inside so need ifx
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		  
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    
    
    
    
        endif
    
    
        setx prospectref to xNextRef prospectref
    
    
    endwhile
    set doonce to 0
    set target to 0 ;target
    endif
    startscript nc_guard_attack
    stopscript nc_warlord_attack
    
    
    end

SCPT
SCHD nc_we_corpse_cure 7 1 0 705 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
SCTX
    begin nc_we_corpse_cure
    
    long mycell
    short temp
    short DoOnce1
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 1
    endif
    
    ifx ( getcell )
    
    set DoOnce1 to 1
    
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
    
    if ( temp != 0 )
    	if ( state == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 0
    			set temp to 0
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			disable
    			player-> additem "nc_bosmer_pauldron" 1
    			set state to 1
    		endif
    	endif
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
    endif
    
    set getcell to 0
    
    endif
    
    
    if ( state == 1 )
    	return
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_bosmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_we_corpse_drop 2 0 0 108 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBJuY19ib3NtZXJfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_we_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_bosmer_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

