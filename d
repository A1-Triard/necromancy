SCHD nc_ghost_scare 3 0 0 182 25
SCVR doonce;currentday;modday;;
SCDT BgEBCSBYoxEgPT0gMCQBBwEGBgEECSBzAQAgPT0gMOYQEW5jX2FuY2VzdG9yX2dob3N0AQAAAJZDAAACEAxzY3JpYiBzY3JlYW0FAXMCAAggRwREYXkAAAUBcwEAAiAxCQEJAQYBAg8gcwIAICE9IEcERGF5AAAFAXMCAAggRwREYXkAAAUBcwMACCBzAwAgMSArCQEGAQIJIHMDACA+PSAzBQFzAwACIDAFAXMBAAIgMAkBAQE=
SCTX
    begin nc_ghost_scare
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    short doonce
    short currentday
    short modday
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCollidingPC == 0 )
    	return
    else
    	if ( doonce == 0 ) ;PC is touching it
    		PlaceAtPC "nc_ancestor_ghost" 1 300 0 ;startle them good
    		PlaySound, "scrib scream"
    		set currentday to Day
    		set doonce to 1
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( currentday != Day ) ;won't happen again for 2 days
    	set currentday to Day
    	set modday to ( modday + 1 )
#else /* ABCD */
    if ( Player->GetItemCount "nc_fire_pauldron" >= 2 )
    	Player->Drop "nc_fire_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( modday >= 3 )
    	set modday to 0
    	set doonce to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
#endif /* ABCD */
    endif
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    stopscript nc_mult_fire
#endif /* ABCD */
    
    end

