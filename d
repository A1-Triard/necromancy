SCPT
SCHD nc_spell_timer 1 0 1 245 12
SCVR state;timer;;
SCDT 
SCTX
    begin nc_spell_timer
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
    endif
#else /* ABCD */
    float timer
    short state
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
#else /* ABCD */
    if ( menumode == 1 )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
#else /* ABCD */
    if ( CellChanged == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectRecall == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectDivineIntervention == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectAlmsiviIntervention == 1 )
    	set state to 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
#else /* ABCD */
    set timer to ( timer + GetSecondsPassed )
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
#else /* ABCD */
    if ( timer >= 120 )
    	set state to 1
    	set timer to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
#else /* ABCD */
    if ( state == 1 )
    	player-> RemoveSpell "nc_summon"
    	stopscript nc_spell_timer
    	set state to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    stopscript nc_mult_orc_sk
    
#endif /* ! ABCD */
    end

