SCPT
#ifndef ABCD
SCHD nc_arg_skelly_pauldron 4 0 1 655 40
#else /* ABCD */
SCHD nc_arg_skelly_pauldron 4 0 1 632 40
#endif /* ABCD */
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_arg_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_arg_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

SCPT
#ifndef ABCD
SCHD nc_bret_corpse_cure 7 1 0 703 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_bret_corpse_cure 5 0 0 559 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_bret_corpse_cure
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
#endif /* ! ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	set getcell to 1
#else /* ABCD */
    	set getcell to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ifx ( getcell )
    
    set DoOnce1 to 1
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_breton_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( temp != 0 )
    	if ( state == 0 )
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
#endif /* ABCD */
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
#ifndef ABCD
    			set getcell to 0
    			set temp to 0
#else /* ABCD */
    			set getcell to 1
#endif /* ABCD */
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
#ifndef ABCD
    			disable
    			player-> additem "nc_breton_pauldron" 1
#endif /* ! ABCD */
    			set state to 1
    		endif
#ifdef ABCD
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
#endif /* ABCD */
    	endif
#ifndef ABCD
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    set getcell to 0
#else /* ABCD */
    	set getcell to 1
#endif /* ABCD */
    
    endif
    
#ifndef ABCD
    
    if ( state == 1 )
    	return
    endif
    
#endif /* ! ABCD */
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_bret_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

