SCPT
#ifndef ABCD
SCHD nc_control_ring 3 2 0 817 35
SCVR OnPCEquip;button;status;lkey;ltemp;;
SCDT 
#else /* ABCD */
SCHD nc_khajiit_skelly_pauldron 4 0 1 644 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_control_ring
#else /* ABCD */
    begin nc_khajiit_skelly_pauldron
#endif /* ABCD */
    
    short OnPCEquip
#ifndef ABCD
    short button
    short status
    long lkey
    long ltemp
#else /* ABCD */
    short doonce
    float timer
    short OnPCDrop
    short counter
#endif /* ABCD */
    
    if ( menumode )
    	return
    endif
    
#ifndef ABCD
    if ( OnPCEquip == 1 )
    	MessageBox "What do you want to control?", "Revenants", "Warlords", "Both", "None", 
    	set OnPCEquip to 0
    	set status to 1
    endif
    
    if ( status == 1 )
    	set button to GetButtonPressed
    	if ( button == -1 )
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
#endif /* ABCD */
    		return
#ifndef ABCD
    	elseif ( button == 0 ) ;revenants
    		set status to 2
    	elseif ( button == 1 ) ;warlords
    		set status to 3
    	elseif ( button == 2 ) ;both
    			set status to 4
    	elseif ( button == 3 ) ;none
    		set status to 5
    	endif
    elseif ( status == 2 ) ;revenants
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
#endif /* ! ABCD */
    	endif
#ifndef ABCD
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest_rev
    	set status to 0
    elseif ( status == 3 ) ;warlords
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	startscript nc_keytest_warlord
    	set status to 0
    elseif ( status == 4 ) ;both
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest
    	set status to 0
    elseif ( status == 5 ) ;none
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	set status to 0
#else /* ABCD */
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    
    end
    

SCPT
SCHD nc_corpse_check 1 0 0 1498 6
SCVR delay;;
SCDT 
SCTX
    begin nc_corpse_check
    
    short delay
    
    set delay to ( delay + 1 )
    
    if ( delay < 30 )
    	return
#else /* ABCD */
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	startscript nc_mult_arg
    elseif ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	startscript nc_mult_alt
    elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	startscript nc_mult_bret
    elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	startscript nc_mult_bos
    elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	startscript nc_mult_dun
    elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	startscript nc_mult_imp
    elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	startscript nc_mult_khajiit
    elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	startscript nc_mult_nord
    elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	startscript nc_mult_orc
    elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	startscript nc_mult_rg
    elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	startscript nc_mult_arg_sk
    elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	startscript nc_mult_alt_sk
    elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	startscript nc_mult_bret_sk
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	startscript nc_mult_bos_sk
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	startscript nc_mult_dun_sk
    elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	startscript nc_mult_imp_sk
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	startscript nc_mult_khajiit_sk
    elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	startscript nc_mult_nord_sk
    elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	startscript nc_mult_orc_sk
    elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	startscript nc_mult_rg_sk
    elseif ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	startscript nc_mult_fire
    elseif ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	startscript nc_mult_frost
    elseif ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	startscript nc_mult_poison
    else
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_khajiit_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
#endif /* ABCD */
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
#ifdef ABCD
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
#ifndef ABCD
    	
#endif /* ! ABCD */
    
#ifndef ABCD
    set delay to 0
#else /* ABCD */
    end
#endif /* ABCD */
    
#ifndef ABCD
    if ( nc_has_corpse == 0 )
    	stopscript nc_corpse_check
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    end
#endif /* ! ABCD */

SCPT
#ifndef ABCD
SCHD nc_corrupter_script 3 0 0 442 22
SCVR gemcount;state;button;;
SCDT 
#else /* ABCD */
SCHD nc_level 1 0 0 49 9
SCVR nc_level;;
SCDT BQFzAQAFIFhXEQAAEBAAzO7pIPPw7uLl7fwgLSAlZwFzAQAAHBAIbmNfbGV2ZWwBAQ==
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_corrupter_script
#else /* ABCD */
    Begin nc_level
#endif /* ABCD */
    
#ifndef ABCD
    short gemcount
    short state
    short button
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( MenuMode )
    	Return
    endif
#else /* ABCD */
    short nc_level
#endif /* ABCD */
    
#ifndef ABCD
    set gemcount to GetItemCount "Misc_SoulGem_Greater"
#else /* ABCD */
    set nc_level to ( Getlevel )
#endif /* ABCD */
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	if ( gemcount < 1 )
    		Activate
    	else
    		set state to 1
    	endif
    endif
#else /* ABCD */
    messagebox "Мой уровень - %g", nc_level
#endif /* ABCD */
    
#ifndef ABCD
    if ( state == 1 )
    	MessageBox "What do you want to do with this?" "Put soulgems inside" "Create blackened soulgems"
    	set state to 2
#else /* ABCD */
    stopscript nc_level
    
    End

SCPT
SCHD nc_mult_alt 0 0 0 2087 0
SCDT 
SCTX
    begin nc_mult_alt
    
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 2 )
    	Player->Drop "nc_altmer_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( state == 2 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		Return
    	elseif ( button == 0 )
    		Activate
    		set state to 0
    	else
    		set state to 3
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	Player->Drop "nc_arg_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( state == 3 )
    	set gemcount to GetItemCount "Misc_SoulGem_Greater"
    	if ( gemcount > 0 )
    		RemoveItem "Misc_SoulGem_Greater" 1
    		AddItem "nc_SoulGem_blackened" 1
    	else
    		MessageBox "All soulgems have been blackened."
    		set state to 0
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    end

