    if ( addOnce == 0 )
      AddTopic "- Бой"
      AddTopic "- Сопровождение"
      AddTopic "- Инструкции"
      ;StartScript 1gr_compLevel
      set companion to 1
      set addOnce to 1
#else /* ABCD */
