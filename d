SCPT
SCHD nc_imp_skelly_pauldron 4 0 1 665 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_imp_skelly_pauldron
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#else /* ABCD */
    if ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	Player->Drop "nc_dunmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( menumode )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	Player->Drop "nc_imperial_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 2 )
    	Player->Drop "nc_khajiit_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_imperial_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    end
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD

SCPT
SCHD nc_keytest 2 2 0 472 25
SCVR counter;stemp;lkey;ltemp;;
SCDT 
SCTX
    Begin nc_keytest
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    short counter
    short stemp
    long lkey
    long ltemp
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( MenuMode == 1 )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk" 1
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ; Only run every 15th frame. 
    set counter to ( counter + 1 )
    if ( counter < nc_counter )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_imperial_pauldron_sk" 1
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
#ifndef ABCD
    set counter to 0
#endif /* ! ABCD */
    
#ifndef ABCD
    ; Player no necromancer, nothing to do
    if ( nc_isnecro != 1 )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ; Get status for customizeable key (default 'b')
    set lkey to nc_keycode  ; Cannot use globals with setx
    setx ltemp to xKeyPressed lkey
    setx ltemp to xKeyPressed lkey
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    ; If the player wants to configure his keycode setting, he can
    ; set nc_keycode to 0 (zero) and this script will just output
    ; the code for the pressed key.
    if ( lkey == 0 )
    	if ( ltemp )  ; Skip if no key was pressed
    		messagebox "Pressed keycode: %g" ltemp
    		return
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ; Our key was pressed?
    ifx ( ltemp )
    else
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ; Give a longer delay to avoid multiple messages
    set counter to -20
#else /* ABCD */
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    ; Sneak: Skip wander
    if ( nc_follow == 2 )
    	if ( GetPCSneaking == 1 )
    		set nc_follow to 0
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( nc_follow == 0 )
    ;	messagebox "Your revenants will stay here."
    	startscript nc_stand_script
    	set nc_follow to 1
    elseif ( nc_follow == 1 )
    ;	MessageBox "Your revenants will follow you."
    	startscript nc_follow_script
    	set nc_follow to 2
    elseif ( nc_follow == 2 )
    ;	MessageBox "I will do nothing."
    	startscript nc_wander_script
    	set nc_follow to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    End
#else /* ABCD */
    stopscript nc_mult_khajiit
#endif /* ABCD */
    
#ifdef ABCD
    
    end
#endif /* ABCD */

