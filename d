    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_khajiit_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
#endif /* ABCD */
    endif
    
