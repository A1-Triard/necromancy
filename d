#ifndef ABCD
SCHD nc_pauldron_arg_script 4 0 1 641 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#else /* ABCD */
SCHD nc_wander_rev_script 1 0 0 225 7
SCVR doonce;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_pauldron_arg_script
#else /* ABCD */
    begin nc_wander_rev_script
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
    if ( menumode )
#else /* ABCD */
    if ( menumode == 1 )
#endif /* ABCD */
    	return
    endif
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_wander_rev_script"
#endif /* ABCD */
    	return
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
#else /* ABCD */
    if ( nc_callrev == 0 )
    	set nc_callrev to 3
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_arg_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
    		endif
#else /* ABCD */
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут бродить вокруг."
#endif /* ABCD */
    	endif
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_arg_corpse" 1 1 1
    	Disable
    	set OnPCDrop to 0
    endif
#else /* ABCD */
    set doonce to 2
#endif /* ABCD */
    
    end
#ifndef ABCD
    
#endif /* ! ABCD */

