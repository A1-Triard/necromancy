SCHD nc_summon_argonian 6 0 0 1215 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_argonian
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
    endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_argonian_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				removeitem "nc_arg_pauldron_sk" 1
                removeitem "nc_argonian_skull" 1
               removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Аргонианина для этого ритуала."
    		set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_argonian" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_argonian_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_argonian_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_argonian
#endif /* ABCD */
    endif
    
#ifndef ABCD
    stopscript nc_mult_rg_sk
    
#endif /* ! ABCD */
    end

