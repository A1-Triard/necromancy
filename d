SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQE25jX25vcmRfcGF1bGRyb25fc2sBAAcBAQAQPABJdCBpcyB0b28gYXdrd2FyZCB0byBjYXJyeSBtb3JlIHRoYW4gb25lIHNrZWxldG9uIGF0IGEgdGltZS4AAAkBCQEBAQ==
#else /* ABCD */
SCHD nc_summon_imperial 6 0 0 1216 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_nord_skelly_drop
#else /* ABCD */
    begin nc_summon_imperial
#endif /* ABCD */
    
#ifndef ABCD
    short counter
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_imperial_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				removeitem "nc_imperial_pauldron_sk" 1
                removeitem "nc_imperial_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Имперца для этого ритуала."
    		set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_nord_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_imperial" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
#endif /* ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_imperial
#endif /* ABCD */
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_nord_skelly_pauldron 4 0 1 658 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#else /* ABCD */
SCHD nc_summon_khajiit 6 0 0 1209 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_nord_skelly_pauldron
#else /* ABCD */
    begin nc_summon_khajiit
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
#endif /* ABCD */
    
#ifndef ABCD
    if ( menumode )
    	return
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_khajiit_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				removeitem "nc_khajiit_pauldron_sk" 1
                removeitem "nc_khajiit_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Хаджита для этого ритуала."
    		set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_nord_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_khajiit" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_khajiit_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_khajiit_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
    	endif
#ifdef ABCD
    	set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_khajiit
#endif /* ABCD */
    endif
    
    end
#ifndef ABCD
    
    
#endif /* ! ABCD */

SCPT
#ifndef ABCD
SCHD nc_npc_zombie_script 17 12 5 5459 187
SCVR state;gender;arg;bret;de;he;imp;kha;nord;orc;rg;we;find;mark;remove;stop;change;npcref;tarref;npcid;zomref;temp;invref;invcount;invid;pcref;pcname;filename;tarid;xpos;ypos;zpos;zrot;stat;;
SCDT 
#else /* ABCD */
SCHD nc_summon_nord 6 0 0 1186 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_npc_zombie_script
    	
    	long npcref
    	long tarref
    	long npcid
    	long zomref
    	long temp
    	long invref
    	long invcount
    	long invid
    	short state
    	short gender ;1=female 0=male
    	;short race ;1=arg 2=bret 3=de 4=he 5=imp 6=kha 7=nord 8=orc 9=rg 10=we
    	short arg
    	short bret
    	short de
    	short he
    	short imp
    	short kha
    	short nord
    	short orc
    	short rg
    	short we
    	float xpos
    	float ypos
    	float zpos
    	float zrot
    	short find
    	short mark
    	short remove
    	short stop
    	short change
    	float stat
    	long pcref
    	long pcname
    	long filename
    	long tarid
    	
    	if ( MenuMode )
    		return
    	endif
    	
    	ifx ( find )
    		ifx ( npcref ) ;working through list
    			setx npcref to xNextRef npcref
    		else ;at the beginning
    			setx npcref to xFirstNPC
    		endif
    		
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 )
    			return
    		endif
    		
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringBuild "%.2s" npcid
    		setx invid to xStringCompare "nc" temp
    		
    		if ( invid ) ;is not from this mod
    			return
    		endif
    		
    		xsetref npcref
    		set temp to GetItemCount "nc_zombie_light"
    		ifx ( temp )
    			return
    		endif
    		
    		setx npcid to npcref->xRefID
    		setx zomref to xGetRef npcid
#else /* ABCD */
    begin nc_summon_nord
#endif /* ABCD */
    
#ifndef ABCD
    		zomref->xPosition xpos ypos zpos zrot
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
#endif /* ABCD */
    
#ifndef ABCD
    		set find to 0
    		set change to 1
    	endif
    	
    	ifx ( mark )
    	else
    		ifx ( npcref ) ;working through list
    			setx npcref to xNextRef npcref
    		else ;at the beginning
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref )
    		else
    			set mark to 1
    			return
    		endif
    		
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 )
    			return
    		endif
    		
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringBuild "%.2s" npcid
    		setx invid to xStringCompare "nc" temp
    		
    		if ( invid == 0 ) ;is from this mod
    			set temp to 1
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_nord_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				removeitem "nc_nord_pauldron_sk" 1
                removeitem "nc_nord_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 15
                set state to 1
             else
                	MessageBox "Вам нужно иметь 15 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			set temp to 0
    		endif
    		
    		ifx ( temp )
    			npcref->xAddItem "nc_zombie_light" 1
    		endif
    		
    		return
    	endif
    	
    	ifx ( state )
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		setx temp to xRefID
    		setx tarref to xGetRef temp
    		
    		setx temp to tarref->xIsFemale
    		if ( temp == 1 ) ;female
    			set gender to 1
    		else
    			set gender to 0
    		endif
    		
    		if ( GetRace "Argonian" == 1 )
    			set arg to 1
    		elseif ( GetRace "Breton" == 1 )
    			set bret to 1
    		elseif ( GetRace "Dark Elf" == 1 )
    			set de to 1
    		elseif ( GetRace "High Elf" == 1 )
    			set he to 1
    		elseif ( GetRace "Imperial" == 1 )
    			set imp to 1
    		elseif ( GetRace "Khajiit" == 1 )
    			set kha to 1
    		elseif ( GetRace "Nord" == 1 )
    			set nord to 1
    		elseif ( GetRace "Orc" == 1 )
    			set orc to 1
    		elseif ( GetRace "Redguard" == 1 )
    			set rg to 1
    		elseif ( GetRace "Wood Elf" == 1 )
    			set we to 1
    		endif
    		
    		Disable
    		
    		set xpos to GetPos x
    		set ypos to GetPos y
    		set zpos to GetPos z
    		set zrot to 0
    		
    		ifx ( gender  ) ;female
    			ifx ( arg );argonian
    				xPlace "nc_argonianf_zombie" 
    				setx zomref to xGetRef "nc_argonianf_zombie"
    				set arg to 0
    			endif
    			ifx ( bret ) ;breton
    				xPlace "nc_bretonf_zombie" 
    				setx zomref to xGetRef "nc_bretonf_zombie"
    				set bret to 0
    			endif
    			ifx ( de ) ;dark elf
    				xPlace "nc_dark elff_zombie" 
    				setx zomref to xGetRef "nc_dark elff_zombie"
    				set de to 0
    			endif
    			ifx ( he ) ;high elf
    				xPlace "nc_high elff_zombie" 
    				setx zomref to xGetRef "nc_high elff_zombie"
    				set he to 0
    			endif
    			ifx ( imp ) ;imperial
    				xPlace "nc_imperialf_zombie" 
    				setx zomref to xGetRef "nc_imperialf_zombie"
    				set imp to 0
    			endif
    			ifx ( kha ) ;khajiit
    				xPlace "nc_khajiitf_zombie" 
    				setx zomref to xGetRef "nc_khajiitf_zombie"
    				set kha to 0
    			endif
    			ifx ( nord ) ;nord
    				xPlace "nc_nordf_zombie" 
    				setx zomref to xGetRef "nc_nordf_zombie"
    				set nord to 0
    			endif
    			ifx ( orc ) ;orc
    				xPlace "nc_orcf_zombie" 
    				setx zomref to xGetRef "nc_orcf_zombie"
    				set orc to 0
    			endif
    			ifx ( rg ) ;redguard
    				xPlace "nc_redguardf_zombie" 
    				setx zomref to xGetRef "nc_redguardf_zombie"
    				set rg to 0
    			endif
    			ifx ( we ) ;wood elf
    				xPlace "nc_wood elff_zombie" 
    				setx zomref to xGetRef "nc_wood elff_zombie"
    				set we to 0
    			endif
    		else ;male
    			ifx ( arg );argonian
    				xPlace "nc_argonian_zombie" 
    				setx zomref to xGetRef "nc_argonian_zombie"
    				set arg to 0
    			endif
    			ifx ( bret ) ;breton
    				xPlace "nc_breton_zombie" 
    				setx zomref to xGetRef "nc_breton_zombie"
    				set bret to 0
    			endif
    			ifx ( de ) ;dark elf
    				xPlace "nc_dark elf_zombie"
    				setx zomref to xGetRef "nc_dark elf_zombie"
    				set de to 0
    			endif
    			ifx ( he ) ;high elf
    				xPlace "nc_high elf_zombie" 
    				setx zomref to xGetRef "nc_high elf_zombie"
    				set he to 0
    			endif
    			ifx ( imp ) ;imperial
    				xPlace "nc_imperial_zombie" 
    				setx zomref to xGetRef "nc_imperial_zombie"
    				set imp to 0
    			endif
    			ifx ( kha ) ;khajiit
    				xPlace "nc_khajiit_zombie" 
    				setx zomref to xGetRef "nc_khajiit_zombie"
    				set kha to 0
    			endif
    			ifx ( nord ) ;nord
    				xPlace "nc_nord_zombie" 
    				setx zomref to xGetRef "nc_nord_zombie"
    				set nord to 0
    			endif
    			ifx ( orc ) ;orc
    				xPlace "nc_orc_zombie" 
    				setx zomref to xGetRef "nc_orc_zombie"
    				set orc to 0
    			endif
    			ifx ( rg ) ;redguard
    				xPlace "nc_redguard_zombie" 
    				setx zomref to xGetRef "nc_redguard_zombie"
    				set rg to 0
    			endif
    			ifx ( we ) ;wood elf
    				xPlace "nc_wood elf_zombie" 
    				setx zomref to xGetRef "nc_wood elf_zombie"
    				set we to 0
    			endif
    		endif
    		
    		xsetref zomref
    		set temp to GetItemCount "nc_zombie_light"
    		
    		ifx ( temp )
    			set npcref to 0
    			set find to 1
    		else
    			zomref->xPosition xpos ypos zpos zrot
    			set change to 1
    		endif
    		set state to 1
    	endif
    	
    	ifx ( change )
    		setx invid invcount invref to tarref->xInventory
    		whilex ( invref )
    			if ( invcount < 0 )
    				set invcount to ( invcount * -1 )
    			endif
    			;setx invid invcount invref to xNextStack invref
    			zomref->xAddItem invid invcount
    			tarref->xRemoveItem invid invcount
    			setx invid invcount invref to tarref->xInventory
    			;else
    			;setx invid invcount invref to tarref->xInventory
    		endwhile
    		if ( invref )
    			return
    		endif
    		
    		xsetref tarref
    		set stat to GetFatigue
    		xsetref zomref
    		SetFatigue stat
    		xsetref tarref
    		set stat to GetMagicka
    		xsetref zomref
    		SetMagicka stat
    		
    		setx stat to tarref->xGetBaseStr
    		xsetref zomref
    		SetStrength stat
    		setx stat to tarref->xGetBaseInt
    		xsetref zomref
    		SetIntelligence stat
    		setx stat to tarref->xGetBaseWil
    		xsetref zomref
    		SetWillpower stat
    		setx stat to tarref->xGetBaseAgi
    		xsetref zomref
    		SetAgility stat
    		setx stat to tarref->xGetBaseSpe
    		xsetref zomref
    		SetSpeed stat
    		setx stat to tarref->xGetBaseEnd
    		xsetref zomref
    		SetEndurance stat
    		setx stat to tarref->xGetBasePer
    		xsetref zomref
    		SetPersonality stat
    		setx stat to tarref->xGetBaseLuc
    		xsetref zomref
    		SetLuck stat
    		
    		xsetref tarref
    		set stat to GetBlock
    		xsetref zomref
    		SetBlock stat	
    		xsetref tarref
    		set stat to GetArmorer
    		xsetref zomref
    		SetArmorer stat
    		xsetref tarref
    		set stat to GetMediumArmor
    		xsetref zomref
    		SetMediumArmor stat
    		xsetref tarref
    		set stat to GetHeavyArmor
    		xsetref zomref
    		SetHeavyArmor stat
    		xsetref tarref
    		set stat to GetBluntWeapon
    		xsetref zomref
    		SetBluntWeapon stat
    		xsetref tarref
    		set stat to GetLongBlade
    		xsetref zomref
    		SetLongBlade stat
    		xsetref tarref
    		set stat to GetAxe
    		xsetref zomref
    		SetAxe stat
    		xsetref tarref
    		set stat to GetSpear
    		xsetref zomref
    		SetSpear stat
    		xsetref tarref
    		set stat to GetAthletics
    		xsetref zomref
    		SetAthletics stat
    		xsetref tarref
    		set stat to GetEnchant
    		xsetref zomref
    		SetEnchant stat
    		xsetref tarref
    		set stat to GetDestruction
    		xsetref zomref
    		SetDestruction stat
    		xsetref tarref
    		set stat to GetAlteration
    		xsetref zomref
    		SetAlteration stat
    		xsetref tarref
    		set stat to GetIllusion
    		xsetref zomref
    		SetIllusion stat
    		xsetref tarref
    		set stat to GetConjuration
    		xsetref zomref
    		SetConjuration stat
    		xsetref tarref
    		set stat to GetMysticism
    		xsetref zomref
    		SetMysticism stat
    		xsetref tarref
    		set stat to GetRestoration
    		xsetref zomref
    		SetRestoration stat
    		xsetref tarref
    		set stat to GetAlchemy
    		xsetref zomref
    		SetAlchemy stat
    		xsetref tarref
    		set stat to GetUnarmored
    		xsetref zomref
    		SetUnarmored stat
    		xsetref tarref
    		set stat to GetSecurity
    		xsetref zomref
    		SetSecurity stat
    		xsetref tarref
    		set stat to GetSneak
    		xsetref zomref
    		SetSneak stat
    		xsetref tarref
    		set stat to GetAcrobatics
    		xsetref zomref
    		SetAcrobatics stat
    		xsetref tarref
    		set stat to GetLightArmor
    		xsetref zomref
    		SetLightArmor stat
    		xsetref tarref
    		set stat to GetShortBlade
    		xsetref zomref
    		SetShortBlade stat
    		xsetref tarref
    		set stat to GetMarksman
    		xsetref zomref
    		SetMarksman stat
    		xsetref tarref
    		set stat to GetMercantile
    		xsetref zomref
    		SetMercantile stat
    		xsetref tarref
    		set stat to GetSpeechcraft
    		xsetref zomref
    		SetSpeechcraft stat
    		xsetref tarref
    		set stat to GetHandToHand
    		xsetref zomref
    		SetHandToHand stat
    		
    		set remove to 1
    		set npcref to 0
    		set change to 0
    	endif
    	
    	ifx ( remove )
    		ifx ( npcref ) ;working through list
    			setx npcref to xNextRef npcref
    		else ;at the beginning
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref )
    		else
    			set remove to 0
    			set stop to 1
    			return
    		endif
    		
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 )
    			return
    		endif
    		
    		xsetref npcref
    		set temp to GetItemCount "nc_zombie_light"
    		
    		ifx ( temp )
    			npcref->xRemoveItem "nc_zombie_light" temp
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Норда для этого ритуала."
    		set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_nord" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_nord_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_nord_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
    	endif
#ifndef ABCD
    	
    	ifx ( stop )
    		set npcref to 0
    		set tarref to 0 
    		set zomref to 0
    		set invref to 0
    		set npcid to 0
    		set find to 0
    		set remove to 0
    		set change to 0
    		set stop to 0
    		set mark to 0
    		set state to 0
    		StopScript nc_npc_zombie_script
    	endif
    	
#else /* ABCD */
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_nord
    endif
    
#endif /* ABCD */
    end

SCPT
#ifndef ABCD
SCHD nc_orc_corpse_cure 7 1 0 681 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_summon_orc 6 0 0 1178 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_orc_corpse_cure
#else /* ABCD */
    begin nc_summon_orc
#endif /* ABCD */
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
#endif /* ABCD */
    short state
#ifndef ABCD
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    ifx ( getcell )
    
    set DoOnce1 to 1
    
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
    
    if ( temp != 0 )
    	if ( state == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 0
    			set temp to 0
    			return
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_orc_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    		  removeitem "nc_orc_pauldron_sk" 1
                removeitem "nc_orc_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 15
                set state to 1
             else
                	MessageBox "Вам нужно иметь 15 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			disable
    			player-> additem "nc_orc_pauldron" 1
    			set state to 1
    		endif
    	endif
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
#endif /* ABCD */
    else
#ifndef ABCD
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
#else /* ABCD */
    		MessageBox "Вам нужен череп Орка для этого ритуала."
#endif /* ABCD */
    		set state to 2
#ifndef ABCD
    	endif
    endif
    
    set getcell to 0
    
#endif /* ! ABCD */
    endif
    
#ifndef ABCD
    
#endif /* ! ABCD */
    if ( state == 1 )
#ifndef ABCD
    	return
#else /* ABCD */
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_orc" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_orc_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_orc_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
#endif /* ABCD */
    endif
    
    if ( state == 2 )
#ifndef ABCD
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_orc_skelly" 1 1 1
    		disable
    		set state to 1
    	endif
#else /* ABCD */
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_orc
#endif /* ABCD */
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_orc_corpse_drop 2 0 0 105 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEA9uY19vcmNfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
#else /* ABCD */
SCHD nc_summon_poison 16 0 0 2978 211
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_cloth;nc_alt_skull1;nc_arg_skull1;nc_bos_skull1;nc_bret_skull1;nc_imp_skull1;nc_dun_skull1;nc_kha_skull1;nc_nord_skull1;nc_orc_skull1;nc_rg_skull1;state;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_orc_corpse_drop
#else /* ABCD */
    begin nc_summon_poison
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCDrop
    short counter
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_cloth
    short nc_alt_skull1
    short nc_arg_skull1
    short nc_bos_skull1
    short nc_bret_skull1
    short nc_imp_skull1
    short nc_dun_skull1
    short nc_kha_skull1
    short nc_nord_skull1
    short nc_orc_skull1
    short nc_rg_skull1
    short state
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_cloth to getItemCount "misc_clothbolt_03"
    set nc_alt_skull1 to getItemCount "nc_altmer_skull"
    set nc_arg_skull1 to getItemCount "nc_argonian_skull"
    set nc_bos_skull1 to getItemCount "nc_bosmer_skull"
    set nc_bret_skull1 to getItemCount "nc_breton_skull"
    set nc_imp_skull1 to getItemCount "nc_imperial_skull"
    set nc_dun_skull1 to getItemCount "nc_dunmer_skull"
    set nc_kha_skull1 to getItemCount "nc_khajiit_skull"
    set nc_nord_skull1 to getItemCount "nc_nord_skull"
    set nc_orc_skull1 to getItemCount "nc_orc_skull"
    set nc_rg_skull1 to getItemCount "nc_redguard_skull"
    
    if ( nc_pclevel >= 29 )
    	set state to 1
    elseif ( nc_pclevel < 29 )
    	MessageBox "Вы недостаточно опытны, чтобы вызвать мумифицированного слугу."
    	set state to 10
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_orc_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
#else /* ABCD */
    if ( nc_p_warlord_glob >= 1 )
    	MessageBox "Вы можете контролировать только одного ядовитого слугу."
    	set state to 10
#endif /* ABCD */
    endif
#ifndef ABCD
    	
    end

