#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_standrev_script"
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
#else /* ABCD */
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
#endif /* ABCD */
    endif
#ifdef ABCD
    ;set timer to timer + GetSecondsPassed 
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 2 )
    	Player->Drop "nc_poison_pauldron" 1
#else /* ABCD */
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    stopscript nc_mult_poison
#else /* ABCD */
    set doonce to 2
#endif /* ABCD */
    
    end
#ifndef ABCD
    
#endif /* ! ABCD */

