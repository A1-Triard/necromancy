    messagebox "Short Blade : %g - Long Blade : %g - Axe : %g - Marksman : %g - Spear : %g - Blunt weapon : %g", nc_shortblade, nc_longblade, nc_axe, nc_marksman, nc_spear, nc_bluntweapon
    messagebox "Hand to hand : %g - Unarmored : %g - Block : %g - Light armor : %g - Medium armor : %g - Heavy armor : %g - Armorer : %g", nc_handtohand, nc_unarmored, nc_block, nc_lightarmor, nc_mediumarmor, nc_heavyarmor, nc_armorer
    messagebox "Enchant : %g - Alchemy : %g - Mysticism : %g - Conjuration : %g - Illusion : %g - Alteration : %g - Destruction : %g - Restoration : %g", nc_enchant, nc_alchemy, nc_mysticism, nc_conjuration, nc_illusion, nc_alteration, nc_destruction, nc_restoration
    
    stopscript nc_skills
    
