SCVR doonce;;
SCDT BgEFCSBYGBAgPT0gMRcQBgECCSBzAQAgPT0gMOYQFmFuY2VzdG9yX2dob3N0X2dyZWF0ZXIBAAAA+kMBAAUBcwEAAiAxCQEJAQEB
SCTX
    begin nc_cursed_mummy
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 1 )
      if ( wwCheck == 0 )
        cast nc_comp2_ww_bog player
        addspell nc_comp2_ww
        set wwCheck to 1
      endif
      endif
#else /* ABCD */
    short doonce
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 0 )
      if ( wwCheck == 1 )
        removespell nc_comp2_ww
        set wwCheck to 0
      endif
      endif
    
      if ( flyCheck == 1 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( Player->GetPos z )
        SetPos z playz
      elseif ( flyCheck == 1 )
      if ( GetCurrentAIPackage != 3 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( GetPos z )
        SetPos z playz
      endif
      endif
      endif
      endif
#else /* ABCD */
    if ( OnActivate == 1 )
    	activate
    	if ( doonce == 0 )
    		PlaceAtPC "ancestor_ghost_greater" 1 500 1
    		set doonce to 1
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetWeaponDrawn == 1 )
      if ( flyCheck == 0 )
        set combatCheck to 1
        set timer to 0
        set warp to 1
        return
      elseif ( GetWeaponDrawn == 1 )
      if ( flyCheck == 1 )
        set combatDone to 1
        set timer to 0
        set warp to 1
        return
      endif
      endif
      endif
#else /* ABCD */
    end

SCPT
SCHD nc_de_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_de_corpse_cure
#endif /* ABCD */
    
#ifndef ABCD
      set ax to ( Player->GetPos x )
      set ay to ( Player->GetPos y )
#else /* ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
#endif /* ABCD */
    
#ifndef ABCD
      if ( doOnce == 0 )
        set bx to ( Player->GetPos x )
        set by to ( Player->GetPos y )
        set bz to ( Player->GetPos z )
        set doOnce to 1
      endif
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
      set t1 to ( ax - bx )
      set t1 to ( t1 * t1 )
      set t2 to ( ay - by )
      set t2 to ( t2 * t2 )
      set t1 to ( t1 + t2 )
      set coDist to ( GetSquareRoot, t1 )
#else /* ABCD */
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 360 )
        set doOnce to 0
      endif
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 180 )
      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_dunmer_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist2 > 360 )
        set doOnce2 to 0
      endif
#else /* ABCD */
    	set getcell to 1
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    		if ( see_p_warlord == 0 )
      if ( warp == 0 )
      if ( coDist > 350 )
      if ( GetDistance Player > 680 )
        SetPos x bx
        SetPos y by
        SetPos z bz
        AiFollow Player 0 0 0 0
      elseif ( warp == 0 )
      if ( coDist2 > 350 )
      if ( GetDistance Player > 680 )
        SetPos x cx
        SetPos y cy
        SetPos z cz
        AiFollow Player 0 0 0 0
      endif
      endif
      endif
      endif
      endif
    endif
    endif
    endif
#endif /* ! ABCD */
    endif
    
#ifndef ABCD
    if ( see_f_warlord == 1 )
    if ( nc_f_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
#else /* ABCD */
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_fr_warlord == 1 )
    if ( nc_fr_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
#else /* ABCD */
    	if ( movingday == 3 )
    		PlaceAtMe "nc_dunmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( see_p_warlord == 1 )
    if ( nc_p_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
#else /* ABCD */
    end

