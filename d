    					set state to 10
    				endif
    			else
    				MessageBox "You need a bolt of green cloth to perform the ritual."
    				set state to 10
    			endif
    		else
    			MessageBox "You need a cured fire corpse to perform the ritual."
    			set state to 10
    		endif
    	endif
    
