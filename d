      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
#endif /* ABCD */
    
