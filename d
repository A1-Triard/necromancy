SCHD nc_summon_altmer 6 0 0 1222 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_altmer
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 2 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	Player->Drop "nc_arg_pauldron" 1
    endif
#else /* ABCD */
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to GetItemCount "nc_altmer_skull"
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	Player->Drop "nc_dunmer_pauldron" 1
#else /* ABCD */
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    					removeitem "nc_altmer_pauldron_sk" 1
       	         removeitem "nc_altmer_skull" 1
          	      removeitem "ingred_scrap_metal_01" 1
             	   removeitem "ingred_netch_leather_01" 1
             	   removeitem "ingred_bonemeal_01" 10
             	   set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Альтмера для этого ритуала."
    		set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	Player->Drop "nc_imperial_pauldron" 1
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		;PlaceAtPC "nc_high elf" 1, 762, 0
    		"nc_rev_placer"->PlaceAtMe "nc_high elf" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_necro_skill <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_high elf_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_high elf_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	Player->Drop "nc_khajiit_pauldron" 1
    endif
#endif /* ! ABCD */
    
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_altmer
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
    endif
#else /* ABCD */
    end

