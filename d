                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_bret_skull1 >= 1 )
    	            			removeitem "nc_breton_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
