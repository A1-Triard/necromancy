SCTX
    begin nc_imp_skelly_pauldron
#endif /* ABCD */
    
#ifndef ABCD
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
#else /* ABCD */
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#endif /* ABCD */
    
#ifndef ABCD
    if ( counter < 20 )
      set counter to counter + 1
      return
#else /* ABCD */
    if ( menumode )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set counter to 0
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
#else /* ABCD */
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_imperial_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ;ADD ADDITIONAL TOPICS IN THIS SECTION IF REQUIRED.
#else /* ABCD */
    end
#endif /* ABCD */
    
#ifndef ABCD
    if ( addOnce == 0 )
      AddTopic "- Бой"
      AddTopic "- Сопровождение"
      AddTopic "- Инструкции"
      ;StartScript 1gr_compLevel
      set companion to 1
      set addOnce to 1
#else /* ABCD */
    

SCPT
SCHD nc_keytest 2 2 0 510 25
SCVR counter;stemp;lkey;ltemp;;
SCDT 
SCTX
    Begin nc_keytest
    
    short counter
    short stemp
    long lkey
    long ltemp
    
    if ( MenuMode == 1 )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetItemCount,  "nc_bosmer_skull" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_bonemeal_01" < 20 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
#else /* ABCD */
    ; Only run every 15th frame. 
    set counter to ( counter + 1 )
    if ( counter < nc_counter )
    	return
    endif
    set counter to 0
    
    ; Player no necromancer, nothing to do
    if ( nc_isnecro != 1 )
    	return
    endif
    
    ; Get status for customizeable key (default 'b')
    set lkey to nc_keycode  ; Cannot use globals with setx
    setx ltemp to xKeyPressed lkey
    
    ; If the player wants to configure his keycode setting, he can
    ; set nc_keycode to 0 (zero) and this script will just output
    ; the code for the pressed key.
    if ( lkey == 0 )
    	if ( ltemp )  ; Skip if no key was pressed
    		messagebox "Код нажатой клавиши: %g" ltemp
    		return
#endif /* ABCD */
    	endif
    endif
    
#ifndef ABCD
    ;if ( OnDeath = 1 )
    if ( GetHealth <= 5 )
    	if ( doOnce3 == 0 )
    		PlaySound, "ThunderClap"
    		;PlaceAtMe "nc_we_skelly_NPC" 1 1 1
    		;PlaceAtMe "nc_bosmer_skull" 1 25 2
    		;disable
    		set doOnce3 to 1
#else /* ABCD */
    ; Our key was pressed?
    ifx ( ltemp )
    else
    	return
    endif
    
    ; Give a longer delay to avoid multiple messages
    set counter to -20
    
    ; Sneak: Skip wander
    if ( nc_follow == 2 )
    	if ( GetPCSneaking == 1 )
    		set nc_follow to 0
#endif /* ABCD */
    	endif
    endif
    
#ifndef ABCD
    end
#else /* ABCD */
    stopscript nc_stand_script
    stopscript nc_follow_script
    stopscript nc_wander_script
    
    if ( nc_follow == 0 )
    ;	messagebox "Your revenants will stay here."
    	startscript nc_stand_script
    	set nc_follow to 1
    elseif ( nc_follow == 1 )
    ;	MessageBox "Your revenants will follow you."
    	startscript nc_follow_script
    	set nc_follow to 2
    elseif ( nc_follow == 2 )
    ;	MessageBox "I will do nothing."
    	startscript nc_wander_script
    	set nc_follow to 0
    endif
    
    End
    
#endif /* ABCD */

SCPT
#ifndef ABCD
SCHD nc_comp3Script_wez 28 0 21 2951 384
SCVR see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;doOnce4;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;counter2;addOnce;p_Check;emb_hook_nominion;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;timer2;sheatheTimer;moveTimer;h_check;zomtimer;;
SCDT 
#else /* ABCD */
SCHD nc_keytest_rev 2 2 0 534 25
SCVR counter;stemp;lkey;ltemp;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_comp3Script_wez
#else /* ABCD */
    begin nc_keytest_rev
#endif /* ABCD */
    
#ifndef ABCD
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
    float timer
    float timer2
    float sheatheTimer
    float moveTimer
    float h_check
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short doOnce4
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
#endif /* ! ABCD */
    short counter
#ifndef ABCD
    short counter2
    short addOnce
    short p_Check
    short emb_hook_nominion
    float zomtimer
#else /* ABCD */
    short stemp
    long lkey
    long ltemp
#endif /* ABCD */
    
    if ( MenuMode == 1 )
#ifndef ABCD
      ;if ( GetPCSleep == 0 )
        return
      ;endif
#else /* ABCD */
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set emb_hook_nominion to 1
#else /* ABCD */
    ; Only run every 15th frame. 
    set counter to ( counter + 1 )
    if ( counter < nc_counter )
    	return
    endif
    set counter to 0
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter2 to ( counter2 + 1 )
    	if ( counter2 < 10 )
#else /* ABCD */
    ; Player no necromancer, nothing to do
    if ( nc_isnecro != 1 )
    	return
    endif
    
    ; Get status for customizeable key (default 'b')
    set lkey to nc_keycode  ; Cannot use globals with setx
    setx ltemp to xKeyPressed lkey
    
    ; If the player wants to configure his keycode setting, he can
    ; set nc_keycode to 0 (zero) and this script will just output
    ; the code for the pressed key.
    if ( lkey == 0 )
    	if ( ltemp )  ; Skip if no key was pressed
    		messagebox "Код нажатой клавиши: %g" ltemp
#endif /* ABCD */
    		return
    	endif
#ifndef ABCD
    	setdelete 1
#else /* ABCD */
    endif
    
    ; Our key was pressed?
    ifx ( ltemp )
    else
#endif /* ABCD */
    	return
    endif
    
#ifndef ABCD
    if ( weapUse == 1 )
      if ( Player->GetWeaponType <= 8 )
        setmarksman 0
      elseif ( weapUse == 1 )
      if ( Player->GetWeaponType >= 9 )
        setmarksman 200
      endif
      endif
#else /* ABCD */
    ; Give a longer delay to avoid multiple messages
    set counter to -20
    
    ; Sneak: Skip wander
    if ( nc_follow == 2 )
    	if ( GetPCSneaking == 1 )
    		set nc_follow to 0
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( combatCheck == 1 )
      set sheathetimer to sheatheTimer + GetSecondsPassed
      if ( GetSoundPlaying, "Weapon Swish" == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, crossbowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, bowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "Weapon Swish" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "crossbowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "bowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      endif
      endif
      endif
      endif
    endif
#else /* ABCD */
    stopscript nc_stand_rev_script
    stopscript nc_follow_rev_script
    stopscript nc_wander_rev_script
#endif /* ABCD */
    
#ifndef ABCD
    if ( soundPlayed == 1 )
      equip "silver dagger"
      removeitem "silver dagger" 1
    ;  messagebox "Working"
      set soundPlayed to 0
      set combatCheck to 0
      set combatDone to 1
#else /* ABCD */
    
    if ( nc_follow == 0 )
    ;	messagebox "Your revenants will stay here."
    	startscript nc_stand_rev_script
    	set nc_follow to 1
    elseif ( nc_follow == 1 )
    ;	MessageBox "Your revenants will follow you."
    	startscript nc_follow_rev_script
    	set nc_follow to 2
    elseif ( nc_follow == 2 )
    ;	MessageBox "I will do nothing."
    	startscript nc_wander_rev_script
    	set nc_follow to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( combatDone == 1 )
        set timer to timer + GetSecondsPassed
      if ( timer > 6 )
        set timer to 0
        set combatDone to 0
        set warp to 0
      endif
    endif  
#else /* ABCD */
    End
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      ;if ( GetPCSleep == 1 )
       ; StartScript 1gr_compLevel
      ;endif
#else /* ABCD */

SCPT
SCHD nc_keytest_warlord 2 2 0 534 25
SCVR counter;stemp;lkey;ltemp;;
SCDT 
SCTX
    begin nc_keytest_warlord
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetPCSneaking == 1 )
        ForceSneak
      elseif ( GetPCSneaking == 0 )
        ClearForceSneak
      endif
#else /* ABCD */
    short counter
    short stemp
    long lkey
    long ltemp
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 1 )
      if ( flyCheck == 0 )
        cast nc_comp2_lev_bog player
        addspell nc_comp2_lev
        set flyCheck to 1
      endif
      endif
#else /* ABCD */
    if ( MenuMode == 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 0 )
      if ( flyCheck == 1 )
        removespell nc_comp2_lev
        set flyCheck to 0
      endif
      endif
#else /* ABCD */
    ; Only run every 15th frame. 
    set counter to ( counter + 1 )
    if ( counter < nc_counter )
    	return
    endif
    set counter to 0
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 1 )
      if ( wwCheck == 0 )
        cast nc_comp2_ww_bog player
        addspell nc_comp2_ww
        set wwCheck to 1
      endif
      endif
#else /* ABCD */
    ; Player no necromancer, nothing to do
    if ( nc_isnecro != 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 0 )
      if ( wwCheck == 1 )
        removespell nc_comp2_ww
        set wwCheck to 0
      endif
      endif
#else /* ABCD */
    ; Get status for customizeable key (default 'b')
    set lkey to nc_keycode  ; Cannot use globals with setx
    setx ltemp to xKeyPressed lkey
#endif /* ABCD */
    
#ifndef ABCD
      if ( flyCheck == 1 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( Player->GetPos z )
        SetPos z playz
      elseif ( flyCheck == 1 )
      if ( GetCurrentAIPackage != 3 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( GetPos z )
        SetPos z playz
      endif
      endif
      endif
      endif
#else /* ABCD */
    ; If the player wants to configure his keycode setting, he can
    ; set nc_keycode to 0 (zero) and this script will just output
    ; the code for the pressed key.
    if ( lkey == 0 )
    	if ( ltemp )  ; Skip if no key was pressed
    		messagebox "Код нажатой клавиши: %g" ltemp
    		return
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetWeaponDrawn == 1 )
      if ( flyCheck == 0 )
        set combatCheck to 1
        set timer to 0
        set warp to 1
        return
      elseif ( GetWeaponDrawn == 1 )
      if ( flyCheck == 1 )
        set combatDone to 1
        set timer to 0
        set warp to 1
        return
      endif
      endif
      endif
#else /* ABCD */
    ; Our key was pressed?
    ifx ( ltemp )
    else
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
      set ax to ( Player->GetPos x )
      set ay to ( Player->GetPos y )
#else /* ABCD */
    ; Give a longer delay to avoid multiple messages
    set counter to -20
#endif /* ABCD */
    
#ifndef ABCD
      if ( doOnce == 0 )
        set bx to ( Player->GetPos x )
        set by to ( Player->GetPos y )
        set bz to ( Player->GetPos z )
        set doOnce to 1
      endif
#else /* ABCD */
    ; Sneak: Skip wander
    if ( nc_follow == 2 )
    	if ( GetPCSneaking == 1 )
    		set nc_follow to 0
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      set t1 to ( ax - bx )
      set t1 to ( t1 * t1 )
      set t2 to ( ay - by )
      set t2 to ( t2 * t2 )
      set t1 to ( t1 + t2 )
      set coDist to ( GetSquareRoot, t1 )
#else /* ABCD */
    stopscript nc_stand_war_script
    stopscript nc_follow_war_script
    stopscript nc_wander_war_script
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 360 )
        set doOnce to 0
      endif
#else /* ABCD */
    if ( nc_follow == 0 )
    ;	messagebox "Your revenants will stay here."
    	startscript nc_stand_war_script
    	set nc_follow to 1
    elseif ( nc_follow == 1 )
    ;	MessageBox "Your revenants will follow you."
    	startscript nc_follow_war_script
    	set nc_follow to 2
    elseif ( nc_follow == 2 )
    ;	MessageBox "I will do nothing."
    	startscript nc_wander_war_script
    	set nc_follow to 0
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 180 )
      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
#else /* ABCD */
    End
#endif /* ABCD */
    
#ifndef ABCD
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
#else /* ABCD */

SCPT
SCHD nc_khajiit_corpse_cure 5 0 0 563 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_khajiit_corpse_cure
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist2 > 360 )
        set doOnce2 to 0
      endif
#else /* ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    		if ( see_p_warlord == 0 )
      if ( warp == 0 )
      if ( coDist > 350 )
      if ( GetDistance Player > 680 )
        SetPos x bx
        SetPos y by
        SetPos z bz
        AiFollow Player 0 0 0 0
      elseif ( warp == 0 )
      if ( coDist2 > 350 )
      if ( GetDistance Player > 680 )
        SetPos x cx
        SetPos y cy
        SetPos z cz
        AiFollow Player 0 0 0 0
      endif
      endif
      endif
      endif
      endif
    endif
    endif
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
#ifdef ABCD
    
    if ( OnActivate == 1 )
    	set getcell to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( see_f_warlord == 1 )
    if ( nc_f_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
    
    if ( see_fr_warlord == 1 )
    if ( nc_fr_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
    
    if ( see_p_warlord == 1 )
    if ( nc_p_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
    set zomtimer to ( zomtimer + GetSecondsPassed )
    if ( zomtimer >= 30 )
    	modcurrenthealth -1
    	set zomtimer to 0
    endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_khajiit_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( compMove == 1 )
      if ( c_move == 0 )
      if ( GetDistance Player < 70 )
        AIWander 300 0 0 0 0
        set c_move to 1
      endif
      endif
      endif
    endif
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( c_move == 1 )
      if ( GetDistance Player > 100 )
        AIFollow Player 0 0 0 0
      set c_move to 0
      endif
    endif
#else /* ABCD */
    	set getcell to 1
#endif /* ABCD */
    
#ifndef ABCD
    if ( oneTimeMove == 1 )
      set moveTimer to moveTimer + GetSecondsPassed
      if ( moveTimer > 4 )
        set moveTimer to 0
        AiFollow Player 0 0 0 0
        set oneTimeMove to 0
      endif
#endif /* ! ABCD */
    endif
    
#ifndef ABCD
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
#else /* ABCD */
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( counter < 20 )
      set counter to counter + 1
      return
#else /* ABCD */
    	if ( movingday == 3 )
    		PlaceAtMe "nc_khajiit_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set counter to 0
#else /* ABCD */
    end

SCPT
SCHD nc_khajiit_corpse_drop 2 0 0 134 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBNuY19raGFqaWl0X3BhdWxkcm9uAQAAAIA/AQDbEAUBcwEAAiAwCQEcEBZuY19raGFqaWl0X2NvcnBzZV9kcm9wAQE=
SCTX
    begin nc_khajiit_corpse_drop
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
#else /* ABCD */
    short OnPCDrop
    short counter
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_khajiit_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
#ifdef ABCD
    	
    stopscript nc_khajiit_corpse_drop
    end

