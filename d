    	setx mycell to xMyCellID
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
#endif /* ABCD */
    
#ifndef ABCD
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
#else /* ABCD */
