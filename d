SCHD nc_get_war\0p 1 3 0 364 34
SCVR state;prospectref;prospectid;temp;;
SCDT 
SCTX
    begin nc_get_war
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
#else /* ABCD */
    short state
    long prospectref   ; reference to an NPC (or creature)
    long prospectid    ; string holding the NPC's id
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
    endif
#else /* ABCD */
    long temp          ; used to control ifx statements
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    ;ADD ADDITIONAL TOPICS IN THIS SECTION IF REQUIRED.
#else /* ABCD */
        ; loop through each of the NPCs and creatures in the cell
        setx prospectref to xFirstNPC
    	 set nc_phere to 0
    	 set nc_fhere to 0
    	 set nc_frhere to 0
        whilex ( prospectref )
#endif /* ABCD */
    
#ifndef ABCD
    if ( addOnce == 0 )
      AddTopic "- Бой"
      AddTopic "- Сопровождение"
      AddTopic "- Инструкции"
      ;StartScript 1gr_compLevel
      set companion to 1
      set addOnce to 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( GetItemCount,  "nc_redguard_skull" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_bonemeal_01" < 25 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
#else /* ABCD */
            ; get this NPCs id and see if it's poison warlord
            setx prospectid to prospectref->xGetBaseID 
            setx temp to xStringCompare prospectid "nc_p_warlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_phere to 1
    				;MessageBox "Got a poison.  nc_phere is %g", nc_phere
            endif
    
            setx temp to xStringCompare prospectid "nc_f_warlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_fhere to 1
    				;MessageBox "Got a fire.  nc_fhere is %g", nc_fhere
            endif
    
            setx temp to xStringCompare prospectid "nc_fr_warlord"
            if ( temp == 0 )          ; it's a warlord
                set nc_frhere to 1
    				;MessageBox "Got a frost.  nc_frhere is %g", nc_frhere
            endif
    
    		 setx prospectref to xNextRef prospectref
        endwhile
    
    ;MessageBox "nc_phere is %g, nc_fhere is %g, nc_frhere is %g", nc_phere, nc_fhere, nc_frhere
    
    stopscript nc_get_war
    end

