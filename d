    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	Player->Drop "nc_khajiit_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    npcref->xRemoveItem "nc_stump_shirt" 1
    npcref->xRemoveItem "nc_stump" 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
    	if ( invcount < 0 )
    		set invcount to ( invcount * -1 )
    	endif
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    disable
    pcref->Additem "nc_orc_pauldron" 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    stopscript nc_death_add_orc
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    end
#else /* ABCD */
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD

SCPT
SCHD nc_death_add_redguard 0 5 1 568 43
SCVR invitem;invcount;invref;pcref;npcref;timer;;
SCDT 
SCTX
    begin nc_death_add_redguard
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk" 1
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    float timer
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_imperial_pauldron_sk" 1
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    set timer to ( timer + GetSecondsPassed )
    	
    if ( timer >= 120 )
    	startscript nc_activate
    	stopscript nc_death_add_redguard
    	set timer to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
#else /* ABCD */
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    npcref->xRemoveItem "nc_stump_shirt" 1
    npcref->xRemoveItem "nc_stump" 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
    	if ( invcount < 0 )
    		set invcount to ( invcount * -1 )
    	endif
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
#else /* ABCD */
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    disable
    pcref->Additem "nc_rg_pauldron" 1
#else /* ABCD */
    stopscript nc_mult_bret
#endif /* ABCD */
    
#ifndef ABCD
    stopscript nc_death_add_redguard
#endif /* ! ABCD */
    
    end
#ifndef ABCD
    
#endif /* ! ABCD */

