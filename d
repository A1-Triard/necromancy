    	Player->Drop "nc_fire_pauldron" 1
#else /* ABCD */
    		if ( localdayspassed >= 6 )
    			removeitem "nc_orc_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
    endif
#else /* ABCD */
    if ( redguard >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
#else /* ABCD */
    		if ( localdayspassed >= 6 )
    			removeitem "nc_rg_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    stopscript nc_mult_imp_sk
    
#endif /* ! ABCD */
    end

SCPT
#ifndef ABCD
SCHD nc_mult_khajiit 0 0 0 2091 0
SCDT 
#else /* ABCD */
SCHD nc_postman 1 0 0 152 5
SCVR rand;;
SCDT DAEGcGxheWVyBgEKCSBYVxEgPj0gNQUBcwEACCBYIRBkADAwBgEHCiBzAQAgPD0gMTAGAQUJIFjgECA9PSAx5hAPbmNfc3RhcnRfc2tlbGx5AQAAAABDAQAAEBgAwvsg8ev7+Ojy5SDx8vDg7e376SD48+wuAADhEBwQCm5jX3Bvc3RtYW4FAXMBAAQgMTAwCQEJAQkBAQE=
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_mult_khajiit
#else /* ABCD */
    begin nc_postman
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	Player->Drop "nc_arg_pauldron" 1
    endif
#else /* ABCD */
    short rand
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
    endif
#else /* ABCD */
    	if ( player->GetLevel >= 5 )
    		set rand to Random, 100
    		if ( rand <= 10 )
    			if ( GetPCSleep == 1 )
    				PlaceAtPC "nc_start_skelly" 1 128 1
    				MessageBox "Вы слышите странный шум."
    				WakeUpPC
    				stopscript nc_postman
    				set rand to 100
    			endif
    		endif
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
    endif
#endif /* ! ABCD */
    
