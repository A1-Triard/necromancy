    	Player->Drop "nc_arg_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		if ( Player->HasItemEquipped "nc_knife" == 0 )
    			return
    		endif
    		if ( Player->GetWeaponDrawn == 0 )
    			return
    		endif
    		
    		setx target to xGetPCTarget
    		if ( target == 0 )
    			set doonce to 0
    			return
    		endif
    		
    		setx temp to target->xRefType
    		if ( temp != 1598246990 )
    			return
    		endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		xSetRef target
    		if ( GetHealth > 0 )
    			return
    		endif
    
    		xsetref target
    		set temp to GetItemCount "nc_stump_shirt"
    
    		if ( temp > 0 )
    			return
    		endif
    
    		set go to 0
    
    		if ( Player->GetSoundPlaying "Weapon Swish" == 1 )
    			set go to 1
    		endif
    
    		ifx ( go )
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    			set temp to ( Player->GetItemCount "nc_SoulGem_blackened" )
    		
    			if ( temp == 0 )
    				if ( doonce == 0 )
    					MessageBox "You need a blackened soulgem to trap a humanoid's soul."
    					set doonce to 1
    					set state to 1
    					return
    				endif
    			endif
    			
    			xSetRef target
    			if ( GetRace "Argonian" == 1 )
    				set race1 to 1
    			endif
    			
    			xSetRef target
    			if ( GetRace "Breton" == 1 )
    				set race2 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Dark Elf" == 1 )
    				set race3 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "High Elf" == 1 )
    				set race4 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Imperial" == 1 )
    				set race5 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Khajiit" == 1 )
    				set race6 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Nord" == 1 )
    				set race7 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Orc" == 1 )
    				set race8 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Redguard" == 1 )
    				set race9 to 1
    			endif
    				
    			xSetRef target
    			if ( GetRace "Wood Elf" == 1 )
    				set race10 to 1
    			endif			
    			
    			xSetRef target
    			if ( GetRace "Argonian Zombie" == 1 )
    				set race1 to 1
    			endif
    			
    			xSetRef target
    			if ( GetRace "Breton Zombie" == 1 )
    				set race2 to 1
