    				set state to 10
    			endif
    		else
    			MessageBox "Вам нужны обрезки металла для этого ритуала."
    			set state to 10
    		endif
    	else
    		MessageBox "Вам нужен рулон зеленой ткани для этого ритуала."
    		set state to 10
#endif /* ABCD */
    	endif
#ifndef ABCD
    	setdelete 1
    	return
#endif /* ! ABCD */
    endif
    
#ifndef ABCD
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_orc_pauldron_sk" 1
#else /* ABCD */
    if ( state == 2 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_p_warlord" 1, 0, 0
    		set nc_p_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
    
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_poison
    endif
    
    end

SCPT
SCHD nc_summon_redguard 6 0 0 1211 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_redguard
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_redguard_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    		  removeitem "nc_rg_pauldron_sk" 1
                removeitem "nc_redguard_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    	MessageBox "Вам нужен череп Редгарда для этого ритуала."
    	set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_redguard" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )	
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
#endif /* ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_redguard
#endif /* ABCD */
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_orc_skelly_pauldron 4 0 1 655 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#else /* ABCD */
SCHD nc_time_bounty 2 0 3 1650 28
SCVR rand;kill;timer;hour;timer2;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_orc_skelly_pauldron
#else /* ABCD */
    begin nc_time_bounty
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
#else /* ABCD */
    short rand
    short kill
#endif /* ABCD */
    float timer
#ifndef ABCD
    short OnPCDrop
    short counter
#else /* ABCD */
    float hour
    float timer2
#endif /* ABCD */
    
#ifndef ABCD
    if ( menumode )
#else /* ABCD */
    if ( MenuMode == 1 )
#endif /* ABCD */
    	return
#ifndef ABCD
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
#else /* ABCD */
    elseif ( GetPCCell "Храм Танатоса" == 1 )
    	return
    elseif ( VH_MaskDummyFlag > 0 )
#endif /* ABCD */
    	return
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
#else /* ABCD */
    set timer2 to ( timer2 + GetSecondsPassed )
    if ( timer2 >= 14 )
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_orc_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
#else /* ABCD */
    		if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		else
    			set kill to 1
#endif /* ABCD */
    		endif
#ifndef ABCD
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
#else /* ABCD */
    endif
    
    set hour to GameHour
    set rand to Random, 11
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 15 )
    
    if ( hour < 8 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			MessageBox "Кто-то увидел, как вы несете тело."
    			ModPCCrimeLevel 500
#endif /* ABCD */
    		endif
#ifndef ABCD
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
#else /* ABCD */
    	endif
    elseif ( hour < 20 )
    	if ( hour >= 8 )
    		;MessageBox "Daytime. Time is %f. Random is %g.", hour, rand
    		if ( rand <= 7 )
    			if ( GetDetected Player == 1 )
    				MessageBox "Кто-то увидел, как вы несете тело."
    				ModPCCrimeLevel 500
#endif /* ABCD */
    			endif
    		endif
    	endif
#ifdef ABCD
    elseif ( hour >= 20 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			MessageBox "Кто-то увидел, как вы несете тело."
    			ModPCCrimeLevel 500
    		endif
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
#else /* ABCD */
    	set timer to 0
    endif
    
    if ( kill == 1 )
    	stopscript nc_time_bounty
#endif /* ABCD */
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
#ifndef ABCD
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
#else /* ABCD */
    	set timer2 to 0
    	set kill to 0
#endif /* ABCD */
    endif
    
    end
#ifndef ABCD
    
    
#endif /* ! ABCD */

SCPT
#ifndef ABCD
SCHD nc_pauldron_altmer_script 4 0 1 650 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
#else /* ABCD */
SCHD nc_tragrilar_death 2 0 0 146 20
SCVR currentday;deathday;;
SCDT BgECDyBzAQAgIT0gRwREYXkAAAUBcwEACCBHBERheQAABQFzAgAIIHMCACAxICsJAQYBBQkgcwIAID49IDIGAQMZIFgSESBjDdXw4Owg0uDt4PLu8eAgPT0gMRsQE25jX2RlYXRoX2FkZF9hbHRtZXKNEAAAAAAcEBJuY190cmFncmlsYXJfZGVhdGgJAQkBAQE=
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_pauldron_altmer_script
#else /* ABCD */
    begin nc_tragrilar_death
#endif /* ABCD */
    
#ifndef ABCD
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#else /* ABCD */
    short currentday
    short deathday
#endif /* ABCD */
    
#ifndef ABCD
    if ( menumode )
    	return
#else /* ABCD */
    if ( currentday != Day )
    	set currentday to Day
    	set deathday to ( deathday + 1 )
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_altmer_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_altmer_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
    		endif
#else /* ABCD */
    if ( deathday >= 2 )
    	if ( GetPCCell "Храм Танатоса" == 1 )
    		startscript nc_death_add_altmer
    		sethealth 0
    		stopscript nc_tragrilar_death
#endif /* ABCD */
    	endif
    endif
    
#ifndef ABCD
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_altmer_corpse" 1 1 1
    	Disable
    	set OnPCDrop to 0
    endif
#endif /* ! ABCD */
    
    end
#ifndef ABCD
    
#endif /* ! ABCD */

