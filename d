SCHD nc_death_add_redguard 0 5 1 551 43
SCVR invitem;invcount;invref;pcref;npcref;timer;;
SCDT 
SCTX
    begin nc_death_add_redguard
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 1 )
      if ( wwCheck == 0 )
        cast nc_comp2_ww_bog player
        addspell nc_comp2_ww
        set wwCheck to 1
      endif
      endif
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 0 )
      if ( wwCheck == 1 )
        removespell nc_comp2_ww
        set wwCheck to 0
      endif
      endif
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( flyCheck == 1 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( Player->GetPos z )
        SetPos z playz
      elseif ( flyCheck == 1 )
      if ( GetCurrentAIPackage != 3 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( GetPos z )
        SetPos z playz
      endif
      endif
      endif
      endif
#else /* ABCD */
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    float timer
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetWeaponDrawn == 1 )
      if ( flyCheck == 0 )
        set combatCheck to 1
        set timer to 0
        set warp to 1
        return
      elseif ( GetWeaponDrawn == 1 )
      if ( flyCheck == 1 )
        set combatDone to 1
        set timer to 0
        set warp to 1
        return
      endif
      endif
      endif
#else /* ABCD */
    set timer to ( timer + GetSecondsPassed )
    	
    if ( timer >= 60 )
    	startscript nc_activate
    	stopscript nc_death_add_redguard
    	set timer to 0
    endif
#endif /* ABCD */
    
#ifndef ABCD
      set ax to ( Player->GetPos x )
      set ay to ( Player->GetPos y )
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( doOnce == 0 )
        set bx to ( Player->GetPos x )
        set by to ( Player->GetPos y )
        set bz to ( Player->GetPos z )
        set doOnce to 1
      endif
#else /* ABCD */
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      set t1 to ( ax - bx )
      set t1 to ( t1 * t1 )
      set t2 to ( ay - by )
      set t2 to ( t2 * t2 )
      set t1 to ( t1 + t2 )
      set coDist to ( GetSquareRoot, t1 )
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( coDist > 360 )
        set doOnce to 0
      endif
#else /* ABCD */
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist > 180 )
      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
#else /* ABCD */
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
#endif /* ABCD */
    
#ifndef ABCD
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
#else /* ABCD */
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_redguard" 1
    pcref->Additem "nc_rg_pauldron" 1
    pcref->AddItem "nc_redguard_skull" 1
#endif /* ABCD */
    
#ifndef ABCD
      if ( coDist2 > 360 )
        set doOnce2 to 0
      endif
#else /* ABCD */
    stopscript nc_death_add_redguard
#endif /* ABCD */
    
#ifndef ABCD
    if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    		if ( see_p_warlord == 0 )
      if ( warp == 0 )
      if ( coDist > 350 )
      if ( GetDistance Player > 680 )
        SetPos x bx
        SetPos y by
        SetPos z bz
        AiFollow Player 0 0 0 0
      elseif ( warp == 0 )
      if ( coDist2 > 350 )
      if ( GetDistance Player > 680 )
        SetPos x cx
        SetPos y cy
        SetPos z cz
        AiFollow Player 0 0 0 0
      endif
      endif
      endif
      endif
      endif
    endif
    endif
    endif
#else /* ABCD */
    end
    

