SCHD nc_he_skelly_pauldron 4 0 1 637 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_he_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( soundPlayed == 1 )
      equip "silver dagger"
      removeitem "silver dagger" 1
    ;  messagebox "Working"
      set soundPlayed to 0
      set combatCheck to 0
      set combatDone to 1
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( combatDone == 1 )
        set timer to timer + GetSecondsPassed
      if ( timer > 6 )
        set timer to 0
        set combatDone to 0
        set warp to 0
      endif
    endif  
#else /* ABCD */
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      ;if ( GetPCSleep == 1 )
       ; StartScript 1gr_compLevel
      ;endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_altmer_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetPCSneaking == 1 )
        ForceSneak
      elseif ( GetPCSneaking == 0 )
        ClearForceSneak
      endif
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 1 )
      if ( flyCheck == 0 )
        cast nc_comp2_lev_bog player
        addspell nc_comp2_lev
        set flyCheck to 1
      endif
      endif
#else /* ABCD */
    end
#endif /* ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectLevitate == 0 )
      if ( flyCheck == 1 )
        removespell nc_comp2_lev
        set flyCheck to 0
      endif
      endif
#endif /* ! ABCD */
    
#ifndef ABCD
      if ( Player->GetEffect sEffectWaterWalking == 1 )
      if ( wwCheck == 0 )
        cast nc_comp2_ww_bog player
        addspell nc_comp2_ww
        set wwCheck to 1
      endif
      endif
#else /* ABCD */

