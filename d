SCHD nc_mult_frost 0 0 0 2089 0
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_ghost_script
#else /* ABCD */
    begin nc_mult_frost
#endif /* ABCD */
    
#ifndef ABCD
    float timer
    short doonce
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( doonce == 0 )
    	setscale 1.08
    	set doonce to 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	Player->Drop "nc_arg_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set timer to ( timer + GetSecondsPassed )
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( timer >= 0.7 )
    	disable
    elseif ( timer >= 4 )
    	SetDelete, 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    return
    
    end

