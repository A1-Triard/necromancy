#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
#else /* ABCD */
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	Player->Drop "nc_arg_pauldron" 1
#else /* ABCD */
    if ( menumode )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
#else /* ABCD */
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем одно тело - неудобно."
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	Player->Drop "nc_dunmer_pauldron" 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_frost_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_frost_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести тело в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
    		endif
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	Player->Drop "nc_imperial_pauldron" 1
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_frost_cured" 1 1 1
    	Disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	Player->Drop "nc_khajiit_pauldron" 1
    endif
#else /* ABCD */
    end
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
    endif
