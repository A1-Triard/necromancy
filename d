SCHD nc_summon_imperial 7 0 0 1366 77
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;nc_skl;state;;
SCDT 
SCTX
    begin nc_summon_imperial
    
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_skull
    	short nc_skl
    	short state
    
    	set nc_pclevel to nc_necro_skill
    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_skull to getItemCount "nc_imperial_skull"
    	set nc_skl to getItemCount "nc_imperial_pauldron_sk"
    
    	if ( nc_skl >= 1 )
    		if ( nc_skull >= 1 )
    			if ( nc_scrap_metal >= 1 )
    				if ( nc_netch_leather >= 1 )
    					if ( nc_bonemeal >= 10 )
    						removeitem "nc_imperial_pauldron_sk" 1
    						removeitem "nc_imperial_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
    						removeitem "ingred_netch_leather_01" 1
    						removeitem "ingred_bonemeal_01" 10
    						Player->RemoveItem "nc_SoulGem_blackened_f" 1
    						Cast "nc_summon_effects" Player
    						set state to 1
    					else
    						MessageBox "You need 10 portions of bonemeal to perform the ritual."
    						set state to 2
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 2
    				endif
    			else
    				MessageBox "You need a piece of netch leather to perform the ritual."
    				set state to 2
    			endif
    		else
    			MessageBox "You need an Imperial skull to perform the ritual."
    			set state to 2
    		endif
    	else
    		MessageBox "You need an Imperial skeleton to perform the ritual."
    		set state to 2
    	endif
    
    	if ( state == 1 )
    		if ( nc_pclevel <= 10 )
    			PlaySound, "ThunderClap"
    			PlaceAtPC "nc_imperial" 1, 762, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_pclevel <= 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_imperial_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_imperial_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
    		endif
    		set state to 2
    	endif
    
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_imperial
    	endif
    
    end

SCPT
