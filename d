SCHD nc_mult_imp_sk 0 0 0 2090 0
SCDT 
SCTX
    begin nc_mult_imp_sk
    
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
        set altmer to getItemCount "nc_altmer_pauldron" 
        set arg to getItemCount "nc_arg_pauldron" 
        set bosmer to getItemCount "nc_bosmer_pauldron" 
        set breton to getItemCount "nc_breton_pauldron" 
        set dunmer to getItemCount "nc_dunmer_pauldron"
        set imperial to getItemCount "nc_imperial_pauldron" 
        set khajiit to getItemCount "nc_khajiit_pauldron" 
        set nord to getItemCount "nc_nord_pauldron" 
        set orc to getItemCount "nc_orc_pauldron" 
        set redguard to getItemCount "nc_rg_pauldron"
       	set salt to getItemCount "nc_salt"
    	set coprinus to getItemCount "ingred_coprinus_01"
    	set russula to getItemCount "ingred_russula_01"
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
#else /* ABCD */
    set pstate to 2
    if ( salt >= 500 )
       if ( coprinus >= 5 )
           if ( russula >= 5 )
               set pstate to 1
           endif
       endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk" 1
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 2 )
    	Player->RemoveItem "nc_imperial_pauldron_sk" 1
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    if ( altmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
#else /* ABCD */
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
    			MessageBox "Ядовитая мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    if ( arg >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
#endif /* ABCD */
    
#ifndef ABCD
