    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_rg_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
#endif /* ABCD */
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
#ifdef ABCD
    	Disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
    
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
    endif
    
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
    endif
    
    stopscript nc_mult_nord
    
#else /* ABCD */
    end
#endif /* ABCD */
    
    
#ifndef ABCD
    end
#endif /* ! ABCD */

SCPT
#ifndef ABCD
SCHD nc_mult_nord_sk 0 0 0 2091 0
SCDT 
#else /* ABCD */
SCHD nc_ritualbook_script 5 0 2 1554 61
SCVR state;button;pc_conjuration;pc_alteration;start;timer;timer2;;
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_mult_nord_sk
    
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    begin nc_ritualbook_script
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    short state
    short button
    short pc_conjuration
    short pc_alteration
    short start
    float timer
    float timer2
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( menumode == 1 )
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( OnActivate == 1 )
    	player-> addspell "nc_summon"
    	if ( ScriptRunning "nc_spell_timer" == 0 )
    		startscript nc_spell_timer
    	endif
    	MessageBox "После прочтения заклинаний из книги вы готовы вызвать мертвеца."
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_dunmer_pauldron_sk" 1
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
#else /* ABCD */
    set timer to ( timer + GetSecondsPassed )
    if ( timer < 2 )
    	if ( state == 0 )  ; ADDED
    		return
    	endif  ; ADDED
#endif /* ABCD */
    endif
#ifdef ABCD
    set timer to 0
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_imperial_pauldron_sk" 1
    	PlaceAtPC "nc_imp_skelly_NPC" 1 50 1
    endif
#else /* ABCD */
    set pc_conjuration to ( Player-> GetConjuration )
    set pc_alteration to ( Player-> GetAlteration )
#endif /* ABCD */
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_khajiit_pauldron_sk" 1
    	PlaceAtPC "nc_khajiit_skelly_NPC" 1 50 1
#else /* ABCD */
    if (  nc_summon_spell == 1 )
    	if ( state == 0 )  ; ADDED
    		set start to 1
    	endif  ; ADDED
    else
    	set start to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >= 2 )
    	Player->RemoveItem "nc_nord_pauldron_sk" 1
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    endif
    
    if ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_orc_pauldron_sk" 1
    	PlaceAtPC "nc_orc_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( start == 1 )
    	set start to 0  ; ADDED
    	set nc_summon_spell to 0  ; MOVED HERE
    	if ( GetJournalIndex "nc_necro_001" >= 70 )
    		if ( nc_isnecro == 1 )
    			if ( pc_conjuration >= 30 )
    				if ( pc_alteration >= 20 )
    					set state to 1
    				else
    					MessageBox "Вы недостаточно знаете искусство изменения."
    					;set nc_summon_spell to 0  REMOVED
    					return
    				endif
    			else
    				MessageBox "Вы недостаточно знаете искусство колдовства."
    				;set nc_summon_spell to 0  REMOVED
    				return
    			endif
    		else
    			MessageBox "Только опытный колдун может использовать эту книгу."
    			;set nc_summon_spell to 0  REMOVED
    			return
    		endif
    	else
    		MessageBox "Только опытный колдун может использовать эту книгу."
    		;set nc_summon_spell to 0  REMOVED
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_rg_pauldron_sk" 1
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
#else /* ABCD */
    if ( state == 1 )
    	player -> PositionCell -2, 4510, -389, 0 "Храм Танатоса"
    	disableplayerlooking
    	MessageBox "Какого мертвеца вы хотите создать?", "Альтмер", "Аргонианин", "Босмер", "Бретон", "Данмер", "Больше"
    	set state to 2
    elseif ( state == 2 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;altmer
    		set nc_ritual to 1
    		set state to 0
    	elseif ( button == 1 ) ;argonian
    		set nc_ritual to 2
    		set state to 0
    	elseif ( button == 2 ) ;bosmer
    		set nc_ritual to 3
    		set state to 0
    	elseif ( button == 3 ) ;breton
    		set nc_ritual to 4
    		set state to 0
    	elseif ( button == 4 ) ;dunmer
    		set nc_ritual to 5
    		set state to 0
    	elseif ( button == 5 ) ;more
    		set state to 3
    	endif
    elseif ( state == 3 )
    	MessageBox "Какого мертвеца вы хотите создать?", "Имперец", "Хаджит", "Норд", "Орк", "Редгард", "Мумифицированный слуга", "Ничего"
    	set state to 4
    elseif ( state == 4 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;imperial
    		set nc_ritual to 6
    		set state to 0
    	elseif ( button == 1 ) ;khajiit
    		set nc_ritual to 7
    		set state to 0
    	elseif ( button == 2 ) ;nord
    		set nc_ritual to 8
    		set state to 0
    	elseif ( button == 3 ) ;orc
    		set nc_ritual to 9
    		set state to 0
    	elseif ( button == 4 ) ;redguard
    		set nc_ritual to 10
    		set state to 0
    	elseif ( button == 5 ) ;warlord
    		set nc_ritual to 11
    		set state to 0
    	elseif ( button == 6 ) ;none
    		set state to 0
    		enableplayerlooking
    		set nc_summon_spell to 0
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
#else /* ABCD */
    end

