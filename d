SCHD nc_summon_frost 17 0 0 3479 218
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_cloth;nc_alt_skull1;nc_arg_skull1;nc_bos_skull1;nc_bret_skull1;nc_imp_skull1;nc_dun_skull1;nc_kha_skull1;nc_nord_skull1;nc_orc_skull1;nc_rg_skull1;nc_skl;state;;
SCDT 
SCTX
    begin nc_summon_frost
    
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_cloth
    	short nc_alt_skull1
    	short nc_arg_skull1
    	short nc_bos_skull1
    	short nc_bret_skull1
    	short nc_imp_skull1
    	short nc_dun_skull1
    	short nc_kha_skull1
    	short nc_nord_skull1
    	short nc_orc_skull1
    	short nc_rg_skull1
    	short nc_skl
    	short state
    
    	set nc_pclevel to nc_necro_skill
    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_cloth to getItemCount "misc_clothbolt_01"
    	set nc_alt_skull1 to getItemCount "nc_altmer_skull"
    	set nc_arg_skull1 to getItemCount "nc_argonian_skull"
    	set nc_bos_skull1 to getItemCount "nc_bosmer_skull"
    	set nc_bret_skull1 to getItemCount "nc_breton_skull"
    	set nc_imp_skull1 to getItemCount "nc_imperial_skull"
    	set nc_dun_skull1 to getItemCount "nc_dunmer_skull"
    	set nc_kha_skull1 to getItemCount "nc_khajiit_skull"
    	set nc_nord_skull1 to getItemCount "nc_nord_skull"
    	set nc_orc_skull1 to getItemCount "nc_orc_skull"
    	set nc_rg_skull1 to getItemCount "nc_redguard_skull"
    	set nc_skl to getItemCount "nc_frost_pauldron"
    
    
    	if ( nc_pclevel >= 29 )
    		set state to 1
    	elseif ( nc_pclevel < 29 )
    		MessageBox "You are not yet experienced enough to summon a Revenant Warlord."
    		set state to 10
    	endif
    
    	if ( nc_fr_warlord_glob >= 1 )
    		MessageBox "You can only command one Frost Warlord."
    		set state to 10
    	endif
    
    	if ( state == 1 )
    		if ( nc_skl >= 1 )
    			if ( nc_cloth >= 1 )
    				if ( nc_scrap_metal >= 1 )
    					if ( nc_netch_leather >= 1 )
    						if ( nc_bonemeal >= 10 )
    							if ( nc_alt_skull1 >= 1 )
    								removeitem "nc_altmer_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_arg_skull1 >= 1 )
    								removeitem "nc_argonian_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_bos_skull1 >= 1 )
    								removeitem "nc_bosmer_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_bret_skull1 >= 1 )
    								removeitem "nc_breton_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_imp_skull1 >= 1 )
    								removeitem "nc_imperial_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_dun_skull1 >= 1 )
    								removeitem "nc_dunmer_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_kha_skull1 >= 1 )
    								removeitem "nc_khajiit_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_nord_skull1 >= 1 )
    								removeitem "nc_nord_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_orc_skull1 >= 1 )
    								removeitem "nc_orc_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_rg_skull1 >= 1 )
    								removeitem "nc_redguard_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							else
    								MessageBox "You need a skull to perform the ritual."
    								set state to 10
    							endif
    						else
    							MessageBox "You need some bonemeal to perform the ritual."
    							set state to 10
    						endif
    					else
    						MessageBox "You need a piece of netch leather to perform the ritual."
    						set state to 10
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 10
    				endif
    			else
    				MessageBox "You need a bolt of blue cloth to perform the ritual."
    				set state to 10
    			endif
    		else
    			MessageBox "You need a cured fire corpse to perform the ritual."
    			set state to 10
    		endif
    	endif
    
    
    	if ( state == 2 )
    		PlaySound, "ThunderClap"
    		PlaceAtPC "nc_fr_warlord" 1, 762, 0
    		Cast "nc_summon_effects" Player
    		set nc_fr_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		set state to 10
    	endif
    
    	if ( state == 10 )
    		enableplayerlooking
    		set state to 0
    		stopscript nc_summon_frost
    	endif
    
    end

