SCDT 
#else /* ABCD */
SCHD nc_mult_fire 0 0 0 2088 0
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_get_corpse
    	
    	
    	long target
    	long temp
    	short race1
    	short race2
    	short race3
    	short race4
    	short race5
    	short race6
    	short race7
    	short race8
    	short race9
    	short race10
    	short state
    	short doonce
    	short go
    	
    	if ( menumode )
    		return
    	endif
    	
    	if ( CellChanged == 1 )
    		set state to 1
    	elseif ( player->GetEffect sEffectRecall == 1 )
    		set state to 1
    	elseif ( player->GetEffect sEffectDivineIntervention == 1 )
    		set state to 1
    	elseif ( player->GetEffect sEffectAlmsiviIntervention == 1 )
    		set state to 1
    	elseif ( player->GetSpellEffects "nc_extraction" == 0 )
    		set state to 1
    	endif
#else /* ABCD */
    begin nc_mult_fire
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	ifx ( state )
    		set state to 0
    		stopscript nc_get_corpse
    	else
