#ifndef ABCD
SCHD nc_he_corpse_cure 7 1 0 705 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_mult_imp 0 0 0 2087 0
SCDT 
#endif /* ABCD */
SCTX
#ifndef ABCD
    begin nc_he_corpse_cure
#else /* ABCD */
    begin nc_mult_imp
#endif /* ABCD */
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
    short state
    short currentday
    short movingday
    short getcell
    short counter
#else /* ABCD */
    if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	Player->Drop "nc_altmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	Player->Drop "nc_arg_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	set getcell to 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	Player->Drop "nc_breton_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ifx ( getcell )
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	Player->Drop "nc_bosmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    set DoOnce1 to 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	Player->Drop "nc_dunmer_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron" >= 2 )
    	Player->Drop "nc_imperial_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	Player->Drop "nc_khajiit_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( temp != 0 )
    	if ( state == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 0
    			set temp to 0
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			disable
    			player-> additem "nc_altmer_pauldron" 1
    			set state to 1
    		endif
    	endif
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set getcell to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
#endif /* ABCD */
    endif
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_altmer_pauldron_sk" 1
    	PlaceAtPC "nc_he_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( state == 1 )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_arg_pauldron_sk" 1
    	PlaceAtPC "nc_arg_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_breton_pauldron_sk" 1
    	PlaceAtPC "nc_bret_skelly_NPC" 1 50 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( movingday == 3 )
    		PlaceAtMe "nc_altmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	Player->RemoveItem "nc_bosmer_pauldron_sk" 1
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    end

