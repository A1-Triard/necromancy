SCVR timer;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEFAWYBAAogZgEAIFgSECArBgEBCCBmAQAgPCAyJAEJAQUBZgEAAiAwBgEBCCBYjREgPCA0jhEAAIBACQEBAQ==
#else /* ABCD */
SCHD nc_cauldron_script 15 0 1 2394 188
SCVR nc_altmer_skl;nc_argonian_skl;nc_bosmer_skl;nc_breton_skl;nc_dunmer_skl;nc_imperial_skl;nc_khajiit_skl;nc_nord_skl;nc_orc_skl;nc_redguard_skl;nc_poison;nc_fire;nc_frost;doonce;state;timer;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_cauldron_script
    
        float timer
#ifdef ABCD
        short nc_altmer_skl
        short nc_argonian_skl
        short nc_bosmer_skl
        short nc_breton_skl
        short nc_dunmer_skl
        short nc_imperial_skl
        short nc_khajiit_skl
        short nc_nord_skl
        short nc_orc_skl
        short nc_redguard_skl
        short nc_poison
        short nc_fire
        short nc_frost
        short doonce
        short state
#endif /* ABCD */
        ;global nc_ritual
    
        if ( menumode == 1 )
            return
        endif
    
        set timer to ( timer + GetSecondsPassed )
        if ( timer < 2 )
            return
        endif
        set timer to 0
    
        if ( getscale < 4 )
            setscale 4
        endif
    
#ifdef ABCD
        if ( nc_ritual == 0 )
            return
        endif
    
        set nc_altmer_skl to getItemCount "nc_altmer_pauldron_sk"
        set nc_argonian_skl to getItemCount "nc_arg_pauldron_sk"
        set nc_bosmer_skl to getItemCount "nc_bosmer_pauldron_sk"
        set nc_breton_skl to getItemCount "nc_breton_pauldron_sk"
        set nc_dunmer_skl to getItemCount "nc_dunmer_pauldron_sk"
        set nc_imperial_skl to getItemCount "nc_imperial_pauldron_sk"
        set nc_khajiit_skl to getItemCount "nc_khajiit_pauldron_sk"
        set nc_nord_skl to getItemCount "nc_nord_pauldron_sk"
        set nc_orc_skl to getItemCount "nc_orc_pauldron_sk"
        set nc_redguard_skl to getItemCount "nc_rg_pauldron_sk"
        set nc_poison to getItemCount "nc_poison_cured"
        set nc_fire to getItemCount "nc_fire_cured"
        set nc_frost to getItemCount "nc_frost_cured"
    
        if ( state == 0 )
            if ( nc_ritual == 1 )
                if ( getItemCount "nc_altmer_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 2
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 2 )
                if ( getItemCount "nc_arg_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 1
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 3 )
                if ( getItemCount "nc_bosmer_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 3
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 4 )
                if ( getItemCount "nc_breton_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 4
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 5 )
                if ( getItemCount "nc_dunmer_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 5
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 6 )
                if ( getItemCount "nc_imperial_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 6
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 7 )
                if ( getItemCount "nc_khajiit_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 7
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 8 )
                if ( getItemCount "nc_nord_pauldron_sk" >= 1 )
                   startscript nc_gem_test
    			 set nc_gem_race to 8
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 9 )
                if ( getItemCount "nc_orc_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 9
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 10 )
                if ( getItemCount "nc_rg_pauldron_sk" >= 1 )
                    startscript nc_gem_test
    			 set nc_gem_race to 10
                    set state to 1
                else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            elseif ( nc_ritual == 11 )
                if ( getItemCount "nc_poison_pauldron" >= 1 )
    			;set nc_gem_race to 11
                  startscript nc_summon_poison
                   set state to 1
                elseif ( getItemCount "nc_fire_pauldron" >= 1 )
    			;set nc_gem_race to 12
                    startscript nc_summon_fire
                    set state to 1
                elseif ( getItemCount "nc_frost_pauldron" >= 1 )
    			;set nc_gem_race to 13
                    startscript nc_summon_frost
                    set state to 1
    	   	  else
                    MessageBox "Вам нужен труп, чтобы провести ритуал."
                    set state to 1
                endif
            endif
    
        elseif ( state == 1 )
            enableplayerlooking
            set doonce to 0
            set nc_ritual to 0
            set state to 0
            return
        endif
#endif /* ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_comp3Script_arg 28 0 20 3327 375
SCVR see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;doOnce4;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;counter2;addOnce;p_Check;emb_hook_nominion;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;timer2;sheatheTimer;moveTimer;h_check;;
SCDT 
#else /* ABCD */
SCHD nc_comp3Script_arg 29 0 20 3540 380
SCVR see_f_warlord;see_fr_warlord;see_p_warlord;doOnce;doOnce2;doOnce3;doOnce4;combatDone;weapUse;combatCheck;soundPlayed;flyCheck;wwCheck;oneTimeMove;compMove;c_move;warp;c_marksman;c_speed;c_athletics;companion;nohello;noLore;counter;counter2;addOnce;p_Check;emb_hook_nominion;call;ax;ay;bx;by;bz;cx;cy;cz;t1;t2;t3;t4;coDist;coDist2;playz;timer;timer2;sheatheTimer;moveTimer;h_check;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_comp3Script_arg
    
    float ax
    float ay
    float bx
    float by
    float bz
    float cx
    float cy
    float cz
    float t1
    float t2
    float t3
    float t4
    float coDist
    float coDist2
    float playz
    float timer
    float timer2
    float sheatheTimer
    float moveTimer
    float h_check
    short see_f_warlord
    short see_fr_warlord
    short see_p_warlord
    short doOnce
    short doOnce2
    short doOnce3
    short doOnce4
    short combatDone
    short weapUse
    short combatCheck
    short soundPlayed
    short flyCheck
    short wwCheck
    short oneTimeMove
    short compMove
    short c_move
    short warp
    short c_marksman
    short c_speed
    short c_athletics
    short companion
    short nohello
    short noLore
    short counter
    short counter2
    short addOnce
    short p_Check
    short emb_hook_nominion
#ifdef ABCD
    short call
#endif /* ABCD */
    
    if ( MenuMode == 1 )
      ;if ( GetPCSleep == 0 )
        return
      ;endif
    endif
    
    set emb_hook_nominion to 1
    
    if ( GetDisabled == 1 )
    	set counter2 to ( counter2 + 1 )
    	if ( counter2 < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( weapUse == 1 )
      if ( Player->GetWeaponType <= 8 )
        setmarksman 0
      elseif ( weapUse == 1 )
      if ( Player->GetWeaponType >= 9 )
        setmarksman 200
      endif
      endif
    endif
    
    if ( combatCheck == 1 )
      set sheathetimer to sheatheTimer + GetSecondsPassed
      if ( GetSoundPlaying, "Weapon Swish" == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, crossbowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( GetSoundPlaying, bowShoot == 1 )
        set sheatheTimer to 0
        return
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "Weapon Swish" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "crossbowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      elseif ( sheatheTimer > 4 )
      if ( GetSoundPlaying "bowShoot" == 0 )
        set sheatheTimer to 0
        set soundPlayed to 1
      endif
      endif
      endif
      endif
    endif
    
    if ( soundPlayed == 1 )
      equip "silver dagger"
      removeitem "silver dagger" 1
    ;  messagebox "Working"
      set soundPlayed to 0
      set combatCheck to 0
      set combatDone to 1
    endif
    
    if ( combatDone == 1 )
        set timer to timer + GetSecondsPassed
      if ( timer > 6 )
        set timer to 0
        set combatDone to 0
        set warp to 0
      endif
    endif  
    
    if ( GetCurrentAIPackage == 3 )
      ;if ( GetPCSleep == 1 )
       ; StartScript 1gr_compLevel
      ;endif
    
      if ( GetPCSneaking == 1 )
        ForceSneak
      elseif ( GetPCSneaking == 0 )
        ClearForceSneak
      endif
    
      if ( Player->GetEffect sEffectLevitate == 1 )
      if ( flyCheck == 0 )
        cast nc_comp2_lev_bog player
        addspell nc_comp2_lev
        set flyCheck to 1
      endif
      endif
    
      if ( Player->GetEffect sEffectLevitate == 0 )
      if ( flyCheck == 1 )
        removespell nc_comp2_lev
        set flyCheck to 0
      endif
      endif
    
      if ( Player->GetEffect sEffectWaterWalking == 1 )
      if ( wwCheck == 0 )
        cast nc_comp2_ww_bog player
        addspell nc_comp2_ww
        set wwCheck to 1
      endif
      endif
    
      if ( Player->GetEffect sEffectWaterWalking == 0 )
      if ( wwCheck == 1 )
        removespell nc_comp2_ww
        set wwCheck to 0
      endif
      endif
    
      if ( flyCheck == 1 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( Player->GetPos z )
        SetPos z playz
      elseif ( flyCheck == 1 )
      if ( GetCurrentAIPackage != 3 )
      if ( GetWeaponDrawn == 0 )
        set playz to ( GetPos z )
        SetPos z playz
      endif
      endif
      endif
      endif
    
      if ( GetWeaponDrawn == 1 )
      if ( flyCheck == 0 )
        set combatCheck to 1
        set timer to 0
        set warp to 1
        return
      elseif ( GetWeaponDrawn == 1 )
      if ( flyCheck == 1 )
        set combatDone to 1
        set timer to 0
        set warp to 1
        return
      endif
      endif
      endif
    
      set ax to ( Player->GetPos x )
      set ay to ( Player->GetPos y )
    
      if ( doOnce == 0 )
        set bx to ( Player->GetPos x )
        set by to ( Player->GetPos y )
        set bz to ( Player->GetPos z )
        set doOnce to 1
      endif
    
      set t1 to ( ax - bx )
      set t1 to ( t1 * t1 )
      set t2 to ( ay - by )
      set t2 to ( t2 * t2 )
      set t1 to ( t1 + t2 )
      set coDist to ( GetSquareRoot, t1 )
    
      if ( coDist > 360 )
        set doOnce to 0
      endif
    
      if ( coDist > 180 )
      if ( doOnce2 == 0 )
        set cx to ( Player->GetPos x )
        set cy to ( Player->GetPos y )
        set cz to ( Player->GetPos z )
        set doOnce2 to 1
      endif
      endif
    
      set t3 to ( ax - cx )
      set t3 to ( t3 * t3 )
      set t4 to ( ay - cy )
      set t4 to ( t4 * t4 )
      set t3 to ( t3 + t4 )
      set coDist2 to ( GetSquareRoot, t3 )
    
      if ( coDist2 > 360 )
        set doOnce2 to 0
      endif
    
    if ( see_f_warlord == 0 )
    	if ( see_fr_warlord == 0 )
    		if ( see_p_warlord == 0 )
      if ( warp == 0 )
      if ( coDist > 350 )
      if ( GetDistance Player > 680 )
        SetPos x bx
        SetPos y by
        SetPos z bz
        AiFollow Player 0 0 0 0
      elseif ( warp == 0 )
      if ( coDist2 > 350 )
      if ( GetDistance Player > 680 )
        SetPos x cx
        SetPos y cy
        SetPos z cz
        AiFollow Player 0 0 0 0
      endif
      endif
      endif
      endif
      endif
    endif
    endif
    endif
    endif
    
    if ( see_f_warlord == 1 )
    if ( nc_f_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
    
    if ( see_fr_warlord == 1 )
    if ( nc_fr_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
    
    if ( see_p_warlord == 1 )
    if ( nc_p_follow == 0 )
    	set warp to 1
    else
    	set warp to 0
    endif
    endif
    
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
    
#ifdef ABCD
    if ( call == 0 )
    	if ( nc_callrev == 1 )
    		AIWander 0 0 0
    		set call to 1
    	elseif ( nc_callrev == 2 )
    		AIFollow player 0 0 0 0
    		set call to 2
    	elseif ( nc_callrev == 3 )
    		AIWander 500 0 0
    		set call to 3
    	endif
    	if ( nc_callmessage == 0 )
    		set nc_callmessage to 1
    	endif
    endif
    
    if ( nc_callrev == 0 )
    	set call to 0
    endif
    
#endif /* ABCD */
    if ( GetCurrentAIPackage == 3 )
      if ( compMove == 1 )
      if ( c_move == 0 )
      if ( GetDistance Player < 70 )
        AIWander 300 0 0 0 0
        set c_move to 1
      endif
      endif
      endif
    endif
    
    if ( c_move == 1 )
      if ( GetDistance Player > 100 )
        AIFollow Player 0 0 0 0
      set c_move to 0
      endif
    endif
    
    if ( oneTimeMove == 1 )
      set moveTimer to moveTimer + GetSecondsPassed
      if ( moveTimer > 4 )
        set moveTimer to 0
        AiFollow Player 0 0 0 0
        set oneTimeMove to 0
      endif
    endif
    
    ;AND ABOVE HERE.
    
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
    
    if ( counter < 20 )
      set counter to counter + 1
      return
    endif
    
    set counter to 0
    
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
    
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
    endif
    
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
    endif
    
    ;ADD ADDITIONAL TOPICS IN THIS SECTION IF REQUIRED.
    
    if ( addOnce == 0 )
      AddTopic "- Бой"
      AddTopic "- Сопровождение"
      AddTopic "- Инструкции"
      ;StartScript 1gr_compLevel
      set companion to 1
      set addOnce to 1
    endif
    
    if ( GetItemCount,  "nc_argonian_skull" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_bonemeal_01" < 25 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    endif
    
    ;if ( OnDeath = 1 )
    if ( GetHealth <= 5 )
    	if ( doOnce3 == 0 )
    		PlaySound, "ThunderClap"
#ifndef ABCD
    		;PlaceAtMe "nc_arg_skelly_NPC" 1 1 1
    		;PlaceAtMe "nc_argonian_skull" 1 25 2
    		;disable
#else /* ABCD */
    		PlaceAtMe "nc_arg_skelly_NPC" 1 1 1
    		PlaceAtMe "nc_argonian_skull" 1 25 2
    		disable
#endif /* ABCD */
    		set doOnce3 to 1
    	endif
    endif
    
    end
    
    

