SCHD nc_loop_test 1 2 0 312 21
SCVR counter;npcref;npcid;;
SCDT 
SCTX
    begin nc_loop_test
#else /* ABCD */
    if ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	Player->Drop "nc_imperial_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    long npcref
    long npcid
    short counter
#else /* ABCD */
    if ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	Player->Drop "nc_khajiit_pauldron" 1
    endif
#endif /* ABCD */
    
    
#ifndef ABCD
    if ( menumode )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	Player->Drop "nc_nord_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set counter to ( counter + 1 )
    if ( counter < 15 )
    	return
#else /* ABCD */
    if ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	Player->Drop "nc_orc_pauldron" 1
#endif /* ABCD */
    endif
    
#ifndef ABCD
    set counter to 0
#else /* ABCD */
    if ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	Player->Drop "nc_rg_pauldron" 1
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	ifx ( npcref )
    		setx npcref to xNextRef npcref
    		setx npcid to npcref->xRefID
    		xMessageFix "checking %s" npcid
    		MessageBox "hee hee hee hee hee hee hee hee hee"
    	else
    		setx npcref to xFirstNPC
    		setx npcid to npcref->xRefID
    		xMessageFix "first: checking %s" npcid
    		MessageBox "hee hee hee hee hee hee hee hee hee"
    	endif
#else /* ABCD */
    if ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	Player->Drop "nc_fire_pauldron" 1
    endif
    
    if ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	Player->Drop "nc_frost_pauldron" 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    if ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	Player->Drop "nc_poison_pauldron" 1
    endif
#endif /* ABCD */
    
#ifdef ABCD
    stopscript nc_mult_nord_sk
#endif /* ABCD */
    
    end

