    begin nc_rg_skelly_drop
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_rg_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
