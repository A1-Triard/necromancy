SCHD nc_skelly_bret 1 0 0 132 8
SCVR counter;;
SCDT BgEBBSBYIBAAJAEJAQYBBgkgWNwQID09IDEFAXMBAAggcwEAIDEgKwYBAQkgcwEAIDwgMTAkAQkBqxEBACQBCQEGAQIFIFgYEADbEAwBBnBsYXllctQQFW5jX2JyZXRvbl9wYXVsZHJvbl9zawEACQEcEA5uY19za2VsbHlfYnJldAEB
SCTX
    begin nc_skelly_bret
    
    short counter
    
    if ( MenuMode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate  )
    	disable
    	player-> additem "nc_breton_pauldron_sk" 1
    endif
    
    
    stopscript nc_skelly_bret
    end

