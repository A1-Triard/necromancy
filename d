SCHD nc_de_skelly_pauldron 4 0 1 637 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_de_skelly_pauldron
#endif /* ABCD */
    
#ifndef ABCD
    if ( counter < 20 )
      set counter to counter + 1
      return
    endif
#else /* ABCD */
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
#endif /* ABCD */
    
#ifndef ABCD
    set counter to 0
#else /* ABCD */
    if ( menumode )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
    
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
#else /* ABCD */
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
#else /* ABCD */
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ;ADD ADDITIONAL TOPICS IN THIS SECTION IF REQUIRED.
    
    if ( addOnce == 0 )
      AddTopic "- Бой"
      AddTopic "- Сопровождение"
      AddTopic "- Инструкции"
      ;StartScript 1gr_compLevel
      set companion to 1
      set addOnce to 1
#else /* ABCD */
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_dunmer_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetItemCount,  "nc_nord_skull" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_bonemeal_01" < 30 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
#else /* ABCD */
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ;if ( OnDeath = 1 )
    if ( GetHealth <= 5 )
    	if ( doOnce3 == 0 )
    		PlaySound, "ThunderClap"
    		;PlaceAtMe "nc_nord_skelly_NPC" 1 1 1
    		;PlaceAtMe "nc_nord_skull" 1 25 2
    		;disable
    		set doOnce3 to 1
#else /* ABCD */
    end
    
    

