    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ;IF ADDITIONS ARE DESIRED, PLEASE ADD THEM BELOW HERE...
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( compMove == 1 )
      if ( c_move == 0 )
      if ( GetDistance Player < 70 )
        AIWander 300 0 0 0 0
        set c_move to 1
      endif
      endif
      endif
    endif
#else /* ABCD */
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
#endif /* ABCD */
    
#ifndef ABCD
    if ( c_move == 1 )
      if ( GetDistance Player > 100 )
        AIFollow Player 0 0 0 0
      set c_move to 0
      endif
    endif
#else /* ABCD */
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
#endif /* ABCD */
    
#ifndef ABCD
    if ( oneTimeMove == 1 )
      set moveTimer to moveTimer + GetSecondsPassed
      if ( moveTimer > 4 )
        set moveTimer to 0
        AiFollow Player 0 0 0 0
        set oneTimeMove to 0
      endif
    endif
#else /* ABCD */
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_khajiit" 1
    pcref->Additem "nc_khajiit_pauldron" 1
    pcref->AddItem "nc_khajiit_skull" 1
#endif /* ABCD */
    
#ifndef ABCD
    ;AND ABOVE HERE.
#else /* ABCD */
    stopscript nc_death_add_khajiit
#endif /* ABCD */
    
#ifndef ABCD
    ;ADDITIONS CAN ALSO BE ADDED TO THIS FOLLOWING SECTION, BUT BEWARE OF THE COUNTER.
    ;Counter can pooch some functions if they need to be checked every frame.
#else /* ABCD */
    end

SCPT
SCHD nc_death_add_nord 0 5 1 537 43
SCVR invitem;invcount;invref;pcref;npcref;timer;;
SCDT 
SCTX
    begin nc_death_add_nord
#endif /* ABCD */
    
#ifndef ABCD
    if ( counter < 20 )
      set counter to counter + 1
      return
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    set counter to 0
#endif /* ! ABCD */
    
#ifndef ABCD
    if ( GetCurrentAIPackage == 3 )
      if ( GetDistance Player < 300 )
        setSpeed 15
        setAthletics 15
      elseif ( GetDistance Player > 300 )
        set c_speed to ( ( Player->GetSpeed ) * 2.25 )
        set c_athletics to ( ( Player->GetAthletics ) * 2.25 )
        setSpeed  c_speed
        setAthletics c_athletics
      endif
#else /* ABCD */
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    float timer
#endif /* ABCD */
    
#ifndef ABCD
      if ( GetItemCount p_restore_health_b >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_c >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_e >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_q >= 1 )
        set p_Check to 1
      elseif ( GetItemCount p_restore_health_s >= 1 )
        set p_Check to 1
      else
        set p_Check to 0
      endif
#else /* ABCD */
    set timer to ( timer + GetSecondsPassed )
    	
    if ( timer >= 60 )
    	startscript nc_activate
    	stopscript nc_death_add_nord
    	set timer to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    if ( GetCurrentAIPackage != 3 )
      setspeed 40
#else /* ABCD */
    
