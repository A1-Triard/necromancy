SCHD nc_cauldron_script 1 0 1 3193 13
SCVR state2;timer;;
SCDT 
SCTX
    begin nc_cauldron_script
    
    float timer
    short state2
    ;global nc_ritual
    
    if ( menumode == 1 )
    	return
    endif
    
    set timer to ( timer + GetSecondsPassed )
    if ( timer < 2 )
    	return
    endif
    set timer to 0
    
    if ( getscale < 4 )
    	setscale 4
    endif
    
    if ( nc_ritual == 0 )
    	return
    endif
    
    if ( state2 == 1 )
    	enableplayerlooking
    	set nc_ritual to 0
    	set state2 to 0
    	return
    endif
    
    if ( nc_ritual == 1 )
    	if ( getItemCount "nc_altmer_pauldron_sk" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 2
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 2 )
    	if ( getItemCount "nc_arg_pauldron_sk" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 1
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 3 )
    	if ( getItemCount "nc_bosmer_pauldron_sk" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 3
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    		set state2 to 1
    	endif
    elseif ( nc_ritual == 4 )
    	if ( getItemCount "nc_breton_pauldron_sk" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 4
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 5 )
