SCPT
SCHD nc_ritualbook_script 6 0 2 1808 68
SCVR state;button;pc_conjuration;pc_alteration;start;gender;timer;timer2;;
SCDT 
SCTX
    begin nc_ritualbook_script
    
    short state
    short button
    short pc_conjuration
    short pc_alteration
    short start
    float timer
    float timer2
    short gender
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( OnActivate == 1 )
    	player-> addspell "nc_summon"
    	if ( ScriptRunning "nc_spell_timer" == 0 )
    		startscript nc_spell_timer
    	endif
    	MessageBox "После прочтения заклинаний из книги вы готовы вызвать мертвеца."
    endif
    
    set timer to ( timer + GetSecondsPassed )
    if ( timer < 2 )
    	if ( state == 0 )  ; ADDED
    		return
    	endif  ; ADDED
    endif
    set timer to 0
    
    set pc_conjuration to ( Player-> GetConjuration )
    set pc_alteration to ( Player-> GetAlteration )
    
    if (  nc_summon_spell == 1 )
    	if ( state == 0 )  ; ADDED
    		set start to 1
    	endif  ; ADDED
    else
    	set start to 0
    endif
    
    if ( start == 1 )
    	set start to 0  ; ADDED
    	set nc_summon_spell to 0  ; MOVED HERE
    	if ( GetJournalIndex "nc_necro_001" >= 70 )
    		if ( nc_isnecro == 1 )
    			if ( pc_conjuration >= 30 )
    				if ( pc_alteration >= 20 )
    					set state to 1
    				else
    					MessageBox "Вы недостаточно знаете искусство изменения."
    					;set nc_summon_spell to 0  REMOVED
    					return
    				endif
    			else
    				MessageBox "Вы недостаточно знаете искусство колдовства."
    				;set nc_summon_spell to 0  REMOVED
    				return
    			endif
    		else
    			MessageBox "Только опытный колдун может использовать эту книгу."
    			;set nc_summon_spell to 0  REMOVED
    			return
    		endif
    	else
    		MessageBox "Только опытный колдун может использовать эту книгу."
    		;set nc_summon_spell to 0  REMOVED
    		return
    	endif
    endif
    
    if ( state == 1 )
    	player -> PositionCell -2, 4510, -389, 0 "Храм Танатоса"
    	disableplayerlooking
    	MessageBox "Какого мертвеца вы хотите создать?", "Скелет мужчины", "Скелет женщины", "Мумифицированный слуга"
    	set state to 2
    elseif ( state == 2 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;male
    		set gender to 0
    		set state to 3
    	elseif ( button == 1 ) ;female
    		set gender to 10
    		set state to 3
    	elseif ( button == 2 ) ;warlord
    		set nc_ritual to 21
    		set state to 0
    	endif
    elseif ( state == 3 )
    	MessageBox "Какого мертвеца вы хотите создать?", "Альтмер", "Аргонианин", "Босмер", "Бретон", "Данмер", "Больше"
    	set state to 4
    elseif ( state == 4 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;altmer
    		set nc_ritual to gender + 1
    		set state to 0
    	elseif ( button == 1 ) ;argonian
    		set nc_ritual to gender + 2
    		set state to 0
    	elseif ( button == 2 ) ;bosmer
    		set nc_ritual to gender + 3
    		set state to 0
    	elseif ( button == 3 ) ;breton
    		set nc_ritual to gender + 4
    		set state to 0
    	elseif ( button == 4 ) ;dunmer
    		set nc_ritual to gender + 5
    		set state to 0
    	elseif ( button == 5 ) ;more
    		set state to 5
    	endif
    elseif ( state == 5 )
    	MessageBox "Какого мертвеца вы хотите создать?", "Имперец", "Хаджит", "Норд", "Орк", "Редгард", "Ничего"
    	set state to 6
    elseif ( state == 6 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;imperial
    		set nc_ritual to gender + 6
    		set state to 0
    	elseif ( button == 1 ) ;khajiit
    		set nc_ritual to gender + 7
    		set state to 0
    	elseif ( button == 2 ) ;nord
    		set nc_ritual to gender + 8
    		set state to 0
    	elseif ( button == 3 ) ;orc
    		set nc_ritual to gender + 9
    		set state to 0
    	elseif ( button == 4 ) ;redguard
    		set nc_ritual to gender + 10
    		set state to 0
    	elseif ( button == 5 ) ;none
    		set state to 0
    		enableplayerlooking
    		set nc_summon_spell to 0
    		return
    	endif
    endif
    
    end

SCPT
SCHD nc_skelly_arg 1 0 0 128 8
SCVR counter;;
SCDT BgEBBSBYIBAAJAEJAQYBBgkgWNwQID09IDEFAXMBAAggcwEAIDEgKwYBAQkgcwEAIDwgMTAkAQkBqxEBACQBCQEGAQIFIFgYEADbEAwBBnBsYXllctQQEm5jX2FyZ19wYXVsZHJvbl9zawEACQEcEA1uY19za2VsbHlfYXJnAQE=
