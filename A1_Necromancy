    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_nord_skull_f"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 2 )
       	if ( nc_netch_leather >= 2 )
          	if ( nc_bonemeal >= 30 )
    				removeitem "nc_nord_pauldron_sk_f" 1
                removeitem "nc_nord_skull_f" 1
                removeitem "ingred_scrap_metal_01" 2
                removeitem "ingred_netch_leather_01" 2
                removeitem "ingred_bonemeal_01" 30
                set state to 1
             else
                	MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны две части обрезков металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп женщины-нордлинга для этого ритуала."
    		set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_nord_f" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_nord_2_f" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_nord_3_f" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_nord_f
    endif
    
    end
    

