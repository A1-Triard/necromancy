    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_arg_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( bosmer >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_bosmer_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( breton >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_breton_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( dunmer >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_dunmer_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( imperial >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_imperial_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( khajiit >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_khajiit_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( orc >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_orc_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
    	endif
    endif
    
    if ( redguard >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_rg_pauldron" 1
    			additem "nc_frost_pauldron" 1
    			MessageBox "Ледяная мумия должна быть уже готова."
    			set localdayspassed to 0
    		endif
