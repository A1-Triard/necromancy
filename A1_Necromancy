    set nc_skull to getItemCount "nc_dunmer_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 2 )
       	if ( nc_netch_leather >= 2 )
          	if ( nc_bonemeal >= 30 )
    				removeitem "nc_dunmer_pauldron_sk" 1
                removeitem "nc_dunmer_skull" 1
                removeitem "ingred_scrap_metal_01" 2
                removeitem "ingred_netch_leather_01" 2
                removeitem "ingred_bonemeal_01" 30
                set state to 1
             else
                	MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны две части обрезков металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп темного эльфа для этого ритуала."
    		set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_dark elf" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_dark elf_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_dark elf_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    	if ( GetJournalIndex "nc_necro_001" <= 70 )
    		Journal "nc_necro_001" 80
    	endif
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_dunmer
    endif
    
    end
    

