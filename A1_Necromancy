SCHD nc_skelly_arg_f 1 0 0 132 8
SCVR counter;;
SCDT BgEBBSBYIBAAJAEJAQYBBgkgWNwQID09IDEFAXMBAAggcwEAIDEgKwYBAQkgcwEAIDwgMTAkAQkBqxEBACQBCQEGAQIFIFgYEADbEAwBBnBsYXllctQQFG5jX2FyZ19wYXVsZHJvbl9za19mAQAJARwQD25jX3NrZWxseV9hcmdfZgEB
