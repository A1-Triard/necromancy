SCPT
SCHD nc_death_add 1 0 0 110 7
SCVR doonce;;
SCDT BgEFGiBY/RAgbw9uY19taXNjX3NrdWxsMTAgPCAxBgEDCSBY8BAgPT0gMdQQD25jX21pc2Nfc2t1bGwxMAEABQFzAQACIDEcEAxuY19kZWF0aF9hZGQJAQcBARwQDG5jX2RlYXRoX2FkZAkBAQE=
SCTX
    begin nc_death_add
    short doonce
    
    if ( GetItemCount,  "nc_misc_skull10" < 1 )
    	if ( OnDeath == 1 )
    		additem "nc_misc_skull10" 1
    		set doonce to 1
    		stopscript nc_death_add
    	endif
    else
    	stopscript nc_death_add
    endif
    
    
    end
    

SCPT
SCHD nc_death_add_altmer 5 11 1 1724 100
SCVR temp;check1;check2;check3;check4;invitem;invcount;invref;pcref;npcref;id;id1;id2;id3;id4;tail;timer;;
SCDT 
SCTX
    begin nc_death_add_altmer
    
    
    
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    float timer
    long id
    short temp
    long id1
    long id2
    long id3
    long id4
    long tail
    short check1
    short check2
    short check3
    short check4
    
    set timer to ( timer + GetSecondsPassed )
    	
    if ( timer >= 60 )
    	startscript nc_activate
    	stopscript nc_death_add_altmer
    	set timer to 0
    endif
    
    
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
    
    
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
    
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
    
    setx id to npcref->xGetBaseID
    setx temp, id1, id2, id3, id4, tail to xStringParse "%.0s %.0s %.0s %.0s %s", id
    set check1 to 0
    set check2 to 0
    set check3 to 0
    set check4 to 0
    if ( temp == 1 )
    	set check1 to 1
    elseif ( temp == 2 )
    	set check2 to 1
    elseif ( temp == 3 )
    	set check3 to 1
    else
    	set check4 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    ifx ( check3 )
    	setx id to xStringBuild "%s_%s_%s", id1, id2, id3
    endif
    ifx ( check4 )
    	setx id to xStringBuild "%s_%s_%s_%s", id1, id2, id3, id4
    endif
    setx temp, id1, id2, tail to xStringParse "%.0s'%.0s'%s", id
    set check1 to 0
    set check2 to 0
    if ( temp == 1 )
    	set check1 to 1
    else
    	set check2 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    setx temp, id1, id2, tail to xStringParse "%.0s-%.0s-%s", id
    set check1 to 0
    set check2 to 0
    if ( temp == 1 )
    	set check1 to 1
    else
    	set check2 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    
    setx id to xStringBuild "A1NPC_%s", id
    xFileRewind id
    setx id to xFileReadString id
    setx id1 to xStringLength id
    
    if ( id1 < 6 )
    	set id2 to 0
    else
    	set id2 to 1
    endif
    
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_altmer" 1
    pcref->Additem "nc_altmer_pauldron" 1
    pcref->AddItem "nc_altmer_skull" 1
    
    ifx ( id2 )
    	pcref->xAddItem id 1
    endif
    
    stopscript nc_death_add_altmer
    
    end

SCPT
SCHD nc_death_add_altmer_f 5 11 1 1732 100
