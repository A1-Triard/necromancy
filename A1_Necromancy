    begin nc_skelly_arg
    
    short counter
    
    if ( MenuMode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate  )
    	disable
    	player-> additem "nc_arg_pauldron_sk" 1
    endif
    
    stopscript nc_skelly_arg
    
    end

