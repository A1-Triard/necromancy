SCHD nc_start_skull 1 0 0 285 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_start_skull
    
    short doonce
    
    if ( nc_start == 1 )
    	enable
    else
    	disable
    endif
    
    if ( OnActivate == 1 )
    	if ( doonce == 0 )
    		MessageBox "Вы чувствуете странную магию, текущую через ваше тело. Каким-то образом вы видите дверь, которой здесь только что не было.  Голос внутри вашей головы говорит: 'Не бойся. Входи. Сила и слава ждут тебя.'"
    		set nc_start to 2
    		return
    	endif
    endif
    
    end

SCPT
SCHD nc_startscript 6 0 2 995 57
SCVR doonce;doonce2;doonce3;doonce4;doonce5;wait;timer;timer2;;
SCDT 
SCTX
    begin nc_startscript
    
    	float timer
    	float timer2
    	short doonce
    	short doonce2
    	short doonce3
    	short doonce4
    	short doonce5
    	short wait
    
    
    	if ( menumode == 1 )
    		return
    	endif
    
    	; Dont waste time checking spells more than once per second
    	set timer to ( timer + getsecondspassed )
    	if ( timer < 1 )		
    		return
    	endif
    	set timer to 0
    		
    	if ( nc_has_corpse > 0 )
    		if ( ScriptRunning "nc_corpse_check" == 0 )
    			startscript nc_corpse_check
    		endif
    	endif
    
    	if ( nc_necro_skill == 10 )
    		if ( doonce4 == 0 )
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать более сильных мертвецов."
    			set doonce4 to 1
    		endif
    	endif	
    
    	if ( nc_necro_skill == 20 )
    		if ( doonce3 == 0 )
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать больших мертвецов."
    			set doonce3 to 1
    		endif
    	endif	
    
    	if ( nc_necro_skill == 30 )
    		if ( doonce2 == 0 )
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Идите и поговорите с Трагриларом."
    			set doonce2 to 1
    		endif
    	endif
    
    
    	if ( Player->getspelleffects nc_skull_spell == 1 )
    		set nc_spell_cast to 1
    		if ( wait == 0 )
    			startscript nc_death_trap
    			set wait to 1
    		endif
    		set timer2 to ( timer2 + GetSecondsPassed )
    		if ( timer2 >= 15 )
    			set wait to 0
    			set timer2 to 0
    		endif
    	else
    		set nc_spell_cast to 0
    		set timer2 to 0
    		set wait to 0
    		stopscript nc_death_trap
    	endif
    
    	if ( player->getspelleffects nc_summon == 1 )
    		if ( doonce5 == 0 )
    			set nc_summon_spell to 1
    			set doonce5 to 1
    		endif
    	else
    		set nc_summon_spell to 0
    		set doonce5 to 0
    	endif
    
    	if ( player->getspelleffects nc_painful_touch == 1 )
    		set nc_warlord_spell to 1
    		startscript nc_warlord_attack
    	else
    		set nc_warlord_spell to 0
    	endif
    
    
    
    end
    	
    

SCPT
SCHD nc_stats 16 0 0 492 198
SCVR nc_health;nc_magicka;nc_fatigue;nc_level;nc_attackbonus;nc_defendbonus;nc_resistmagicka;nc_speed;nc_athletics;nc_strength;nc_willpower;nc_intelligence;nc_agility;nc_endurance;nc_personality;nc_luck;;
SCDT 
SCTX
    Begin nc_stats
    
    short nc_health
    short nc_magicka
    short nc_fatigue
    short nc_level
    short nc_attackbonus
    short nc_defendbonus
    short nc_resistmagicka
    short nc_speed
    short nc_athletics
    short nc_strength
    short nc_willpower
    short nc_intelligence
    short nc_agility
    short nc_endurance
    short nc_personality
    short nc_luck
    
      set nc_health to ( Gethealth )
      set nc_magicka to ( Getmagicka )
      set nc_fatigue to ( Getfatigue )
      set nc_level to ( Getlevel )
      set nc_attackbonus to ( GetAttackBonus )
      set nc_defendbonus to ( GetDefendBonus )
      set nc_resistmagicka to ( GetResistMagicka )
    
      set nc_strength to ( GetStrength )  
      set nc_willpower to ( GetWillpower )  
      set nc_intelligence to ( GetIntelligence )
      set nc_agility to ( GetAgility )
      set nc_endurance to ( GetEndurance )
      set nc_personality to ( GetPersonality )
      set nc_speed to ( GetSpeed )
      set nc_luck to ( GetLuck )
    
    
    messagebox "Уровень : %g - Здоровье : %g - Магия : %g - Усталость : %g - Бонус атаки : %g - Бонус защиты : %g - Сопротивление магии : %g", nc_level, nc_health, nc_magicka, nc_fatigue, nc_attackbonus, nc_defendbonus, nc_resistmagicka
    messagebox "Сила : %g - Ловкость : %g - Выносливость : %g - Скорость : %g - Сила воли : %g - Интеллект : %g - Привлекательность : %g - Удача : %g", nc_strength, nc_agility, nc_endurance, nc_speed, nc_willpower, nc_intelligence, nc_personality, nc_luck
    
    stopscript nc_stats
    
    End

SCPT
SCHD nc_summon_altmer 6 0 0 1228 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_altmer
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to GetItemCount "nc_altmer_skull"
    
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 2 )
       	if ( nc_netch_leather >= 2 )
          	if ( nc_bonemeal >= 30 )
    					removeitem "nc_altmer_pauldron_sk" 1
       	         removeitem "nc_altmer_skull" 1
          	      removeitem "ingred_scrap_metal_01" 2
             	   removeitem "ingred_netch_leather_01" 2
             	   removeitem "ingred_bonemeal_01" 30
             	   set state to 1
             else
                	MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны две части обрезков металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп высокого эльфа для этого ритуала."
    		set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		;PlaceAtPC "nc_high elf" 1, 762, 0
    		"nc_rev_placer"->PlaceAtMe "nc_high elf" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_necro_skill <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_high elf_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_high elf_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    endif
    
    
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_altmer
    endif
    
    end

SCPT
SCHD nc_summon_altmer_f 6 0 0 1244 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
