SCHD nc_he_skelly_drop_f 1 0 0 188 8
SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQF25jX2FsdG1lcl9wYXVsZHJvbl9za19mAQAHAQEAEDAAzeXx8ugg4e7r/PjlLCD35ewg7uTo7SDx6uXr5fIg5+Ag8ODnIC0g7eXz5O7h7e4uAAAJAQkBAQE=
SCTX
    begin nc_he_skelly_drop_f
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_altmer_pauldron_sk_f" 1
    	else
