    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_scrap_metal_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    elseif ( GetItemCount,  "ingred_netch_leather_01" < 1 )
    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    endif
    
    ;if ( OnDeath = 1 )
    if ( GetHealth <= 5 )
    	if ( doOnce3 == 0 )
    		PlaySound, "ThunderClap"
    		AddItem "nc_rg_pauldron_sk" 1
    		set doOnce3 to 1
    	endif
    	if ( doOnce5 == 0 )
    		if ( GetItemCount "nc_rg_pauldron_sk" < 1 )
    			set doOnce5 to 1
    			Disable
    		endif
    	endif
    endif
    
    end

