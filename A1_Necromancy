SCPT
SCHD nc_control_ring 3 2 0 829 35
SCVR OnPCEquip;button;status;lkey;ltemp;;
SCDT 
SCTX
    begin nc_control_ring
    
    short OnPCEquip
    short button
    short status
    long lkey
    long ltemp
    
    if ( menumode )
    	return
    endif
    
    if ( OnPCEquip == 1 )
    	MessageBox "Кому вы хотите отдать приказ?", "Мертвецам", "Слугам", "И тем, и другим", "Никому", 
    	set OnPCEquip to 0
    	set status to 1
    endif
    
    if ( status == 1 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;revenants
    		set status to 2
    	elseif ( button == 1 ) ;warlords
    		set status to 3
    	elseif ( button == 2 ) ;both
    			set status to 4
    	elseif ( button == 3 ) ;none
    		set status to 5
    	endif
    elseif ( status == 2 ) ;revenants
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest_rev
    	set status to 0
    elseif ( status == 3 ) ;warlords
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	startscript nc_keytest_warlord
    	set status to 0
    elseif ( status == 4 ) ;both
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	startscript nc_keytest
    	set status to 0
    elseif ( status == 5 ) ;none
    	if ( ScriptRunning "nc_keytest" == 1 )
    		stopscript nc_keytest
    	endif
    	if ( ScriptRunning "nc_keytest_rev" == 1 )
    		stopscript nc_keytest_rev
    	endif
    	if ( ScriptRunning "nc_keytest_warlord" == 1 )
    		stopscript nc_keytest_warlord
    	endif
    	set status to 0
    endif
    
    
    end
    

SCPT
SCHD nc_corpse_check 1 0 0 2770 6
SCVR delay;;
SCDT 
SCTX
    begin nc_corpse_check
    
    short delay
    
    set delay to ( delay + 1 )
    
    if ( delay < 30 )
    	return
    endif
    
    if ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    	startscript nc_mult_arg
    elseif ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    	startscript nc_mult_alt
    elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    	startscript nc_mult_bret
    elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    	startscript nc_mult_bos
    elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    	startscript nc_mult_dun
    elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    	startscript nc_mult_imp
    elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    	startscript nc_mult_khajiit
    elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    	startscript nc_mult_nord
    elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    	startscript nc_mult_orc
    elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    	startscript nc_mult_rg
    elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    	startscript nc_mult_arg_sk
    elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    	startscript nc_mult_alt_sk
    elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    	startscript nc_mult_bret_sk
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    	startscript nc_mult_bos_sk
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    	startscript nc_mult_dun_sk
    elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    	startscript nc_mult_imp_sk
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    	startscript nc_mult_khajiit_sk
    elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    	startscript nc_mult_nord_sk
    elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    	startscript nc_mult_orc_sk
    elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    	startscript nc_mult_rg_sk
    elseif ( Player->GetItemCount "nc_arg_pauldron_f" >= 1 )
    	startscript nc_mult_arg_f
    elseif ( Player->GetItemCount "nc_altmer_pauldron_f" >= 1 )
    	startscript nc_mult_alt_f
    elseif ( Player->GetItemCount "nc_breton_pauldron_f" >= 1 )
    	startscript nc_mult_bret_f
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_f" >= 1 )
    	startscript nc_mult_bos_f
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_f" >= 1 )
    	startscript nc_mult_dun_f
    elseif ( Player->GetItemCount "nc_imperial_pauldron_f" >= 1 )
    	startscript nc_mult_imp_f
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_f" >= 1 )
    	startscript nc_mult_khajiit_f
    elseif ( Player->GetItemCount "nc_nord_pauldron_f" >= 1 )
    	startscript nc_mult_nord_f
    elseif ( Player->GetItemCount "nc_orc_pauldron_f" >= 1 )
    	startscript nc_mult_orc_f
    elseif ( Player->GetItemCount "nc_rg_pauldron_f" >= 1 )
    	startscript nc_mult_rg_f
    elseif ( Player->GetItemCount "nc_arg_pauldron_sk_f" >= 1 )
    	startscript nc_mult_arg_sk_f
    elseif ( Player->GetItemCount "nc_altmer_pauldron_sk_f" >= 1 )
    	startscript nc_mult_alt_sk_f
    elseif ( Player->GetItemCount "nc_breton_pauldron_sk_f" >= 1 )
    	startscript nc_mult_bret_sk_f
    elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk_f" >= 1 )
    	startscript nc_mult_bos_sk_f
    elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk_f" >= 1 )
    	startscript nc_mult_dun_sk_f
    elseif ( Player->GetItemCount "nc_imperial_pauldron_sk_f" >= 1 )
    	startscript nc_mult_imp_sk_f
    elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk_f" >= 1 )
    	startscript nc_mult_khajiit_sk_f
    elseif ( Player->GetItemCount "nc_nord_pauldron_sk_f" >= 1 )
    	startscript nc_mult_nord_sk_f
    elseif ( Player->GetItemCount "nc_orc_pauldron_sk_f" >= 1 )
    	startscript nc_mult_orc_sk_f
    elseif ( Player->GetItemCount "nc_rg_pauldron_sk_f" >= 1 )
    	startscript nc_mult_rg_sk_f
    elseif ( Player->GetItemCount "nc_fire_pauldron" >= 1 )
    	startscript nc_mult_fire
    elseif ( Player->GetItemCount "nc_frost_pauldron" >= 1 )
    	startscript nc_mult_frost
    elseif ( Player->GetItemCount "nc_poison_pauldron" >= 1 )
    	startscript nc_mult_poison
    else
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    endif
    	
    
    set delay to 0
    
    if ( nc_has_corpse == 0 )
    	stopscript nc_corpse_check
    endif
    
    end

