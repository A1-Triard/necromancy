    setx ltemp to xKeyPressed lkey
    
    ; If the player wants to configure his keycode setting, he can
    ; set nc_keycode to 0 (zero) and this script will just output
    ; the code for the pressed key.
    if ( lkey == 0 )
    	if ( ltemp )  ; Skip if no key was pressed
    		messagebox "Код нажатой клавиши: %g" ltemp
    		return
    	endif
    endif
    
    ; Our key was pressed?
    ifx ( ltemp )
    else
    	return
    endif
    
    ; Give a longer delay to avoid multiple messages
    set counter to -20
    
    ; Sneak: Skip wander
    if ( nc_follow == 2 )
    	if ( GetPCSneaking == 1 )
    		set nc_follow to 0
    	endif
    endif
    
    stopscript nc_stand_war_script
    stopscript nc_follow_war_script
    stopscript nc_wander_war_script
    
    if ( nc_follow == 0 )
    ;	messagebox "Your revenants will stay here."
    	startscript nc_stand_war_script
    	set nc_follow to 1
    elseif ( nc_follow == 1 )
    ;	MessageBox "Your revenants will follow you."
    	startscript nc_follow_war_script
    	set nc_follow to 2
    elseif ( nc_follow == 2 )
    ;	MessageBox "I will do nothing."
    	startscript nc_wander_war_script
    	set nc_follow to 0
    endif
    
    End
    

SCPT
SCHD nc_khajiit_corpse_cure 5 0 0 563 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_khajiit_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_khajiit_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_khajiit_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
