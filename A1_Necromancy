    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    setx temp, id1, id2, tail to xStringParse "%.0s-%.0s-%s", id
    set check1 to 0
    set check2 to 0
    if ( temp == 1 )
    	set check1 to 1
    else
    	set check2 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    
    setx id to xStringBuild "A1NPC_%s", id
    xFileRewind id
    setx id to xFileReadString id
    setx id1 to xStringLength id
    
    if ( id1 < 6 )
    	set id2 to 0
    else
    	set id2 to 1
    endif
    
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_khajiit" 1
    pcref->Additem "nc_khajiit_pauldron_f" 1
    pcref->AddItem "nc_khajiit_skull_f" 1
    
    ifx ( id2 )
    	pcref->xAddItem id 1
    endif
    
    stopscript nc_death_add_khajiit_f
    
    end

