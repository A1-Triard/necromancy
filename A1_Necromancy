    set check2 to 0
    set check3 to 0
    set check4 to 0
    if ( temp == 1 )
    	set check1 to 1
    elseif ( temp == 2 )
    	set check2 to 1
    elseif ( temp == 3 )
    	set check3 to 1
    else
    	set check4 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    ifx ( check3 )
    	setx id to xStringBuild "%s_%s_%s", id1, id2, id3
    endif
