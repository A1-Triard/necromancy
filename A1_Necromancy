    		startscript nc_gem_test
    		set nc_gem_race to 8
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 9 )
    	if ( getItemCount "nc_orc_pauldron_sk" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 9
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 10 )
    	if ( getItemCount "nc_rg_pauldron_sk" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 10
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 11 )
    	if ( getItemCount "nc_altmer_pauldron_sk_f" >= 1 )
    		startscript nc_gem_test
    		set nc_gem_race to 12
    	else
    		MessageBox "Вам нужен труп, чтобы провести ритуал."
    	endif
    elseif ( nc_ritual == 12 )
