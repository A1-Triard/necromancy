SCHD nc_summon_fire 7 0 0 2722 75
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_cloth;race;state;;
SCDT 
SCTX
    begin nc_summon_fire
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_cloth
    short race
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_cloth to getItemCount "misc_clothbolt_02"
    set race to 0
    if ( getItemCount "nc_altmer_skull" > 0 )
    	set race to 1
    elseif ( getItemCount "nc_argonian_skull" > 0 )
    	set race to 2
    elseif ( getItemCount "nc_bosmer_skull" > 0 )
    	set race to 3
    elseif ( getItemCount "nc_breton_skull" > 0 )
    	set race to 4
    elseif ( getItemCount "nc_imperial_skull" > 0 )
    	set race to 5
    elseif ( getItemCount "nc_dunmer_skull" > 0 )
    	set race to 6
    elseif ( getItemCount "nc_khajiit_skull" > 0 )
    	set race to 7
    elseif ( getItemCount "nc_nord_skull" > 0 )
    	set race to 8
    elseif ( getItemCount "nc_orc_skull" > 0 )
    	set race to 9
    elseif ( getItemCount "nc_redguard_skull" > 0 )
    	set race to 10
    elseif ( getItemCount "nc_altmer_skull_f" > 0 )
    	set race to 11
    elseif ( getItemCount "nc_argonian_skull_f" > 0 )
    	set race to 12
    elseif ( getItemCount "nc_bosmer_skull_f" > 0 )
    	set race to 13
    elseif ( getItemCount "nc_breton_skull_f" > 0 )
    	set race to 14
    elseif ( getItemCount "nc_imperial_skull_f" > 0 )
    	set race to 15
    elseif ( getItemCount "nc_dunmer_skull_f" > 0 )
    	set race to 16
    elseif ( getItemCount "nc_khajiit_skull_f" > 0 )
    	set race to 17
    elseif ( getItemCount "nc_nord_skull_f" > 0 )
    	set race to 18
    elseif ( getItemCount "nc_orc_skull_f" > 0 )
    	set race to 19
    elseif ( getItemCount "nc_redguard_skull_f" > 0 )
    	set race to 20
    endif
    
    if ( nc_pclevel >= 29 )
    	set state to 1
    elseif ( nc_pclevel < 29 )
    	MessageBox "Вы недостаточно опытны, чтобы вызвать мумифицированного слугу."
    	set state to 10
    endif
    
    if ( nc_f_warlord_glob >= 1 )
    	MessageBox "Вы можете контролировать только одного огненного слугу."
    	set state to 10
    endif
    
    if ( state == 1 )
    	if ( nc_cloth >= 2 )
    		if ( nc_scrap_metal >= 2 )
    			if ( nc_netch_leather >= 2 )
    				if ( nc_bonemeal >= 10 )
    					if ( race == 0 )
    	            			MessageBox "Вам нужен череп для этого ритуала."
    						set state to 10
    					else
    						if ( race == 1 )
    		            			removeitem "nc_altmer_skull" 1
    						elseif ( race == 2 )
    	          	  			removeitem "nc_argonian_skull" 1
    						elseif ( race == 3 )
    	            				removeitem "nc_bosmer_skull" 1
    						elseif ( race == 4 )
    		            			removeitem "nc_breton_skull" 1
    						elseif ( race == 5 )
    	          	  			removeitem "nc_imperial_skull" 1
    						elseif ( race == 6 )
    	            				removeitem "nc_dunmer_skull" 1
    						elseif ( race == 7 )
    		            			removeitem "nc_khajiit_skull" 1
    						elseif ( race == 8 )
    	          	  			removeitem "nc_nord_skull" 1
    						elseif ( race == 9 )
    	            				removeitem "nc_orc_skull" 1
    						elseif ( race == 10 )
    		            			removeitem "nc_redguard_skull" 1
    						elseif ( race == 11 )
    		            			removeitem "nc_altmer_skull_f" 1
    						elseif ( race == 12 )
    	          	  			removeitem "nc_argonian_skull_f" 1
    						elseif ( race == 13 )
    	            				removeitem "nc_bosmer_skull_f" 1
    						elseif ( race == 14 )
    		            			removeitem "nc_breton_skull_f" 1
    						elseif ( race == 15 )
    	          	  			removeitem "nc_imperial_skull_f" 1
    						elseif ( race == 16 )
    	            				removeitem "nc_dunmer_skull_f" 1
    						elseif ( race == 17 )
    		            			removeitem "nc_khajiit_skull_f" 1
    						elseif ( race == 18 )
    	          	  			removeitem "nc_nord_skull_f" 1
    						elseif ( race == 19 )
    	            				removeitem "nc_orc_skull_f" 1
    						elseif ( race == 20 )
    		            			removeitem "nc_redguard_skull_f" 1
    						endif
    						removeitem "ingred_scrap_metal_01" 2
              	  			removeitem "ingred_netch_leather_01" 2
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 2
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					endif
    				else
    					MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
    					set state to 10
    				endif
    			else
    				MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    				set state to 10
    			endif
    		else
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    			set state to 10
    		endif
    	else
    		MessageBox "Вам нужны два рулона красной ткани для этого ритуала."
    		set state to 10
    	endif
    endif
    
    if ( state == 2 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_f_warlord" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		set nc_f_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
    
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_fire
    endif
    
    
    end

SCPT
SCHD nc_summon_frost 7 0 0 2726 75
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_cloth;race;state;;
SCDT 
SCTX
    begin nc_summon_frost
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_cloth
    short race
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_cloth to getItemCount "misc_clothbolt_01"
    set race to 0
    if ( getItemCount "nc_altmer_skull" > 0 )
    	set race to 1
    elseif ( getItemCount "nc_argonian_skull" > 0 )
    	set race to 2
    elseif ( getItemCount "nc_bosmer_skull" > 0 )
    	set race to 3
    elseif ( getItemCount "nc_breton_skull" > 0 )
    	set race to 4
    elseif ( getItemCount "nc_imperial_skull" > 0 )
    	set race to 5
    elseif ( getItemCount "nc_dunmer_skull" > 0 )
    	set race to 6
    elseif ( getItemCount "nc_khajiit_skull" > 0 )
    	set race to 7
    elseif ( getItemCount "nc_nord_skull" > 0 )
    	set race to 8
    elseif ( getItemCount "nc_orc_skull" > 0 )
    	set race to 9
    elseif ( getItemCount "nc_redguard_skull" > 0 )
    	set race to 10
    elseif ( getItemCount "nc_altmer_skull_f" > 0 )
    	set race to 11
    elseif ( getItemCount "nc_argonian_skull_f" > 0 )
    	set race to 12
    elseif ( getItemCount "nc_bosmer_skull_f" > 0 )
    	set race to 13
    elseif ( getItemCount "nc_breton_skull_f" > 0 )
    	set race to 14
    elseif ( getItemCount "nc_imperial_skull_f" > 0 )
    	set race to 15
    elseif ( getItemCount "nc_dunmer_skull_f" > 0 )
    	set race to 16
    elseif ( getItemCount "nc_khajiit_skull_f" > 0 )
    	set race to 17
    elseif ( getItemCount "nc_nord_skull_f" > 0 )
    	set race to 18
    elseif ( getItemCount "nc_orc_skull_f" > 0 )
    	set race to 19
    elseif ( getItemCount "nc_redguard_skull_f" > 0 )
    	set race to 20
    endif
    
    if ( nc_pclevel >= 29 )
    	set state to 1
    elseif ( nc_pclevel < 29 )
    	MessageBox "Вы недостаточно опытны, чтобы вызвать мумифицированного слугу."
    	set state to 10
    endif
    
    if ( nc_fr_warlord_glob >= 1 )
    	MessageBox "Вы можете контролировать только одного ледяного слугу."
    	set state to 10
    endif
    
    if ( state == 1 )
    	if ( nc_cloth >= 2 )
    		if ( nc_scrap_metal >= 2 )
    			if ( nc_netch_leather >= 2 )
    				if ( nc_bonemeal >= 10 )
    					if ( race == 0 )
    	            			MessageBox "Вам нужен череп для этого ритуала."
    						set state to 10
    					else
    						if ( race == 1 )
    		            			removeitem "nc_altmer_skull" 1
    						elseif ( race == 2 )
    	          	  			removeitem "nc_argonian_skull" 1
    						elseif ( race == 3 )
    	            				removeitem "nc_bosmer_skull" 1
    						elseif ( race == 4 )
    		            			removeitem "nc_breton_skull" 1
    						elseif ( race == 5 )
    	          	  			removeitem "nc_imperial_skull" 1
    						elseif ( race == 6 )
    	            				removeitem "nc_dunmer_skull" 1
    						elseif ( race == 7 )
    		            			removeitem "nc_khajiit_skull" 1
    						elseif ( race == 8 )
    	          	  			removeitem "nc_nord_skull" 1
    						elseif ( race == 9 )
    	            				removeitem "nc_orc_skull" 1
    						elseif ( race == 10 )
    		            			removeitem "nc_redguard_skull" 1
    						elseif ( race == 11 )
    		            			removeitem "nc_altmer_skull_f" 1
    						elseif ( race == 12 )
    	          	  			removeitem "nc_argonian_skull_f" 1
    						elseif ( race == 13 )
    	            				removeitem "nc_bosmer_skull_f" 1
    						elseif ( race == 14 )
    		            			removeitem "nc_breton_skull_f" 1
    						elseif ( race == 15 )
    	          	  			removeitem "nc_imperial_skull_f" 1
    						elseif ( race == 16 )
    	            				removeitem "nc_dunmer_skull_f" 1
    						elseif ( race == 17 )
    		            			removeitem "nc_khajiit_skull_f" 1
    						elseif ( race == 18 )
    	          	  			removeitem "nc_nord_skull_f" 1
    						elseif ( race == 19 )
    	            				removeitem "nc_orc_skull_f" 1
    						elseif ( race == 20 )
    		            			removeitem "nc_redguard_skull_f" 1
    						endif
    						removeitem "ingred_scrap_metal_01" 2
              	  			removeitem "ingred_netch_leather_01" 2
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 2
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					endif
    				else
    					MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
    					set state to 10
    				endif
    			else
    				MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
    				set state to 10
    			endif
    		else
    			MessageBox "Вам нужны две части обрезков металла для этого ритуала."
    			set state to 10
    		endif
    	else
    		MessageBox "Вам нужны два рулона голубой ткани для этого ритуала."
    		set state to 10
    	endif
    endif
    
    if ( state == 2 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_fr_warlord" 1, 0, 0
    		set nc_fr_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
    
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_frost
    endif
    
    end

SCPT
SCHD nc_summon_imperial 6 0 0 1224 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_imperial
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_imperial_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 2 )
       	if ( nc_netch_leather >= 2 )
          	if ( nc_bonemeal >= 30 )
    				removeitem "nc_imperial_pauldron_sk" 1
                removeitem "nc_imperial_skull" 1
                removeitem "ingred_scrap_metal_01" 2
                removeitem "ingred_netch_leather_01" 2
                removeitem "ingred_bonemeal_01" 30
                set state to 1
             else
                	MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны две части обрезков металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп мужчины-имперца для этого ритуала."
    		set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_imperial" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_2" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_imperial
    endif
    
    end

SCPT
SCHD nc_summon_imperial_f 6 0 0 1238 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
SCTX
    begin nc_summon_imperial_f
    
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_imperial_skull_f"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 2 )
       	if ( nc_netch_leather >= 2 )
          	if ( nc_bonemeal >= 30 )
    				removeitem "nc_imperial_pauldron_sk_f" 1
                removeitem "nc_imperial_skull_f" 1
                removeitem "ingred_scrap_metal_01" 2
                removeitem "ingred_netch_leather_01" 2
                removeitem "ingred_bonemeal_01" 30
                set state to 1
             else
                	MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны две части обрезков металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп женщины-имперца для этого ритуала."
    		set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_imperial_f" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_2_f" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_3_f" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_imperial_f
    endif
    
    end
    

