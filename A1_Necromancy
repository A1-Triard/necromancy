    player-> additem "nc_soulgem_breton" 10
    player-> additem "nc_soulgem_bosmer" 10
    player-> additem "nc_soulgem_dunmer" 10
    player-> additem "nc_soulgem_imperial" 10
    player-> additem "nc_soulgem_khajiit" 10
    player-> additem "nc_soulgem_nord" 10
    player-> additem "nc_soulgem_orc" 10
    player-> additem "nc_soulgem_redguard" 10
    
    stopscript nc_gem_loot
    
    end

