    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    ifx ( check3 )
    	setx id to xStringBuild "%s_%s_%s", id1, id2, id3
    endif
    ifx ( check4 )
    	setx id to xStringBuild "%s_%s_%s_%s", id1, id2, id3, id4
    endif
    setx temp, id1, id2, tail to xStringParse "%.0s'%.0s'%s", id
    set check1 to 0
    set check2 to 0
    if ( temp == 1 )
    	set check1 to 1
    else
    	set check2 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    setx temp, id1, id2, tail to xStringParse "%.0s-%.0s-%s", id
    set check1 to 0
    set check2 to 0
    if ( temp == 1 )
    	set check1 to 1
    else
    	set check2 to 1
    endif
    ifx ( check1 )
    	setx id to xStringBuild "%s", id1
    endif
    ifx ( check2 )
    	setx id to xStringBuild "%s_%s", id1, id2
    endif
    
    setx id to xStringBuild "A1NPC_%s", id
    xFileRewind id
    setx id to xFileReadString id
    setx id1 to xStringLength id
    
    if ( id1 < 6 )
    	set id2 to 0
    else
    	set id2 to 1
    endif
    
    disable
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_redguard" 1
    pcref->Additem "nc_rg_pauldron_f" 1
    pcref->AddItem "nc_redguard_skull_f" 1
    
    ifx ( id2 )
    	pcref->xAddItem id 1
    endif
    
    stopscript nc_death_add_redguard_f
    
    end
    

SCPT
SCHD nc_death_trap 13 7 0 2201 133
SCVR trap;race1;race2;race3;race4;race5;race6;race7;race8;race9;race10;is_female;soulgem;invitem;invcount;invref;pcref;npcref;target;temp;;
SCDT 
SCTX
    begin nc_death_trap
    
    
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    long target
    long temp
    short trap
    short race1
    short race2
    short race3
    short race4
    short race5
    short race6
    short race7
    short race8
    short race9
    short race10
    short is_female
    short soulgem
    
    if ( Player->GetItemCount "nc_SoulGem_blackened" == 0 )
    	MessageBox "Вам нужен черный камень душ, чтобы заключить душу разумного существа."
    	stopscript nc_death_trap
    endif
    
    setx target to xGetPCTarget
    
    
    ifx ( target )
    
    	set trap to 1
    
    	setx temp to target->xRefType
    	if ( temp != 1598246990 )   ; it's not an NPC
    		set trap to 0
    	endif
    
    	xSetRef target
    	if ( GetHealth < 1 )
    		stopscript nc_death_trap
    	endif
    
    	ifx ( trap )
    		setx temp to target->xIsFemale
    		set is_female to temp
    
    		set race1 to 0
    		set race2 to 0
    		set race3 to 0
    		set race4 to 0
    		set race5 to 0
    		set race6 to 0
    		set race7 to 0
    		set race8 to 0
    		set race9 to 0
    		set race10 to 0
    
    		xSetRef target
    		if ( GetRace "Argonian" == 1 )
    			set race1 to 1
    		endif
    		
    		xSetRef target
    		if ( GetRace "Breton" == 1 )
    			set race2 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Dark Elf" == 1 )
    			set race3 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "High Elf" == 1 )
    			set race4 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Imperial" == 1 )
    			set race5 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Khajiit" == 1 )
    			set race6 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Nord" == 1 )
    			set race7 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Orc" == 1 )
    			set race8 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Redguard" == 1 )
    			set race9 to 1
    		endif
    
    		xSetRef target
    		if ( GetRace "Wood Elf" == 1 )
    			set race10 to 1
    		endif
    	endif
    endif
    
    ifx ( race1 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_argonian_f
    	else
    		xSetRef target
    		startscript nc_death_add_argonian
    	endif
    	set race1 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race2 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_breton_f
    	else
    		xSetRef target
    		startscript nc_death_add_breton
    	endif
    	set race2 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race3 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_dunmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_dunmer
    	endif
    	set race3 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race4 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_altmer_f
    	else
    		xSetRef target
    		startscript nc_death_add_altmer
    	endif
    	set race4 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race5 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_imperial_f
    	else
    		xSetRef target
    		startscript nc_death_add_imperial
    	endif
    	set race5 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race6 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_khajiit_f
    	else
    		xSetRef target
    		startscript nc_death_add_khajiit
    	endif
    	set race6 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race7 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_nord_f
    	else
    		xSetRef target
    		startscript nc_death_add_nord
    	endif
    	set race7 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race8 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_orc_f
    	else
    		xSetRef target
    		startscript nc_death_add_orc
    	endif
    	set race8 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race9 )
    	ifx ( is_female )
    		xSetRef target
    		startscript nc_death_add_redguard_f
    	else
    		xSetRef target
    		startscript nc_death_add_redguard
    	endif
    	set race9 to 0
    	stopscript nc_death_trap
    endif
    
    ifx ( race10 )
