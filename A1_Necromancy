SCPT
SCHD nc_nord_corpse_cure 5 0 0 557 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_nord_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_nord_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_nord_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_nord_corpse_cure_f 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_nord_corpse_cure_f
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_nord_pauldron_f" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_nord_skelly_f" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end
    

SCPT
SCHD nc_nord_skelly_drop 1 0 0 184 8
SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQE25jX25vcmRfcGF1bGRyb25fc2sBAAcBAQAQMADN5fHy6CDh7uv8+OUsIPfl7CDu5OjtIPHq5evl8iDn4CDw4OcgLSDt5fPk7uHt7i4AAAkBCQEBAQ==
SCTX
    begin nc_nord_skelly_drop
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_nord_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
    end

SCPT
SCHD nc_nord_skelly_drop_f 1 0 0 186 8
SCVR counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX25vcmRfcGF1bGRyb25fc2tfZgEABwEBABAwAM3l8fLoIOHu6/z45Swg9+XsIO7k6O0g8erl6+XyIOfgIPDg5yAtIO3l8+Tu4e3uLgAACQEJAQEB
SCTX
    begin nc_nord_skelly_drop_f
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_nord_pauldron_sk_f" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
    end
    

SCPT
SCHD nc_nord_skelly_pauldron 4 0 1 635 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_nord_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_nord_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_nord_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_nord_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

SCPT
SCHD nc_nord_skelly_pauldron_f 4 0 1 641 40
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
SCDT 
SCTX
    begin nc_nord_skelly_pauldron_f
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_nord_pauldron_sk_f" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_nord_pauldron_sk_f"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_nord_skelly_NPC_f" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    
    

SCPT
SCHD nc_orc_corpse_cure 5 0 0 555 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
SCTX
    begin nc_orc_corpse_cure
    
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_orc_pauldron" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_orc_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
