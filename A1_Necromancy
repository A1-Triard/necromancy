    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_redguard_skull_f"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 2 )
       	if ( nc_netch_leather >= 2 )
          	if ( nc_bonemeal >= 30 )
    		  removeitem "nc_rg_pauldron_sk_f" 1
                removeitem "nc_redguard_skull_f" 1
                removeitem "ingred_scrap_metal_01" 2
                removeitem "ingred_netch_leather_01" 2
                removeitem "ingred_bonemeal_01" 30
                set state to 1
             else
                	MessageBox "Вам нужно иметь 30 порций костяной муки для этого ритуала."
                   set state to 2
             endif
    		else
             	MessageBox "Вам нужны две части обрезков металла для этого ритуала."
                set state to 2
          endif
    	else
          	MessageBox "Вам нужны два куска кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    	MessageBox "Вам нужен череп женщины-редгарда для этого ритуала."
    	set state to 2
    endif
    
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_redguard_f" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )	
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_2_f" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_3_f" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		endif
    	endif
    	set state to 2
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_redguard_f
    endif
    
    end
    

SCPT
SCHD nc_time_bounty 4 0 4 1716 63
SCVR rand;kill;detectedDate;detected;timer;hour;timer2;detectedHour;;
SCDT 
SCTX
    begin nc_time_bounty
    
    short rand
    short kill
    float timer
    float hour
    float timer2
    float detectedHour
    short detectedDate
    short detected
    
    if ( MenuMode == 1 )
    	return
    elseif ( GetPCCell "Храм Танатоса" == 1 )
    	return
    elseif ( VH_MaskDummyFlag > 0 )
    	return
    endif
    
    set timer2 to ( timer2 + GetSecondsPassed )
    if ( timer2 >= 14 )
    
    		if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		else
    			set kill to 1
    		endif
    endif
    
    set hour to GameHour
    set rand to Random, 11
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 15 )
    
    set detected to 0
    
    if ( hour < 8 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			set detected to 1
    		endif
    	endif
    elseif ( hour < 20 )
    	if ( hour >= 8 )
    		;MessageBox "Daytime. Time is %f. Random is %g.", hour, rand
    		if ( rand <= 7 )
    			if ( GetDetected Player == 1 )
    				set detected to 1
    			endif
    		endif
    	endif
    elseif ( hour >= 20 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
    			set detected to 1
    		endif
    	endif
    endif
    
    if ( detected == 1 )
    	if ( detectedDate > DaysPassed )
    		set detected to 0
    	else if ( detectedDate == DaysPassed )
    		if ( detectedHour > hour )
    			set detected to 0
    		endif
    	endif
    	if ( detected == 1 )
    		set detectedDate to DaysPassed + 1
    		set detectedHour to hour
    		MessageBox "Кто-то увидел, как вы несете тело."
    		ModPCCrimeLevel 500
    	endif
    endif
    
    set timer to 0
    
    endif
    
    if ( kill == 1 )
    	stopscript nc_time_bounty
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	set timer2 to 0
    	set kill to 0
    endif
    
    end

SCPT
SCHD nc_tragrilar_death 2 0 0 146 20
SCVR currentday;deathday;;
SCDT BgECDyBzAQAgIT0gRwREYXkAAAUBcwEACCBHBERheQAABQFzAgAIIHMCACAxICsJAQYBBQkgcwIAID49IDIGAQMZIFgSESBjDdXw4Owg0uDt4PLu8eAgPT0gMRsQE25jX2RlYXRoX2FkZF9hbHRtZXKNEAAAAAAcEBJuY190cmFncmlsYXJfZGVhdGgJAQkBAQE=
SCTX
    begin nc_tragrilar_death
    
    short currentday
    short deathday
    
    if ( currentday != Day )
    	set currentday to Day
    	set deathday to ( deathday + 1 )
    endif
    
    if ( deathday >= 2 )
    	if ( GetPCCell "Храм Танатоса" == 1 )
    		startscript nc_death_add_altmer
    		sethealth 0
    		stopscript nc_tragrilar_death
    	endif
    endif
    
    
    end

