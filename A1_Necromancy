SCHD nc_skills 27 0 0 872 343
SCVR nc_unarmored;nc_alchemy;nc_mysticism;nc_conjuration;nc_illusion;nc_alteration;nc_destruction;nc_enchant;nc_spear;nc_axe;nc_bluntweapon;nc_marksman;nc_shortblade;nc_security;nc_restoration;nc_block;nc_speechcraft;nc_mercantile;nc_heavyarmor;nc_lightarmor;nc_handtohand;nc_acrobatics;nc_armorer;nc_athletics;nc_sneak;nc_mediumarmor;nc_longblade;;
SCDT 
SCTX
    Begin nc_skills
    
    short nc_unarmored
    short nc_alchemy
    short nc_mysticism
    short nc_conjuration
    short nc_illusion
    short nc_alteration
    short nc_destruction
    short nc_enchant
    short nc_spear
    short nc_axe
    short nc_bluntweapon
    short nc_marksman
    short nc_shortblade
    short nc_security
    short nc_restoration
    short nc_block
    short nc_speechcraft
    short nc_mercantile
    short nc_heavyarmor
    short nc_lightarmor
    short nc_handtohand
    short nc_acrobatics
    short nc_armorer
    short nc_athletics
    short nc_sneak
    short nc_mediumarmor
    short nc_longblade
    
      set nc_acrobatics to ( GetAcrobatics )
      set nc_athletics to ( GetAthletics )
      set nc_speechcraft to ( GetSpeechcraft )
      set nc_mercantile to ( GetMercantile )
      set nc_sneak to ( GetSneak )
      set nc_security to ( GetSecurity )
    
      set nc_shortblade to ( GetShortblade )
      set nc_longblade to ( GetLongblade )
      set nc_axe to ( GetAxe )
      set nc_marksman to ( GetMarksman )
      set nc_spear to ( GetSpear )
      set nc_bluntweapon to ( GetBluntweapon )
    
      set nc_handtohand to ( GetHandtohand )
      set nc_unarmored to ( GetUnarmored )
      set nc_block to ( GetBlock )
      set nc_lightarmor to ( GetLightarmor )
      set nc_mediumarmor to ( GetMediumarmor )
      set nc_heavyarmor to ( GetHeavyarmor )
      set nc_armorer to ( GetArmorer )
    
      set nc_enchant to ( GetEnchant )
      set nc_alchemy to ( GetAlchemy )
      set nc_mysticism to ( GetMysticism )
      set nc_conjuration to ( GetConjuration )
      set nc_illusion to ( Player->GetIllusion )
      set nc_Alteration to ( GetAlteration )
      set nc_destruction to ( GetDestruction )
      set nc_restoration to ( GetRestoration )
    
    messagebox "Акробатика : %g - Атлетика: %g - Красноречие: %g - Торговля: %g - Красться: %g - Безопасность: %g", nc_acrobatics, nc_athletics, nc_speechcraft, nc_mercantile, nc_sneak, nc_security
    messagebox "Короткие клинки: %g - Длинные клинки: %g - Секиры: %g - Меткость: %g - Копья: %g - Тупое оружие: %g", nc_shortblade, nc_longblade, nc_axe, nc_marksman, nc_spear, nc_bluntweapon
    messagebox "Рукопашаная: %g - Бездоспешный бой: %g - Блок: %g - Легкая броня: %g - Средняя броня: %g - Тяжелая броня: %g - Кузнец: %g", nc_handtohand, nc_unarmored, nc_block, nc_lightarmor, nc_mediumarmor, nc_heavyarmor, nc_armorer
    messagebox "Зачарование: %g - Алхимия: %g - Мистицизм: %g - Колдовство: %g - Иллюзия: %g - Изменение: %g - Разрушение: %g - Восстановление: %g", nc_enchant, nc_alchemy, nc_mysticism, nc_conjuration, nc_illusion, nc_alteration, nc_destruction, nc_restoration
    
    stopscript nc_skills
    
    End

SCPT
SCHD nc_spell_timer 1 0 1 245 12
SCVR state;timer;;
SCDT 
SCTX
    begin nc_spell_timer
    
    float timer
    short state
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( CellChanged == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectRecall == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectDivineIntervention == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectAlmsiviIntervention == 1 )
    	set state to 1
    endif
    
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 120 )
    	set state to 1
    	set timer to 0
    endif
    
    if ( state == 1 )
    	player-> RemoveSpell "nc_summon"
    	stopscript nc_spell_timer
    	set state to 0
    endif
    
    end

SCPT
SCHD nc_stand_rev_script 1 0 0 222 7
SCVR doonce;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEGAQQJIHMBACA9PSAyBQFzAQACIDAFAUcKbmNfY2FsbHJldgIgMBwQE25jX3N0YW5kX3Jldl9zY3JpcHQkAQkBBgEBEiBHCm5jX2NhbGxyZXYgPT0gMAUBRwpuY19jYWxscmV2AiAxCQEGAQQWIEcObmNfY2FsbG1lc3NhZ2UgPT0gMQYBAgkgcwEAID09IDAFAXMBAAIgMQAQIQDC4PjoIOzl8PLi5fb7IOHz5PPyIPHy7v/y/CDn5OXx/C4AAAkBCQEFAXMBAAIgMgEB
SCTX
    begin nc_stand_rev_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_stand_rev_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
    endif
    ;set timer to timer + GetSecondsPassed 
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
    	endif
    endif
    
    set doonce to 2
    
    end

SCPT
SCHD nc_stand_script 1 0 0 276 7
SCVR doonce;;
SCDT 
SCTX
    begin nc_stand_script
    
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_stand_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 1
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
    	endif
    endif
    
    set doonce to 2
    
    end

