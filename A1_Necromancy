    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	set getcell to 0
    endif
    
    if ( getcell == 0 )
    
    	if ( state == 1 )
    		disable
    		player-> additem "nc_breton_pauldron_f" 1
    		return
    	endif
    
    	if ( GetPCCell "Район Горького Берега" == 0 )
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
    			set getcell to 1
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
    			set state to 1
    		endif
    	else
    		if ( state == 0 )
    			MessageBox "Через два-три дня от тела останется лишь скелет."
    			set state to 2
    		endif
    	endif
    
    	set getcell to 1
    
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_bret_skelly_f" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end
    

