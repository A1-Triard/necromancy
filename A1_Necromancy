    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_khajiit_corpse" 1 1 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    

