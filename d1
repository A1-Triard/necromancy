SCHD nc_postman 1 0 0 152 5
SCVR rand;;
#ifndef ABCD
SCDT DAEGcGxheWVyBgEKCSBYVxEgPj0gNQUBcwEAByBYIRBkAAAGAQcKIHMBACA8PSAxMAYBBQkgWOAQID09IDHmEA9uY19zdGFydF9za2VsbHkBAAAAAEMBAAAQGQBZb3UgaGVhciBhIHN0cmFuZ2Ugbm9pc2UuAADhEBwQCm5jX3Bvc3RtYW4FAXMBAAQgMTAwCQEJAQkBAQE=
#else /* ABCD */
SCDT DAEGcGxheWVyBgEKCSBYVxEgPj0gNQUBcwEACCBYIRBkADAwBgEHCiBzAQAgPD0gMTAGAQUJIFjgECA9PSAx5hAPbmNfc3RhcnRfc2tlbGx5AQAAAABDAQAAEBgAwvsg8ev7+Ojy5SDx8vDg7e376SD48+wuAADhEBwQCm5jX3Bvc3RtYW4FAXMBAAQgMTAwCQEJAQkBAQE=
#endif /* ABCD */
SCTX
    begin nc_postman
    
    short rand
    
    	if ( player->GetLevel >= 5 )
    		set rand to Random, 100
    		if ( rand <= 10 )
    			if ( GetPCSleep == 1 )
    				PlaceAtPC "nc_start_skelly" 1 128 1
#ifndef ABCD
    				MessageBox "You hear a strange noise."
#else /* ABCD */
    				MessageBox "Вы слышите странный шум."
#endif /* ABCD */
    				WakeUpPC
    				stopscript nc_postman
    				set rand to 100
    			endif
    		endif
    	endif
    
    
    end

