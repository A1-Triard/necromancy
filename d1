SCHD nc_start_censure 2 0 0 105 13
SCVR doonce;timer;;
SCDT BgEBECBHCG5jX3N0YXJ0ID09IDHaEAcBAdsQCQEGAQIJIHMBACA9PSAwjhEAAIA+BQFzAQACIDEJAQUBcwIACCBzAgAgMSArBgECCSBzAgAgPj0gMwUBcwEAAiAwBQFzAgACIDAJAQEB
SCTX
    begin nc_start_censure
    
    short doonce
    short timer
    
    if ( nc_start == 1 )
    	enable
    else
    	disable
    endif
    
    if ( doonce == 0 )
#ifndef ABCD
    	setscale 0.25
#else /* ABCD */
    	setscale .25
#endif /* ABCD */
    	set doonce to 1
    endif
    
    set timer to ( timer + 1 )
    
    if ( timer >= 3 )
    	set doonce to 0
    	set timer to 0
    endif
    
    end

