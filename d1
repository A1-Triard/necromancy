#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set fstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( orc >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_orc_pauldron" 1
    			additem "nc_fire_pauldron" 1
#ifndef ABCD
    			MessageBox "Your fire cured corpse should be ready by now."
#else /* ABCD */
    			MessageBox "Огненная мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set fstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( redguard >= 1 )
    	if ( fstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_rg_pauldron" 1
    			additem "nc_fire_pauldron" 1
#ifndef ABCD
    			MessageBox "Your fire cured corpse should be ready by now."
#else /* ABCD */
    			MessageBox "Огненная мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( fstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set fstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set fstate to 0
#endif /* ABCD */
    	endif
    endif
    
    
    end
    

SCPT
#ifndef ABCD
SCHD nc_follow_rev_script 3 3 0 741 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_follow_rev_script 1 0 0 228 7
SCVR doonce;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_follow_rev_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_follow_rev_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_follow_rev_script"
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIFollow player 0 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will follow you."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
    
#endif /* ABCD */
    end

SCPT
#ifndef ABCD
SCHD nc_follow_script 3 3 0 554 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_follow_script 1 0 0 282 7
SCVR doonce;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_follow_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_follow_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIFollow player 0 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will follow you."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_follow_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 2
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 2
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут следовать за вами."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
    
#endif /* ABCD */
    end

SCPT
#ifndef ABCD
SCHD nc_follow_war_script 6 3 0 829 52
SCVR doonce;call;state;fwar;frwar;pwar;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_follow_war_script 1 0 0 225 7
SCVR doonce;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_follow_war_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	short fwar
    	short frwar
    	short pwar
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_follow_war_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    			set fwar to 0
    			set frwar to 0
    			set pwar to 0
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			set fwar to 1
    		endif
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			set frwar to 1
    		endif
#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callwar to 0
    	StopScript "nc_follow_war_script"
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			set pwar to 1
    		endif
    		
    		if ( fwar == 0 );not a fire
    			if ( frwar == 0 );not a frost
    				if ( pwar == 0 );not a warlord
    					return
    				endif
    			endif
    		endif
#else /* ABCD */
    if ( nc_callwar == 0 )
    	set nc_callwar to 2
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		npcref->AIFollow player 0 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your warlords will follow you."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши слуги будут следовать за вами."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
    
#endif /* ABCD */
    end

