    set nc_cloth to getItemCount "misc_clothbolt_02"
    set nc_alt_skull1 to getItemCount "nc_altmer_skull"
    set nc_arg_skull1 to getItemCount "nc_argonian_skull"
    set nc_bos_skull1 to getItemCount "nc_bosmer_skull"
    set nc_bret_skull1 to getItemCount "nc_breton_skull"
    set nc_imp_skull1 to getItemCount "nc_imperial_skull"
    set nc_dun_skull1 to getItemCount "nc_dunmer_skull"
    set nc_kha_skull1 to getItemCount "nc_khajiit_skull"
    set nc_nord_skull1 to getItemCount "nc_nord_skull"
    set nc_orc_skull1 to getItemCount "nc_orc_skull"
    set nc_rg_skull1 to getItemCount "nc_redguard_skull"
#endif /* ABCD */
    
#ifndef ABCD
    	if ( nc_f_warlord_glob >= 1 )
    		MessageBox "You can only command one Fire Warlord."
    		set state to 10
    	endif
#else /* ABCD */
    if ( nc_pclevel >= 29 )
    	set state to 1
    elseif ( nc_pclevel < 29 )
    	MessageBox "Вы недостаточно опытны, чтобы вызвать мумифицированного слугу."
    	set state to 10
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 1 )
    		if ( nc_skl >= 1 )
    			if ( nc_cloth >= 1 )
    				if ( nc_scrap_metal >= 1 )
    					if ( nc_netch_leather >= 1 )
    						if ( nc_bonemeal >= 10 )
    							if ( nc_alt_skull1 >= 1 )
    								removeitem "nc_altmer_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_arg_skull1 >= 1 )
    								removeitem "nc_argonian_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_bos_skull1 >= 1 )
    								removeitem "nc_bosmer_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_bret_skull1 >= 1 )
    								removeitem "nc_breton_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_imp_skull1 >= 1 )
    								removeitem "nc_imperial_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_dun_skull1 >= 1 )
    								removeitem "nc_dunmer_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_kha_skull1 >= 1 )
    								removeitem "nc_khajiit_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_nord_skull1 >= 1 )
    								removeitem "nc_nord_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_orc_skull1 >= 1 )
    								removeitem "nc_orc_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_rg_skull1 >= 1 )
    								removeitem "nc_redguard_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_02" 1
    								RemoveItem "nc_fire_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							else
    								MessageBox "You need a skull to perform the ritual."
    								set state to 10
    							endif
    						else
    							MessageBox "You need some bonemeal to perform the ritual."
    							set state to 10
    						endif
#else /* ABCD */
    if ( nc_f_warlord_glob >= 1 )
    	MessageBox "Вы можете контролировать только одного огненного слугу."
    	set state to 10
    endif
    
    if ( state == 1 )
    	if ( nc_cloth >= 1 )
    		if ( nc_scrap_metal >= 1 )
    			if ( nc_netch_leather >= 1 )
    				if ( nc_bonemeal >= 10 )
    					if ( nc_alt_skull1 >= 1 )
    	            			removeitem "nc_altmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_arg_skull1 >= 1 )
    	            			removeitem "nc_argonian_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_bos_skull1 >= 1 )
    	            			removeitem "nc_bosmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_bret_skull1 >= 1 )
    	            			removeitem "nc_breton_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_imp_skull1 >= 1 )
    	            			removeitem "nc_imperial_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_dun_skull1 >= 1 )
    	            			removeitem "nc_dunmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_kha_skull1 >= 1 )
    	            			removeitem "nc_khajiit_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_nord_skull1 >= 1 )
    	            			removeitem "nc_nord_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_orc_skull1 >= 1 )
    	            			removeitem "nc_orc_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
    					elseif ( nc_rg_skull1 >= 1 )
    	            			removeitem "nc_redguard_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_02" 1
                				removeitem "nc_fire_cured" 1
                				set state to 2
#endif /* ABCD */
    					else
#ifndef ABCD
    						MessageBox "You need a piece of netch leather to perform the ritual."
#else /* ABCD */
    	            			MessageBox "Вам нужен череп для этого ритуала."
#endif /* ABCD */
    						set state to 10
    					endif
    				else
#ifndef ABCD
    					MessageBox "You need a piece of scrap metal to perform the ritual."
#else /* ABCD */
    					MessageBox "Вам нужно некоторое количество костяной муки для этого ритуала."
#endif /* ABCD */
    					set state to 10
    				endif
    			else
#ifndef ABCD
    				MessageBox "You need a bolt of red cloth to perform the ritual."
#else /* ABCD */
    				MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
#endif /* ABCD */
    				set state to 10
    			endif
    		else
#ifndef ABCD
    			MessageBox "You need a cured fire corpse to perform the ritual."
#else /* ABCD */
    			MessageBox "Вам нужны обрезки металла для этого ритуала."
#endif /* ABCD */
    			set state to 10
    		endif
#ifdef ABCD
    	else
    		MessageBox "Вам нужен рулон красной ткани для этого ритуала."
    		set state to 10
#endif /* ABCD */
    	endif
#ifdef ABCD
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
#else /* ABCD */
    if ( state == 2 )
#endif /* ABCD */
    		PlaySound, "ThunderClap"
#ifndef ABCD
    		PlaceAtPC "nc_f_warlord" 1, 762, 0
    		Cast "nc_summon_effects" Player
#else /* ABCD */
    		"nc_rev_placer"->PlaceAtMe "nc_f_warlord" 1, 0, 0
#endif /* ABCD */
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		set nc_f_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
#ifndef ABCD
    		MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		set state to 10
    	endif
#else /* ABCD */
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 10 )
    		enableplayerlooking
    		set state to 0
    		stopscript nc_summon_fire
    	endif
#else /* ABCD */
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_fire
    endif
#endif /* ABCD */
    
    
    end

