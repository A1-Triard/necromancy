SCPT
#ifndef ABCD
SCHD nc_poison_vat 17 0 0 3483 130
SCVR currentday;localdayspassed;pstate;salt;coprinus;russula;nord;orc;altmer;arg;bosmer;breton;khajiit;dunmer;redguard;imperial;doonce;;
SCDT 
#else /* ABCD */
SCHD nc_poison_vat 16 0 0 3098 123
SCVR currentday;localdayspassed;pstate;salt;coprinus;russula;nord;orc;altmer;arg;bosmer;breton;khajiit;dunmer;redguard;imperial;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_poison_vat
    
    short currentday
    short localdayspassed
    short pstate
    short salt
    short coprinus
    short russula
    short nord
    short orc
    short altmer
    short arg
    short bosmer
    short breton
    short khajiit
    short dunmer
    short redguard
    short imperial
#ifndef ABCD
    short doonce
#endif /* ! ABCD */
    
    if ( menumode == 1 )
    	return
    endif
    
#ifndef ABCD
    if ( OnActivate == 1 )
    	set doonce to 0
    	Activate
    endif
    
#endif /* ! ABCD */
        set altmer to getItemCount "nc_altmer_pauldron" 
        set arg to getItemCount "nc_arg_pauldron" 
        set bosmer to getItemCount "nc_bosmer_pauldron" 
        set breton to getItemCount "nc_breton_pauldron" 
        set dunmer to getItemCount "nc_dunmer_pauldron"
        set imperial to getItemCount "nc_imperial_pauldron" 
        set khajiit to getItemCount "nc_khajiit_pauldron" 
        set nord to getItemCount "nc_nord_pauldron" 
        set orc to getItemCount "nc_orc_pauldron" 
        set redguard to getItemCount "nc_rg_pauldron"
       	set salt to getItemCount "nc_salt"
    	set coprinus to getItemCount "ingred_coprinus_01"
    	set russula to getItemCount "ingred_russula_01"
    
    
    set pstate to 2
    if ( salt >= 500 )
       if ( coprinus >= 5 )
           if ( russula >= 5 )
               set pstate to 1
           endif
       endif
    endif
    
    
    if ( altmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_altmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
#ifndef ABCD
    			MessageBox "Your poisonous corpse should be cured by now."
#else /* ABCD */
    			MessageBox "Ядовитая мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set pstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( arg >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_arg_pauldron" 1
    			additem "nc_poison_pauldron" 1
#ifndef ABCD
    			MessageBox "Your poisonous corpse should be cured by now."
#else /* ABCD */
    			MessageBox "Ядовитая мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set pstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( bosmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_bosmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
#ifndef ABCD
    			MessageBox "Your poisonous corpse should be cured by now."
#else /* ABCD */
    			MessageBox "Ядовитая мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set pstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( breton >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_breton_pauldron" 1
    			additem "nc_poison_pauldron" 1
#ifndef ABCD
    			MessageBox "Your poisonous corpse should be cured by now."
#else /* ABCD */
    			MessageBox "Ядовитая мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set pstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( dunmer >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_dunmer_pauldron" 1
    			additem "nc_poison_pauldron" 1
#ifndef ABCD
    			MessageBox "Your poisonous corpse should be cured by now."
#else /* ABCD */
    			MessageBox "Ядовитая мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( pstate == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set pstate to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set pstate to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( imperial >= 1 )
    	if ( pstate == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
