SCPT
#ifndef ABCD
SCHD nc_de_corpse_cure 7 1 0 705 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_de_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_de_corpse_cure
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
#endif /* ! ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	set getcell to 1
#else /* ABCD */
    	set getcell to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ifx ( getcell )
    
    set DoOnce1 to 1
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_dunmer_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( temp != 0 )
    	if ( state == 0 )
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
#endif /* ABCD */
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
#ifndef ABCD
    			set getcell to 0
    			set temp to 0
#else /* ABCD */
    			set getcell to 1
#endif /* ABCD */
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
#ifndef ABCD
    			disable
    			player-> additem "nc_dunmer_pauldron" 1
#endif /* ! ABCD */
    			set state to 1
    		endif
#ifdef ABCD
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
#endif /* ABCD */
    	endif
#ifndef ABCD
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
    endif
    
    set getcell to 0
    
    endif
#endif /* ! ABCD */
    
#ifdef ABCD
    	set getcell to 1
#endif /* ABCD */
    
#ifndef ABCD
    if ( state == 1 )
    	return
#endif /* ! ABCD */
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_dunmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_de_corpse_drop 2 0 0 108 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBJuY19kdW5tZXJfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_de_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_dunmer_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

SCPT
#ifndef ABCD
SCHD nc_de_skelly_drop 1 0 0 198 8
#else /* ABCD */
SCHD nc_de_skelly_drop 1 0 0 186 8
#endif /* ABCD */
SCVR counter;;
#ifndef ABCD
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX2R1bm1lcl9wYXVsZHJvbl9zawEABwEBABA8AEl0IGlzIHRvbyBhd2t3YXJkIHRvIGNhcnJ5IG1vcmUgdGhhbiBvbmUgc2tlbGV0b24gYXQgYSB0aW1lLgAACQEJAQEB
#else /* ABCD */
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX2R1bm1lcl9wYXVsZHJvbl9zawEABwEBABAwAM3l8fLoIOHu6/z45Swg9+XsIO7k6O0g8erl6+XyIOfgIPDg5yAtIO3l8+Tu4e3uLgAACQEJAQEB
#endif /* ABCD */
SCTX
    begin nc_de_skelly_drop
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_dunmer_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_de_skelly_pauldron 4 0 1 660 40
#else /* ABCD */
SCHD nc_de_skelly_pauldron 4 0 1 637 40
#endif /* ABCD */
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_de_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_dunmer_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_dunmer_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_de_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

SCPT
#ifndef ABCD
SCHD nc_dead_z_script 1 0 1 81 19
SCVR castonce;ringtimer;;
SCDT BgEDCSBzAQAgPT0gMK0RDm5jX2RlYWRfem9tYmll2xAFAXMBAAIgMQkBBgEBCCBmAQAgPiAzqxEBAAcBAQUBZgEACiBmAQAgWBIQICsJAQEB
SCTX
    Begin nc_dead_z_script
    
    float ringtimer
    
    short castonce
    
    if ( castonce == 0 )
    	ExplodeSpell "nc_dead_zombie"
    	Disable
    	set castonce to 1
    endif
    
    if ( ringtimer > 3 )
    	SetDelete 1
    else
    	set ringtimer to ( ringtimer + GetSecondsPassed )
    endif
    
    End 

SCPT
#endif /* ! ABCD */
SCHD nc_death_add 1 0 0 110 7
SCVR doonce;;
SCDT BgEFGiBY/RAgbw9uY19taXNjX3NrdWxsMTAgPCAxBgEDCSBY8BAgPT0gMdQQD25jX21pc2Nfc2t1bGwxMAEABQFzAQACIDEcEAxuY19kZWF0aF9hZGQJAQcBARwQDG5jX2RlYXRoX2FkZAkBAQE=
SCTX
    begin nc_death_add
#ifndef ABCD
    
#endif /* ! ABCD */
    short doonce
    
    if ( GetItemCount,  "nc_misc_skull10" < 1 )
    	if ( OnDeath == 1 )
    		additem "nc_misc_skull10" 1
    		set doonce to 1
    		stopscript nc_death_add
    	endif
    else
    	stopscript nc_death_add
    endif
    
    
    end
    

SCPT
#ifndef ABCD
SCHD nc_death_add_altmer 0 5 1 568 43
#else /* ABCD */
SCHD nc_death_add_altmer 0 5 1 547 43
#endif /* ABCD */
SCVR invitem;invcount;invref;pcref;npcref;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_death_add_altmer
    
    
    
    long invitem    ; name of the item (i.e. gold_001, ingred_ruby_01, etc.
    long invcount   ; amount of the item (i.e. 327 gold_001, etc.)
    long invref     ; pointer to next item (unused in this script)
    long pcref
    long npcref
    float timer
    
    set timer to ( timer + GetSecondsPassed )
    	
#ifndef ABCD
    if ( timer >= 120 )
#else /* ABCD */
    if ( timer >= 60 )
#endif /* ABCD */
    	startscript nc_activate
    	stopscript nc_death_add_altmer
    	set timer to 0
    endif
    
    
    if ( OnActivate == 0 )    ; Nothing to do until player tries to open it.
       return
    else
    	if ( nc_has_corpse == 1 )
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
    		return
    	endif
    endif
    
    
    ; If we get here, the trap is active, take everything the player carries.
    setx pcref to xGetRef "player"
    setx npcref to xGetPCTarget
    
#ifndef ABCD
    npcref->xRemoveItem "nc_stump_shirt" 1
    npcref->xRemoveItem "nc_stump" 1
    
#endif /* ! ABCD */
    setx invitem,invcount,invref to npcref->xInventory      ; get first item
    whilex ( invcount )
#ifndef ABCD
    	if ( invcount < 0 )
    		set invcount to ( invcount * -1 )
    	endif
#endif /* ! ABCD */
        xRemoveItem invitem invcount                          ; add to container
        pcref->xAddItem invitem invcount                ; take from player
        setx invitem,invcount,invref to npcref->xInventory  ; get another item
    endwhile
    
    disable
#ifdef ABCD
    pcref->Removeitem "nc_soulgem_blackened" 1
    pcref->Additem "nc_SoulGem_altmer" 1
#endif /* ABCD */
    pcref->Additem "nc_altmer_pauldron" 1
#ifdef ABCD
    pcref->AddItem "nc_altmer_skull" 1
#endif /* ABCD */
    
    stopscript nc_death_add_altmer
    
    end

