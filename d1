    short nc_orc_skull1
    short nc_rg_skull1
    short state
#endif /* ABCD */
    
#ifndef ABCD
    	if ( nc_pclevel >= 29 )
    		set state to 1
    	elseif ( nc_pclevel < 29 )
    		MessageBox "You are not yet experienced enough to summon a Revenant Warlord."
    		set state to 10
    	endif
#else /* ABCD */
