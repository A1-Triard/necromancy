    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_03" 1
    								removeitem "nc_poison_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							else
    								MessageBox "You need a skull to perform the ritual."
    								set state to 10
    							endif
    						else
    							MessageBox "You need some bonemeal to perform the ritual."
    							set state to 10
    						endif
#else /* ABCD */
    if ( nc_p_warlord_glob >= 1 )
    	MessageBox "Вы можете контролировать только одного ядовитого слугу."
    	set state to 10
    endif
    
    if ( state == 1 )
    	if ( nc_cloth >= 1 )
    		if ( nc_scrap_metal >= 1 )
    			if ( nc_netch_leather >= 1 )
    				if ( nc_bonemeal >= 10 )
    					if ( nc_alt_skull1 >= 1 )
    	            			removeitem "nc_altmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_arg_skull1 >= 1 )
    	            			removeitem "nc_argonian_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_bos_skull1 >= 1 )
    	            			removeitem "nc_bosmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_bret_skull1 >= 1 )
    	            			removeitem "nc_breton_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_imp_skull1 >= 1 )
    	            			removeitem "nc_imperial_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_dun_skull1 >= 1 )
    	            			removeitem "nc_dunmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_kha_skull1 >= 1 )
    	            			removeitem "nc_khajiit_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_nord_skull1 >= 1 )
    	            			removeitem "nc_nord_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_orc_skull1 >= 1 )
    	            			removeitem "nc_orc_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
    					elseif ( nc_rg_skull1 >= 1 )
    	            			removeitem "nc_redguard_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_03" 1
                				removeitem "nc_poison_cured" 1
                				set state to 2
#endif /* ABCD */
    					else
#ifndef ABCD
    						MessageBox "You need a piece of netch leather to perform the ritual."
#else /* ABCD */
    	            			MessageBox "Вам нужен череп для этого ритуала."
#endif /* ABCD */
    						set state to 10
    					endif
    				else
#ifndef ABCD
    					MessageBox "You need a piece of scrap metal to perform the ritual."
#else /* ABCD */
    					MessageBox "Вам нужно некоторое количество костяной муки для этого ритуала."
#endif /* ABCD */
    					set state to 10
    				endif
    			else
#ifndef ABCD
    				MessageBox "You need a bolt of green cloth to perform the ritual."
#else /* ABCD */
    				MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
#endif /* ABCD */
    				set state to 10
    			endif
    		else
#ifndef ABCD
    			MessageBox "You need a cured fire corpse to perform the ritual."
#else /* ABCD */
    			MessageBox "Вам нужны обрезки металла для этого ритуала."
#endif /* ABCD */
    			set state to 10
    		endif
#ifdef ABCD
    	else
    		MessageBox "Вам нужен рулон зеленой ткани для этого ритуала."
    		set state to 10
#endif /* ABCD */
    	endif
#ifdef ABCD
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
#else /* ABCD */
    if ( state == 2 )
#endif /* ABCD */
    		PlaySound, "ThunderClap"
#ifndef ABCD
    		PlaceAtPC "nc_p_warlord" 1, 762, 0
    		Cast "nc_summon_effects" Player
#else /* ABCD */
    		"nc_rev_placer"->PlaceAtMe "nc_p_warlord" 1, 0, 0
#endif /* ABCD */
    		set nc_p_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    		MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		set state to 10
    	endif
#else /* ABCD */
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 10 )
    		enableplayerlooking
    		set state to 0
    		stopscript nc_summon_poison
    	endif
#else /* ABCD */
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_poison
    endif
#endif /* ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_summon_redguard 7 0 0 1352 77
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;nc_skl;state;;
SCDT 
#else /* ABCD */
SCHD nc_summon_redguard 6 0 0 1211 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_summon_redguard
    
#ifndef ABCD
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_skull
    	short nc_skl
    	short state
    
    	set nc_pclevel to nc_necro_skill
    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_skull to getItemCount "nc_redguard_skull"
    	set nc_skl to getItemCount "nc_rg_pauldron_sk"
    
    
    	if ( nc_skl >= 1 )
    		if ( nc_skull >= 1 )
    			if ( nc_scrap_metal >= 1 )
    				if ( nc_netch_leather >= 1 )
    					if ( nc_bonemeal >= 10 )
    						removeitem "nc_rg_pauldron_sk" 1
    						removeitem "nc_redguard_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
    						removeitem "ingred_netch_leather_01" 1
    						removeitem "ingred_bonemeal_01" 10
    						Player->RemoveItem "nc_SoulGem_blackened_f" 1
    						Cast "nc_summon_effects" Player
    						set state to 1
    					else
    						MessageBox "You need 10 portions of bonemeal to perform the ritual."
    						set state to 2
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 2
    				endif
    			else
    				MessageBox "You need a piece of netch leather to perform the ritual."
    				set state to 2
    			endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_redguard_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    		  removeitem "nc_rg_pauldron_sk" 1
                removeitem "nc_redguard_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			MessageBox "You need a Redguard skull to perform the ritual."
    			set state to 2
    		endif
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "You need a Redguard skeleton to perform the ritual."
    		set state to 2
    	endif
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    	MessageBox "Вам нужен череп Редгарда для этого ритуала."
    	set state to 2
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 1 )
    		if ( nc_pclevel <= 10 )
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_redguard" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )	
#endif /* ABCD */
    			PlaySound, "ThunderClap"
#ifndef ABCD
    			PlaceAtPC "nc_redguard" 1, 762, 0
#else /* ABCD */
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_2" 1, 0, 0
#endif /* ABCD */
    			set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_pclevel <= 20 )	
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_redguard_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_redguard_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
#else /* ABCD */
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_redguard_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
#ifndef ABCD
    		set state to 2
#endif /* ! ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_redguard
    	endif
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_redguard
    endif
#endif /* ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_time_bounty 2 0 3 1667 28
#else /* ABCD */
SCHD nc_time_bounty 2 0 3 1650 28
#endif /* ABCD */
SCVR rand;kill;timer;hour;timer2;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_time_bounty
    
    short rand
    short kill
    float timer
    float hour
    float timer2
    
    if ( MenuMode == 1 )
    	return
#ifndef ABCD
    elseif ( GetPCCell "Thanatos Temple" == 1 )
#else /* ABCD */
    elseif ( GetPCCell "Храм Танатоса" == 1 )
#endif /* ABCD */
    	return
    elseif ( VH_MaskDummyFlag > 0 )
    	return
    endif
    
    set timer2 to ( timer2 + GetSecondsPassed )
    if ( timer2 >= 14 )
    
    		if ( Player->GetItemCount "nc_altmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_altmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_arg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_bosmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_breton_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_dunmer_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_imperial_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_khajiit_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_nord_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_orc_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron" >= 1 )
    			set timer2 to 0
    			return
    		elseif ( Player->GetItemCount "nc_rg_pauldron_sk" >= 1 )
    			set timer2 to 0
    			return
    		else
    			set kill to 1
    		endif
    endif
    
    set hour to GameHour
    set rand to Random, 11
    set timer to ( timer + GetSecondsPassed )
    
    if ( timer >= 15 )
    
    if ( hour < 8 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
#ifndef ABCD
    			MessageBox "Someone has seen you carrying a corpse."
    			ModPCCrimeLevel 100
#else /* ABCD */
    			MessageBox "Кто-то увидел, как вы несете тело."
    			ModPCCrimeLevel 500
#endif /* ABCD */
    		endif
    	endif
    elseif ( hour < 20 )
    	if ( hour >= 8 )
    		;MessageBox "Daytime. Time is %f. Random is %g.", hour, rand
    		if ( rand <= 7 )
    			if ( GetDetected Player == 1 )
#ifndef ABCD
    				MessageBox "Someone has seen you carrying a corpse."
    				ModPCCrimeLevel 100
#else /* ABCD */
    				MessageBox "Кто-то увидел, как вы несете тело."
    				ModPCCrimeLevel 500
#endif /* ABCD */
    			endif
    		endif
    	endif
    elseif ( hour >= 20 )
    	;MessageBox "Nighttime. Time is %f. Random is %g.", hour, rand
    	if ( rand <= 3 )
    		if ( GetDetected Player == 1 )
#ifndef ABCD
    			MessageBox "Someone has seen you carrying a corpse."
    			ModPCCrimeLevel 100
#else /* ABCD */
    			MessageBox "Кто-то увидел, как вы несете тело."
    			ModPCCrimeLevel 500
#endif /* ABCD */
    		endif
    	endif
    endif
    
    	set timer to 0
    endif
    
    if ( kill == 1 )
    	stopscript nc_time_bounty
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	set timer2 to 0
    	set kill to 0
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_tragrilar_death 4 0 0 165 33
SCVR currentday;deathday;state;nolore;;
SCDT BgEBCSBzAwAgPT0gMCQBCQEGAQIPIHMBACAhPSBHBERheQAABQFzAQAIIEcERGF5AAAFAXMCAAggcwIAIDEgKwkBBgEECSBzAgAgPj0gMgYBAhsgWBIRIGMPVGhhbmF0b3MgVGVtcGxlID09IDAbEBNuY19kZWF0aF9hZGRfYWx0bWVyjRAAAAAACQEJARwQEm5jX3RyYWdyaWxhcl9kZWF0aAEB
#else /* ABCD */
SCHD nc_tragrilar_death 2 0 0 146 20
SCVR currentday;deathday;;
SCDT BgECDyBzAQAgIT0gRwREYXkAAAUBcwEACCBHBERheQAABQFzAgAIIHMCACAxICsJAQYBBQkgcwIAID49IDIGAQMZIFgSESBjDdXw4Owg0uDt4PLu8eAgPT0gMRsQE25jX2RlYXRoX2FkZF9hbHRtZXKNEAAAAAAcEBJuY190cmFncmlsYXJfZGVhdGgJAQkBAQE=
#endif /* ABCD */
SCTX
    begin nc_tragrilar_death
    
    short currentday
    short deathday
#ifndef ABCD
    short state
    short nolore
    
    if ( state == 0 )
    	return
    endif
#endif /* ! ABCD */
    
    if ( currentday != Day )
    	set currentday to Day
    	set deathday to ( deathday + 1 )
    endif
    
    if ( deathday >= 2 )
#ifndef ABCD
    	if ( GetPCCell "Thanatos Temple" == 0 )
#else /* ABCD */
    	if ( GetPCCell "Храм Танатоса" == 1 )
#endif /* ABCD */
    		startscript nc_death_add_altmer
    		sethealth 0
#ifdef ABCD
    		stopscript nc_tragrilar_death
#endif /* ABCD */
    	endif
    endif
    
#ifndef ABCD
    stopscript nc_tragrilar_death
#endif /* ! ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_wander_rev_script 3 3 0 740 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_wander_rev_script 1 0 0 225 7
SCVR doonce;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_wander_rev_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_wander_rev_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_wander_rev_script"
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIWander 500 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will stay around here."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    if ( nc_callrev == 0 )
    	set nc_callrev to 3
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут бродить вокруг."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
    
#endif /* ABCD */
    end

SCPT
#ifndef ABCD
SCHD nc_wander_script 3 3 0 553 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_wander_script 1 0 0 279 7
SCVR doonce;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_wander_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_wander_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIWander 500 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will stay around here."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    short doonce 
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_wander_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 3
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 3
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут бродить вокруг."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
#endif /* ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_wander_war_script 6 3 0 828 52
SCVR doonce;call;state;fwar;frwar;pwar;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_wander_war_script 1 0 0 222 7
SCVR doonce;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEGAQQJIHMBACA9PSAyBQFzAQACIDAFAUcKbmNfY2FsbHdhcgIgMBwQFG5jX3dhbmRlcl93YXJfc2NyaXB0JAEJAQYBARIgRwpuY19jYWxsd2FyID09IDAFAUcKbmNfY2FsbHdhcgIgMwkBBgEEFiBHDm5jX2NhbGxtZXNzYWdlID09IDEGAQIJIHMBACA9PSAwBQFzAQACIDEAECAAwuD46CDx6/Pj6CDh8+Tz8iDh8O7k6PL8IOLu6vDz4y4AAAkBCQEFAXMBAAIgMgEB
#endif /* ABCD */
SCTX
    begin nc_wander_war_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	short fwar
    	short frwar
    	short pwar
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_wander_war_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    			set fwar to 0
    			set frwar to 0
    			set pwar to 0
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			set fwar to 1
    		endif
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			set frwar to 1
    		endif
#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callwar to 0
    	StopScript "nc_wander_war_script"
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			set pwar to 1
    		endif
    		
    		if ( fwar == 0 );not a fire
    			if ( frwar == 0 );not a frost
    				if ( pwar == 0 );not a warlord
    					return
    				endif
    			endif
    		endif
#else /* ABCD */
    if ( nc_callwar == 0 )
    	set nc_callwar to 3
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		npcref->AIWander 500 0 0
    		if ( doonce == 0 )
    			MessageBox "Your warlords will stay around here."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши слуги будут бродить вокруг."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
    
#endif /* ABCD */
    end

SCPT
SCHD nc_warlord_attack 4 5 1 818 80
SCVR doonce;call;call2;call3;prospectref;prospectid;temp;target;target2;targethealth;;
SCDT 
SCTX
    begin nc_warlord_attack
    
    
    short doonce
    short call ; status variable - should this NPC be called?
    short call2
    short call3
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
    long target
    long target2
    float targethealth
    
    
    if ( menumode == 1 )
        return
    endif
    
    
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
    setx target to xGetPCTarget
    
    if ( target == 0 )
    	return
    endif
    
    ifx ( target )
    
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
    
        ; start by assuming we want to call this one
        set call to 1
    	 set call2 to 1
    	 set call3 to 1
    
    
    
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
            set call2 to 0
            set call3 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_f_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_fr_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call2 to 0
        endif
    
        ; get this NPCs id and see if it's a guard
        setx prospectid to prospectref->xGetBaseID
        setx temp to xStringCompare prospectid "nc_p_warlord"
        if ( temp != 0 ) ; it's not a guard
            set call3 to 0
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call3 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
    
        ; if call is still 1 we want to call this NPC to the player
    	ifx ( call2 )
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    	
    			;prospectref->xStartCombat target2
    
    		
    
    
        endif
        ifx ( call ) ; we use extended functions inside so need ifx
    		  ;setx target to xGetPCTarget ; So MWEdit compiles without Morrowind.esm loaded
    		  ;setx target2 to xGetPCTarget
            ; Following line will operate on prospectref, that's the meaning of xSetRef
            ; However, only *one following* command will do that,
    
    		  
    		 xSetRef target
    			set targethealth to GetHealth
    
    		ifx ( targethealth )
            prospectref->xstartcombat target
    		  set targethealth to 0
    		endif
    
    
    
    
        endif
    
    
        setx prospectref to xNextRef prospectref
    
    
    endwhile
    set doonce to 0
    set target to 0 ;target
    endif
    startscript nc_guard_attack
    stopscript nc_warlord_attack
    
    
    end

SCPT
#ifndef ABCD
SCHD nc_we_corpse_cure 7 1 0 705 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_we_corpse_cure 5 0 0 561 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_we_corpse_cure
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
#endif /* ! ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	set getcell to 1
#else /* ABCD */
    	set getcell to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ifx ( getcell )
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
    set DoOnce1 to 1
    
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_bosmer_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( temp != 0 )
    	if ( state == 0 )
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
#endif /* ABCD */
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
#ifndef ABCD
    			set getcell to 0
    			set temp to 0
#else /* ABCD */
    			set getcell to 1
#endif /* ABCD */
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
#ifndef ABCD
    			disable
    			player-> additem "nc_bosmer_pauldron" 1
#endif /* ! ABCD */
    			set state to 1
    		endif
#ifdef ABCD
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
#endif /* ABCD */
    	endif
#ifndef ABCD
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
    endif
    
    set getcell to 0
    
    endif
#endif /* ! ABCD */
    
#ifdef ABCD
    	set getcell to 1
#endif /* ABCD */
    
#ifndef ABCD
    if ( state == 1 )
    	return
#endif /* ! ABCD */
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_bosmer_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

