#ifndef ABCD
SCHD nc_corrupter_script 3 0 0 442 22
SCVR gemcount;state;button;;
SCDT 
SCTX
    begin nc_corrupter_script
    
    short gemcount
    short state
    short button
    
    if ( MenuMode )
    	Return
    endif
    
    set gemcount to GetItemCount "Misc_SoulGem_Greater"
    
    if ( OnActivate == 1 )
    	if ( gemcount < 1 )
    		Activate
    	else
    		set state to 1
    	endif
    endif
    
    if ( state == 1 )
    	MessageBox "What do you want to do with this?" "Put soulgems inside" "Create blackened soulgems"
    	set state to 2
    endif
    
    if ( state == 2 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		Return
    	elseif ( button == 0 )
    		Activate
    		set state to 0
    	else
    		set state to 3
    	endif
    endif
    
    if ( state == 3 )
    	set gemcount to GetItemCount "Misc_SoulGem_Greater"
    	if ( gemcount > 0 )
    		RemoveItem "Misc_SoulGem_Greater" 1
    		AddItem "nc_SoulGem_blackened" 1
    	else
    		MessageBox "All soulgems have been blackened."
    		set state to 0
    	endif
    endif
    
    end

