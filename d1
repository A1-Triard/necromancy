    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
#else /* ABCD */
    		MessageBox "Нести больше, чем одно тело - неудобно."
#endif /* ABCD */
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_khajiit_pauldron" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_khajiit_pauldron"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
#ifndef ABCD
    			MessageBox "A corpse is too cumbersome to carry in your inventory.  So you throw it over your shoulder."
#else /* ABCD */
    			MessageBox "Нести тело в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
#endif /* ABCD */
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -4
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_khajiit_corpse" 1 1 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    

