SCPT
#ifndef ABCD
SCHD nc_start_skull 1 0 0 311 7
#else /* ABCD */
SCHD nc_start_skull 1 0 0 285 7
#endif /* ABCD */
SCVR doonce;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_start_skull
    
    short doonce
    
    if ( nc_start == 1 )
    	enable
    else
    	disable
    endif
    
    if ( OnActivate == 1 )
    	if ( doonce == 0 )
#ifndef ABCD
    		MessageBox "You can feel a strange magic coursing throughout your body.  Somehow you are now able to see a door that wasn't there before.  A voice inside your head says 'Fear not.  Walk forward and come inside.  Power and glory await you.'"
#else /* ABCD */
    		MessageBox "Вы чувствуете странную магию, текущую через ваше тело. Каким-то образом вы видите дверь, которой здесь только что не было.  Голос внутри вашей головы говорит: 'Не бойся. Входи. Сила и слава ждут тебя.'"
#endif /* ABCD */
    		set nc_start to 2
    		return
    	endif
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_startscript 6 0 2 1094 57
#else /* ABCD */
SCHD nc_startscript 6 0 2 995 57
#endif /* ABCD */
SCVR doonce;doonce2;doonce3;doonce4;doonce5;wait;timer;timer2;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_startscript
    
    	float timer
    	float timer2
    	short doonce
    	short doonce2
    	short doonce3
    	short doonce4
    	short doonce5
    	short wait
    
    
    	if ( menumode == 1 )
    		return
    	endif
    
    	; Dont waste time checking spells more than once per second
    	set timer to ( timer + getsecondspassed )
    	if ( timer < 1 )		
    		return
    	endif
    	set timer to 0
    		
    	if ( nc_has_corpse > 0 )
    		if ( ScriptRunning "nc_corpse_check" == 0 )
    			startscript nc_corpse_check
    		endif
    	endif
    
    	if ( nc_necro_skill == 10 )
    		if ( doonce4 == 0 )
#ifndef ABCD
    			MessageBox "You have gained skill in Revenant summoning.  You can now summon stronger Revenants."
#else /* ABCD */
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать более сильных мертвецов."
#endif /* ABCD */
    			set doonce4 to 1
    		endif
    	endif	
    
    	if ( nc_necro_skill == 20 )
    		if ( doonce3 == 0 )
#ifndef ABCD
    			MessageBox "You have gained skill in Revenant summoning.  You can now summon a Greater Revenant."
#else /* ABCD */
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Теперь вы можете вызывать больших мертвецов."
#endif /* ABCD */
    			set doonce3 to 1
    		endif
    	endif	
    
    	if ( nc_necro_skill == 30 )
    		if ( doonce2 == 0 )
#ifndef ABCD
    			MessageBox "You have gained skill in Revenant summoning.  Go and speak with Tragrilar."
#else /* ABCD */
    			MessageBox "Вы увеличили свое умение вызова мертвецов. Идите и поговорите с Трагриларом."
#endif /* ABCD */
    			set doonce2 to 1
    		endif
    	endif
    
#ifndef ABCD
    	if ( Player->GetSpellEffects nc_extraction == 1 )
    		if ( ScriptRunning "nc_get_corpse" == 0 )
    			StartScript "nc_get_corpse"
#else /* ABCD */
    
    	if ( Player->getspelleffects nc_skull_spell == 1 )
    		set nc_spell_cast to 1
    		if ( wait == 0 )
    			startscript nc_death_trap
    			set wait to 1
    		endif
    		set timer2 to ( timer2 + GetSecondsPassed )
    		if ( timer2 >= 15 )
    			set wait to 0
    			set timer2 to 0
#endif /* ABCD */
    		endif
#ifndef ABCD
    		;Player->RemoveSpellEffects nc_extraction
#else /* ABCD */
    	else
    		set nc_spell_cast to 0
    		set timer2 to 0
    		set wait to 0
    		stopscript nc_death_trap
#endif /* ABCD */
    	endif
    
    	if ( player->getspelleffects nc_summon == 1 )
    		if ( doonce5 == 0 )
    			set nc_summon_spell to 1
    			set doonce5 to 1
    		endif
#ifndef ABCD
    		Player->RemoveSpellEffects nc_summon
#endif /* ! ABCD */
    	else
    		set nc_summon_spell to 0
    		set doonce5 to 0
    	endif
    
    	if ( player->getspelleffects nc_painful_touch == 1 )
    		set nc_warlord_spell to 1
#ifndef ABCD
    		if ( ScriptRunning nc_warlord_attack == 0 )
    			startscript nc_warlord_attack
    		endif
    		Player->RemoveSpellEffects nc_painful_touch
#else /* ABCD */
    		startscript nc_warlord_attack
#endif /* ABCD */
    	else
    		set nc_warlord_spell to 0
    	endif
    
#ifndef ABCD
    	if ( player->GetSpellEffects nc_raise_zombie == 1 )
    		if ( ScriptRunning nc_raise_zombie_script == 0 )
    			if ( ScriptRunning nc_npc_zombie_script == 0 )
    				StartScript nc_raise_zombie_script
    			endif
    		endif
    	endif
#else /* ABCD */
    
#endif /* ABCD */
    
    end
    	
    

SCPT
#ifndef ABCD
SCHD nc_stats 16 0 0 477 198
#else /* ABCD */
SCHD nc_stats 16 0 0 492 198
#endif /* ABCD */
SCVR nc_health;nc_magicka;nc_fatigue;nc_level;nc_attackbonus;nc_defendbonus;nc_resistmagicka;nc_speed;nc_athletics;nc_strength;nc_willpower;nc_intelligence;nc_agility;nc_endurance;nc_personality;nc_luck;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    Begin nc_stats
    
    short nc_health
    short nc_magicka
    short nc_fatigue
    short nc_level
    short nc_attackbonus
    short nc_defendbonus
    short nc_resistmagicka
    short nc_speed
    short nc_athletics
    short nc_strength
    short nc_willpower
    short nc_intelligence
    short nc_agility
    short nc_endurance
    short nc_personality
    short nc_luck
    
      set nc_health to ( Gethealth )
      set nc_magicka to ( Getmagicka )
      set nc_fatigue to ( Getfatigue )
      set nc_level to ( Getlevel )
      set nc_attackbonus to ( GetAttackBonus )
      set nc_defendbonus to ( GetDefendBonus )
      set nc_resistmagicka to ( GetResistMagicka )
    
      set nc_strength to ( GetStrength )  
      set nc_willpower to ( GetWillpower )  
      set nc_intelligence to ( GetIntelligence )
      set nc_agility to ( GetAgility )
      set nc_endurance to ( GetEndurance )
      set nc_personality to ( GetPersonality )
      set nc_speed to ( GetSpeed )
      set nc_luck to ( GetLuck )
    
    
#ifndef ABCD
    messagebox "Level : %g - Health : %g - Magicka : %g - Fatigue : %g - Attack bonus : %g - Defend bonus : %g - Resist Magicka : %g", nc_level, nc_health, nc_magicka, nc_fatigue, nc_attackbonus, nc_defendbonus, nc_resistmagicka
    messagebox "Strength : %g - Agility : %g - Endurance : %g - Speed : %g - Willpower : %g - Intelligence : %g - Personality : %g - Luck : %g", nc_strength, nc_agility, nc_endurance, nc_speed, nc_willpower, nc_intelligence, nc_personality, nc_luck
#else /* ABCD */
    messagebox "Уровень : %g - Здоровье : %g - Магия : %g - Усталость : %g - Бонус атаки : %g - Бонус защиты : %g - Сопротивление магии : %g", nc_level, nc_health, nc_magicka, nc_fatigue, nc_attackbonus, nc_defendbonus, nc_resistmagicka
    messagebox "Сила : %g - Ловкость : %g - Выносливость : %g - Скорость : %g - Сила воли : %g - Интеллект : %g - Привлекательность : %g - Удача : %g", nc_strength, nc_agility, nc_endurance, nc_speed, nc_willpower, nc_intelligence, nc_personality, nc_luck
#endif /* ABCD */
    
    stopscript nc_stats
    
    End

SCPT
#ifndef ABCD
SCHD nc_summon_altmer 7 0 0 1365 77
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;nc_skl;state;;
SCDT 
#else /* ABCD */
SCHD nc_summon_altmer 6 0 0 1222 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_summon_altmer
    
#ifndef ABCD
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_skull
    	short nc_skl
    	short state
    
    	set nc_pclevel to nc_necro_skill
    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_skull to GetItemCount "nc_altmer_skull"
    	set nc_skl to getItemCount "nc_altmer_pauldron_sk"
    
    	if ( nc_skl >= 1 )
    		if ( nc_skull >= 1 )
    			if ( nc_scrap_metal >= 1 )
    				if ( nc_netch_leather >= 1 )
    					if ( nc_bonemeal >= 10 ) 
    						removeitem "nc_altmer_pauldron_sk" 1
    						removeitem "nc_altmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
    						removeitem "ingred_netch_leather_01" 1
    						removeitem "ingred_bonemeal_01" 10
    						Player->RemoveItem "nc_SoulGem_blackened_f" 1
    						Cast "nc_summon_effects" Player
    						set state to 1
    					else
    						MessageBox "You need 10 portions of bonemeal to perform the ritual."
    						set state to 2
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 2
    				endif
    			else
    				MessageBox "You need a piece of netch leather to perform the ritual."
    				set state to 2
    			endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to GetItemCount "nc_altmer_skull"
    
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    					removeitem "nc_altmer_pauldron_sk" 1
       	         removeitem "nc_altmer_skull" 1
          	      removeitem "ingred_scrap_metal_01" 1
             	   removeitem "ingred_netch_leather_01" 1
             	   removeitem "ingred_bonemeal_01" 10
             	   set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			MessageBox "You need an Altmer skull to perform the ritual."
    			set state to 2
    		endif
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "You need an Altmer skeleton to perform the ritual."
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Альтмера для этого ритуала."
#endif /* ABCD */
    		set state to 2
#ifndef ABCD
    	endif
    
#else /* ABCD */
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 1 )
    		if ( nc_pclevel <= 10 )
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		;PlaceAtPC "nc_high elf" 1, 762, 0
    		"nc_rev_placer"->PlaceAtMe "nc_high elf" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_necro_skill <= 20 )
#endif /* ABCD */
    			PlaySound, "ThunderClap"
#ifndef ABCD
    			PlaceAtPC "nc_high elf" 1, 762, 0
#else /* ABCD */
    			"nc_rev_placer"->PlaceAtMe "nc_high elf_2" 1, 0, 0
#endif /* ABCD */
    			set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_necro_skill <= 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_high elf_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_high elf_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
#else /* ABCD */
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_high elf_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
#ifndef ABCD
    		set state to 2
#endif /* ! ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
    
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_altmer
    	endif
#else /* ABCD */
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_altmer
    endif
#endif /* ABCD */
    
    end

