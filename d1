    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_khajiit_pauldron" 1
    			additem "nc_frost_pauldron" 1
#ifndef ABCD
    			MessageBox "Your frost cured corpse should be ready by now."
#else /* ABCD */
    			MessageBox "Ледяная мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set state to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( orc >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_orc_pauldron" 1
    			additem "nc_frost_pauldron" 1
#ifndef ABCD
    			MessageBox "Your frost cured corpse should be ready by now."
#else /* ABCD */
    			MessageBox "Ледяная мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set state to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
#endif /* ABCD */
    	endif
    endif
    
    if ( redguard >= 1 )
    	if ( state == 1 )
    		if ( currentday != Day )
    			set currentday to Day
    			set localdayspassed to ( localdayspassed + 1 )
    		endif
    
    		if ( localdayspassed >= 6 )
    			removeitem "nc_rg_pauldron" 1
    			additem "nc_frost_pauldron" 1
#ifndef ABCD
    			MessageBox "Your frost cured corpse should be ready by now."
#else /* ABCD */
    			MessageBox "Ледяная мумия должна быть уже готова."
#endif /* ABCD */
    			set localdayspassed to 0
    		endif
    	elseif ( state == 2 )
#ifndef ABCD
    		if ( doonce == 0 )
    			MessageBox "The chest does not contain all of the ingredients needed to properly cure a corpse."
    			set state to 0
    			set doonce to 1
    		endif
#else /* ABCD */
    		MessageBox "Ванна не содержит всех ингредиентов, чтобы правильно мумифицировать тело."
    		set state to 0
#endif /* ABCD */
    	endif
    endif
    
    end
    

