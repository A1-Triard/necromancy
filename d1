#ifndef ABCD
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFm5jX2toYWppaXRfcGF1bGRyb25fc2sBAAcBAQAQPABJdCBpcyB0b28gYXdrd2FyZCB0byBjYXJyeSBtb3JlIHRoYW4gb25lIHNrZWxldG9uIGF0IGEgdGltZS4AAAkBCQEBAQ==
#else /* ABCD */
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFm5jX2toYWppaXRfcGF1bGRyb25fc2sBAAcBAQAQMADN5fHy6CDh7uv8+OUsIPfl7CDu5OjtIPHq5evl8iDn4CDw4OcgLSDt5fPk7uHt7i4AAAkBCQEBAQ==
