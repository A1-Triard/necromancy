    begin nc_poison_corpse_obj
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_poison_pauldron" 1
    	else
#ifndef ABCD
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
#else /* ABCD */
