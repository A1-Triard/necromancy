    	if ( GetJournalIndex "nc_necro_001" <= 70 )
    		Journal "nc_necro_001" 80
#endif /* ABCD */
    	endif
#ifdef ABCD
    endif
    
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_dunmer
    endif
#endif /* ABCD */
    
    end
    

