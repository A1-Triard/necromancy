    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIWander 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will wait right here."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
    
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	set nc_callwar to 0
    	StopScript "nc_stand_script"
    	return
    endif
    
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
    endif
    
    if ( nc_callwar == 0 )
    	set nc_callwar to 1
    endif
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
#endif /* ABCD */
    	endif
#ifdef ABCD
    endif
    
    set doonce to 2
#endif /* ABCD */
    
    end

