SCPT
#ifndef ABCD
SCHD nc_spell_timer 1 0 1 278 12
#else /* ABCD */
SCHD nc_spell_timer 1 0 1 245 12
#endif /* ABCD */
SCVR state;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_spell_timer
    
    float timer
    short state
    
    if ( menumode == 1 )
    	return
    endif
    
    if ( CellChanged == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectRecall == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectDivineIntervention == 1 )
    	set state to 1
    elseif ( player->GetEffect sEffectAlmsiviIntervention == 1 )
    	set state to 1
    endif
    
    set timer to ( timer + GetSecondsPassed )
    
#ifndef ABCD
    if ( timer >= 10 )
    	if ( timer < 11 )
    		EnablePlayerLooking
    	endif
    endif
    
#endif /* ! ABCD */
    if ( timer >= 120 )
    	set state to 1
    	set timer to 0
    endif
    
    if ( state == 1 )
    	player-> RemoveSpell "nc_summon"
    	stopscript nc_spell_timer
    	set state to 0
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_stand_rev_script 3 3 0 738 36
SCVR doonce;call;state;npcref;npcid;temp;;
SCDT 
#else /* ABCD */
SCHD nc_stand_rev_script 1 0 0 222 7
SCVR doonce;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEGAQQJIHMBACA9PSAyBQFzAQACIDAFAUcKbmNfY2FsbHJldgIgMBwQE25jX3N0YW5kX3Jldl9zY3JpcHQkAQkBBgEBEiBHCm5jX2NhbGxyZXYgPT0gMAUBRwpuY19jYWxscmV2AiAxCQEGAQQWIEcObmNfY2FsbG1lc3NhZ2UgPT0gMQYBAgkgcwEAID09IDAFAXMBAAIgMQAQIQDC4PjoIOzl8PLi5fb7IOHz5PPyIPHy7v/y/CDn5OXx/C4AAAkBCQEFAXMBAAIgMgEB
#endif /* ABCD */
SCTX
    begin nc_stand_rev_script
#ifndef ABCD
    	
    	short doonce
    	short call ; status variable - should this NPC be called?
    	long npcref ; reference to an NPC (or creature)
    	long npcid ; string holding the NPC's id
    	long temp ; used to control ifx statements
    	short state
    	
    	
    	if ( menumode == 1 )
    		return
    	endif
    	
    	set nc_f_follow to 0
    	set nc_fr_follow to 0
    	set nc_p_follow to 0
    	
    	ifx ( state )
    		set state to 0
    		set doonce to 0
    		stopscript nc_stand_rev_script
    	else
    		; loop through each of the NPCs and creatures in the cell
    		ifx ( npcref )
    			setx npcref to xNextRef npcref
    		else
    			setx npcref to xFirstNPC
    		endif
    		if ( npcref == 0 );end of list
    			set state to 1
    			return
    		endif
    		
    		; check the type because we don't want creatures (Non-NPCs)
    		setx temp to npcref->xRefType
    		if ( temp != 1598246990 ) ; it's not an NPC
    			return
    		endif
    		
    		; get this NPCs id and see if it's a revenant
    		setx temp to npcref->xGetBaseID
    		setx npcid to xStringBuild "%.3s" temp
    		setx temp to xStringCompare npcid "nc_"
    		if ( temp != 0 ) 
    			return
    		endif
#endif /* ! ABCD */
    
#ifndef ABCD
    		setx npcid to npcref->xGetBaseID
    		setx temp to xStringCompare npcid "nc_Tragrilar"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    short doonce 
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_f_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( menumode == 1 )
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_fr_warlord"
    		if ( temp == 0 )
    			return
    		endif
#else /* ABCD */
    if ( doonce == 2 )
    	set doonce to 0
    	set nc_callrev to 0
    	StopScript "nc_stand_rev_script"
    	return
    endif
#endif /* ABCD */
    
#ifndef ABCD
    		setx temp to xStringCompare npcid "nc_p_warlord"
    		if ( temp == 0 )
    			return
    		endif
    		
    		npcref->AIWander 0 0 0
    		if ( doonce == 0 )
    			MessageBox "Your revenants will wait right here."
    			set doonce to 1
    		endif
    		
#else /* ABCD */
    if ( nc_callrev == 0 )
    	set nc_callrev to 1
    endif
    ;set timer to timer + GetSecondsPassed 
    
    if ( nc_callmessage == 1 )
    	if ( doonce == 0 )
    		set doonce to 1
    		MessageBox "Ваши мертвецы будут стоять здесь."
#endif /* ABCD */
    	endif
#ifndef ABCD
    	
#else /* ABCD */
    endif
    
    set doonce to 2
    
#endif /* ABCD */
    end

