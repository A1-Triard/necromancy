    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_skull to getItemCount "nc_argonian_skull"
    	set nc_skl to getItemCount "nc_arg_pauldron_sk"
    
    	if ( nc_skl >= 1 )
    		if ( nc_skull >= 1 )
    			if ( nc_scrap_metal >= 1 )
    				if ( nc_netch_leather >= 1 )
    					if ( nc_bonemeal >= 10 )
    						removeitem "nc_arg_pauldron_sk" 1
    						removeitem "nc_argonian_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
    						removeitem "ingred_netch_leather_01" 1
    						removeitem "ingred_bonemeal_01" 10
    						Player->RemoveItem "nc_SoulGem_blackened_f" 1
    						Cast "nc_summon_effects" Player
    						set state to 1
    					else
    						MessageBox "You need 10 portions of bonemeal to perform the ritual."
    						set state to 2
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 2
    				endif
    			else
    				MessageBox "You need a piece of netch leather to perform the ritual."
    				set state to 2
    			endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_argonian_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				removeitem "nc_arg_pauldron_sk" 1
                removeitem "nc_argonian_skull" 1
               removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			MessageBox "You need an Argonian skull to perform the ritual."
    			set state to 2
    		endif
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "You need an Argonian skeleton to perform the ritual."
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Аргонианина для этого ритуала."
#endif /* ABCD */
    		set state to 2
#ifndef ABCD
    	endif
#else /* ABCD */
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 1 )
    		if ( nc_pclevel <= 10 )
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_argonian" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
#endif /* ABCD */
    			PlaySound, "ThunderClap"
#ifndef ABCD
    			PlaceAtPC "nc_argonian" 1, 762, 0
#else /* ABCD */
    			"nc_rev_placer"->PlaceAtMe "nc_argonian_2" 1, 0, 0
#endif /* ABCD */
    			set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_pclevel <= 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_argonian_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_argonian_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
#else /* ABCD */
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_argonian_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
#ifndef ABCD
    		set state to 2
#endif /* ! ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_argonian
    	endif
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_argonian
    endif
#endif /* ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_summon_bosmer 7 0 0 1349 77
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;nc_skl;state;;
SCDT 
#else /* ABCD */
SCHD nc_summon_bosmer 6 0 0 1333 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_summon_bosmer
    
#ifndef ABCD
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_skull
    	short nc_skl
    	short state
    
    	set nc_pclevel to nc_necro_skill
    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_skull to getItemCount "nc_bosmer_skull"
    	set nc_skl to getItemCount "nc_bosmer_pauldron_sk"
    
    	if ( nc_skl >= 1 )
    		if ( nc_skull >= 1 )
    			if ( nc_scrap_metal >= 1 )
    				if ( nc_netch_leather >= 1 )
    					if ( nc_bonemeal >= 10 )
    						removeitem "nc_bosmer_pauldron_sk" 1
    						removeitem "nc_bosmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
    						removeitem "ingred_netch_leather_01" 1
    						removeitem "ingred_bonemeal_01" 8
    						Player->RemoveItem "nc_SoulGem_blackened_f" 1
    						Cast "nc_summon_effects" Player
    						set state to 1
    					else
    						MessageBox "You need 8 portions of bonemeal to perform the ritual."
    						set state to 2
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 2
    				endif
    			else
    				MessageBox "You need a piece of netch leather to perform the ritual."
    				set state to 2
    			endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to "nc_cauldron"->getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to "nc_cauldron"->getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to "nc_cauldron"->getItemCount "ingred_bonemeal_01"
    set nc_skull to "nc_cauldron"->getItemCount "nc_bosmer_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				"nc_cauldron"->removeitem "nc_bosmer_pauldron_sk" 1
                "nc_cauldron"->removeitem "nc_bosmer_skull" 1
                "nc_cauldron"->removeitem "ingred_scrap_metal_01" 1
                "nc_cauldron"->removeitem "ingred_netch_leather_01" 1
                "nc_cauldron"->removeitem "ingred_bonemeal_01" 8
                set state to 1
             else
                	MessageBox "Вам нужно иметь 8 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			MessageBox "You need a Bosmer skull to perform the ritual."
    			set state to 2
    		endif
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "You need a Bosmer skeleton to perform the ritual."
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Босмера для этого ритуала."
#endif /* ABCD */
    		set state to 2
#ifndef ABCD
    	endif
#else /* ABCD */
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 1 )
    		if ( nc_pclevel <= 10 )
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_wood elf" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
#endif /* ABCD */
    			PlaySound, "ThunderClap"
#ifndef ABCD
    			PlaceAtPC "nc_wood elf" 1, 762, 0
#else /* ABCD */
    			"nc_rev_placer"->PlaceAtMe "nc_wood elf_2" 1, 0, 0
#endif /* ABCD */
    			set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_pclevel <= 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_wood elf_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_wood elf_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
#else /* ABCD */
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_wood elf_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
#ifndef ABCD
    		set state to 2
#endif /* ! ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_bosmer
    	endif
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_bosmer
    endif
#endif /* ABCD */
    
    end

