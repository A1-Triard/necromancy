#ifndef ABCD
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEW5jX2Zyb3N0X3BhdWxkcm9uAQAHAQEAEDwASXQgaXMgdG9vIGF3a3dhcmQgdG8gY2FycnkgbW9yZSB0aGFuIG9uZSBza2VsZXRvbiBhdCBhIHRpbWUuAAAJAQkBAQE=
