SCDT 
#endif /* ABCD */
SCTX
    begin nc_summon_argonian
    
#ifndef ABCD
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_skull
    	short nc_skl
    	short state
    
