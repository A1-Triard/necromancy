#endif /* ABCD */
SCVR counter;;
#ifndef ABCD
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEG5jX2ZpcmVfcGF1bGRyb24BAAcBAQAQPABJdCBpcyB0b28gYXdrd2FyZCB0byBjYXJyeSBtb3JlIHRoYW4gb25lIHNrZWxldG9uIGF0IGEgdGltZS4AAAkBCQEBAQ==
#else /* ABCD */
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEG5jX2ZpcmVfcGF1bGRyb24BAAcBAQAQLgDN5fHy6CDh7uv8+OUsIPfl7CDu5O3uIPLl6+4g5+Ag8ODnIC0g7eXz5O7h7e4uAAAJAQkBAQE=
