SCHD nc_raise_z_script 1 0 1 83 19
SCVR castonce;ringtimer;;
SCDT BgEDCSBzAQAgPT0gMK0REG5jX3JhaXNlX3pfc3BlbGzbEAUBcwEAAiAxCQEGAQEIIGYBACA+IDOrEQEABwEBBQFmAQAKIGYBACBYEhAgKwkBAQE=
SCTX
    Begin nc_raise_z_script
    
    float ringtimer
    
    short castonce
    
    if ( castonce == 0 )
    	ExplodeSpell "nc_raise_z_spell"
    	Disable
    	set castonce to 1
    endif
    
    if ( ringtimer > 3 )
    	SetDelete 1
    else
    	set ringtimer to ( ringtimer + GetSecondsPassed )
    endif
    
    End 

SCPT
SCHD nc_raise_zombie_script 2 3 0 2751 30
SCVR state;brnpc;npcref;temp;npcid;;
SCDT 
SCTX
    begin nc_raise_zombie_script
    
    long npcref
    long temp
    short state
    long npcid
    short brnpc
    
    if ( MenuMode )
    	return
    endif
    
    if ( Player->GetSpellEffects "nc_raise_zombie" == 0 )
    	set state to 1
    endif
    
    ifx ( state )
    	set state to 0
    	set temp to 0
    	StopScript nc_raise_zombie_script
    	Player->RemoveSpellEffects "nc_raise_zombie"
    else
    	setx npcref to xGetPCTarget
    
    	if ( npcref == 0 )
    		return
    	endif
    
    	setx temp to npcref->xRefType
    	if ( temp != 1598246990 )
    		return
    	endif
    
    	xsetref npcref
    	set temp to GetHealth
    	if ( temp != 0 )
    		return
    	endif
    
    	setx npcid to npcref->xGetBaseID
    
    	set brnpc to 0
    
    	setx temp to xStringCompare npcid "nc_arg_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_bret_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_de_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_he_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_imp_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_khajiit_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_nord_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_orc_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_rg_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	setx temp to xStringCompare npcid "nc_we_skelly_NPC"
    	if ( temp == 0 );equal
    		set brnpc to 1
    	endif
    
    	if ( brnpc == 1 )
    		MessageBox "You cannot raise a skeleton."
    		return
    	endif
    
    	setx temp to xStringCompare npcid "nc_argonian_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_breton_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_dark elf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_high elf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_imperial_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_khajiit_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_nord_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_orc_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_redguard_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_wood elf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_argonianf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_bretonf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_dark elff_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_high elff_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_imperialf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_khajiitf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_nordf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_orcf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_redguardf_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	setx temp to xStringCompare npcid "nc_wood elff_zombie"
    	if ( temp == 0 );equal
    		set brnpc to 2
    	endif
    
    	if ( brnpc == 2 );should be a dead zombie
    		MessageBox "A zombie can only be raised once.  Perhaps it will make a good revenant."
    		return
    	endif
    
    	if ( ScriptRunning "nc_npc_zombie_script" == 0 )
    		set temp to 1
    	endif
    
    	ifx ( temp )
    		xSetRef npcref
    		StartScript nc_npc_zombie_script
    		xSetRef npcref
    		PlaceAtMe "nc_raise_z_ring" 1 1 1
    		set state to 1
    	endif
    endif
    
    
    end

SCPT
#endif /* ! ABCD */
SCHD nc_random_kill 3 3 0 192 50
SCVR OnPCEquip;doonce;call;prospectref;prospectid;temp;;
SCDT BgEBCSBYIBAgPT0gMSQBCQEaPBM4AAATOGwAAjwTOAAAEzhsAAA8DzgEAAw4rQAFAXMDAAIgMRM4AAATOGwAADwYPAg8EzgCABM4bAACPAYBARIgbAMAICE9IDE1OTgyNDY5OTAFAXMDAAIgMAkBEzgCABM4cwAAPA84BAAMOJMAEzgAABM4bAAAPB08jRAAAAAAEzgAABM4bAAAPBs8EzgAABM4bAACPAo4HQAcEA5uY19yYW5kb21fa2lsbAEB
SCTX
    begin nc_random_kill
    
    short OnPCEquip
    short doonce
    short call ; status variable - should this NPC be called?
    long prospectref ; reference to an NPC (or creature)
    long prospectid ; string holding the NPC's id
    long temp ; used to control ifx statements
    
    
    if ( menumode == 1 )
        return
    endif
    
    ; Not equipped, abort
    ;if ( OnPCEquip == 0 )
    ;    return
    ;endif
    
    ;set OnPCEquip to 0
    
    ; Must be called outside the loop, else the loop freezes
    setx prospectref to xFirstNPC
    
    whilex ( prospectref )
        ; loop through each of the NPCs and creatures in the cell
    
        ; start by assuming we want to call this one
        set call to 1
    	
    
        ; check the type because we don't want creatures (Non-NPCs)
        setx temp to prospectref->xRefType
        if ( temp != 1598246990 ) ; it's not an NPC
            set call to 0
        endif
    
    
    
        ; if call is still 1 we want to call this NPC to the player
        ifx ( call ) ; we use extended functions inside so need ifx		  
          xGetRef prospectref
    		sethealth 0
    
    
        endif
        ; The MWSE docs example script has a typo here.
        ; prospectref, not prospect
        setx prospectref to xNextRef prospectref
    
    
    ;temp->xstartcombat "player"
    
    endwhile
    stopscript nc_random_kill
    
    ; this only needs to run for one frame
    
    
    
    end
    

SCPT
SCHD nc_real_startscript 0 0 1 182 6
SCVR timer;;
SCDT BQFmAQAKIGYBACBYEhAgKwYBCgkgZgEAID49IDIGAQMWIFgdECBtCm5jX3Bvc3RtYW4gPT0gMAYBARcgWM0QIGQMbmNfbmVjcm9fMDAxIDwgNRsQCm5jX3Bvc3RtYW4JAQkBBgEBGiBYHRAgbQ5uY19zdGFydHNjcmlwdCA9PSAwGxAObmNfc3RhcnRzY3JpcHQJAQUBZgEAAiAwHBATbmNfcmVhbF9zdGFydHNjcmlwdAkBAQE=
SCTX
    begin nc_real_startscript
    
    float timer
    
    set timer to ( timer + GetSecondsPassed )
    if ( timer >= 2 )
    	
    	if ( ScriptRunning "nc_postman" == 0 )
    		if ( GetJournalIndex "nc_necro_001" < 5 )
    			startscript nc_postman
    		endif
    	endif
    
    	if ( ScriptRunning "nc_startscript" == 0 )
    		startscript nc_startscript
    	endif
    	set timer to 0
    	stopscript nc_real_startscript
    endif
    
    
    
    end

SCPT
#ifndef ABCD
SCHD nc_rg_corpse_cure 7 1 0 703 63
SCVR temp;DoOnce1;state;currentday;movingday;getcell;counter;mycell;;
SCDT 
#else /* ABCD */
SCHD nc_rg_corpse_cure 5 0 0 559 43
SCVR state;currentday;movingday;getcell;counter;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_rg_corpse_cure
    
#ifndef ABCD
    long mycell
    short temp
    short DoOnce1
#endif /* ! ABCD */
    short state
    short currentday
    short movingday
    short getcell
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	set getcell to 1
#else /* ABCD */
    	set getcell to 0
#endif /* ABCD */
    endif
    
#ifndef ABCD
    ifx ( getcell )
#else /* ABCD */
    if ( getcell == 0 )
#endif /* ABCD */
    
#ifndef ABCD
    set DoOnce1 to 1
    
    ifx ( DoOnce1 )
    	setx mycell to xMyCellID
    
    	setx temp to xStringCompare mycell "Район Горького Берега"
    	set DoOnce1 to 0
    endif
#else /* ABCD */
    	if ( state == 1 )
    		disable
    		player-> additem "nc_rg_pauldron" 1
    		return
    	endif
#endif /* ABCD */
    
#ifndef ABCD
    if ( temp != 0 )
    	if ( state == 0 )
#else /* ABCD */
    	if ( GetPCCell "Район Горького Берега" == 0 )
#endif /* ABCD */
    		if ( nc_has_corpse == 1 )
    			MessageBox "Нести больше, чем одно тело за раз - неудобно."
#ifndef ABCD
    			set getcell to 0
    			set temp to 0
#else /* ABCD */
    			set getcell to 1
#endif /* ABCD */
    			return
    		else
    			MessageBox "Здесь тело будет очень долго оставаться нетронутым."
#ifndef ABCD
    			disable
    			player-> additem "nc_rg_pauldron" 1
#endif /* ! ABCD */
    			set state to 1
    		endif
#ifdef ABCD
    	else
    		if ( state == 0 )
    			MessageBox "Через пару дней от тела останется лишь скелет."
    			set state to 2
    		endif
#endif /* ABCD */
    	endif
#ifndef ABCD
    else
    	if ( state == 0 )
    		MessageBox "Через пару дней от тела останется лишь скелет."
    		set state to 2
    	endif
    endif
    
    set getcell to 0
    
    endif
#endif /* ! ABCD */
    
#ifdef ABCD
    	set getcell to 1
#endif /* ABCD */
    
#ifndef ABCD
    if ( state == 1 )
    	return
#endif /* ! ABCD */
    endif
    
    if ( state == 2 )
    	if ( currentday != Day )
    		set currentday to Day
    		set movingday to ( movingday + 1 )
    	endif
    
    	if ( movingday == 3 )
    		PlaceAtMe "nc_redguard_skelly" 1 1 1
    		PlaceAtMe "mudcrab" 1 100 0
    		disable
    		set state to 1
    	endif
    endif
    
    end

SCPT
SCHD nc_rg_corpse_drop 2 0 0 104 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEA5uY19yZ19wYXVsZHJvbgEAAACAPwEA2xAFAXMBAAIgMAkBAQE=
SCTX
    begin nc_rg_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_rg_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

SCPT
#ifndef ABCD
SCHD nc_rg_skelly_drop 1 0 0 194 8
#else /* ABCD */
SCHD nc_rg_skelly_drop 1 0 0 182 8
#endif /* ABCD */
SCVR counter;;
#ifndef ABCD
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEW5jX3JnX3BhdWxkcm9uX3NrAQAHAQEAEDwASXQgaXMgdG9vIGF3a3dhcmQgdG8gY2FycnkgbW9yZSB0aGFuIG9uZSBza2VsZXRvbiBhdCBhIHRpbWUuAAAJAQkBAQE=
#else /* ABCD */
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQEW5jX3JnX3BhdWxkcm9uX3NrAQAHAQEAEDAAzeXx8ugg4e7r/PjlLCD35ewg7uTo7SDx6uXr5fIg5+Ag8ODnIC0g7eXz5O7h7e4uAAAJAQkBAQE=
#endif /* ABCD */
SCTX
    begin nc_rg_skelly_drop
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_rg_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_rg_skelly_pauldron 4 0 1 652 40
#else /* ABCD */
SCHD nc_rg_skelly_pauldron 4 0 1 629 40
#endif /* ABCD */
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_rg_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_rg_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_rg_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_rg_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

SCPT
#ifndef ABCD
SCHD nc_ritual_book_script 16 1 0 2737 86
SCVR soultype;button;state;alt;arg;bos;bre;dun;imp;kha;nor;orc;red;warf;warfr;warp;contref;;
SCDT 
#else /* ABCD */
SCHD nc_ritual_book_script 5 0 2 1554 61
SCVR state;button;pc_conjuration;pc_alteration;start;timer;timer2;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_ritual_book_script
    
#ifndef ABCD
    short soultype ;1-altmer, 2-argonian, 3-bosmer, 4-breton, 5-dunmer, 6-imperial, 7-khajiit, 8-nord, 9-orc, 10-redguard, 11-warlord
    short button
#endif /* ! ABCD */
    short state
#ifndef ABCD
    short alt
    short arg
    short bos
    short bre
    short dun
    short imp
    short kha
    short nor
    short orc
    short red
    short warf
    short warfr
    short warp
    
    long contref
#else /* ABCD */
    short button
    short pc_conjuration
    short pc_alteration
    short start
    float timer
    float timer2
#endif /* ABCD */
    
#ifndef ABCD
    if ( MenuMode == 1 )
    	Return
#else /* ABCD */
    if ( menumode == 1 )
    	return
#endif /* ABCD */
    endif
    
    if ( OnActivate == 1 )
#ifndef ABCD
    	if ( nc_isnecro == 0 )
    		MessageBox "Only a well-trained summoner may use this book."
    		Return
    	else
    		if ( GetJournalIndex "nc_necro_001" < 70 )
    			MessageBox "Only a well-trained summoner may use this book."
    			Return
    		else
    			if ( Player->GetItemCount "nc_SoulGem_blackened_f" == 0 )
    				MessageBox "You must have a filled blackened soulgem to summon a revenant."
    				Return
#else /* ABCD */
    	player-> addspell "nc_summon"
    	if ( ScriptRunning "nc_spell_timer" == 0 )
    		startscript nc_spell_timer
    	endif
    	MessageBox "После прочтения заклинаний из книги вы готовы вызвать мертвеца."
    endif
    
    set timer to ( timer + GetSecondsPassed )
    if ( timer < 2 )
    	if ( state == 0 )  ; ADDED
    		return
    	endif  ; ADDED
    endif
    set timer to 0
    
    set pc_conjuration to ( Player-> GetConjuration )
    set pc_alteration to ( Player-> GetAlteration )
    
    if (  nc_summon_spell == 1 )
    	if ( state == 0 )  ; ADDED
    		set start to 1
    	endif  ; ADDED
    else
    	set start to 0
    endif
    
    if ( start == 1 )
    	set start to 0  ; ADDED
    	set nc_summon_spell to 0  ; MOVED HERE
    	if ( GetJournalIndex "nc_necro_001" >= 70 )
    		if ( nc_isnecro == 1 )
    			if ( pc_conjuration >= 30 )
    				if ( pc_alteration >= 20 )
    					set state to 1
    				else
    					MessageBox "Вы недостаточно знаете искусство изменения."
    					;set nc_summon_spell to 0  REMOVED
    					return
    				endif
#endif /* ABCD */
    			else
#ifndef ABCD
    				set state to 1
#else /* ABCD */
    				MessageBox "Вы недостаточно знаете искусство колдовства."
    				;set nc_summon_spell to 0  REMOVED
    				return
#endif /* ABCD */
    			endif
#ifdef ABCD
    		else
    			MessageBox "Только опытный колдун может использовать эту книгу."
    			;set nc_summon_spell to 0  REMOVED
    			return
#endif /* ABCD */
    		endif
#ifdef ABCD
    	else
    		MessageBox "Только опытный колдун может использовать эту книгу."
    		;set nc_summon_spell to 0  REMOVED
    		return
#endif /* ABCD */
    	endif
    endif
    
    if ( state == 1 )
#ifndef ABCD
    	set alt to 0
    	set arg to 0
    	player -> PositionCell -2, 4510, -389, 0 "Thanatos Temple"
#else /* ABCD */
    	player -> PositionCell -2, 4510, -389, 0 "Храм Танатоса"
#endif /* ABCD */
    	disableplayerlooking
#ifndef ABCD
    	if ( ScriptRunning "nc_spell_timer" == 0 )
    		StartScript "nc_spell_timer"
    	endif
    	MessageBox "What type of Revenant do you wish to bring forth?", "High Elf", "Argonian", "Wood Elf", "Breton", "Dark Elf", "More"
#else /* ABCD */
    	MessageBox "Какого мертвеца вы хотите создать?", "Альтмер", "Аргонианин", "Босмер", "Бретон", "Данмер", "Больше"
#endif /* ABCD */
    	set state to 2
    elseif ( state == 2 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;altmer
#ifndef ABCD
    		set alt to 1
#else /* ABCD */
    		set nc_ritual to 1
#endif /* ABCD */
    		set state to 0
    	elseif ( button == 1 ) ;argonian
#ifndef ABCD
    		set arg to 1
#else /* ABCD */
    		set nc_ritual to 2
#endif /* ABCD */
    		set state to 0
    	elseif ( button == 2 ) ;bosmer
#ifndef ABCD
    		set bos to 1
#else /* ABCD */
    		set nc_ritual to 3
#endif /* ABCD */
    		set state to 0
    	elseif ( button == 3 ) ;breton
#ifndef ABCD
    		set bre to 1
#else /* ABCD */
    		set nc_ritual to 4
#endif /* ABCD */
    		set state to 0
    	elseif ( button == 4 ) ;dunmer
#ifndef ABCD
    		set dun to 1
#else /* ABCD */
    		set nc_ritual to 5
#endif /* ABCD */
    		set state to 0
    	elseif ( button == 5 ) ;more
    		set state to 3
    	endif
    elseif ( state == 3 )
#ifndef ABCD
    	MessageBox "What type of Revenant do you wish to bring forth?", "Imperial", "Khajiit", "Nord", "Orc", "Redguard", "Revenant Warlord", "Nothing"
#else /* ABCD */
    	MessageBox "Какого мертвеца вы хотите создать?", "Имперец", "Хаджит", "Норд", "Орк", "Редгард", "Мумифицированный слуга", "Ничего"
#endif /* ABCD */
    	set state to 4
    elseif ( state == 4 )
    	set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;imperial
#ifndef ABCD
    		set imp to 1
#else /* ABCD */
    		set nc_ritual to 6
#endif /* ABCD */
    		set state to 0
    	elseif ( button == 1 ) ;khajiit
#ifndef ABCD
    		set kha to 1
    		set state to 0
    	elseif ( button == 2 ) ;nord
    		set nor to 1
    		set state to 0
    	elseif ( button == 3 ) ;orc
    		set orc to 1
    		set state to 0
    	elseif ( button == 4 ) ;redguard
    		set red to 1
    		set state to 0
    	elseif ( button == 5 ) ;warlord
    		set state to 5
    	elseif ( button == 6 ) ;none
    		set state to 0
    		enableplayerlooking
    		return
    	endif
    elseif ( state == 5 )
    	if ( nc_necro_skill >= 29 )
    		MessageBox "What type of Warlord do you wish to bring forth?", "Fire Elemental", "Frost Elemental", "Poison Elemental", "None"
    		set state to 6
    	elseif ( nc_necro_skill < 29 )
    		MessageBox "You are not yet experienced enough to summon a Revenant Warlord."
#else /* ABCD */
    		set nc_ritual to 7
#endif /* ABCD */
    		set state to 0
#ifndef ABCD
    		EnablePlayerLooking
    	endif
    elseif ( state == 6 )
    		set button to GetButtonPressed
    	if ( button == -1 )
    		return
    	elseif ( button == 0 ) ;fire
    		set warf to 1
#else /* ABCD */
    	elseif ( button == 2 ) ;nord
    		set nc_ritual to 8
#endif /* ABCD */
    		set state to 0
#ifndef ABCD
    	elseif ( button == 1 ) ;frost
    		set warfr to 1
#else /* ABCD */
    	elseif ( button == 3 ) ;orc
    		set nc_ritual to 9
#endif /* ABCD */
    		set state to 0
#ifndef ABCD
    	elseif ( button == 2 ) ;poison
    		set warp to 1
#else /* ABCD */
    	elseif ( button == 4 ) ;redguard
    		set nc_ritual to 10
#endif /* ABCD */
    		set state to 0
#ifndef ABCD
    	elseif ( button == 3 ) ;none
#else /* ABCD */
    	elseif ( button == 5 ) ;warlord
    		set nc_ritual to 11
    		set state to 0
    	elseif ( button == 6 ) ;none
#endif /* ABCD */
    		set state to 0
    		enableplayerlooking
#ifdef ABCD
    		set nc_summon_spell to 0
#endif /* ABCD */
    		return
    	endif
    endif
    
#ifndef ABCD
    ifx ( alt )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_altmer"
    	set alt to 0
    endif 
    
    ifx ( arg )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_argonian"
    	set arg to 0
    endif 
    
    ifx ( bos )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_bosmer"
    	set bos to 0
    endif 
    
    ifx ( bre )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_breton"
    	set bre to 0
    endif 
    
    ifx ( dun )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_dunmer"
    	set dun to 0
    endif 
    
    ifx ( imp )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_imperial"
    	set imp to 0
    endif 
    
    ifx ( kha )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_khajiit"
    	set kha to 0
    endif 
    
    ifx ( nor )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_nord"
    	set nor to 0
    endif 
    
    ifx ( orc )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_orc"
    	set orc to 0
    endif 
    
    ifx ( red )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_redguard"
    	set red to 0
    endif 
    
    ifx ( warf )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_fire"
    	set warf to 0
    endif 
    
    ifx ( warfr )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_frost"
    	set warfr to 0
    endif 
    
    ifx ( warp )
    	setx contref to xGetRef "nc_cauldron"
    	xSetRef contref
    	StartScript "nc_summon_poison"
    	set warp to 0
    endif 
    
#endif /* ! ABCD */
    end

