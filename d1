    	if ( doOnce4 == 0 )
    		MessageBox "Чары распались."
    		sethealth 0
    		set doOnce4 to 1
    	endif
    endif
    
    ;if ( OnDeath = 1 )
    if ( GetHealth <= 5 )
    	if ( doOnce3 == 0 )
    		PlaySound, "ThunderClap"
    		;PlaceAtMe "nc_nord_skelly_NPC" 1 1 1
    		;PlaceAtMe "nc_nord_skull" 1 25 2
    		;disable
    		set doOnce3 to 1
    	endif
    endif
    
    end

