SCHD nc_we_corpse_drop 2 0 0 108 17
SCVR OnPCDrop;counter;;
SCDT BgEGCSBY3BAgPT0gMQUBcwIACCBzAgAgMSArBgEBCSBzAgAgPCAxMCQBCQGrEQEAJAEJAQYBAwkgcwEAID09IDHmEBJuY19ib3NtZXJfcGF1bGRyb24BAAAAgD8BANsQBQFzAQACIDAJAQEB
SCTX
    begin nc_we_corpse_drop
    
    short OnPCDrop
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnPCDrop == 1 )
    	PlaceAtPC "nc_bosmer_pauldron" 1 1 1
    	disable
    	set OnPCDrop to 0
    endif
    	
    end

SCPT
#ifndef ABCD
SCHD nc_we_skelly_drop 1 0 0 198 8
#else /* ABCD */
SCHD nc_we_skelly_drop 1 0 0 186 8
#endif /* ABCD */
SCVR counter;;
#ifndef ABCD
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX2Jvc21lcl9wYXVsZHJvbl9zawEABwEBABA8AEl0IGlzIHRvbyBhd2t3YXJkIHRvIGNhcnJ5IG1vcmUgdGhhbiBvbmUgc2tlbGV0b24gYXQgYSB0aW1lLgAACQEJAQEB
#else /* ABCD */
SCDT BgEGCSBY3BAgPT0gMQUBcwEACCBzAQAgMSArBgEBCSBzAQAgPCAxMCQBCQGrEQEAJAEJAQYBBgUgWBgQAAYBAhUgRw1uY19oYXNfY29ycHNlID09IDDbEAwBBnBsYXllctQQFW5jX2Jvc21lcl9wYXVsZHJvbl9zawEABwEBABAwAM3l8fLoIOHu6/z45Swg9+XsIO7k6O0g8erl6+XyIOfgIPDg5yAtIO3l8+Tu4e3uLgAACQEJAQEB
#endif /* ABCD */
SCTX
    begin nc_we_skelly_drop
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_bosmer_pauldron_sk" 1
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    	endif
    endif
    
    end

SCPT
#ifndef ABCD
SCHD nc_we_skelly_pauldron 4 0 1 660 40
#else /* ABCD */
SCHD nc_we_skelly_pauldron 4 0 1 637 40
#endif /* ABCD */
SCVR OnPCEquip;doonce;OnPCDrop;counter;timer;;
#ifndef ABCD
SCDT 
#else /* ABCD */
SCDT 
#endif /* ABCD */
SCTX
    begin nc_we_skelly_pauldron
    
    short OnPCEquip
    short doonce
    float timer
    short OnPCDrop
    short counter
    
    if ( menumode )
    	return
    endif
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate == 1 )
    	if ( nc_has_corpse == 0 )
    		activate
    	else
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
    		return
    	endif
    endif
    
    if ( Player->GetItemCount "nc_bosmer_pauldron_sk" >=1 )
    	set nc_has_corpse to 1
    	if ( OnPCEquip == 0 )
    		set timer to ( timer + GetSecondsPassed )
    		if ( timer >= 1.5 )
    			Player-> equip "nc_bosmer_pauldron_sk"
    			set OnPCEquip to 1
    			set timer to 0
    		endif
    	elseif ( OnPCEquip == 1 )
    		if ( doonce == 0 )
    			MessageBox "Нести скелет в инвентаре слишком тяжело. Поэтому вы перебрасываете его через плечо."
    			startscript nc_time_bounty
    			set doonce to 1
    		endif
    		DisablePlayerJumping
    		DisablePlayerFighting
    		if ( GetPCRunning )
    			set timer to ( timer + GetSecondsPassed )
    			if ( timer >= 1 )
    				ModCurrentFatigue -2
    				set timer to 0
    			endif
    		endif
    	endif
    endif
    
    if ( OnPCDrop == 1 )
    	set doonce to 0
    	set nc_has_corpse to 0
    	EnablePlayerJumping
    	EnablePlayerFighting
    	stopscript nc_time_bounty
    	PlaceAtPC "nc_we_skelly_NPC" 1 50 1
    	Disable
    	set OnPCDrop to 0
    endif
    
    end
    
    

