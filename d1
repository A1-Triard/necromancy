SCHD nc_loop_test 1 2 0 312 21
SCVR counter;npcref;npcid;;
SCDT 
SCTX
    begin nc_loop_test
    
    long npcref
    long npcid
    short counter
    
    
    if ( menumode )
    	return
    endif
#else /* ABCD */
SCHD nc_level 1 0 0 49 9
SCVR nc_level;;
SCDT BQFzAQAFIFhXEQAAEBAAzO7pIPPw7uLl7fwgLSAlZwFzAQAAHBAIbmNfbGV2ZWwBAQ==
SCTX
    Begin nc_level
#endif /* ABCD */
    
#ifndef ABCD
    set counter to ( counter + 1 )
    if ( counter < 15 )
    	return
    endif
#endif /* ! ABCD */
    
#ifndef ABCD
    set counter to 0
#else /* ABCD */
    short nc_level
#endif /* ABCD */
    
#ifndef ABCD
    	ifx ( npcref )
    		setx npcref to xNextRef npcref
    		setx npcid to npcref->xRefID
    		xMessageFix "checking %s" npcid
    		MessageBox "hee hee hee hee hee hee hee hee hee"
    	else
    		setx npcref to xFirstNPC
    		setx npcid to npcref->xRefID
    		xMessageFix "first: checking %s" npcid
    		MessageBox "hee hee hee hee hee hee hee hee hee"
    	endif
#else /* ABCD */
    set nc_level to ( Getlevel )
#endif /* ABCD */
    
#ifdef ABCD
    messagebox "Мой уровень - %g", nc_level
#endif /* ABCD */
    
#ifdef ABCD
    stopscript nc_level
#endif /* ABCD */
    
#ifndef ABCD
    end
#else /* ABCD */
    End
#endif /* ABCD */

