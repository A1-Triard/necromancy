    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							elseif ( nc_rg_skull1 >= 1 )
    								removeitem "nc_redguard_skull" 1
    								removeitem "ingred_scrap_metal_01" 1
    								removeitem "ingred_netch_leather_01" 1
    								removeitem "ingred_bonemeal_01" 10
    								removeitem "misc_clothbolt_01" 1
    								removeitem "nc_frost_pauldron" 1
    								Player->RemoveItem "nc_SoulGem_blackened_f" 1
    								set state to 2
    							else
    								MessageBox "You need a skull to perform the ritual."
    								set state to 10
    							endif
    						else
    							MessageBox "You need some bonemeal to perform the ritual."
    							set state to 10
    						endif
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_cloth >= 1 )
    		if ( nc_scrap_metal >= 1 )
    			if ( nc_netch_leather >= 1 )
    				if ( nc_bonemeal >= 10 )
    					if ( nc_alt_skull1 >= 1 )
    	            			removeitem "nc_altmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_arg_skull1 >= 1 )
    	            			removeitem "nc_argonian_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_bos_skull1 >= 1 )
    	            			removeitem "nc_bosmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_bret_skull1 >= 1 )
    	            			removeitem "nc_breton_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_imp_skull1 >= 1 )
    	            			removeitem "nc_imperial_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_dun_skull1 >= 1 )
    	            			removeitem "nc_dunmer_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_kha_skull1 >= 1 )
    	            			removeitem "nc_khajiit_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_nord_skull1 >= 1 )
    	            			removeitem "nc_nord_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_orc_skull1 >= 1 )
    	            			removeitem "nc_orc_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
    					elseif ( nc_rg_skull1 >= 1 )
    	            			removeitem "nc_redguard_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
              	  			removeitem "ingred_netch_leather_01" 1
                				removeitem "ingred_bonemeal_01" 10
                				removeitem "misc_clothbolt_01" 1
                				removeitem "nc_frost_cured" 1
                				set state to 2
#endif /* ABCD */
    					else
#ifndef ABCD
    						MessageBox "You need a piece of netch leather to perform the ritual."
#else /* ABCD */
    	            			MessageBox "Вам нужен череп для этого ритуала."
#endif /* ABCD */
    						set state to 10
    					endif
    				else
#ifndef ABCD
    					MessageBox "You need a piece of scrap metal to perform the ritual."
#else /* ABCD */
    					MessageBox "Вам нужно некоторое количество костяной муки для этого ритуала."
#endif /* ABCD */
    					set state to 10
    				endif
    			else
#ifndef ABCD
    				MessageBox "You need a bolt of blue cloth to perform the ritual."
#else /* ABCD */
    				MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
#endif /* ABCD */
    				set state to 10
    			endif
    		else
#ifndef ABCD
    			MessageBox "You need a cured fire corpse to perform the ritual."
#else /* ABCD */
    			MessageBox "Вам нужны обрезки металла для этого ритуала."
#endif /* ABCD */
    			set state to 10
    		endif
#ifdef ABCD
    	else
    		MessageBox "Вам нужен рулон голубой ткани для этого ритуала."
    		set state to 10
#endif /* ABCD */
    	endif
#ifdef ABCD
    endif
#endif /* ABCD */
    
#ifndef ABCD
    
    	if ( state == 2 )
#else /* ABCD */
    if ( state == 2 )
#endif /* ABCD */
    		PlaySound, "ThunderClap"
#ifndef ABCD
    		PlaceAtPC "nc_fr_warlord" 1, 762, 0
    		Cast "nc_summon_effects" Player
#else /* ABCD */
    		"nc_rev_placer"->PlaceAtMe "nc_fr_warlord" 1, 0, 0
#endif /* ABCD */
    		set nc_fr_warlord_glob to 1
    		if ( GetJournalIndex "nc_necro_001" <= 100 )
    			Journal "nc_necro_001" 105 
    		endif
    		set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    		MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		set state to 10
    	endif
#else /* ABCD */
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	set state to 10
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 10 )
    		enableplayerlooking
    		set state to 0
    		stopscript nc_summon_frost
    	endif
#else /* ABCD */
    if ( state == 10 )
    	enableplayercontrols
    	enableplayerlooking
    	set state to 0
    	stopscript nc_summon_frost
    endif
#endif /* ABCD */
    
    end

SCPT
#ifndef ABCD
SCHD nc_summon_imperial 7 0 0 1366 77
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;nc_skl;state;;
SCDT 
#else /* ABCD */
SCHD nc_summon_imperial 6 0 0 1216 70
SCVR nc_pclevel;nc_scrap_metal;nc_netch_leather;nc_bonemeal;nc_skull;state;;
SCDT 
#endif /* ABCD */
SCTX
    begin nc_summon_imperial
    
#ifndef ABCD
    	short nc_pclevel
    	short nc_scrap_metal
    	short nc_netch_leather
    	short nc_bonemeal
    	short nc_skull
    	short nc_skl
    	short state
    
    	set nc_pclevel to nc_necro_skill
    	set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    	set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    	set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    	set nc_skull to getItemCount "nc_imperial_skull"
    	set nc_skl to getItemCount "nc_imperial_pauldron_sk"
    
    	if ( nc_skl >= 1 )
    		if ( nc_skull >= 1 )
    			if ( nc_scrap_metal >= 1 )
    				if ( nc_netch_leather >= 1 )
    					if ( nc_bonemeal >= 10 )
    						removeitem "nc_imperial_pauldron_sk" 1
    						removeitem "nc_imperial_skull" 1
    						removeitem "ingred_scrap_metal_01" 1
    						removeitem "ingred_netch_leather_01" 1
    						removeitem "ingred_bonemeal_01" 10
    						Player->RemoveItem "nc_SoulGem_blackened_f" 1
    						Cast "nc_summon_effects" Player
    						set state to 1
    					else
    						MessageBox "You need 10 portions of bonemeal to perform the ritual."
    						set state to 2
    					endif
    				else
    					MessageBox "You need a piece of scrap metal to perform the ritual."
    					set state to 2
    				endif
    			else
    				MessageBox "You need a piece of netch leather to perform the ritual."
    				set state to 2
    			endif
#else /* ABCD */
    short nc_pclevel
    short nc_scrap_metal
    short nc_netch_leather
    short nc_bonemeal
    short nc_skull
    short state
    
    set nc_pclevel to nc_necro_skill
    set nc_scrap_metal to getItemCount "ingred_scrap_metal_01"
    set nc_netch_leather to getItemCount "ingred_netch_leather_01"
    set nc_bonemeal to getItemCount "ingred_bonemeal_01"
    set nc_skull to getItemCount "nc_imperial_skull"
    
    
    if ( nc_skull >= 1 )
    	if ( nc_scrap_metal >= 1 )
       	if ( nc_netch_leather >= 1 )
          	if ( nc_bonemeal >= 10 )
    				removeitem "nc_imperial_pauldron_sk" 1
                removeitem "nc_imperial_skull" 1
                removeitem "ingred_scrap_metal_01" 1
                removeitem "ingred_netch_leather_01" 1
                removeitem "ingred_bonemeal_01" 10
                set state to 1
             else
                	MessageBox "Вам нужно иметь 10 порций костяной муки для этого ритуала."
                   set state to 2
             endif
#endif /* ABCD */
    		else
#ifndef ABCD
    			MessageBox "You need an Imperial skull to perform the ritual."
    			set state to 2
    		endif
#else /* ABCD */
             	MessageBox "Вам нужны обрезки металла для этого ритуала."
                set state to 2
          endif
#endif /* ABCD */
    	else
#ifndef ABCD
    		MessageBox "You need an Imperial skeleton to perform the ritual."
#else /* ABCD */
          	MessageBox "Вам нужен кусок кожи нетча для этого ритуала."
             set state to 2
       endif
    else
    		MessageBox "Вам нужен череп Имперца для этого ритуала."
#endif /* ABCD */
    		set state to 2
#ifndef ABCD
    	endif
#else /* ABCD */
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 1 )
    		if ( nc_pclevel <= 10 )
#else /* ABCD */
    if ( state == 1 )
    	if ( nc_pclevel <= 10 )
    		PlaySound, "ThunderClap"
    		"nc_rev_placer"->PlaceAtMe "nc_imperial" 1, 0, 0
    		set nc_necro_skill to ( nc_necro_skill + 1 )
    		MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    	elseif ( nc_pclevel > 10 )
    		if ( nc_pclevel <= 20 )
#endif /* ABCD */
    			PlaySound, "ThunderClap"
#ifndef ABCD
    			PlaceAtPC "nc_imperial" 1, 762, 0
#else /* ABCD */
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_2" 1, 0, 0
#endif /* ABCD */
    			set nc_necro_skill to ( nc_necro_skill + 1 )
#ifndef ABCD
    			MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    		elseif ( nc_pclevel > 10 )
    			if ( nc_pclevel <= 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_imperial_2" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			elseif ( nc_pclevel > 20 )
    				PlaySound, "ThunderClap"
    				PlaceAtPC "nc_imperial_3" 1, 762, 0
    				set nc_necro_skill to ( nc_necro_skill + 1 )
    				MessageBox "Your Necromancy skill has increased to %g.", nc_necro_skill
    			endif
#else /* ABCD */
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
    		elseif ( nc_pclevel > 20 )
    			PlaySound, "ThunderClap"
    			"nc_rev_placer"->PlaceAtMe "nc_imperial_3" 1, 0, 0
    			set nc_necro_skill to ( nc_necro_skill + 1 )
    			MessageBox "Ваше искусство Некромантии увеличилось до %g.", nc_necro_skill
#endif /* ABCD */
    		endif
#ifndef ABCD
    		set state to 2
#endif /* ! ABCD */
    	endif
#ifdef ABCD
    	set state to 2
    endif
#endif /* ABCD */
    
#ifndef ABCD
    	if ( state == 2 )
    		enableplayerlooking
    		stopscript nc_summon_imperial
    	endif
#else /* ABCD */
    if ( state == 2 )
    	enableplayercontrols
    	enableplayerlooking
    	stopscript nc_summon_imperial
    endif
#endif /* ABCD */
    
    end

