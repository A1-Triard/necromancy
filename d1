SCTX
    begin nc_fire_corpse_obj
    
    short counter
    
    if ( GetDisabled == 1 )
    	set counter to ( counter + 1 )
    	if ( counter < 10 )
    		return
    	endif
    	setdelete 1
    	return
    endif
    
    if ( OnActivate )
    	if ( nc_has_corpse == 0 )
    		disable
    		player-> additem "nc_fire_pauldron" 1
    	else
#ifndef ABCD
    		MessageBox "Нести больше, чем один скелет за раз - неудобно."
#else /* ABCD */
    		MessageBox "Нести больше, чем одно тело за раз - неудобно."
#endif /* ABCD */
    	endif
    endif
    
    end
    

